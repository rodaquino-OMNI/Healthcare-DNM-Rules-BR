<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_VAS_VEN_007"
             name="Sindrome Pos-Trombotica - Prevencao e Tratamento"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    VAS-VEN-007 - Sindrome Pos-Trombotica - Prevencao e Tratamento
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Cirurgia Vascular / Angiologia
    Categoria: Doenca Venosa Cronica
    TUSS Relacionados: 30907020, 30907039, 40907100

    Sindrome Pos-Trombotica (SPT):
    - Complicacao cronica de TVP (20-50% dos pacientes)
    - Resulta de obstrucao residual + refluxo + disfuncao valvular
    - Sintomas: dor, edema, peso, alteracoes cutaneas, ulcera

    Classificacao Villalta:
    - Ausente: 0-4 pontos
    - Leve: 5-9 pontos
    - Moderada: 10-14 pontos
    - Grave: 15+ pontos ou ulcera venosa

    Prevencao (CHEST 2016):
    - Anticoagulacao adequada: previne extensao e recorrencia
    - Compressao elastica: controverso (estudo SOX negativo)
    - Trombolise em TVP iliofemoral: reduz SPT (estudo ATTRACT)

    Tratamento:
    - Conservador: compressao, exercicio, elevacao
    - Intervencao: stent venoso iliaco se obstrucao significativa
    - Ablacao de refluxo superficial: se contribui para hipertensao venosa

    Referencias Normativas:
    - CHEST 2016 - Antithrombotic Guidelines
    - SOX Trial 2014
    - ATTRACT Trial 2017
    - SVS/AVF 2012 - Iliac Vein Stenting
    - DUT ANS 2024
  -->

  <decision id="Decision_VAS_VEN_007" name="Criterios de Manejo - Sindrome Pos-Trombotica">
    <decisionTable id="DecisionTable_VAS_VEN_007" hitPolicy="FIRST">

      <!-- Input 1: Classificacao Villalta -->
      <input id="Input_villalta" label="Classificacao Villalta (Gravidade)">
        <inputExpression id="InputExpr_villalta" typeRef="string">
          <text>classificacaoVillalta</text>
        </inputExpression>
        <inputValues id="InputValues_villalta">
          <text>"Ausente_0_4","Leve_5_9","Moderada_10_14","Grave_15_Mais","Ulcera_Ativa"</text>
        </inputValues>
      </input>

      <!-- Input 2: Componente Obstrutivo -->
      <input id="Input_obstrucao" label="Obstrucao Iliofemoral Residual">
        <inputExpression id="InputExpr_obstrucao" typeRef="string">
          <text>obstrucaoResidual</text>
        </inputExpression>
        <inputValues id="InputValues_obstrucao">
          <text>"Ausente","Menor_50","Maior_50","Oclusao_Total","Nao_Avaliado"</text>
        </inputValues>
      </input>

      <!-- Input 3: Refluxo Superficial -->
      <input id="Input_refluxo" label="Refluxo Superficial Associado">
        <inputExpression id="InputExpr_refluxo" typeRef="string">
          <text>refluxoSuperficial</text>
        </inputExpression>
        <inputValues id="InputValues_refluxo">
          <text>"Ausente","Presente_Leve","Presente_Significativo","Nao_Avaliado"</text>
        </inputValues>
      </input>

      <!-- Input 4: Tratamento Conservador -->
      <input id="Input_conservador" label="Tratamento Conservador Previo">
        <inputExpression id="InputExpr_conservador" typeRef="string">
          <text>tratamentoConservador</text>
        </inputExpression>
        <inputValues id="InputValues_conservador">
          <text>"Nao_Realizado","Parcial_Menos_6_Meses","Completo_Falhou"</text>
        </inputValues>
      </input>

      <!-- Input 5: Documentacao -->
      <input id="Input_documentacao" label="Duplex + IVUS/Venografia">
        <inputExpression id="InputExpr_documentacao" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>

      <!-- Outputs -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- ============================================== -->
      <!-- REGRAS DE REPROVACAO                          -->
      <!-- ============================================== -->

      <rule id="Rule_VAS_VEN_007_Reprovacao_SemDoc">
        <description>Reprovacao - Documentacao incompleta</description>
        <inputEntry id="IE_Rep1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"Documentacao incompleta - duplex venoso + avaliacao de obstrucao iliaca (IVUS ou venografia) obrigatorios para planejar intervencao"</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"SVS/AVF 2012 - Mapeamento venoso completo necessario antes de intervencao"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_VEN_007_Reprovacao_SemConservador">
        <description>Reprovacao - SPT leve/moderada sem tratamento conservador</description>
        <inputEntry id="IE_Rep2_1"><text>"Leve_5_9","Moderada_10_14"</text></inputEntry>
        <inputEntry id="IE_Rep2_2"><text>"Ausente","Menor_50"</text></inputEntry>
        <inputEntry id="IE_Rep2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep2_4"><text>"Nao_Realizado","Parcial_Menos_6_Meses"</text></inputEntry>
        <inputEntry id="IE_Rep2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Rep2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep2_2"><text>"Intervencao NAO indicada - SPT leve/moderada sem obstrucao significativa - tratamento conservador (compressao + exercicio) por minimo 6 meses"</text></outputEntry>
        <outputEntry id="OE_Rep2_3"><text>"CHEST 2016 - Tratamento conservador primeira linha em SPT sem obstrucao"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_VEN_007_Reprovacao_Ausente">
        <description>Reprovacao - Villalta ausente (sem SPT)</description>
        <inputEntry id="IE_Rep3_1"><text>"Ausente_0_4"</text></inputEntry>
        <inputEntry id="IE_Rep3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Rep3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep3_2"><text>"Intervencao NAO indicada - Villalta menor 5 indica ausencia de SPT clinicamente significativa - manter vigilancia"</text></outputEntry>
        <outputEntry id="OE_Rep3_3"><text>"Villalta Score - SPT definida por escore 5 ou mais pontos"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- REGRAS DE APROVACAO                           -->
      <!-- ============================================== -->

      <rule id="Rule_VAS_VEN_007_Aprovacao_Ulcera_Obstrucao">
        <description>Aprovacao - SPT grave com ulcera e obstrucao</description>
        <inputEntry id="IE_Apr1_1"><text>"Ulcera_Ativa"</text></inputEntry>
        <inputEntry id="IE_Apr1_2"><text>"Maior_50","Oclusao_Total"</text></inputEntry>
        <inputEntry id="IE_Apr1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr1_2"><text>"Stent venoso iliaco APROVADO - SPT com ulcera ativa e obstrucao iliofemoral significativa - correcao do outflow acelera cicatrizacao"</text></outputEntry>
        <outputEntry id="OE_Apr1_3"><text>"SVS/AVF 2012 Classe I - Stent indicado em SPT grave com obstrucao"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_VEN_007_Aprovacao_Grave_Obstrucao">
        <description>Aprovacao - SPT grave com obstrucao significativa</description>
        <inputEntry id="IE_Apr2_1"><text>"Grave_15_Mais"</text></inputEntry>
        <inputEntry id="IE_Apr2_2"><text>"Maior_50","Oclusao_Total"</text></inputEntry>
        <inputEntry id="IE_Apr2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr2_4"><text>"Completo_Falhou"</text></inputEntry>
        <inputEntry id="IE_Apr2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr2_2"><text>"Stent venoso iliaco APROVADO - SPT Villalta grave com obstrucao maior 50% e falha de tratamento conservador"</text></outputEntry>
        <outputEntry id="OE_Apr2_3"><text>"SVS/AVF 2012 Classe IIa - Stenting em SPT sintomatica refrataria"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_VEN_007_Aprovacao_Refluxo">
        <description>Aprovacao - SPT com refluxo superficial significativo</description>
        <inputEntry id="IE_Apr3_1"><text>"Moderada_10_14","Grave_15_Mais"</text></inputEntry>
        <inputEntry id="IE_Apr3_2"><text>"Ausente","Menor_50"</text></inputEntry>
        <inputEntry id="IE_Apr3_3"><text>"Presente_Significativo"</text></inputEntry>
        <inputEntry id="IE_Apr3_4"><text>"Completo_Falhou"</text></inputEntry>
        <inputEntry id="IE_Apr3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr3_2"><text>"Ablacao de refluxo superficial APROVADA - SPT moderada/grave com refluxo superficial significativo contribuindo para hipertensao venosa"</text></outputEntry>
        <outputEntry id="OE_Apr3_3"><text>"ESVS 2015 - Ablacao de refluxo superficial indicada mesmo com doenca profunda"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_VEN_007_Aprovacao_Trombolise">
        <description>Aprovacao - Trombolise em TVP iliofemoral aguda para prevencao</description>
        <inputEntry id="IE_Apr4_1"><text>"Ausente_0_4"</text></inputEntry>
        <inputEntry id="IE_Apr4_2"><text>"Oclusao_Total"</text></inputEntry>
        <inputEntry id="IE_Apr4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr4_2"><text>"Trombolise + stent APROVADO para PREVENCAO de SPT - TVP iliofemoral aguda com oclusao total - intervencao precoce reduz incidencia de SPT (ATTRACT)"</text></outputEntry>
        <outputEntry id="OE_Apr4_3"><text>"ATTRACT Trial 2017 - Trombolise reduz SPT grave em TVP iliofemoral"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- REGRAS DE PENDENCIA                           -->
      <!-- ============================================== -->

      <rule id="Rule_VAS_VEN_007_Pendente_Obstrucao">
        <description>Pendente - Obstrucao nao avaliada</description>
        <inputEntry id="IE_Pend1_1"><text>"Moderada_10_14","Grave_15_Mais","Ulcera_Ativa"</text></inputEntry>
        <inputEntry id="IE_Pend1_2"><text>"Nao_Avaliado"</text></inputEntry>
        <inputEntry id="IE_Pend1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Pend1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend1_2"><text>"Obstrucao iliofemoral nao avaliada - realizar IVUS ou venografia para identificar componente obstrutivo corrigivel"</text></outputEntry>
        <outputEntry id="OE_Pend1_3"><text>"SVS/AVF 2012 - IVUS essencial para avaliar obstrucao em SPT"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_VEN_007_Pendente_Refluxo">
        <description>Pendente - Refluxo nao avaliado</description>
        <inputEntry id="IE_Pend2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend2_3"><text>"Nao_Avaliado"</text></inputEntry>
        <inputEntry id="IE_Pend2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Pend2_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend2_2"><text>"Refluxo superficial nao avaliado - realizar duplex para identificar componente de refluxo corrigivel"</text></outputEntry>
        <outputEntry id="OE_Pend2_3"><text>"ESVS 2015 - Avaliacao de refluxo necessaria para planejamento"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- REGRA FALLBACK                                -->
      <!-- ============================================== -->

      <rule id="Rule_Fallback_VAS_VEN_007">
        <description>Fallback - Casos nao cobertos requerem analise manual</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso requer analise por especialista em doenca venosa pos-trombotica"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Auditoria especializada conforme RN 428/2017"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
