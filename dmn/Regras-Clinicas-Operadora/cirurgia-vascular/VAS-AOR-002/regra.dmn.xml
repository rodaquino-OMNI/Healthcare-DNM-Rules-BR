<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_VAS_AOR_002"
             name="Disseccao Aortica Tipo B - Tratamento Medico vs Cirurgico"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    VAS-AOR-002 - Disseccao Aortica Tipo B - Tratamento Medico vs Cirurgico
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Cirurgia Vascular / Cirurgia Cardiovascular
    Categoria: Cirurgia Aortica
    TUSS Relacionados: 30907044, 30907052, 40901130

    Classificacao Stanford:
    - Tipo A: Envolve aorta ascendente (emergencia cirurgica)
    - Tipo B: Nao envolve aorta ascendente

    Indicacoes para Intervencao em Tipo B (SVS/ESVS 2019):
    - Disseccao Tipo B Complicada:
      - Ruptura ou ruptura iminente
      - Ma perfusao visceral/renal/membros
      - Dor refrataria a analgesia
      - Hipertensao refrataria a medicacao
      - Progressao rapida da disseccao
      - Diametro maior que 4cm na fase aguda

    - Disseccao Tipo B Nao Complicada:
      - Tratamento clinico intensivo eh primeira linha
      - TEVAR eletivo pode ser considerado apos estabilizacao

    Documentacao Obrigatoria:
    - AngioTC com fase arterial e portal
    - Avaliacao de perfusao de orgaos
    - Monitoramento de funcao renal
    - Controle de pressao arterial documentado

    Referencias Normativas:
    - SVS/ESVS 2019 - Aortic Dissection Guidelines
    - INSTEAD-XL Trial - TEVAR vs BMT in Type B
    - ACC/AHA 2022
    - DUT ANS 2024
  -->

  <decision id="Decision_VAS_AOR_002" name="Criterios de Tratamento - Disseccao Aortica Tipo B">
    <decisionTable id="DecisionTable_VAS_AOR_002" hitPolicy="FIRST">

      <!-- Input 1: Classificacao da Disseccao -->
      <input id="Input_classificacao" label="Classificacao da Disseccao Tipo B">
        <inputExpression id="InputExpr_classificacao" typeRef="string">
          <text>classificacaoDisseccao</text>
        </inputExpression>
        <inputValues id="InputValues_classificacao">
          <text>"Complicada_Ruptura","Complicada_Ma_Perfusao","Complicada_Dor_Refrataria","Complicada_HAS_Refrataria","Nao_Complicada_Aguda","Nao_Complicada_Subaguda","Cronica"</text>
        </inputValues>
      </input>

      <!-- Input 2: Status de Perfusao -->
      <input id="Input_perfusao" label="Status de Perfusao de Orgaos">
        <inputExpression id="InputExpr_perfusao" typeRef="string">
          <text>statusPerfusao</text>
        </inputExpression>
        <inputValues id="InputValues_perfusao">
          <text>"Normal","Isquemia_Visceral","Isquemia_Renal","Isquemia_Membros","Multiplas_Isquemias"</text>
        </inputValues>
      </input>

      <!-- Input 3: Diametro da Aorta -->
      <input id="Input_diametro" label="Diametro da Aorta na Disseccao (cm)">
        <inputExpression id="InputExpr_diametro" typeRef="string">
          <text>diametroAorta</text>
        </inputExpression>
        <inputValues id="InputValues_diametro">
          <text>"Menor_4cm","Entre_4_5.5cm","Maior_5.5cm"</text>
        </inputValues>
      </input>

      <!-- Input 4: Fase Temporal -->
      <input id="Input_fase" label="Fase Temporal da Disseccao">
        <inputExpression id="InputExpr_fase" typeRef="string">
          <text>faseTemporal</text>
        </inputExpression>
        <inputValues id="InputValues_fase">
          <text>"Aguda_Menor_14dias","Subaguda_14_90dias","Cronica_Maior_90dias"</text>
        </inputValues>
      </input>

      <!-- Input 5: Documentacao de Imagem -->
      <input id="Input_documentacao" label="AngioTC com Avaliacao de Perfusao">
        <inputExpression id="InputExpr_documentacao" typeRef="boolean">
          <text>documentacaoImagem</text>
        </inputExpression>
      </input>

      <!-- Outputs -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- ============================================== -->
      <!-- REGRAS DE REPROVACAO                          -->
      <!-- ============================================== -->

      <rule id="Rule_VAS_AOR_002_Reprovacao_SemDoc">
        <description>Reprovacao - Documentacao de imagem inadequada</description>
        <inputEntry id="IE_Rep1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Rep1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"AngioTC com avaliacao de perfusao de orgaos eh obrigatoria para estratificacao e planejamento terapeutico"</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"SVS/ESVS 2019 - Imagem completa obrigatoria para manejo de disseccao aortica"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_AOR_002_Reprovacao_NaoComplicadaAguda">
        <description>Reprovacao TEVAR - Disseccao Tipo B nao complicada aguda sem indicacao de intervencao</description>
        <inputEntry id="IE_Rep2_1"><text>"Nao_Complicada_Aguda"</text></inputEntry>
        <inputEntry id="IE_Rep2_2"><text>"Normal"</text></inputEntry>
        <inputEntry id="IE_Rep2_3"><text>"Menor_4cm"</text></inputEntry>
        <inputEntry id="IE_Rep2_4"><text>"Aguda_Menor_14dias"</text></inputEntry>
        <inputEntry id="IE_Rep2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Rep2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep2_2"><text>"Disseccao Tipo B nao complicada aguda - TEVAR NAO indicado na fase aguda - tratamento clinico intensivo eh primeira linha"</text></outputEntry>
        <outputEntry id="OE_Rep2_3"><text>"SVS/ESVS 2019 Classe III - TEVAR nao indicado em Tipo B nao complicada aguda"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- REGRAS DE APROVACAO - EMERGENCIAS             -->
      <!-- ============================================== -->

      <rule id="Rule_VAS_AOR_002_Aprovacao_Ruptura">
        <description>Aprovacao Emergencia - Disseccao com ruptura</description>
        <inputEntry id="IE_Apr1_1"><text>"Complicada_Ruptura"</text></inputEntry>
        <inputEntry id="IE_Apr1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr1_2"><text>"TEVAR EMERGENCIAL APROVADO - disseccao Tipo B com ruptura ou ruptura iminente - indicacao vital imediata"</text></outputEntry>
        <outputEntry id="OE_Apr1_3"><text>"SVS/ESVS 2019 Classe I - Intervencao emergencial obrigatoria em ruptura"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_AOR_002_Aprovacao_MaPerfusao">
        <description>Aprovacao Emergencia - Ma perfusao de orgaos</description>
        <inputEntry id="IE_Apr2_1"><text>"Complicada_Ma_Perfusao"</text></inputEntry>
        <inputEntry id="IE_Apr2_2"><text>"Isquemia_Visceral","Isquemia_Renal","Isquemia_Membros","Multiplas_Isquemias"</text></inputEntry>
        <inputEntry id="IE_Apr2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr2_2"><text>"TEVAR EMERGENCIAL APROVADO - disseccao Tipo B complicada com sindrome de ma perfusao - risco de disfuncao organica irreversivel"</text></outputEntry>
        <outputEntry id="OE_Apr2_3"><text>"SVS/ESVS 2019 Classe I - TEVAR indicado em ma perfusao de orgaos"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_AOR_002_Aprovacao_DorRefrataria">
        <description>Aprovacao Urgencia - Dor refrataria</description>
        <inputEntry id="IE_Apr3_1"><text>"Complicada_Dor_Refrataria"</text></inputEntry>
        <inputEntry id="IE_Apr3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr3_4"><text>"Aguda_Menor_14dias"</text></inputEntry>
        <inputEntry id="IE_Apr3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr3_2"><text>"TEVAR APROVADO - disseccao Tipo B com dor refrataria a analgesia otimizada - indica instabilidade ou progressao da disseccao"</text></outputEntry>
        <outputEntry id="OE_Apr3_3"><text>"SVS/ESVS 2019 Classe I - Dor persistente eh marcador de complicacao"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_AOR_002_Aprovacao_HASRefrataria">
        <description>Aprovacao Urgencia - Hipertensao refrataria</description>
        <inputEntry id="IE_Apr4_1"><text>"Complicada_HAS_Refrataria"</text></inputEntry>
        <inputEntry id="IE_Apr4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr4_4"><text>"Aguda_Menor_14dias"</text></inputEntry>
        <inputEntry id="IE_Apr4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr4_2"><text>"TEVAR APROVADO - disseccao Tipo B com hipertensao refrataria a medicacao anti-hipertensiva otimizada"</text></outputEntry>
        <outputEntry id="OE_Apr4_3"><text>"SVS/ESVS 2019 Classe I - HAS refrataria indica instabilidade"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- REGRAS DE APROVACAO - ELETIVAS                -->
      <!-- ============================================== -->

      <rule id="Rule_VAS_AOR_002_Aprovacao_DilatacaoProgressiva">
        <description>Aprovacao Eletiva - Dilatacao aortica maior que 5.5cm pos-disseccao</description>
        <inputEntry id="IE_Apr5_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr5_2"><text>"Normal"</text></inputEntry>
        <inputEntry id="IE_Apr5_3"><text>"Maior_5.5cm"</text></inputEntry>
        <inputEntry id="IE_Apr5_4"><text>"Subaguda_14_90dias","Cronica_Maior_90dias"</text></inputEntry>
        <inputEntry id="IE_Apr5_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr5_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr5_2"><text>"TEVAR APROVADO - dilatacao aortica pos-disseccao maior que 5.5cm - risco aumentado de ruptura tardia"</text></outputEntry>
        <outputEntry id="OE_Apr5_3"><text>"SVS/ESVS 2019 Classe I - TEVAR indicado em dilatacao pos-disseccao maior que 5.5cm"</text></outputEntry>
      </rule>

      <rule id="Rule_VAS_AOR_002_Aprovacao_CronicaComplexa">
        <description>Aprovacao Eletiva - Disseccao cronica com anatomia favoravel para TEVAR profilatico</description>
        <inputEntry id="IE_Apr6_1"><text>"Cronica"</text></inputEntry>
        <inputEntry id="IE_Apr6_2"><text>"Normal"</text></inputEntry>
        <inputEntry id="IE_Apr6_3"><text>"Entre_4_5.5cm"</text></inputEntry>
        <inputEntry id="IE_Apr6_4"><text>"Cronica_Maior_90dias"</text></inputEntry>
        <inputEntry id="IE_Apr6_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr6_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr6_2"><text>"TEVAR ELETIVO APROVADO - disseccao cronica estabilizada com anatomia favoravel - INSTEAD-XL demonstrou beneficio de TEVAR preventivo"</text></outputEntry>
        <outputEntry id="OE_Apr6_3"><text>"INSTEAD-XL Trial - Beneficio de TEVAR em fase cronica para prevenir dilatacao"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- REGRAS DE PENDENCIA                           -->
      <!-- ============================================== -->

      <rule id="Rule_VAS_AOR_002_Pendente_SubagudaIntermedaria">
        <description>Pendente - Disseccao subaguda com diametro intermediario</description>
        <inputEntry id="IE_Pend1_1"><text>"Nao_Complicada_Subaguda"</text></inputEntry>
        <inputEntry id="IE_Pend1_2"><text>"Normal"</text></inputEntry>
        <inputEntry id="IE_Pend1_3"><text>"Entre_4_5.5cm"</text></inputEntry>
        <inputEntry id="IE_Pend1_4"><text>"Subaguda_14_90dias"</text></inputEntry>
        <inputEntry id="IE_Pend1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Pend1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend1_2"><text>"Disseccao Tipo B nao complicada subaguda com diametro intermediario - decisao entre TEVAR preventivo vs tratamento clinico continuado requer analise individualizada"</text></outputEntry>
        <outputEntry id="OE_Pend1_3"><text>"SVS/ESVS 2019 Classe IIb - Evidencia limitada para TEVAR em casos intermediarios"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- REGRA FALLBACK                                -->
      <!-- ============================================== -->

      <rule id="Rule_Fallback_VAS_AOR_002">
        <description>Fallback - Casos nao cobertos requerem analise manual</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso requer analise manual por Heart Team especializado em doenca aortica"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Auditoria especializada conforme RN 428/2017"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
