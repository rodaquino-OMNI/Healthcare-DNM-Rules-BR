<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_REC012"
             name="Expansores Teciduras"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    TUSS: REC-012
    Procedimento: Expansores Teciduas para Reconstrucao
    Especialidade: Cirurgia Reconstrutiva / Cirurgia Plastica
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS (SBCP 2022, ASPS 2023):
    - Expansao pre-reconstrucao mamaria
    - Alopecia cicatricial
    - Nevos gigantes
    - Cicatrizes extensas
    - Reconstrucao auricular

    PROTOCOLO:
    - Implante do expansor
    - Expansoes semanais (10-15% do volume)
    - Tempo total: 8-16 semanas
    - Segundo tempo: retalho ou protese definitiva

    REFERENCIAS TECNICAS:
    - SBCP 2022 - Expansores Teciduras
    - ASPS 2023 - Tissue Expansion
    - Radovan Principle (1982)
    - NCCN 2023 - Breast Reconstruction

    ===============================================================================
  -->

  <decision id="Decision_REC012" name="Regra Clinica - Expansores Teciduras">
    <decisionTable id="DecisionTable_REC012" hitPolicy="FIRST">

      <!-- ========== INPUTS (5) ========== -->

      <input id="Input_indicacaoExpansao" label="Indicacao da Expansao" camunda:inputVariable="indicacaoExpansao">
        <inputExpression id="InputExpression_indicacaoExpansao" typeRef="string">
          <text>indicacaoExpansao</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoExpansao">
          <text>"ReconstrucaoMamaria","AlopeciaCicatricial","NevoGigante","CicatrizExtensa","ReconstrucaoAuricular","DefeitoCongenito"</text>
        </inputValues>
      </input>

      <input id="Input_localExpansao" label="Local da Expansao" camunda:inputVariable="localExpansao">
        <inputExpression id="InputExpression_localExpansao" typeRef="string">
          <text>localExpansao</text>
        </inputExpression>
        <inputValues id="InputValues_localExpansao">
          <text>"Mama","Couro","Face","Tronco","Membro","Pescoco"</text>
        </inputValues>
      </input>

      <input id="Input_faseExpansao" label="Fase do Procedimento" camunda:inputVariable="faseExpansao">
        <inputExpression id="InputExpression_faseExpansao" typeRef="string">
          <text>faseExpansao</text>
        </inputExpression>
        <inputValues id="InputValues_faseExpansao">
          <text>"ImplanteExpansor","ExpansoesSemanais","SegundoTempo","TrocaProtese","RevisaoComplicacao"</text>
        </inputValues>
      </input>

      <input id="Input_complicacaoAtual" label="Complicacao Atual" camunda:inputVariable="complicacaoAtual">
        <inputExpression id="InputExpression_complicacaoAtual" typeRef="string">
          <text>complicacaoAtual</text>
        </inputExpression>
        <inputValues id="InputValues_complicacaoAtual">
          <text>"Infeccao","Exposicao","Ruptura","Dor","Nenhuma"</text>
        </inputValues>
      </input>

      <input id="Input_tratamentoOncologicoPendente" label="Tratamento Oncologico Pendente" camunda:inputVariable="tratamentoOncologicoPendente">
        <inputExpression id="InputExpression_tratamentoOncologicoPendente" typeRef="string">
          <text>tratamentoOncologicoPendente</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentoOncologicoPendente">
          <text>"RadioteraplaPendente","QuimioterapiaPendente","Nenhum","Concluido"</text>
        </inputValues>
      </input>

      <!-- ========== OUTPUTS (3 - Padrao TIER-1) ========== -->

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      <!-- Output 4: Alertas Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- ========== RULES ========== -->

      <!-- APROVACOES URGENTES - COMPLICACOES -->
      <rule id="Rule_Aprovado_InfeccaoExpansor">
        <description>Aprovado Urgente: Infeccao de expansor</description>
        <inputEntry id="InputEntry_INF_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_INF_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_INF_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_INF_4"><text>"Infeccao"</text></inputEntry>
        <inputEntry id="InputEntry_INF_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_INF_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_INF_observacao"><text>"URGENTE - Remocao de expansor infectado para prevenir sepse"</text></outputEntry>
        <outputEntry id="OutputEntry_INF_fundamentacao"><text>"SBCP 2022, ASPS 2023 - Infeccao de expansor requer remocao imediata"</text></outputEntry>
      <outputEntry id="OE_Aprovado_InfeccaoExpansor_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_InfeccaoExpansor_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ExposicaoExpansor">
        <description>Aprovado Urgente: Exposicao de expansor</description>
        <inputEntry id="InputEntry_EXP_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_EXP_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_EXP_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_EXP_4"><text>"Exposicao"</text></inputEntry>
        <inputEntry id="InputEntry_EXP_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_EXP_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_EXP_observacao"><text>"URGENTE - Exposicao de expansor - remocao ou cobertura imediata"</text></outputEntry>
        <outputEntry id="OutputEntry_EXP_fundamentacao"><text>"SBCP 2022 - Exposicao evolui para infeccao, intervencao imediata"</text></outputEntry>
      <outputEntry id="OE_Aprovado_ExposicaoExpansor_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_ExposicaoExpansor_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES - IMPLANTE -->
      <rule id="Rule_Aprovado_ImplanteMama">
        <description>Aprovado: Implante de expansor mamario</description>
        <inputEntry id="InputEntry_IM_1"><text>"ReconstrucaoMamaria"</text></inputEntry>
        <inputEntry id="InputEntry_IM_2"><text>"Mama"</text></inputEntry>
        <inputEntry id="InputEntry_IM_3"><text>"ImplanteExpansor"</text></inputEntry>
        <inputEntry id="InputEntry_IM_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_IM_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_IM_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_IM_observacao"><text>"Implante de expansor mamario para reconstrucao pos-mastectomia"</text></outputEntry>
        <outputEntry id="OutputEntry_IM_fundamentacao"><text>"Lei 12.802/2013, NCCN 2023 - Expansor e opcao para reconstrucao em dois tempos"</text></outputEntry>
      <outputEntry id="OE_Aprovado_ImplanteMama_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_ImplanteMama_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ImplanteCouro">
        <description>Aprovado: Expansor para alopecia cicatricial</description>
        <inputEntry id="InputEntry_IC_1"><text>"AlopeciaCicatricial"</text></inputEntry>
        <inputEntry id="InputEntry_IC_2"><text>"Couro"</text></inputEntry>
        <inputEntry id="InputEntry_IC_3"><text>"ImplanteExpansor"</text></inputEntry>
        <inputEntry id="InputEntry_IC_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_IC_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_IC_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_IC_observacao"><text>"Expansor para gerar tecido piloso adjacente a area de alopecia"</text></outputEntry>
        <outputEntry id="OutputEntry_IC_fundamentacao"><text>"SBCP 2022 - Expansao de couro cabeludo e padrao para alopecia cicatricial"</text></outputEntry>
      <outputEntry id="OE_Aprovado_ImplanteCouro_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_ImplanteCouro_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_NevoGigante">
        <description>Aprovado: Expansor para resseccao de nevo gigante</description>
        <inputEntry id="InputEntry_NG_1"><text>"NevoGigante"</text></inputEntry>
        <inputEntry id="InputEntry_NG_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_NG_3"><text>"ImplanteExpansor"</text></inputEntry>
        <inputEntry id="InputEntry_NG_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_NG_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_NG_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_NG_observacao"><text>"Expansao para resseccao seriada de nevo melanocitico gigante"</text></outputEntry>
        <outputEntry id="OutputEntry_NG_fundamentacao"><text>"SBCP 2022 - Nevos gigantes tem risco de melanoma, resseccao indicada"</text></outputEntry>
      <outputEntry id="OE_Aprovado_NevoGigante_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_NevoGigante_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES - EXPANSOES E SEGUNDO TEMPO -->
      <rule id="Rule_Aprovado_ExpansoesSemanais">
        <description>Aprovado: Sessoes de expansao semanal</description>
        <inputEntry id="InputEntry_ES_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_ES_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_ES_3"><text>"ExpansoesSemanais"</text></inputEntry>
        <inputEntry id="InputEntry_ES_4"><text>"Nenhuma","Dor"</text></inputEntry>
        <inputEntry id="InputEntry_ES_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_ES_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_ES_observacao"><text>"Sessoes de expansao semanal ambulatorial - 10-15% do volume por sessao"</text></outputEntry>
        <outputEntry id="OutputEntry_ES_fundamentacao"><text>"ASPS 2023 - Expansoes regulares otimizam resultado e reduzem tempo total"</text></outputEntry>
      <outputEntry id="OE_Aprovado_ExpansoesSemanais_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_ExpansoesSemanais_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_SegundoTempo">
        <description>Aprovado: Segundo tempo cirurgico</description>
        <inputEntry id="InputEntry_ST_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_ST_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_ST_3"><text>"SegundoTempo"</text></inputEntry>
        <inputEntry id="InputEntry_ST_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_ST_5"><text>"Nenhum","Concluido"</text></inputEntry>
        <outputEntry id="OutputEntry_ST_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_ST_observacao"><text>"Segundo tempo - troca por protese definitiva ou avanco de retalho"</text></outputEntry>
        <outputEntry id="OutputEntry_ST_fundamentacao"><text>"SBCP 2022 - Segundo tempo apos expansao completa e estabilizacao"</text></outputEntry>
      <outputEntry id="OE_Aprovado_SegundoTempo_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_SegundoTempo_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_TrocaProtese">
        <description>Aprovado: Troca de expansor por protese definitiva</description>
        <inputEntry id="InputEntry_TP_1"><text>"ReconstrucaoMamaria"</text></inputEntry>
        <inputEntry id="InputEntry_TP_2"><text>"Mama"</text></inputEntry>
        <inputEntry id="InputEntry_TP_3"><text>"TrocaProtese"</text></inputEntry>
        <inputEntry id="InputEntry_TP_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_TP_5"><text>"Concluido","Nenhum"</text></inputEntry>
        <outputEntry id="OutputEntry_TP_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_TP_observacao"><text>"Troca de expansor por protese mamaria definitiva"</text></outputEntry>
        <outputEntry id="OutputEntry_TP_fundamentacao"><text>"NCCN 2023, Lei 12.802/2013 - Segundo tempo da reconstrucao mamaria"</text></outputEntry>
      <outputEntry id="OE_Aprovado_TrocaProtese_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_TrocaProtese_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- PENDENTE -->
      <rule id="Rule_Pendente_RadioterapiaPendente">
        <description>Pendente: Radioterapia pendente</description>
        <inputEntry id="InputEntry_RP_1"><text>"ReconstrucaoMamaria"</text></inputEntry>
        <inputEntry id="InputEntry_RP_2"><text>"Mama"</text></inputEntry>
        <inputEntry id="InputEntry_RP_3"><text>"SegundoTempo","TrocaProtese"</text></inputEntry>
        <inputEntry id="InputEntry_RP_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_RP_5"><text>"RadioteraplaPendente"</text></inputEntry>
        <outputEntry id="OutputEntry_RP_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_RP_observacao"><text>"Aguardar conclusao da radioterapia antes do segundo tempo"</text></outputEntry>
        <outputEntry id="OutputEntry_RP_fundamentacao"><text>"NCCN 2023 - Radioterapia apos expansor pode ser mantida, troca apos RT"</text></outputEntry>
      <outputEntry id="OE_Pendente_RadioterapiaPendente_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Pendente_RadioterapiaPendente_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Pendente_Ruptura">
        <description>Pendente: Ruptura de expansor - avaliar</description>
        <inputEntry id="InputEntry_RUP_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_RUP_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_RUP_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_RUP_4"><text>"Ruptura"</text></inputEntry>
        <inputEntry id="InputEntry_RUP_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_RUP_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_RUP_observacao"><text>"Ruptura de expansor - avaliar se troca imediata ou segundo tempo antecipado"</text></outputEntry>
        <outputEntry id="OutputEntry_RUP_fundamentacao"><text>"SBCP 2022 - Ruptura sem infeccao permite troca eletiva"</text></outputEntry>
      <outputEntry id="OE_Pendente_Ruptura_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Pendente_Ruptura_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK RULE -->
      <rule id="Rule_Fallback_REC012">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Avaliar indicacao de expansao tecidual com equipe de cirurgia plastica"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"SBCP 2022, ASPS 2023 - Expansao tecidual requer planejamento individualizado"</text></outputEntry>
      <outputEntry id="OE_Fallback_REC012_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Fallback_REC012_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
