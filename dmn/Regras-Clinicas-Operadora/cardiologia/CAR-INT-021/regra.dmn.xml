<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-021" name="Janela Pericárdica Cirúrgica" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-021" name="Janela Pericárdica Cirúrgica">
    <decisionTable id="decisionTable_CAR-INT-021" hitPolicy="FIRST">
      <input id="input_1_car_int_021" label="Recorrenciaderrame" camunda:inputVariable="recorrenciaDerrame">
        <inputExpression id="inputExpression_1_car_int_021" typeRef="string">
          <text>recorrenciaDerrame</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_021" label="Localizacao" camunda:inputVariable="localizacao">
        <inputExpression id="inputExpression_2_car_int_021" typeRef="string">
          <text>localizacao</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_021" label="Etiologia" camunda:inputVariable="etiologia">
        <inputExpression id="inputExpression_3_car_int_021" typeRef="string">
          <text>etiologia</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_021" label="Condicaoclinica" camunda:inputVariable="condicaoClinica">
        <inputExpression id="inputExpression_4_car_int_021" typeRef="string">
          <text>condicaoClinica</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-021" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-021">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-021" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-021" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_021_r1">
        <inputEntry id="inputEntry_car_int_021_r1_1">
          <text>&gt;= 2_episodios</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r1_2">
          <text>posterior,loculated</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r1_3">
          <text>neoplasico</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r1_4">
          <text>estavel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_021_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_021_r1_2">
          <text>"Janela pericárdica: derrames recorrentes"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_021_r1_3">
          <text>"Recorrência pós-dreno ou derrames loculados: janela definitiva"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_021_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_021_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_021_r2">
        <inputEntry id="inputEntry_car_int_021_r2_1">
          <text>primeira_vez</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r2_2">
          <text>anterior</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r2_3">
          <text>idiopatico</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_021_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_021_r2_2">
          <text>"Primeiro episódio: pericardiocentese suficiente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_021_r2_3">
          <text>"Janela cirúrgica reservada para falhas drenagem percutânea"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_021_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_021_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_021_r3">
        <inputEntry id="inputEntry_car_int_021_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r3_3">
          <text>purulento</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r3_4">
          <text>sepse</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_021_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_021_r3_2">
          <text>"Janela urgente: pericardite purulenta"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_021_r3_3">
          <text>"Drenagem cirúrgica obrigatória em empiema pericárdico"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_021_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_021_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_021_r4">
        <inputEntry id="inputEntry_car_int_021_r4_1">
          <text>&gt;= 3_episodios</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_r4_4">
          <text>refratario_colchicina</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_021_r4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_021_r4_2">
          <text>"Janela para pericardite recorrente refratária"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_021_r4_3">
          <text>"Alternativa quando colchicina + corticoide falham"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_021_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_021_r4_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-021_fallback">
        <inputEntry id="inputEntry_car_int_021_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_021_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-021_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-021_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-021_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-021_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-021_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-021">
      <dmndi:DMNShape id="DMNShape_CAR-INT-021" dmnElementRef="CAR-INT-021">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
