<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_INT_006"
             name="Angioplastia Coronaria (PCI) - Lesao Uni/Biarterial"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-INT-006
    Procedimento: PCI - Intervencao Coronaria Percutanea
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    Referencias: ACC/AHA/SCAI PCI 2023, ESC Revascularization 2024
    ===============================================================================
  -->

  <decision id="Decision_CDQ_INT_006" name="Regra Clinica - PCI">
    <decisionTable id="DecisionTable_CDQ_INT_006" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_INT006" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_INT006" typeRef="string"><text>condicaoPaciente</text></inputExpression>
        <inputValues id="InputValues_condicaoPaciente_INT006"><text>"SCAComSST","SCAsSemSST","AnginaEstavel","IsquemiaSilenciosa","FFRPositivo"</text></inputValues>
      </input>

      <input id="Input_historicoClinico_INT006" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_INT006" typeRef="string"><text>historicoClinico</text></inputExpression>
        <inputValues id="InputValues_historicoClinico_INT006"><text>"Adequado","Incompleto","NaoDocumentado"</text></inputValues>
      </input>

      <input id="Input_classeIndicacao_INT006" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_INT006" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_INT006"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
      </input>

      <input id="Input_anatomiaCoronaria_INT006" label="Anatomia Coronaria" camunda:inputVariable="anatomiaCoronaria">
        <inputExpression id="InputExpression_anatomiaCoronaria_INT006" typeRef="string"><text>anatomiaCoronaria</text></inputExpression>
        <inputValues id="InputValues_anatomiaCoronaria_INT006"><text>"Uniarterial","Biarterial","TCE","Triarterial","SintaxBaixo","SintaxAlto"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_INT006" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_INT006" typeRef="boolean"><text>expectativaVidaSuperior1Ano</text></inputExpression>
      </input>

      <output id="Output_resultado_INT006" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_INT006"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_INT006" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao_INT006" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraind_ClasseIII_INT006">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="ie1_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie2_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie3_INT006"><text>"Classe III"</text></inputEntry>
        <inputEntry id="ie4_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie5_INT006"><text>-</text></inputEntry>
        <outputEntry id="oe1_INT006"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2_INT006"><text>"PCI nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="oe3_INT006"><text>"ACC/AHA/SCAI 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_INT006_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_INT006_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_INT006">
        <inputEntry id="ie1b_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie2b_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie3b_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie4b_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie5b_INT006"><text>false</text></inputEntry>
        <outputEntry id="oe1b_INT006"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2b_INT006"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="oe3b_INT006"><text>"ESC 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_INT006_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_INT006_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_SCA_INT006">
        <description>Aprovado: SCA com indicacao classe I</description>
        <inputEntry id="ie1c_INT006"><text>"SCAComSST","SCAsSemSST"</text></inputEntry>
        <inputEntry id="ie2c_INT006"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3c_INT006"><text>"Classe I"</text></inputEntry>
        <inputEntry id="ie4c_INT006"><text>"Uniarterial","Biarterial"</text></inputEntry>
        <inputEntry id="ie5c_INT006"><text>true</text></inputEntry>
        <outputEntry id="oe1c_INT006"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2c_INT006"><text>"PCI indicado para SCA"</text></outputEntry>
        <outputEntry id="oe3c_INT006"><text>"ACC/AHA 2023 - PCI primaria ou precoce em SCA"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_SCA_INT006_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_SCA_INT006_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_CIIa_INT006">
        <inputEntry id="ie1d_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie2d_INT006"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3d_INT006"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="ie4d_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie5d_INT006"><text>true</text></inputEntry>
        <outputEntry id="oe1d_INT006"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2d_INT006"><text>"Classe IIa - PCI razoavel"</text></outputEntry>
        <outputEntry id="oe3d_INT006"><text>"SCAI 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_CIIa_INT006_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_CIIa_INT006_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_CIIb_INT006">
        <inputEntry id="ie1e_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie2e_INT006"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3e_INT006"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="ie4e_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie5e_INT006"><text>true</text></inputEntry>
        <outputEntry id="oe1e_INT006"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2e_INT006"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="oe3e_INT006"><text>"ESC 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_CIIb_INT006_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_CIIb_INT006_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_Hist_INT006">
        <inputEntry id="ie1f_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie2f_INT006"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="ie3f_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie4f_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie5f_INT006"><text>-</text></inputEntry>
        <outputEntry id="oe1f_INT006"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2f_INT006"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="oe3f_INT006"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_INT006_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_INT006_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_INT006">
        <inputEntry id="ie1g_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie2g_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie3g_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie4g_INT006"><text>-</text></inputEntry>
        <inputEntry id="ie5g_INT006"><text>-</text></inputEntry>
        <outputEntry id="oe1g_INT006"><text>"Pendente"</text></outputEntry>
        <outputEntry id="oe2g_INT006"><text>"Heart Team discussion recomendada"</text></outputEntry>
        <outputEntry id="oe3g_INT006"><text>"ACC/AHA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_INT006_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_INT006_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
