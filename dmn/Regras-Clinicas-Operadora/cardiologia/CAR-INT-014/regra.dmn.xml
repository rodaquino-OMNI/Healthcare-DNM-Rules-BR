<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-014" name="OCT - Tomografia Coerência Óptica" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-014" name="OCT - Tomografia Coerência Óptica">
    <decisionTable id="decisionTable_CAR-INT-014" hitPolicy="FIRST">
      <input id="input_1_car_int_014" label="Indicacaoprimaria" camunda:inputVariable="indicacaoPrimaria">
        <inputExpression id="inputExpression_1_car_int_014" typeRef="string">
          <text>indicacaoPrimaria</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_014" label="Funcaorenal" camunda:inputVariable="funcaoRenal">
        <inputExpression id="inputExpression_2_car_int_014" typeRef="string">
          <text>funcaoRenal</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_014" label="Clearance Contraste" camunda:inputVariable="clearance Contraste">
        <inputExpression id="inputExpression_3_car_int_014" typeRef="string">
          <text>clearance Contraste</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_014" label="Acessoguiacateter" camunda:inputVariable="acessoGuiaCateter">
        <inputExpression id="inputExpression_4_car_int_014" typeRef="string">
          <text>acessoGuiaCateter</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-014" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-014">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-014" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-014" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_014_r1">
        <inputEntry id="inputEntry_car_int_014_r1_1">
          <text>trombose_stent,disseccao</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r1_2">
          <text>&gt;= 60</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r1_3">
          <text>rapido</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r1_4">
          <text>adequado</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_014_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_014_r1_2">
          <text>"OCT aprovado: investigação falha stent ou complicação"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_014_r1_3">
          <text>"Resolução 10-20μm superior IVUS para detecção trombose/dissecção"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_014_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_014_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_014_r2">
        <inputEntry id="inputEntry_car_int_014_r2_1">
          <text>rotina_ics</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_014_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_014_r2_2">
          <text>"OCT não rotineiro: sem superioridade vs IVUS"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_014_r2_3">
          <text>"Evidência ILUMIEN IV: equivalência OCT vs IVUS em outcomes"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_014_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_014_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_014_r3">
        <inputEntry id="inputEntry_car_int_014_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r3_2">
          <text>&lt; 30</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_014_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_014_r3_2">
          <text>"OCT contraindicado: IRC severa"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_014_r3_3">
          <text>"Flush contraste necessário: 10-15ml/pullback - risco nefrotoxicidade"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_014_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_014_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_014_r4">
        <inputEntry id="inputEntry_car_int_014_r4_1">
          <text>otimizacao_resultado</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r4_2">
          <text>&gt;= 45</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r4_3">
          <text>moderado</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_r4_4">
          <text>adequado</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_014_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_014_r4_2">
          <text>"OCT vs IVUS: considerar expertise local"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_014_r4_3">
          <text>"Ambas técnicas válidas: escolha baseada experiência operador"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_014_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_014_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-014_fallback">
        <inputEntry id="inputEntry_car_int_014_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_014_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-014_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-014_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-014_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-014_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-014_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-014">
      <dmndi:DMNShape id="DMNShape_CAR-INT-014" dmnElementRef="CAR-INT-014">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
