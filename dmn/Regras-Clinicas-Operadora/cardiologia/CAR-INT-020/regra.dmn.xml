<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-020" name="Pericardiocentese - Derrame Pericárdico" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-020" name="Pericardiocentese - Derrame Pericárdico">
    <decisionTable id="decisionTable_CAR-INT-020" hitPolicy="FIRST">
      <input id="input_1_car_int_020" label="Volumederrame" camunda:inputVariable="volumeDerrame">
        <inputExpression id="inputExpression_1_car_int_020" typeRef="string">
          <text>volumeDerrame</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_020" label="Tamponamento" camunda:inputVariable="tamponamento">
        <inputExpression id="inputExpression_2_car_int_020" typeRef="string">
          <text>tamponamento</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_020" label="Etiologiasuspeita" camunda:inputVariable="etiologiaSuspeita">
        <inputExpression id="inputExpression_3_car_int_020" typeRef="string">
          <text>etiologiaSuspeita</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_020" label="Disponibilidadeeco" camunda:inputVariable="disponibilidadeEco">
        <inputExpression id="inputExpression_4_car_int_020" typeRef="integer">
          <text>disponibilidadeEco</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-020" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-020">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-020" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-020" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_020_r1">
        <inputEntry id="inputEntry_car_int_020_r1_1">
          <text>&gt; 20mm</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r1_2">
          <text>presente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r1_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r1_4">
          <text>disponivel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_020_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_020_r1_2">
          <text>"Pericardiocentese urgente: tamponamento cardíaco"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_020_r1_3">
          <text>"Hipotensão + turgência jugular + pulso paradoxal: drenagem imediata"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_020_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_020_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_020_r2">
        <inputEntry id="inputEntry_car_int_020_r2_1">
          <text>&gt; 20mm</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r2_2">
          <text>ausente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r2_3">
          <text>neoplasico,tuberculose,purulento</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r2_4">
          <text>disponivel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_020_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_020_r2_2">
          <text>"Drenagem diagnóstica/terapêutica"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_020_r2_3">
          <text>"Derrames grandes sintomáticos ou necessidade diagnóstica"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_020_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_020_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_020_r3">
        <inputEntry id="inputEntry_car_int_020_r3_1">
          <text>&lt; 10mm</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_020_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_020_r3_2">
          <text>"Derrame pequeno: observação clínica"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_020_r3_3">
          <text>"Derrame &lt;10mm raramente sintomático - vigilância suficiente"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_020_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_020_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_020_r4">
        <inputEntry id="inputEntry_car_int_020_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_r4_4">
          <text>indisponivel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_020_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_020_r4_2">
          <text>"Pericardiocentese às cegas contraindicada"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_020_r4_3">
          <text>"Guia ecocardiográfico obrigatório - risco perfuração 1-2%"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_020_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_020_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-020_fallback">
        <inputEntry id="inputEntry_car_int_020_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_020_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-020_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-020_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-020_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-020_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-020_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-020">
      <dmndi:DMNShape id="DMNShape_CAR-INT-020" dmnElementRef="CAR-INT-020">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
