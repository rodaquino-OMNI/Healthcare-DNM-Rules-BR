<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-009" name="Watchman/LAA Closure - Oclusão Apêndice Atrial" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-009" name="Watchman/LAA Closure - Oclusão Apêndice Atrial">
    <decisionTable id="decisionTable_CAR-DEV-009" hitPolicy="FIRST">
      <input id="input_1_car_dev_009" label="Chadsscore" camunda:inputVariable="chadsScore">
        <inputExpression id="inputExpression_1_car_dev_009" typeRef="integer">
          <text>chadsScore</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_009" label="Contraind Icacaoanticoagulacao" camunda:inputVariable="contraind icacaoAnticoagulacao">
        <inputExpression id="inputExpression_2_car_dev_009" typeRef="string">
          <text>contraind icacaoAnticoagulacao</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_009" label="Sangramento Maior" camunda:inputVariable="sangramento Maior">
        <inputExpression id="inputExpression_3_car_dev_009" typeRef="string">
          <text>sangramento Maior</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_009" label="Anatomialaa" camunda:inputVariable="anatomiaLAA">
        <inputExpression id="inputExpression_4_car_dev_009" typeRef="string">
          <text>anatomiaLAA</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-009" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-009">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-009" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-009" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_009_r1">
        <inputEntry id="inputEntry_car_dev_009_r1_1">
          <text>&gt;= 2</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r1_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r1_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r1_4">
          <text>adequada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_009_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_009_r1_2">
          <text>"Aprovado Watchman por contraindicação anticoagulação"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_009_r1_3">
          <text>"CHA2DS2-VASc ≥2 + intolerância ACO (ESC 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_009_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_009_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_009_r2">
        <inputEntry id="inputEntry_car_dev_009_r2_1">
          <text>&gt;= 3</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r2_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r2_3">
          <text>recorrente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r2_4">
          <text>adequada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_009_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_009_r2_2">
          <text>"Aprovado LAA closure por sangramento recorrente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_009_r2_3">
          <text>"História 2+ eventos hemorrágicos maiores apesar ACO otimizado"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_009_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_009_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_009_r3">
        <inputEntry id="inputEntry_car_dev_009_r3_1">
          <text>&lt; 2</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_009_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_009_r3_2">
          <text>"Oclusão LAA não indicada: baixo risco embólico"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_009_r3_3">
          <text>"CHA2DS2-VASc &lt;2 não justifica procedimento"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_009_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_009_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_009_r4">
        <inputEntry id="inputEntry_car_dev_009_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_r4_4">
          <text>desfavoravel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_009_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_009_r4_2">
          <text>"Anatomia LAA inadequada para dispositivo"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_009_r4_3">
          <text>"Chicken wing ou diâmetro &gt;30mm contraindicam Watchman"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_009_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_009_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-009_fallback">
        <inputEntry id="inputEntry_car_dev_009_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_009_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-009_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-009_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-009_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-009_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-009_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-009">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-009" dmnElementRef="CAR-DEV-009">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
