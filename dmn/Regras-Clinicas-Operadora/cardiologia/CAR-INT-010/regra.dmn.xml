<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-010" name="Bifurcação Coronária - Técnica Stent" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-010" name="Bifurcação Coronária - Técnica Stent">
    <decisionTable id="decisionTable_CAR-INT-010" hitPolicy="FIRST">
      <input id="input_1_car_int_010" label="Tipobifurcacao" camunda:inputVariable="tipoBifurcacao">
        <inputExpression id="inputExpression_1_car_int_010" typeRef="string">
          <text>tipoBifurcacao</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_010" label="Diametroramolateral" camunda:inputVariable="diametroRamoLateral">
        <inputExpression id="inputExpression_2_car_int_010" typeRef="string">
          <text>diametroRamoLateral</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_010" label="Extensaodoenca" camunda:inputVariable="extensaoDoenca">
        <inputExpression id="inputExpression_3_car_int_010" typeRef="string">
          <text>extensaoDoenca</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_010" label="Angulobifurcacao" camunda:inputVariable="anguloBifurcacao">
        <inputExpression id="inputExpression_4_car_int_010" typeRef="string">
          <text>anguloBifurcacao</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-010" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-010">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-010" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-010" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_010_r1">
        <inputEntry id="inputEntry_car_int_010_r1_1">
          <text>medina_1_1_1</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r1_2">
          <text>&gt;= 2.5</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r1_3">
          <text>&gt; 10mm</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r1_4">
          <text>&lt; 70</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_010_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_010_r1_2">
          <text>"Técnica 2-stents aprovada: bifurcação verdadeira"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_010_r1_3">
          <text>"Medina 1,1,1 + ramo importante: DK-Crush ou Culotte (EBC 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_010_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_010_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_010_r2">
        <inputEntry id="inputEntry_car_int_010_r2_1">
          <text>medina_1_0_1,0_1_1</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r2_2">
          <text>&lt; 2.5</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_010_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_010_r2_2">
          <text>"Ramo lateral &lt;2.5mm não justifica 2-stents"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_010_r2_3">
          <text>"Provisional stenting suficiente para ramos pequenos"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_010_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_010_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_010_r3">
        <inputEntry id="inputEntry_car_int_010_r3_1">
          <text>medina_1_1_0,1_0_0</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_010_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_010_r3_2">
          <text>"Bifurcação não verdadeira: provisional stenting"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_010_r3_3">
          <text>"Técnica simples equivalente em bifurcações não verdadeiras"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_010_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_010_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_010_r4">
        <inputEntry id="inputEntry_car_int_010_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_r4_4">
          <text>&gt;= 70</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_010_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_010_r4_2">
          <text>"Angulação desfavorável: avaliar estratégia individualizada"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_010_r4_3">
          <text>"Ângulo ≥70° aumenta risco oclusão ramo: considerar imaging intracoronário"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_010_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_010_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-010_fallback">
        <inputEntry id="inputEntry_car_int_010_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_010_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-010_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-010_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-010_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-010_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-010_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-010">
      <dmndi:DMNShape id="DMNShape_CAR-INT-010" dmnElementRef="CAR-INT-010">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
