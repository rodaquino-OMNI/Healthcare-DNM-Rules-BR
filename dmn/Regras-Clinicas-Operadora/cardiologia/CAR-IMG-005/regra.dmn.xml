<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-IMG-005" name="AngioTC Coronárias - Apropriação" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-IMG-005" name="AngioTC Coronárias - Apropriação">
    <decisionTable id="decisionTable_CAR-IMG-005" hitPolicy="FIRST">
      <input id="input_1_car_img_005" label="Probabilidadedac" camunda:inputVariable="probabilidadeDAC">
        <inputExpression id="inputExpression_1_car_img_005" typeRef="integer">
          <text>probabilidadeDAC</text>
        </inputExpression>
      </input>
      <input id="input_2_car_img_005" label="Fc Basal" camunda:inputVariable="fc Basal">
        <inputExpression id="inputExpression_2_car_img_005" typeRef="string">
          <text>fc Basal</text>
        </inputExpression>
      </input>
      <input id="input_3_car_img_005" label="Clearancerenal" camunda:inputVariable="clearanceRenal">
        <inputExpression id="inputExpression_3_car_img_005" typeRef="string">
          <text>clearanceRenal</text>
        </inputExpression>
      </input>
      <input id="input_4_car_img_005" label="Sintomas" camunda:inputVariable="sintomas">
        <inputExpression id="inputExpression_4_car_img_005" typeRef="string">
          <text>sintomas</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-IMG-005" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-IMG-005">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-IMG-005" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-IMG-005" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_img_005_r1">
        <inputEntry id="inputEntry_car_img_005_r1_1">
          <text>baixa_intermediaria</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r1_2">
          <text>&lt; 65</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r1_3">
          <text>&gt; 60</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r1_4">
          <text>dor_atipica</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_005_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_005_r1_2">
          <text>"CCTA apropriada: baixa-intermediária probabilidade"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_005_r1_3">
          <text>"Excelente VPN 95-99% - afasta DAC com segurança (SCCT 2021)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_005_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_005_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_005_r2">
        <inputEntry id="inputEntry_car_img_005_r2_1">
          <text>alta</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r2_4">
          <text>anginaEstavel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_005_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_005_r2_2">
          <text>"Probabilidade alta: teste funcional ou cateterismo"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_005_r2_3">
          <text>"CCTA valor limitado quando probabilidade pré-teste &gt;70%"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_005_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_005_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_005_r3">
        <inputEntry id="inputEntry_car_img_005_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r3_2">
          <text>&gt; 75</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_005_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_005_r3_2">
          <text>"FC elevada compromete qualidade CCTA"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_005_r3_3">
          <text>"Artefato movimento: pré-tratamento betabloqueador ou considerar alternativa"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_005_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_005_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_005_r4">
        <inputEntry id="inputEntry_car_img_005_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r4_3">
          <text>&lt; 45</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_005_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_005_r4_2">
          <text>"IRC severa: evitar contraste iodado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_005_r4_3">
          <text>"TFG &lt;45: risco nefropatia contraste - considerar stress eco/SPECT"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_005_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_005_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-IMG-005_fallback">
        <inputEntry id="inputEntry_car_img_005_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_005_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-IMG-005_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-005_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-005_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-IMG-005_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-IMG-005_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-IMG-005">
      <dmndi:DMNShape id="DMNShape_CAR-IMG-005" dmnElementRef="CAR-IMG-005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
