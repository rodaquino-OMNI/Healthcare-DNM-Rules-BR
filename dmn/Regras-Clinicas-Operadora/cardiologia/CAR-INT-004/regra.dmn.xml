<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" id="Definitions_CAR_INT_004" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_CAR_INT_004" name="Critérios Ablação Fibrilação Atrial">
    <decisionTable id="DecisionTable_CAR_INT_004" hitPolicy="FIRST">
      <input id="Input_afType" label="Tipo Fibrilação Atrial">
        <inputExpression id="InputExpression_afType" typeRef="string">
          <text>atrialFibrillationType</text>
        </inputExpression>
        <inputValues id="UnaryTests_afType">
          <text>"PAROXYSMAL","PERSISTENT","LONG_PERSISTENT","PERMANENT"</text>
        </inputValues>
      </input>
      <input id="Input_aadFailure" label="Falha Antiarrítmicos (AAD)">
        <inputExpression id="InputExpression_aadFailure" typeRef="boolean">
          <text>hasAadFailure</text>
        </inputExpression>
      </input>
      <input id="Input_symptomaticSeverity" label="Sintomas Incapacitantes">
        <inputExpression id="InputExpression_symptomaticSeverity" typeRef="boolean">
          <text>hasDisablingSymptoms</text>
        </inputExpression>
      </input>
      <input id="Input_leftAtriumSize" label="Átrio Esquerdo &lt; 50mm">
        <inputExpression id="InputExpression_leftAtriumSize" typeRef="boolean">
          <text>hasNormalAtriumSize</text>
        </inputExpression>
      </input>
      <input id="Input_heartFailure" label="Insuficiência Cardíaca (FEVE &lt; 40%)">
        <inputExpression id="InputExpression_heartFailure" typeRef="boolean">
          <text>hasHeartFailure</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"APROVADO_ABLACAO","REPROVADO_ABLACAO","PENDENTE_AAD"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" name="fundamentacao" typeRef="string"/>
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- Ablação Aprovada - FA Paroxística -->
      <rule id="Rule_Ablation_Paroxysmal_AAD">
        <description>Ablação indicada: FA paroxística sintomática + falha AAD</description>
        <inputEntry id="UnaryTests_afType_1">
          <text>"PAROXYSMAL"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_1">
          <text>"APROVADO_ABLACAO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_1">
          <text>"Ablação indicada (FA paroxística + falha AAD + sintomas). HRS/EHRA Classe IA: sucesso 70-85% em centro experiente"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_1">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Ablation_Paroxysmal_AAD_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Ablation_Paroxysmal_AAD_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Ablation_Paroxysmal_FirstLine">
        <description>Ablação primeira linha: FA paroxística + sintomas graves</description>
        <inputEntry id="UnaryTests_afType_2">
          <text>"PAROXYSMAL"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_2">
          <text>"APROVADO_ABLACAO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_2">
          <text>"Ablação primeira linha (FA paroxística + sintomas incapacitantes + AE favorável). ESC 2020: ablação vs AAD equivalente eficácia (NE=IIA)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_2">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Ablation_Paroxysmal_FirstLine_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Ablation_Paroxysmal_FirstLine_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- Ablação Aprovada - FA Persistente -->
      <rule id="Rule_Ablation_Persistent_AAD">
        <description>Ablação indicada: FA persistente + falha AAD</description>
        <inputEntry id="UnaryTests_afType_3">
          <text>"PERSISTENT"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_3">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_3">
          <text>"APROVADO_ABLACAO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_3">
          <text>"Ablação indicada (FA persistente + falha AAD + AE&lt;50mm). HRS/EHRA: sucesso 60-75% com isolamento veias pulmonares (NE=IB)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_3">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Ablation_Persistent_AAD_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Ablation_Persistent_AAD_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Ablation_HF_LVEF">
        <description>Ablação indicada: IC + FA (taquimiocardiopatia)</description>
        <inputEntry id="UnaryTests_afType_4">
          <text>"PAROXYSMAL","PERSISTENT"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_4">
          <text>"APROVADO_ABLACAO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_4">
          <text>"Ablação indicada (IC + FA). CASTLE-AF: ablação reduz mortalidade 38% e hospitalização 44% vs tratamento clínico (NE=IB)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_4">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Ablation_HF_LVEF_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Ablation_HF_LVEF_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- Reprovações -->
      <rule id="Rule_Reject_Permanent">
        <description>Ablação reprovada: FA permanente</description>
        <inputEntry id="UnaryTests_afType_5">
          <text>"PERMANENT"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_5">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_5">
          <text>"REPROVADO_ABLACAO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_5">
          <text>"Ablação reprovada (FA permanente aceita). Controle FC + anticoagulação estratégia estabelecida. Taxa sucesso &lt;30%"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_5">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Reject_Permanent_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reject_Permanent_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Reject_LargeAtrium">
        <description>Ablação reprovada: átrio muito dilatado</description>
        <inputEntry id="UnaryTests_afType_6">
          <text>"LONG_PERSISTENT"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_6">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_6">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_6">
          <text>"REPROVADO_ABLACAO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_6">
          <text>"Ablação reprovada (FA persistente longa + AE≥50mm). Remodelamento atrial avançado - taxa recorrência &gt;60% em 1 ano"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_6">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Reject_LargeAtrium_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reject_LargeAtrium_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Reject_NoAAD">
        <description>Ablação reprovada: sem tentativa AAD</description>
        <inputEntry id="UnaryTests_afType_7">
          <text>"PERSISTENT"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_7">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_7">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_7">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_7">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_7">
          <text>"REPROVADO_ABLACAO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_7">
          <text>"Ablação reprovada sem tentativa AAD. SBC: tratamento farmacológico primeira linha em FA oligossintomática (NE=IA)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_7">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Reject_NoAAD_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reject_NoAAD_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- Pendências -->
      <rule id="Rule_Pending_AAD_Trial">
        <description>Pendente: tentativa AAD antes de ablação</description>
        <inputEntry id="UnaryTests_afType_8">
          <text>"PAROXYSMAL","PERSISTENT"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_8">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_8">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_8">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_8">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_8">
          <text>"PENDENTE_AAD"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_8">
          <text>"Tentar antiarrítmico (betabloqueador, propafenona, amiodarona) por 3-6 meses. Ablação se falha/intolerância medicamentosa"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_8">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Pending_AAD_Trial_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Pending_AAD_Trial_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Pending_LongPersistent">
        <description>Pendente: FA persistente longa - avaliação criteriosa</description>
        <inputEntry id="UnaryTests_afType_9">
          <text>"LONG_PERSISTENT"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_9">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_9">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_9">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_9">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_9">
          <text>"PENDENTE_AAD"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_9">
          <text>"Avaliar ablação em centro experiente (FA &gt;1 ano). Discutir riscos: sucesso 40-60%, múltiplos procedimentos, complicações 3-5%"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_9">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Pending_LongPersistent_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Pending_LongPersistent_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- Regra de Fallback -->
      <rule id="Rule_Default_Fallback">
        <description>Fallback padrão</description>
        <inputEntry id="UnaryTests_afType_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_aadFailure_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_symptomaticSeverity_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_leftAtriumSize_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_heartFailure_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_fallback">
          <text>"PENDENTE_AAD"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_fallback">
          <text>"Necessária avaliação arritmologista. Classificar tipo FA, gravidade sintomas, e resposta antiarrítmicos"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_fallback">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Default_Fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_Fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_CAR_INT_004">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
