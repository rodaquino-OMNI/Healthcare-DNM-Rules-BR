        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_EP013_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_EP013_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
