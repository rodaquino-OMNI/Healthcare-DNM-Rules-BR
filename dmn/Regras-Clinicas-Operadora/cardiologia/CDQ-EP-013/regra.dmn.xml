      <input id="Input_classeIndicacao_EP013" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_EP013" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_EP013"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
      </input>

      <input id="Input_013" label="IdadePaciente" camunda:inputVariable="idadepaciente">
        <inputExpression id="InputExpression_013" typeRef="string"><text>idadepaciente</text></inputExpression>
        <inputValues id="InputValues_013"><text>"Pediatrico,Adulto"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_EP013" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
