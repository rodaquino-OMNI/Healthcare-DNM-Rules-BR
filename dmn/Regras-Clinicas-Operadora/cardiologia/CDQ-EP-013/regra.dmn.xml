        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_EP013_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_EP013">
        <inputEntry id="ie1d_EP013"><text>-</text></inputEntry>
        <inputEntry id="ie2d_EP013"><text>"Adequado"</text></inputEntry>
