      <rule id="Rule_Contraind_ClasseIII_EP013">
        <inputEntry id="ie1_EP013"><text>-</text></inputEntry>
        <inputEntry id="ie2_EP013"><text>-</text></inputEntry>
        <inputEntry id="ie3_EP013"><text>"Classe III"</text></inputEntry>
        <inputEntry id="ie4_EP013"><text>-</text></inputEntry>
        <inputEntry id="ie5_EP013"><text>-</text></inputEntry>
