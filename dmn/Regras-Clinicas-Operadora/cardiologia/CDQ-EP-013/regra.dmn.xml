      <input id="Input_condicaoPaciente_EP013" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_EP013" typeRef="string"><text>condicaoPaciente</text></inputExpression>
