      <output id="Output_resultado_EP013" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_EP013"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_EP013" label="Observacao Clinica" name="observacao" typeRef="string" />
