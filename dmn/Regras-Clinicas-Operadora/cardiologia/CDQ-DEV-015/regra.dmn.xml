<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_015"
             name="Extracao de Eletrodos Cardiacos"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-015
    Procedimento: Extracao de Eletrodos de Marcapasso/CDI
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    Infeccao dispositivo/endocardite, mau funcionamento eletrodo,
    necessidade de acessos venosos, trombose venosa sintomatica

    CONTRAINDICACOES:
    Risco cirurgico proibitivo, eletrodo funcionante sem complicacao

    REFERENCIAS TECNICAS:
    HRS Lead Extraction 2024, SBC/DECA 2023

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_015" name="Regra Clinica - Extracao Eletrodos">
    <decisionTable id="DecisionTable_CDQ_DEV_015" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_015" label="Indicacao Extracao" camunda:inputVariable="indicacaoExtracao">
        <inputExpression id="InputExpression_condicaoPaciente_015" typeRef="string">
          <text>indicacaoExtracao</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_015">
          <text>"InfeccaoDispositivo","EndocarditeCabo","MauFuncionamentoCabo","TromboseVenosaSintomatica","NecessidadeAcesso","RecallCabo"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_015" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_015" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_015">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_015" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_015" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_015">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_centroExperiencia_015" label="Centro com Experiencia Extracao" camunda:inputVariable="centroExperiencia">
        <inputExpression id="InputExpression_centroExperiencia_015" typeRef="boolean">
          <text>centroExperiencia</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_015" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_015" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_015" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_015">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_015" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_015" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_015">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_015_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_015_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_015_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_015_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_015_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_015_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_015_observacao"><text>"Extracao nao indicada - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_015_fundamentacao"><text>"HRS Lead Extraction 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_015_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_015_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_SemCentro_015">
        <description>Avaliar: Centro sem experiencia em extracao</description>
        <inputEntry id="InputEntry_Contraind_Centro_015_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Centro_015_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Centro_015_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Centro_015_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Centro_015_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Centro_015_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Centro_015_observacao"><text>"Encaminhar para centro de referencia em extracao"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Centro_015_fundamentacao"><text>"HRS 2024 - Extracao requer equipe experiente e suporte cirurgia cardiaca"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SemCentro_015_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SemCentro_015_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe I -->
      <rule id="Rule_Aprovado_ClasseI_Infeccao_015">
        <description>Aprovado: Infeccao/Endocardite</description>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_015_1"><text>"InfeccaoDispositivo","EndocarditeCabo"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_015_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_015_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_015_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_015_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Inf_015_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Inf_015_observacao"><text>"Extracao mandatoria por infeccao - Classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Inf_015_fundamentacao"><text>"HRS 2024, SBC/DECA 2023 - Extracao completa obrigatoria em infeccao"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Infeccao_015_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Infeccao_015_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_Cabo_015">
        <description>Aprovado: Mau funcionamento cabo/Recall</description>
        <inputEntry id="InputEntry_Aprovado_CI_Cab_015_1"><text>"MauFuncionamentoCabo","RecallCabo"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Cab_015_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Cab_015_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Cab_015_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Cab_015_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Cab_015_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Cab_015_observacao"><text>"Extracao indicada por mau funcionamento/recall"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Cab_015_fundamentacao"><text>"HRS 2024 - Recall classe I requer extracao"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Cabo_015_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Cabo_015_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_015">
        <description>Aprovado: Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_015_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_015_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_015_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_015_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_015_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_015_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_015_observacao"><text>"Classe IIa - extracao provavelmente util"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_015_fundamentacao"><text>"HRS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_015_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_015_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_015">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_015_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_015_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_015_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_015_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_015_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_015_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_015_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_015_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_015_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_015_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_015">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_015_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_015_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_015_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_015_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_015_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_015_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_015_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_015_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_015_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_015_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_015">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_015_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_015_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_015_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_015_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_015_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_015_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_015_observacao"><text>"Analise adicional por especialista em extracao"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_015_fundamentacao"><text>"HRS 2024 - Decisao multidisciplinar"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_015_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_015_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
