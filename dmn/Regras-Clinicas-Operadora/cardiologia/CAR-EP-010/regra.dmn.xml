<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-EP-010" name="Teste QT Longo - Epinefrina" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-EP-010" name="Teste QT Longo - Epinefrina">
    <decisionTable id="decisionTable_CAR-EP-010" hitPolicy="FIRST">
      <input id="input_1_car_ep_010" label="Qtcbasal" camunda:inputVariable="qtcBasal">
        <inputExpression id="inputExpression_1_car_ep_010" typeRef="string">
          <text>qtcBasal</text>
        </inputExpression>
      </input>
      <input id="input_2_car_ep_010" label="Sincope" camunda:inputVariable="sincope">
        <inputExpression id="inputExpression_2_car_ep_010" typeRef="string">
          <text>sincope</text>
        </inputExpression>
      </input>
      <input id="input_3_car_ep_010" label="Historiafamiliar" camunda:inputVariable="historiaFamiliar">
        <inputExpression id="inputExpression_3_car_ep_010" typeRef="string">
          <text>historiaFamiliar</text>
        </inputExpression>
      </input>
      <input id="input_4_car_ep_010" label="Genotipo" camunda:inputVariable="genotipo">
        <inputExpression id="inputExpression_4_car_ep_010" typeRef="string">
          <text>genotipo</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-EP-010" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-EP-010">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-EP-010" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-EP-010" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_ep_010_r1">
        <inputEntry id="inputEntry_car_ep_010_r1_1">
          <text>[440..470]</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r1_2">
          <text>esforco</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r1_3">
          <text>presente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r1_4">
          <text>desconhecido</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_010_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_010_r1_2">
          <text>"Teste epinefrina: QTc borderline + história"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_010_r1_3">
          <text>"Prolongamento &gt;30ms ou TdP induzida: diagnóstico SQTL"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_010_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_010_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_010_r2">
        <inputEntry id="inputEntry_car_ep_010_r2_1">
          <text>&gt; 480</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_010_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_010_r2_2">
          <text>"QTc claramente prolongado: diagnóstico clínico"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_010_r2_3">
          <text>"Critérios Schwartz ≥3.5: SQTL definido - teste desnecessário"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_010_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_010_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_010_r3">
        <inputEntry id="inputEntry_car_ep_010_r3_1">
          <text>&lt; 430</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r3_2">
          <text>ausente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r3_3">
          <text>negativa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_010_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_010_r3_2">
          <text>"QTc normal sem fatores risco: não indicado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_010_r3_3">
          <text>"Probabilidade SQTL baixíssima - teste não altera conduta"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_010_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_010_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_010_r4">
        <inputEntry id="inputEntry_car_ep_010_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_r4_4">
          <text>lqt2,lqt3</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_010_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_010_r4_2">
          <text>"Genótipo confirmado: teste não essencial"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_010_r4_3">
          <text>"Diagnóstico genético estabelece SQTL - teste funcional opcional"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_010_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_010_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-EP-010_fallback">
        <inputEntry id="inputEntry_car_ep_010_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_010_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-EP-010_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-010_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-010_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-EP-010_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-EP-010_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-EP-010">
      <dmndi:DMNShape id="DMNShape_CAR-EP-010" dmnElementRef="CAR-EP-010">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
