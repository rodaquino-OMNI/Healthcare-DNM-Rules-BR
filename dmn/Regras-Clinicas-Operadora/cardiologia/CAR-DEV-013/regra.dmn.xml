<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-013" name="ECMO - Indicação Suporte Cardiopulmonar" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-013" name="ECMO - Indicação Suporte Cardiopulmonar">
    <decisionTable id="decisionTable_CAR-DEV-013" hitPolicy="FIRST">
      <input id="input_1_car_dev_013" label="Indicacao" camunda:inputVariable="indicacao">
        <inputExpression id="inputExpression_1_car_dev_013" typeRef="string">
          <text>indicacao</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_013" label="Tempoparadacardiaca" camunda:inputVariable="tempoParadaCardiaca">
        <inputExpression id="inputExpression_2_car_dev_013" typeRef="integer">
          <text>tempoParadaCardiaca</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_013" label="Laktat" camunda:inputVariable="laktat">
        <inputExpression id="inputExpression_3_car_dev_013" typeRef="string">
          <text>laktat</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_013" label="Disfuncaobiventricular" camunda:inputVariable="disfuncaoBiventricular">
        <inputExpression id="inputExpression_4_car_dev_013" typeRef="string">
          <text>disfuncaoBiventricular</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-013" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-013">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-013" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-013" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_013_r1">
        <inputEntry id="inputEntry_car_dev_013_r1_1">
          <text>pcr_refrataria</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r1_2">
          <text>&lt; 60min</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r1_3">
          <text>&gt; 15</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r1_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_013_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_013_r1_2">
          <text>"eCPR: ECMO em PCR refratária"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_013_r1_3">
          <text>"Sobrevida 30% vs 15% RCP convencional se tempo &lt;60min"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_013_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_013_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_013_r2">
        <inputEntry id="inputEntry_car_dev_013_r2_1">
          <text>choque_cardiogenico</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r2_3">
          <text>&gt; 8</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r2_4">
          <text>presente</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_013_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_013_r2_2">
          <text>"VA-ECMO: choque refratário biventricular"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_013_r2_3">
          <text>"SCAI E: suporte completo cardiopulmonar até recuperação/Tx"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_013_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_013_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_013_r3">
        <inputEntry id="inputEntry_car_dev_013_r3_1">
          <text>insuficiencia_respiratoria</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_013_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_013_r3_2">
          <text>"VV-ECMO: SDRA severa refratária"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_013_r3_3">
          <text>"PaO2/FiO2 &lt;80 ou pH &lt;7.2 apesar VMI otimizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_013_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_013_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_013_r4">
        <inputEntry id="inputEntry_car_dev_013_r4_1">
          <text>pcr_prolongada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r4_2">
          <text>&gt; 60min</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_013_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_013_r4_2">
          <text>"Tempo isquemia cerebral excessivo"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_013_r4_3">
          <text>"PCR &gt;60min: mortalidade &gt;95% mesmo com ECMO"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_013_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_013_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-013_fallback">
        <inputEntry id="inputEntry_car_dev_013_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_013_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-013_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-013_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-013_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-013_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-013_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-013">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-013" dmnElementRef="CAR-DEV-013">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
