<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_car_dev_006" name="His Bundle Pacing - Estimulação Fisiológica" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-006" name="His Bundle Pacing vs RV Pacing">
    <decisionTable id="decisionTable_car_dev_006" hitPolicy="FIRST">
      <input id="input_feve_car_dev_006" label="FEVE" camunda:inputVariable="fracaoEjecao">
        <inputExpression id="inputExpression_feve_car_dev_006" typeRef="integer">
          <text>fracaoEjecao</text>
        </inputExpression>
      </input>
      <input id="input_qrs_car_dev_006" label="Duração QRS basal" camunda:inputVariable="duracaoQRS">
        <inputExpression id="inputExpression_qrs_car_dev_006" typeRef="integer">
          <text>duracaoQRS</text>
        </inputExpression>
      </input>
      <input id="input_burden_car_dev_006" label="% Estimulação Prevista" camunda:inputVariable="percentualEstimulacao">
        <inputExpression id="inputExpression_burden_car_dev_006" typeRef="integer">
          <text>percentualEstimulacao</text>
        </inputExpression>
      </input>
      <input id="input_ic_car_dev_006" label="Insuficiência Cardíaca" camunda:inputVariable="insuficienciaCardiaca">
        <inputExpression id="inputExpression_ic_car_dev_006" typeRef="boolean">
          <text>insuficienciaCardiaca</text>
        </inputExpression>
      </input>
      <output id="output_resultado_car_dev_006" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_car_dev_006">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_car_dev_006" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_car_dev_006" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- APROVADO: FEVE reduzida + alta carga estimulação -->
      <rule id="rule_car_dev_006_r1">
        <inputEntry id="inputEntry_car_dev_006_r1_1">
          <text>&lt; 40</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r1_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r1_3">
          <text>&gt; 40</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r1_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_006_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_r1_2">
          <text>"Aprovado His Bundle Pacing para prevenir cardiomiopatia induzida por estimulação"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_r1_3">
          <text>"FEVE &lt;40% + burden estimulação &gt;40%: risco 15-20% de piora função VE com RV pacing convencional. HBP mantém sincronia fisiológica (HRS Consensus 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_006_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_006_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVADO: IC + QRS alargado -->
      <rule id="rule_car_dev_006_r2">
        <inputEntry id="inputEntry_car_dev_006_r2_1">
          <text>&lt;= 50</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r2_2">
          <text>&gt;= 120</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_006_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_r2_2">
          <text>"Aprovado His Bundle Pacing como alternativa à TRC"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_r2_3">
          <text>"IC + FEVE ≤50% + QRS ≥120ms: HBP demonstra benefício similar TRC em subgrupos selecionados. Considerar se falha captação VE ou anatomia desfavorável (ESC Guidelines 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_006_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_006_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVADO: Função preservada + baixa carga -->
      <rule id="rule_car_dev_006_r3">
        <inputEntry id="inputEntry_car_dev_006_r3_1">
          <text>&gt;= 55</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r3_2">
          <text>&lt; 120</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r3_3">
          <text>&lt;= 20</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r3_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_006_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_r3_2">
          <text>"His Bundle Pacing não indicado: função preservada e baixo burden de estimulação"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_r3_3">
          <text>"Custo e complexidade técnica não justificados. RV pacing convencional adequado neste cenário (HRS 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_006_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_006_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- PENDENTE: Avaliação individualizada -->
      <rule id="rule_car_dev_006_r4">
        <inputEntry id="inputEntry_car_dev_006_r4_1">
          <text>[40..55)</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r4_3">
          <text>[20..40]</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_006_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_r4_2">
          <text>"Avaliar indicação individualizada considerando expertise do centro"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_r4_3">
          <text>"Zona cinzenta: considerar HBP se experiência local &gt;50 procedimentos. Alternativa: LBBP (Left Bundle Branch Pacing) em centros especializados (HRS 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_006_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_006_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_car_dev_006_fallback">
        <inputEntry id="inputEntry_car_dev_006_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_006_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_006_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_fb_2">
          <text>"Caso requer avaliação por eletrofisiologista com experiência em estimulação fisiológica"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_006_fb_3">
          <text>"HBP técnica emergente: considerar envio para centro de referência"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_006_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_006_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
