<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_010"
             name="Implante de CRT-P (Ressincronizador sem Desfibrilador)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-010
    Procedimento: Implante de CRT-P (Terapia de Ressincronizacao Cardiaca sem CDI)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    IC sintomatica com dissincronia, sem indicacao de CDI, pacientes idosos,
    comorbidades limitantes, preferencia do paciente

    CONTRAINDICACOES:
    QRS menor 130ms, expectativa vida muito limitada

    REFERENCIAS TECNICAS:
    SBC/DECA TRC 2023, ESC CRT Guidelines 2024

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_010" name="Regra Clinica - CRT-P">
    <decisionTable id="DecisionTable_CDQ_DEV_010" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_010" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_010" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_010">
          <text>"ICFEReduzida","ICFEModerada","BRECompleto","DissincroniaSevera","UpgradeMP"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_010" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_010" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_010">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_010" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_010" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_010">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_indicacaoCDI_010" label="Indicacao Concomitante CDI" camunda:inputVariable="indicacaoCDI">
        <inputExpression id="InputExpression_indicacaoCDI_010" typeRef="boolean">
          <text>indicacaoCDI</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_010" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_010" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_010" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_010">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_010" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_010" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_010">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_010_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_010_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_010_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_010_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_010_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_010_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_010_observacao"><text>"CRT-P nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_010_fundamentacao"><text>"SBC/DECA 2023 - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_010_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_010_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_010">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_010_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_010_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_010_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_010_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_010_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_010_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_010_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_010_fundamentacao"><text>"ESC 2024 - Beneficio requer sobrevida adequada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_010_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_010_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_IndicacaoCDI_010">
        <description>Avaliar: Se tem indicacao CDI, preferir CRT-D</description>
        <inputEntry id="InputEntry_Contraind_CDI_010_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CDI_010_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CDI_010_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CDI_010_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CDI_010_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_CDI_010_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CDI_010_observacao"><text>"Indicacao concomitante CDI - avaliar CRT-D em vez de CRT-P"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CDI_010_fundamentacao"><text>"SBC/DECA 2023 - Se indicacao CDI presente, CRT-D preferivel"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_IndicacaoCDI_010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_IndicacaoCDI_010_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe I -->
      <rule id="Rule_Aprovado_ClasseI_SemCDI_010">
        <description>Aprovado: Classe I sem indicacao CDI</description>
        <inputEntry id="InputEntry_Aprovado_CI_010_1"><text>"ICFEReduzida","BRECompleto","DissincroniaSevera"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_010_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_010_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_010_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_010_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_010_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_010_observacao"><text>"CRT-P indicado - sem necessidade CDI"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_010_fundamentacao"><text>"ESC 2024 - CRT-P adequado quando CDI nao indicado ou contraindicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_SemCDI_010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_SemCDI_010_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_Upgrade_010">
        <description>Aprovado: Upgrade de marcapasso para CRT-P</description>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_010_1"><text>"UpgradeMP"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_010_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_010_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_010_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_010_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Upg_010_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Upg_010_observacao"><text>"Upgrade MP para CRT-P por cardiomiopatia induzida"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Upg_010_fundamentacao"><text>"SBC/DECA 2023 - Upgrade indicado se FEVE deteriorou com estimulacao VD"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Upgrade_010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Upgrade_010_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_010">
        <description>Aprovado: Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_010_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_010_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_010_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_010_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_010_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_010_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_010_observacao"><text>"Classe IIa - CRT-P provavelmente util"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_010_fundamentacao"><text>"ESC 2024 - Beneficio maior Risco"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_010_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_010">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_010_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_010_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_010_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_010_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_010_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_010_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_010_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_010_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_010_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_010">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_010_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_010_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_010_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_010_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_010_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_010_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_010_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_010_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_010_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_010_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_010">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_010_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_010_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_010_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_010_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_010_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_010_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_010_observacao"><text>"Analise adicional necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_010_fundamentacao"><text>"Avaliacao individualizada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_010_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
