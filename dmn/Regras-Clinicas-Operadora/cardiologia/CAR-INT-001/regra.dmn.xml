<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" id="Definitions_CAR_INT_001" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_CAR_INT_001" name="ICP vs Cirurgia de Revascularização (SYNTAX Score)">
    <decisionTable id="DecisionTable_CAR_INT_001" hitPolicy="FIRST">
      <input id="Input_syntaxScore" label="SYNTAX Score">
        <inputExpression id="InputExpression_syntaxScore" typeRef="string">
          <text>syntaxScoreCategory</text>
        </inputExpression>
        <inputValues id="UnaryTests_syntaxScore">
          <text>"LOW","INTERMEDIATE","HIGH"</text>
        </inputValues>
      </input>
      <input id="Input_diabetes" label="Diabetes Mellitus">
        <inputExpression id="InputExpression_diabetes" typeRef="boolean">
          <text>hasDiabetes</text>
        </inputExpression>
      </input>
      <input id="Input_lvef" label="FEVE ≤ 50%">
        <inputExpression id="InputExpression_lvef" typeRef="boolean">
          <text>hasReducedLvef</text>
        </inputExpression>
      </input>
      <input id="Input_threeVessel" label="Triarterial">
        <inputExpression id="InputExpression_threeVessel" typeRef="boolean">
          <text>hasThreeVesselDisease</text>
        </inputExpression>
      </input>
      <input id="Input_surgicalRisk" label="Alto Risco Cirúrgico (EuroSCORE)">
        <inputExpression id="InputExpression_surgicalRisk" typeRef="boolean">
          <text>hasHighSurgicalRisk</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" name="fundamentacao" typeRef="string"/>
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- ICP Aprovada - SYNTAX Baixo -->
      <rule id="Rule_PCI_LowSyntax">
        <description>ICP indicada: SYNTAX baixo (0-22)</description>
        <inputEntry id="UnaryTests_syntaxScore_1">
          <text>"LOW"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_1">
          <text>"ICP aprovada (SYNTAX 0-22). ACC/AHA: equivalência ICP vs CRM em anatomia favorável. Menor morbimortalidade perioperatória"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_1">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_PCI_LowSyntax_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_PCI_LowSyntax_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_PCI_HighRisk">
        <description>ICP preferencial: alto risco cirúrgico</description>
        <inputEntry id="UnaryTests_syntaxScore_2">
          <text>"INTERMEDIATE"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_2">
          <text>true</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_2">
          <text>"ICP indicada (SYNTAX intermediário + alto risco cirúrgico). ESC: ICP preferencial quando EuroSCORE &gt;6% (NE=IIA)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_2">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_PCI_HighRisk_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_PCI_HighRisk_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- CRM Aprovada - SYNTAX Alto -->
      <rule id="Rule_CABG_HighSyntax">
        <description>CRM obrigatória: SYNTAX alto (≥33)</description>
        <inputEntry id="UnaryTests_syntaxScore_3">
          <text>"HIGH"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_3">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_3">
          <text>"Cirurgia obrigatória (SYNTAX ≥33). SYNTAX Trial: mortalidade CRM 50% menor vs ICP em anatomia complexa (NE=IA)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_3">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_CABG_HighSyntax_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_CABG_HighSyntax_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_CABG_Diabetes_ThreeVessel">
        <description>CRM preferencial: diabetes + triarterial</description>
        <inputEntry id="UnaryTests_syntaxScore_4">
          <text>"INTERMEDIATE"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_4">
          <text>"Cirurgia indicada (diabetes + triarterial + SYNTAX intermediário). FREEDOM Trial: CRM superior em diabéticos (NE=IA)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_4">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_CABG_Diabetes_ThreeVessel_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_CABG_Diabetes_ThreeVessel_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_CABG_LVEF_ThreeVessel">
        <description>CRM indicada: disfunção VE + triarterial</description>
        <inputEntry id="UnaryTests_syntaxScore_5">
          <text>"INTERMEDIATE"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_5">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_5">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_5">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_5">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_5">
          <text>"Cirurgia indicada (FEVE≤50% + triarterial). STICH Trial: CRM melhora sobrevida em disfunção VE + DAC extensa (NE=IB)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_5">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_CABG_LVEF_ThreeVessel_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_CABG_LVEF_ThreeVessel_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- Reprovação ICP -->
      <rule id="Rule_Reject_PCI_HighSyntax">
        <description>ICP reprovada: SYNTAX alto sem contraindicação cirúrgica</description>
        <inputEntry id="UnaryTests_syntaxScore_6">
          <text>"HIGH"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_6">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_6">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_6">
          <text>"ICP reprovada (SYNTAX ≥33). Risco reintervenção 2-3x maior. Encaminhar para avaliação cirurgia cardíaca"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_6">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Reject_PCI_HighSyntax_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reject_PCI_HighSyntax_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reject_PCI_Diabetes_Complex">
        <description>ICP reprovada: diabetes + anatomia complexa</description>
        <inputEntry id="UnaryTests_syntaxScore_7">
          <text>"HIGH"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_7">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_7">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_7">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_7">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_7">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_7">
          <text>"ICP contraindicada (diabetes + triarterial + SYNTAX alto). Mortalidade CRM 40% menor em 5 anos (FREEDOM/SYNTAX)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_7">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Reject_PCI_Diabetes_Complex_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reject_PCI_Diabetes_Complex_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- Pendências - Heart Team -->
      <rule id="Rule_Pending_Intermediate">
        <description>Pendente: SYNTAX intermediário sem critérios definidores</description>
        <inputEntry id="UnaryTests_syntaxScore_8">
          <text>"INTERMEDIATE"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_8">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_8">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_8">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_8">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_8">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_8">
          <text>"Avaliação Heart Team (SYNTAX 23-32). ESC: decisão compartilhada cardiologista intervencionista + cirurgião cardíaco (NE=IA)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_8">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Pending_Intermediate_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Pending_Intermediate_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- Regra de Fallback -->
      <rule id="Rule_Default_Fallback">
        <description>Fallback padrão</description>
        <inputEntry id="UnaryTests_syntaxScore_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_diabetes_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_lvef_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_threeVessel_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_surgicalRisk_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_observacao_fallback">
          <text>"Necessário cálculo SYNTAX Score. Encaminhar para Heart Team (cardiologia intervencionista + cirurgia cardíaca)"</text>
        </outputEntry>
      <outputEntry id="LiteralExpression_fundamentacao_fallback">
          <text>"Conforme protocolo ANS/TISS 2024"</text>
        </outputEntry>        <outputEntry id="OutputEntry_Rule_Default_Fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_Fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_CAR_INT_001">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
