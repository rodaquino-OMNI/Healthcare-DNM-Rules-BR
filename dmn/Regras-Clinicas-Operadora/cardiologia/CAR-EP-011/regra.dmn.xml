<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-EP-011" name="Estimulação Atrial Programada - Flutter" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-EP-011" name="Estimulação Atrial Programada - Flutter">
    <decisionTable id="decisionTable_CAR-EP-011" hitPolicy="FIRST">
      <input id="input_1_car_ep_011" label="Arritmiabasal" camunda:inputVariable="arritmiaBasal">
        <inputExpression id="inputExpression_1_car_ep_011" typeRef="string">
          <text>arritmiaBasal</text>
        </inputExpression>
      </input>
      <input id="input_2_car_ep_011" label="Objetivoestudo" camunda:inputVariable="objetivoEstudo">
        <inputExpression id="inputExpression_2_car_ep_011" typeRef="string">
          <text>objetivoEstudo</text>
        </inputExpression>
      </input>
      <input id="input_3_car_ep_011" label="Anatomiaad" camunda:inputVariable="anatomiaAD">
        <inputExpression id="inputExpression_3_car_ep_011" typeRef="string">
          <text>anatomiaAD</text>
        </inputExpression>
      </input>
      <input id="input_4_car_ep_011" label="Ablacaoplanejada" camunda:inputVariable="ablacaoPlanejada">
        <inputExpression id="inputExpression_4_car_ep_011" typeRef="string">
          <text>ablacaoPlanejada</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-EP-011" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-EP-011">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-EP-011" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-EP-011" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_ep_011_r1">
        <inputEntry id="inputEntry_car_ep_011_r1_1">
          <text>flutter_atipico</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r1_2">
          <text>mapear_circuito</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r1_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r1_4">
          <text>sim</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_011_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_011_r1_2">
          <text>"Mapeamento eletro-anatômico + estimulação"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_011_r1_3">
          <text>"Entrainment confirma circuito - orienta ablação flutter atípico"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_011_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_011_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_011_r2">
        <inputEntry id="inputEntry_car_ep_011_r2_1">
          <text>flutter_tipico</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r2_2">
          <text>confirmar_dependente_istmo</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r2_3">
          <text>normal</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r2_4">
          <text>sim</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_011_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_011_r2_2">
          <text>"Estimulação confirma istmo-dependência"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_011_r2_3">
          <text>"Flutter típico: ablação istmo cavo-tricuspídeo direta"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_011_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_011_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_011_r3">
        <inputEntry id="inputEntry_car_ep_011_r3_1">
          <text>ritmo_sinusal</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r3_4">
          <text>nao</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_011_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_011_r3_2">
          <text>"Sem arritmia basal: estimulação não diagnóstica"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_011_r3_3">
          <text>"Impossível mapear circuito sem arritmia presente"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_011_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_011_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_011_r4">
        <inputEntry id="inputEntry_car_ep_011_r4_1">
          <text>fa_concomitante</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r4_2">
          <text>diferenciar_fa_flutter</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_011_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_011_r4_2">
          <text>"FA + flutter: complexidade ablação aumenta"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_011_r4_3">
          <text>"Considerar ablação FA + linhas adicionais se ambas presentes"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_011_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_011_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-EP-011_fallback">
        <inputEntry id="inputEntry_car_ep_011_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_011_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-EP-011_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-011_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-011_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-EP-011_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-EP-011_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-EP-011">
      <dmndi:DMNShape id="DMNShape_CAR-EP-011" dmnElementRef="CAR-EP-011">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
