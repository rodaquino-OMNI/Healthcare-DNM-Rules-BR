<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_014"
             name="Troca de Gerador de Marcapasso/CDI"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-014
    Procedimento: Troca de Gerador (Marcapasso/CDI/CRT)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    Fim de vida bateria (ERI/EOL), necessidade upgrade sistema,
    recall de dispositivo, infeccao com necessidade reimplante

    CONTRAINDICACOES:
    Infeccao ativa no sitio, indicacao original nao mais presente

    REFERENCIAS TECNICAS:
    SBC/DECA Dispositivos 2023, HRS/EHRA 2024

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_014" name="Regra Clinica - Troca Gerador">
    <decisionTable id="DecisionTable_CDQ_DEV_014" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_014" label="Motivo Troca" camunda:inputVariable="motivoTroca">
        <inputExpression id="InputExpression_condicaoPaciente_014" typeRef="string">
          <text>motivoTroca</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_014">
          <text>"FimVidaBateria","UpgradeSistema","RecallDispositivo","ReimplanteAposInfeccao","MauFuncionamento"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_014" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_014" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_014">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_014" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_014" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_014">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_indicacaoOriginalValida_014" label="Indicacao Original Ainda Valida" camunda:inputVariable="indicacaoOriginalValida">
        <inputExpression id="InputExpression_indicacaoOriginalValida_014" typeRef="boolean">
          <text>indicacaoOriginalValida</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_014" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_014" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_014" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_014">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_014" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_014" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_014">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_014_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_014_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_014_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_014_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_014_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_014_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_014_observacao"><text>"Troca nao indicada - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_014_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_014_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_014_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_014">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_014_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_014_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_014_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_014_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_014_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_014_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_014_observacao"><text>"Avaliar beneficio real da troca dado prognostico"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_014_fundamentacao"><text>"HRS 2024 - Considerar desativacao vs troca em fim de vida"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_014_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_014_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_IndicacaoInvalida_014">
        <description>Contraindicacao: Indicacao original nao mais valida</description>
        <inputEntry id="InputEntry_Contraind_Ind_014_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Ind_014_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Ind_014_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Ind_014_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Ind_014_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Ind_014_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Ind_014_observacao"><text>"Reavaliar indicacao antes da troca"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Ind_014_fundamentacao"><text>"SBC/DECA 2023 - Ex: FEVE recuperou acima 35% apos CDI"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_IndicacaoInvalida_014_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_IndicacaoInvalida_014_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe I -->
      <rule id="Rule_Aprovado_ClasseI_Bateria_014">
        <description>Aprovado: Fim de vida bateria</description>
        <inputEntry id="InputEntry_Aprovado_CI_Bat_014_1"><text>"FimVidaBateria"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Bat_014_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Bat_014_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Bat_014_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Bat_014_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Bat_014_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Bat_014_observacao"><text>"Troca por ERI/EOL indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Bat_014_fundamentacao"><text>"SBC/DECA 2023 - Troca mandatoria em ERI se dependente"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Bateria_014_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Bateria_014_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_Recall_014">
        <description>Aprovado: Recall de dispositivo</description>
        <inputEntry id="InputEntry_Aprovado_CI_Rec_014_1"><text>"RecallDispositivo","MauFuncionamento"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Rec_014_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Rec_014_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Rec_014_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Rec_014_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Rec_014_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Rec_014_observacao"><text>"Troca por recall/mau funcionamento indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Rec_014_fundamentacao"><text>"HRS 2024 - Substituicao mandatoria em recall classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Recall_014_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Recall_014_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_Upgrade_014">
        <description>Aprovado: Upgrade de sistema</description>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_014_1"><text>"UpgradeSistema"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_014_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_014_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_014_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Upg_014_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Upg_014_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Upg_014_observacao"><text>"Upgrade indicado - nova indicacao adicionada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Upg_014_fundamentacao"><text>"SBC/DECA 2023 - Ex: MP para CRT, CDI para CRT-D"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Upgrade_014_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_Upgrade_014_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_014">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_014_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_014_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_014_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_014_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_014_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_014_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_014_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_014_fundamentacao"><text>"HRS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_014_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_014_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_014">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_014_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_014_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_014_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_014_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_014_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_014_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_014_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_014_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_014_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_014_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_014">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_014_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_014_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_014_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_014_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_014_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_014_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_014_observacao"><text>"Analise adicional necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_014_fundamentacao"><text>"Avaliacao especializada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_014_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_014_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
