        <inputEntry id="ie3f_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie4f_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie5f_HF002"><text>-</text></inputEntry>
        <outputEntry id="oe1f_HF002"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2f_HF002"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="oe3f_HF002"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_HF002_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_HF002_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_HF002">
        <inputEntry id="ie1g_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie2g_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie3g_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie4g_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie5g_HF002"><text>-</text></inputEntry>
        <outputEntry id="oe1g_HF002"><text>"Pendente"</text></outputEntry>
        <outputEntry id="oe2g_HF002"><text>"Analise adicional - Junta multidisciplinar IC"</text></outputEntry>
        <outputEntry id="oe3g_HF002"><text>"ISHLT MCS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_HF002_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
