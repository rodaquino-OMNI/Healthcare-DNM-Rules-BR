    Referencias: ISHLT MCS 2024, SBC IC 2023
    ===============================================================================
  -->

  <decision id="Decision_CDQ_HF_002" name="Regra Clinica - HF 002">
    <decisionTable id="DecisionTable_CDQ_HF_002" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_HF002" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_HF002" typeRef="string"><text>condicaoPaciente</text></inputExpression>
        <inputValues id="InputValues_condicaoPaciente_HF002"><text>"ICTerminalNaoCandidatoTx,ContraindicacaoTx,IdadeAvancada,ComorbidadesGraves"</text></inputValues>
      </input>

      <input id="Input_historicoClinico_HF002" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_HF002" typeRef="string"><text>historicoClinico</text></inputExpression>
        <inputValues id="InputValues_historicoClinico_HF002"><text>"Adequado","Incompleto","NaoDocumentado"</text></inputValues>
      </input>

      <input id="Input_classeIndicacao_HF002" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_HF002" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_HF002"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
      </input>

      <input id="Input_002" label="ScoreINTERMACS" camunda:inputVariable="scoreintermacs">
        <inputExpression id="InputExpression_002" typeRef="string"><text>scoreintermacs</text></inputExpression>
        <inputValues id="InputValues_002"><text>"1,2,3,4a7"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_HF002" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_HF002" typeRef="boolean"><text>expectativaVidaSuperior1Ano</text></inputExpression>
      </input>

      <output id="Output_resultado_HF002" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_HF002"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_HF002" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao_HF002" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraind_ClasseIII_HF002">
        <inputEntry id="ie1_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie2_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie3_HF002"><text>"Classe III"</text></inputEntry>
        <inputEntry id="ie4_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie5_HF002"><text>-</text></inputEntry>
        <outputEntry id="oe1_HF002"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2_HF002"><text>"Procedimento nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="oe3_HF002"><text>"ISHLT MCS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_HF002_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_HF002_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
