        <outputEntry id="oe2d_HF002"><text>"Procedimento razoavel - Classe IIa"</text></outputEntry>
        <outputEntry id="oe3d_HF002"><text>"SBC IC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_HF002_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_HF002_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_HF002">
        <inputEntry id="ie1e_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie2e_HF002"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3e_HF002"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="ie4e_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie5e_HF002"><text>true</text></inputEntry>
        <outputEntry id="oe1e_HF002"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2e_HF002"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="oe3e_HF002"><text>"SBC IC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_HF002_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_HF002_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

