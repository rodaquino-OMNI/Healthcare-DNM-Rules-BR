        <outputEntry id="oe2b_HF002"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="oe3b_HF002"><text>"SBC IC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_HF002_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_HF002_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_HF002">
        <inputEntry id="ie1c_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie2c_HF002"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3c_HF002"><text>"Classe I"</text></inputEntry>
        <inputEntry id="ie4c_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie5c_HF002"><text>true</text></inputEntry>
        <outputEntry id="oe1c_HF002"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2c_HF002"><text>"Procedimento indicado - Classe I"</text></outputEntry>
        <outputEntry id="oe3c_HF002"><text>"ISHLT MCS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_HF002_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_HF002_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_HF002">
        <inputEntry id="ie1d_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie2d_HF002"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3d_HF002"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="ie4d_HF002"><text>-</text></inputEntry>
