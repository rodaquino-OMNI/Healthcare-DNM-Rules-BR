        <inputEntry id="ie2b_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie3b_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie4b_HF002"><text>-</text></inputEntry>
        <inputEntry id="ie5b_HF002"><text>false</text></inputEntry>
