<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-EP-001" name="Loop Recorder Implantável - Indicação" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-EP-001" name="Loop Recorder Implantável - Indicação">
    <decisionTable id="decisionTable_CAR-EP-001" hitPolicy="FIRST">
      <input id="input_1_car_ep_001" label="Sintoma" camunda:inputVariable="sintoma">
        <inputExpression id="inputExpression_1_car_ep_001" typeRef="string">
          <text>sintoma</text>
        </inputExpression>
      </input>
      <input id="input_2_car_ep_001" label="Frequenciaevento" camunda:inputVariable="frequenciaEvento">
        <inputExpression id="inputExpression_2_car_ep_001" typeRef="string">
          <text>frequenciaEvento</text>
        </inputExpression>
      </input>
      <input id="input_3_car_ep_001" label="Investigacaopreviarnegativa" camunda:inputVariable="investigacaoPreviarNegativa">
        <inputExpression id="inputExpression_3_car_ep_001" typeRef="string">
          <text>investigacaoPreviarNegativa</text>
        </inputExpression>
      </input>
      <input id="input_4_car_ep_001" label="Impactoqualidadevida" camunda:inputVariable="impactoQualidadeVida">
        <inputExpression id="inputExpression_4_car_ep_001" typeRef="integer">
          <text>impactoQualidadeVida</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-EP-001" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-EP-001">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-EP-001" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-EP-001" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_ep_001_r1">
        <inputEntry id="inputEntry_car_ep_001_r1_1">
          <text>sincope_inexplicada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r1_2">
          <text>rara_imprevisivel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r1_3">
          <text>extensa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r1_4">
          <text>significativo</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_001_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_001_r1_2">
          <text>"Loop recorder: síncope inexplicada de alto risco"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_001_r1_3">
          <text>"Yield diagnóstico 75% em 12 meses vs 20% com estratégia convencional"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_001_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_001_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_001_r2">
        <inputEntry id="inputEntry_car_ep_001_r2_1">
          <text>palpitacao</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r2_2">
          <text>mensal</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r2_3">
          <text>holter_negativo</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r2_4">
          <text>leve</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_001_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_001_r2_2">
          <text>"Sintomas leves: loop externo ou Holter estendido"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_001_r2_3">
          <text>"Custo loop implantável não justificado para sintomas não-incapacitantes"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_001_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_001_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_001_r3">
        <inputEntry id="inputEntry_car_ep_001_r3_1">
          <text>sincope_explicada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_001_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_001_r3_2">
          <text>"Causa definida: monitorização adicional desnecessária"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_001_r3_3">
          <text>"Loop não altera conduta se diagnóstico já estabelecido"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_001_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_001_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_001_r4">
        <inputEntry id="inputEntry_car_ep_001_r4_1">
          <text>criptogenico_stroke</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_001_r4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_001_r4_2">
          <text>"Loop para FA oculta pós-AVC criptogênico"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_001_r4_3">
          <text>"CRYSTAL-AF: detecção FA 8.9% vs 1.4% controle - impacta anticoagulação"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_001_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_001_r4_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-EP-001_fallback">
        <inputEntry id="inputEntry_car_ep_001_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_001_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-EP-001_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-001_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-001_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-EP-001_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-EP-001_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-EP-001">
      <dmndi:DMNShape id="DMNShape_CAR-EP-001" dmnElementRef="CAR-EP-001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
