<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_HF_001"
             name="Avaliacao para Transplante Cardiaco"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-HF-001
    Procedimento: Avaliacao para Transplante Cardiaco
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    Referencias: ISHLT Transplant 2024, SBC IC 2023
    ===============================================================================
  -->

  <decision id="Decision_CDQ_HF_001" name="Regra Clinica - HF 001">
    <decisionTable id="DecisionTable_CDQ_HF_001" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_HF001" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_HF001" typeRef="string"><text>condicaoPaciente</text></inputExpression>
        <inputValues id="InputValues_condicaoPaciente_HF001"><text>"ICAvancadaRefrataria,ChoqueCerdiogenicoRecorrente,DependenteInootropico,ArritmiasRefratarias"</text></inputValues>
      </input>

      <input id="Input_historicoClinico_HF001" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_HF001" typeRef="string"><text>historicoClinico</text></inputExpression>
        <inputValues id="InputValues_historicoClinico_HF001"><text>"Adequado","Incompleto","NaoDocumentado"</text></inputValues>
      </input>

      <input id="Input_classeIndicacao_HF001" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_HF001" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_HF001"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
      </input>

      <input id="Input_001" label="ScoreHFSS" camunda:inputVariable="scorehfss">
        <inputExpression id="InputExpression_001" typeRef="string"><text>scorehfss</text></inputExpression>
        <inputValues id="InputValues_001"><text>"BaixoRisco,MedioRisco,AltoRisco"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_HF001" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_HF001" typeRef="boolean"><text>expectativaVidaSuperior1Ano</text></inputExpression>
      </input>

      <output id="Output_resultado_HF001" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_HF001"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_HF001" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao_HF001" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraind_ClasseIII_HF001">
        <inputEntry id="ie1_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie2_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie3_HF001"><text>"Classe III"</text></inputEntry>
        <inputEntry id="ie4_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie5_HF001"><text>-</text></inputEntry>
        <outputEntry id="oe1_HF001"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2_HF001"><text>"Procedimento nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="oe3_HF001"><text>"ISHLT Transplant 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_HF001_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_HF001_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_HF001">
        <inputEntry id="ie1b_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie2b_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie3b_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie4b_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie5b_HF001"><text>false</text></inputEntry>
        <outputEntry id="oe1b_HF001"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2b_HF001"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="oe3b_HF001"><text>"SBC IC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_HF001_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_HF001_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_HF001">
        <inputEntry id="ie1c_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie2c_HF001"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3c_HF001"><text>"Classe I"</text></inputEntry>
        <inputEntry id="ie4c_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie5c_HF001"><text>true</text></inputEntry>
        <outputEntry id="oe1c_HF001"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2c_HF001"><text>"Procedimento indicado - Classe I"</text></outputEntry>
        <outputEntry id="oe3c_HF001"><text>"ISHLT Transplant 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_HF001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_HF001_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_HF001">
        <inputEntry id="ie1d_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie2d_HF001"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3d_HF001"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="ie4d_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie5d_HF001"><text>true</text></inputEntry>
        <outputEntry id="oe1d_HF001"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2d_HF001"><text>"Procedimento razoavel - Classe IIa"</text></outputEntry>
        <outputEntry id="oe3d_HF001"><text>"SBC IC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_HF001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_HF001_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_HF001">
        <inputEntry id="ie1e_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie2e_HF001"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3e_HF001"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="ie4e_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie5e_HF001"><text>true</text></inputEntry>
        <outputEntry id="oe1e_HF001"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2e_HF001"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="oe3e_HF001"><text>"SBC IC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_HF001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_HF001_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_Hist_HF001">
        <inputEntry id="ie1f_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie2f_HF001"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="ie3f_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie4f_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie5f_HF001"><text>-</text></inputEntry>
        <outputEntry id="oe1f_HF001"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2f_HF001"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="oe3f_HF001"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_HF001_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_HF001_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_HF001">
        <inputEntry id="ie1g_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie2g_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie3g_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie4g_HF001"><text>-</text></inputEntry>
        <inputEntry id="ie5g_HF001"><text>-</text></inputEntry>
        <outputEntry id="oe1g_HF001"><text>"Pendente"</text></outputEntry>
        <outputEntry id="oe2g_HF001"><text>"Analise adicional - Junta multidisciplinar IC"</text></outputEntry>
        <outputEntry id="oe3g_HF001"><text>"ISHLT Transplant 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_HF001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_HF001_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
