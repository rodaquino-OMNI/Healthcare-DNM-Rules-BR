<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_020"
             name="Dispositivo de Oclusao de Apendice Atrial Esquerdo (LAAO)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-020
    Procedimento: LAAO - Left Atrial Appendage Occlusion (Watchman, Amulet)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    FA com indicacao anticoagulacao e contraindicacao/alto risco sangramento,
    historia sangramento maior sob anticoagulacao

    CONTRAINDICACOES:
    Trombo em AAE, endocardite, necessidade anticoagulacao por outra causa

    REFERENCIAS TECNICAS:
    ACC/AHA/HRS FA 2024, ESC EHRA LAAO 2024, SBC 2023

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_020" name="Regra Clinica - LAAO">
    <decisionTable id="DecisionTable_CDQ_DEV_020" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_020" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_020" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_020">
          <text>"FAContraindicacaoACO","FAAltoRiscoSangramento","FASangramentoMaiorPrevio","FAIntoleranciaACO"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_020" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_020" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_020">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_020" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_020" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_020">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_chadsVasc_020" label="CHA2DS2-VASc Score" camunda:inputVariable="chadsVasc">
        <inputExpression id="InputExpression_chadsVasc_020" typeRef="string">
          <text>chadsVasc</text>
        </inputExpression>
        <inputValues id="InputValues_chadsVasc_020">
          <text>"0","1","2","3ouMais"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_020" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_020" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_020" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_020">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_020" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_020" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_020">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_020_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_020_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_020_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_020_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_020_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_020_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_020_observacao"><text>"LAAO nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_020_fundamentacao"><text>"ACC/AHA FA 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_020_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_020_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_020">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_020_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_020_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_020_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_020_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_020_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_020_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_020_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_020_fundamentacao"><text>"ESC LAAO 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_020_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_020_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_CHADSBaixo_020">
        <description>Contraindicacao: CHA2DS2-VASc baixo - sem indicacao ACO</description>
        <inputEntry id="InputEntry_Contraind_CHADS_020_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CHADS_020_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CHADS_020_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CHADS_020_4"><text>"0"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CHADS_020_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_CHADS_020_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CHADS_020_observacao"><text>"CHA2DS2-VASc 0 - sem indicacao ACO, LAAO desnecessario"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CHADS_020_fundamentacao"><text>"ACC/AHA 2024 - LAAO apenas quando ACO indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_CHADSBaixo_020_alertasDesperdicio"><text>"ESC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_CHADSBaixo_020_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_ClasseI_020">
        <description>Aprovado: FA com contraindicacao ACO e alto risco AVC</description>
        <inputEntry id="InputEntry_Aprovado_CI_020_1"><text>"FAContraindicacaoACO","FASangramentoMaiorPrevio"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_020_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_020_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_020_4"><text>"2","3ouMais"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_020_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_020_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_020_observacao"><text>"LAAO indicado - contraindicacao ACO com alto risco AVC"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_020_fundamentacao"><text>"ACC/AHA FA 2024, PROTECT-AF/PREVAIL Trials - Alternativa eficaz a ACO"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_020_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_020_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_020">
        <description>Aprovado: FA alto risco sangramento</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_020_1"><text>"FAAltoRiscoSangramento","FAIntoleranciaACO"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_020_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_020_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_020_4"><text>"2","3ouMais"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_020_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_020_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_020_observacao"><text>"LAAO indicado - alto risco sangramento sob ACO"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_020_fundamentacao"><text>"ESC LAAO 2024 - HAS-BLED elevado com indicacao ACO"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_020_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_020_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_020">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_020_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_020_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_020_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_020_4"><text>"2","3ouMais"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_020_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_020_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_020_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_020_fundamentacao"><text>"SBC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_020_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_020_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_020">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_020_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_020_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_020_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_020_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_020_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_020_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_020_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_020_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_020_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_020_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_020">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_020_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_020_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_020_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_020_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_020_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_020_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_020_observacao"><text>"Analise Heart Team - ETE pre-procedimento obrigatorio"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_020_fundamentacao"><text>"ACC/AHA 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_020_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_020_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
