<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_EP_003"
             name="Ablacao de Taquicardia Ventricular (TV)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-EP-003
    Procedimento: Ablacao de Taquicardia Ventricular
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    TV monomorficasustentada sintomatica, TV com choques CDI,
    TV storm, TV idiopatica

    CONTRAINDICACOES:
    Trombo em VE, instabilidade hemodinamica nao controlada

    REFERENCIAS TECNICAS:
    HRS/EHRA VT Ablation 2023, SOBRAC 2023, VANISH Trial

    ===============================================================================
  -->

  <decision id="Decision_CDQ_EP_003" name="Regra Clinica - Ablacao TV">
    <decisionTable id="DecisionTable_CDQ_EP_003" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_EP003" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_EP003" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_EP003">
          <text>"TVMonomorfica","TVComChoquesCDI","TVStorm","TVIdiopatica","TVIsquemica"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_EP003" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_EP003" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_EP003">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_EP003" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_EP003" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_EP003">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_falhaAAD_EP003" label="Falha Antiarritmico" camunda:inputVariable="falhaAAD">
        <inputExpression id="InputExpression_falhaAAD_EP003" typeRef="string">
          <text>falhaAAD</text>
        </inputExpression>
        <inputValues id="InputValues_falhaAAD_EP003">
          <text>"FalhaAmiodarona","IntoleranciaSotalol","PrimeiraLinha","NaoTentado"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_EP003" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_EP003" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_EP003" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_EP003">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_EP003" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_EP003" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_EP003">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP003_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP003_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP003_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP003_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP003_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP003_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP003_observacao"><text>"Ablacao TV nao indicada - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP003_fundamentacao"><text>"HRS/EHRA VT 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_EP003_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_EP003_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_EP003">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP003_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP003_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP003_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP003_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP003_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP003_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP003_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP003_fundamentacao"><text>"SOBRAC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_EP003_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_EP003_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_TVStorm_EP003">
        <description>Aprovado: TV Storm - urgencia</description>
        <inputEntry id="InputEntry_Aprovado_Storm_EP003_1"><text>"TVStorm"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Storm_EP003_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Storm_EP003_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Storm_EP003_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Storm_EP003_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Storm_EP003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Storm_EP003_observacao"><text>"Ablacao TV Storm urgente indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Storm_EP003_fundamentacao"><text>"HRS/EHRA 2023 - Ablacao precoce em TV Storm"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_TVStorm_EP003_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_TVStorm_EP003_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_EP003">
        <description>Aprovado: TV com choques CDI apos falha AAD</description>
        <inputEntry id="InputEntry_Aprovado_CI_EP003_1"><text>"TVComChoquesCDI","TVMonomorfica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP003_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP003_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP003_4"><text>"FalhaAmiodarona","IntoleranciaSotalol"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP003_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP003_observacao"><text>"Ablacao TV indicada - falha AAD com choques recorrentes"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP003_fundamentacao"><text>"VANISH Trial - Ablacao superior escalonamento AAD"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_EP003_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_EP003_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_TVIdiopatica_EP003">
        <description>Aprovado: TV Idiopatica - primeira linha</description>
        <inputEntry id="InputEntry_Aprovado_Idio_EP003_1"><text>"TVIdiopatica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Idio_EP003_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Idio_EP003_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Idio_EP003_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Idio_EP003_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Idio_EP003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Idio_EP003_observacao"><text>"TV Idiopatica - ablacao curativa"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Idio_EP003_fundamentacao"><text>"HRS/EHRA 2023 - Alta taxa cura em TV via saida VD/Fascicular"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_TVIdiopatica_EP003_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_TVIdiopatica_EP003_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_EP003">
        <description>Aprovado: TV isquemica primeira linha</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP003_1"><text>"TVIsquemica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP003_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP003_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP003_4"><text>"PrimeiraLinha"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP003_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP003_observacao"><text>"Ablacao TV isquemica razoavel como primeira linha"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP003_fundamentacao"><text>"SOBRAC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_EP003_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_EP003_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_EP003">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_EP003_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP003_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP003_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP003_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP003_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP003_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP003_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP003_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_EP003_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_EP003_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_EP_003">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_EP003_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP003_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP003_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP003_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP003_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_EP003_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_EP003_observacao"><text>"Analise adicional - RNM cardiaca e mapeamento 3D"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_EP003_fundamentacao"><text>"HRS/EHRA VT 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_EP_003_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_EP_003_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
