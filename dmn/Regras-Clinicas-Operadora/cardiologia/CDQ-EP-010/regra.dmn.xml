        <inputExpression id="InputExpression_classeIndicacao_EP010" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_EP010"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
