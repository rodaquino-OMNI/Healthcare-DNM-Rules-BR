      <output id="Output_resultado_EP010" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_EP010"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_EP010" label="Observacao Clinica" name="observacao" typeRef="string" />
