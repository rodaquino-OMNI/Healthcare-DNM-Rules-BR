      <input id="Input_010" label="TipoMapeamento" camunda:inputVariable="tipomapeamento">
        <inputExpression id="InputExpression_010" typeRef="string"><text>tipomapeamento</text></inputExpression>
        <inputValues id="InputValues_010"><text>"CARTO,EnSite,Rhythmia"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_EP010" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
