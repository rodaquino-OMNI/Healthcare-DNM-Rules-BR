<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-018" name="Intervenção Tricúspide - TEER/Anuloplastia" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-018" name="Intervenção Tricúspide - TEER/Anuloplastia">
    <decisionTable id="decisionTable_CAR-DEV-018" hitPolicy="FIRST">
      <input id="input_1_car_dev_018" label="Grauit" camunda:inputVariable="grauIT">
        <inputExpression id="inputExpression_1_car_dev_018" typeRef="string">
          <text>grauIT</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_018" label="Etiologia" camunda:inputVariable="etiologia">
        <inputExpression id="inputExpression_2_car_dev_018" typeRef="string">
          <text>etiologia</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_018" label="Hipertensaopulmonar" camunda:inputVariable="hipertensaoPulmonar">
        <inputExpression id="inputExpression_3_car_dev_018" typeRef="string">
          <text>hipertensaoPulmonar</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_018" label="Feve" camunda:inputVariable="feve">
        <inputExpression id="inputExpression_4_car_dev_018" typeRef="integer">
          <text>feve</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-018" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-018">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-018" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-018" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_018_r1">
        <inputEntry id="inputEntry_car_dev_018_r1_1">
          <text>severa_torrencial</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r1_2">
          <text>secundaria</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r1_3">
          <text>moderada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r1_4">
          <text>&gt; 40</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_018_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_018_r1_2">
          <text>"TriClip aprovado: IT secundária severa sintomática"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_018_r1_3">
          <text>"TRILUMINATE: melhora QV + IC em IT secundária refratária"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_018_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_018_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_018_r2">
        <inputEntry id="inputEntry_car_dev_018_r2_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r2_2">
          <text>primaria_organica</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_018_r2_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_018_r2_2">
          <text>"IT primária: considerar cirurgia se concomitante"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_018_r2_3">
          <text>"Doença folhetos: resultado TEER menos previsível"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_018_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_018_r2_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_018_r3">
        <inputEntry id="inputEntry_car_dev_018_r3_1">
          <text>leve_moderada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_018_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_018_r3_2">
          <text>"IT não-severa: tratamento conservador"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_018_r3_3">
          <text>"IT &lt;severa sem sintomas: observação suficiente"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_018_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_018_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_018_r4">
        <inputEntry id="inputEntry_car_dev_018_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r4_3">
          <text>severa_irreversivel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_r4_4">
          <text>&lt; 30</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_018_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_018_r4_2">
          <text>"Alto risco: HP severa + FE reduzida"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_018_r4_3">
          <text>"Mortalidade proibitiva em HP fixa + disfunção biventricular"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_018_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_018_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-018_fallback">
        <inputEntry id="inputEntry_car_dev_018_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_018_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-018_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-018_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-018_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-018_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-018_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-018">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-018" dmnElementRef="CAR-DEV-018">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
