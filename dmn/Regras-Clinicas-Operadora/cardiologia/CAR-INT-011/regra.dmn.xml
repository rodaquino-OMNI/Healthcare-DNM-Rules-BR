<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-011" name="Aterectomia Rotacional - Indicação Calcificação" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-011" name="Aterectomia Rotacional - Indicação Calcificação">
    <decisionTable id="decisionTable_CAR-INT-011" hitPolicy="FIRST">
      <input id="input_1_car_int_011" label="Calcificacaocoronaria" camunda:inputVariable="calcificacaoCoronaria">
        <inputExpression id="inputExpression_1_car_int_011" typeRef="string">
          <text>calcificacaoCoronaria</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_011" label="Falhapredilatacao" camunda:inputVariable="falhaPreDilatacao">
        <inputExpression id="inputExpression_2_car_int_011" typeRef="string">
          <text>falhaPreDilatacao</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_011" label="Anatomiavaso" camunda:inputVariable="anatomiaVaso">
        <inputExpression id="inputExpression_3_car_int_011" typeRef="string">
          <text>anatomiaVaso</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_011" label="Complicacaopotencial" camunda:inputVariable="complicacaoPotencial">
        <inputExpression id="inputExpression_4_car_int_011" typeRef="string">
          <text>complicacaoPotencial</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-011" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-011">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-011" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-011" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_011_r1">
        <inputEntry id="inputEntry_car_int_011_r1_1">
          <text>severa_360graus</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r1_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r1_3">
          <text>tortuosa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r1_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_011_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_011_r1_2">
          <text>"Rotablator aprovado: calcificação circunferencial não expansível"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_011_r1_3">
          <text>"Calcificação 360° + falha balão: indicação Classe I (ACC/SCAI 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_011_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_011_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_011_r2">
        <inputEntry id="inputEntry_car_int_011_r2_1">
          <text>moderada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r2_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_011_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_011_r2_2">
          <text>"Aterectomia não indicada: pré-dilatação bem-sucedida"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_011_r2_3">
          <text>"Balão NC ou cutting balloon suficiente para calcificação moderada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_011_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_011_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_011_r3">
        <inputEntry id="inputEntry_car_int_011_r3_1">
          <text>severa_ostial</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r3_3">
          <text>ostio_aorta,ostio_rca</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_011_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_011_r3_2">
          <text>"Rotablator aprovado: lesão ostial calcificada"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_011_r3_3">
          <text>"Lesões ostiais: aterectomia facilita resultado ótimo"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_011_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_011_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_011_r4">
        <inputEntry id="inputEntry_car_int_011_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_r4_4">
          <text>disseccao_perfuracao</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_011_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_011_r4_2">
          <text>"Alto risco complicação: evitar aterectomia"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_011_r4_3">
          <text>"Vasos finos &lt;2.5mm ou anatomia desfavorável: risco perfuração 1-2%"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_011_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_011_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-011_fallback">
        <inputEntry id="inputEntry_car_int_011_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_011_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-011_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-011_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-011_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-011_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-011_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-011">
      <dmndi:DMNShape id="DMNShape_CAR-INT-011" dmnElementRef="CAR-INT-011">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
