<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-006" name="Choque Cardiogênico - Estratégia Suporte" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-006" name="Choque Cardiogênico - Estratégia Suporte">
    <decisionTable id="decisionTable_CAR-INT-006" hitPolicy="FIRST">
      <input id="input_1_car_int_006" label="Scai Stage" camunda:inputVariable="scai Stage">
        <inputExpression id="inputExpression_1_car_int_006" typeRef="string">
          <text>scai Stage</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_006" label="Lactatemia" camunda:inputVariable="lactatemia">
        <inputExpression id="inputExpression_2_car_int_006" typeRef="string">
          <text>lactatemia</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_006" label="Suportevasoativo" camunda:inputVariable="suporteVasoativo">
        <inputExpression id="inputExpression_3_car_int_006" typeRef="string">
          <text>suporteVasoativo</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_006" label="Oliguria" camunda:inputVariable="oliguria">
        <inputExpression id="inputExpression_4_car_int_006" typeRef="string">
          <text>oliguria</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-006" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-006">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-006" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-006" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_006_r1">
        <inputEntry id="inputEntry_car_int_006_r1_1">
          <text>A,B</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r1_2">
          <text>&lt; 2</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r1_3">
          <text>nenhum</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r1_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_006_r1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_006_r1_2">
          <text>"Suporte mecânico não indicado: estágio leve"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_006_r1_3">
          <text>"SCAI A-B: otimização farmacológica suficiente"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_006_r1_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_006_r1_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_006_r2">
        <inputEntry id="inputEntry_car_int_006_r2_1">
          <text>C</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r2_2">
          <text>[2..4]</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r2_3">
          <text>unica_droga</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r2_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_006_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_006_r2_2">
          <text>"Aprovado IABP + escalonamento suporte"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_006_r2_3">
          <text>"SCAI C: considerar IABP como ponte decisão"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_006_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_006_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_006_r3">
        <inputEntry id="inputEntry_car_int_006_r3_1">
          <text>D,E</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r3_2">
          <text>&gt; 4</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r3_3">
          <text>multiplas_drogas</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_006_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_006_r3_2">
          <text>"Aprovado Impella/ECMO urgente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_006_r3_3">
          <text>"SCAI D-E: mortalidade &gt;50% sem suporte mecânico (SCAI Consensus 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_006_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_006_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_006_r4">
        <inputEntry id="inputEntry_car_int_006_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r4_2">
          <text>&gt; 8</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_r4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_006_r4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_006_r4_2">
          <text>"Suporte mecânico emergencial: acidose metabólica"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_006_r4_3">
          <text>"Lactato &gt;8 + oligúria: indicação Classe I ECMO/Impella"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_006_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_006_r4_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-006_fallback">
        <inputEntry id="inputEntry_car_int_006_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_006_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-006_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-006_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-006_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-006_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-006_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-006">
      <dmndi:DMNShape id="DMNShape_CAR-INT-006" dmnElementRef="CAR-INT-006">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
