        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_IMG008_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_IMG008">
