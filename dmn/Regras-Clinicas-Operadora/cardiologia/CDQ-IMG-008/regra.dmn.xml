  <decision id="Decision_CDQ_IMG_008" name="Regra Clinica - IMG 008">
    <decisionTable id="DecisionTable_CDQ_IMG_008" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_IMG008" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_IMG008" typeRef="string"><text>condicaoPaciente</text></inputExpression>
        <inputValues id="InputValues_condicaoPaciente_IMG008"><text>"HipertensaoSuspeita,AvaliacaoTerapiaAntiHipertensiva,HipertensaoMascarada,HipotensaoNoturna"</text></inputValues>
      </input>

      <input id="Input_historicoClinico_IMG008" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_IMG008" typeRef="string"><text>historicoClinico</text></inputExpression>
        <inputValues id="InputValues_historicoClinico_IMG008"><text>"Adequado","Incompleto","NaoDocumentado"</text></inputValues>
      </input>

      <input id="Input_classeIndicacao_IMG008" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_IMG008" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_IMG008"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
      </input>

      <input id="Input_008" label="ObjetivoMAPA" camunda:inputVariable="objetivomapa">
        <inputExpression id="InputExpression_008" typeRef="string"><text>objetivomapa</text></inputExpression>
        <inputValues id="InputValues_008"><text>"DiagnosticoHA,MonitoramentoTerapia,HipertensaoResistente"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_IMG008" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_IMG008" typeRef="boolean"><text>expectativaVidaSuperior1Ano</text></inputExpression>
      </input>

      <output id="Output_resultado_IMG008" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_IMG008"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_IMG008" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao_IMG008" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraind_ClasseIII_IMG008">
