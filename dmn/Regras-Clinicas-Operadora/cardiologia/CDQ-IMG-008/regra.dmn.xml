        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_IMG008_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_IMG008">
        <inputEntry id="ie1g_IMG008"><text>-</text></inputEntry>
        <inputEntry id="ie2g_IMG008"><text>-</text></inputEntry>
