        <outputEntry id="oe3d_IMG008"><text>"SBC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_IMG008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_IMG008_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_IMG008">
