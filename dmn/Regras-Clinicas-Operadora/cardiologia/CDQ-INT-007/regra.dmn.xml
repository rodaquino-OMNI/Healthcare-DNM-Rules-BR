<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_INT_007"
             name="PCI Complexa - Tronco Coronaria Esquerda"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-INT-007
    Procedimento: PCI de Tronco de Coronaria Esquerda (TCE)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    Referencias: EXCEL Trial, ACC/AHA/SCAI 2023
    ===============================================================================
  -->

  <decision id="Decision_CDQ_INT_007" name="Regra Clinica - PCI TCE">
    <decisionTable id="DecisionTable_CDQ_INT_007" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_INT007" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_INT007" typeRef="string"><text>condicaoPaciente</text></inputExpression>
        <inputValues id="InputValues_condicaoPaciente_INT007"><text>"LesaoTCEOstial","LesaoTCEMedio","LesaoTCEDistal","LesaoTCEBifurcacao"</text></inputValues>
      </input>

      <input id="Input_historicoClinico_INT007" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_INT007" typeRef="string"><text>historicoClinico</text></inputExpression>
        <inputValues id="InputValues_historicoClinico_INT007"><text>"Adequado","Incompleto","NaoDocumentado"</text></inputValues>
      </input>

      <input id="Input_classeIndicacao_INT007" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_INT007" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_INT007"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
      </input>

      <input id="Input_syntaxScore_INT007" label="SYNTAX Score" camunda:inputVariable="syntaxScore">
        <inputExpression id="InputExpression_syntaxScore_INT007" typeRef="string"><text>syntaxScore</text></inputExpression>
        <inputValues id="InputValues_syntaxScore_INT007"><text>"Baixo0-22","Intermediario23-32","Alto33ouMais"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_INT007" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_INT007" typeRef="boolean"><text>expectativaVidaSuperior1Ano</text></inputExpression>
      </input>

      <output id="Output_resultado_INT007" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_INT007"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_INT007" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao_INT007" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraind_ClasseIII_INT007">
        <inputEntry id="ie1_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie2_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie3_INT007"><text>"Classe III"</text></inputEntry>
        <inputEntry id="ie4_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie5_INT007"><text>-</text></inputEntry>
        <outputEntry id="oe1_INT007"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2_INT007"><text>"PCI TCE nao indicado"</text></outputEntry>
        <outputEntry id="oe3_INT007"><text>"ACC/AHA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_INT007_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_INT007_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_SyntaxAlto_INT007">
        <inputEntry id="ie1b_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie2b_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie3b_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie4b_INT007"><text>"Alto33ouMais"</text></inputEntry>
        <inputEntry id="ie5b_INT007"><text>-</text></inputEntry>
        <outputEntry id="oe1b_INT007"><text>"Pendente"</text></outputEntry>
        <outputEntry id="oe2b_INT007"><text>"SYNTAX alto - CABG preferivel, Heart Team obrigatorio"</text></outputEntry>
        <outputEntry id="oe3b_INT007"><text>"EXCEL Trial - CABG superior em SYNTAX alto"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SyntaxAlto_INT007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SyntaxAlto_INT007_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_SyntaxBaixo_INT007">
        <inputEntry id="ie1c_INT007"><text>"LesaoTCEOstial","LesaoTCEMedio"</text></inputEntry>
        <inputEntry id="ie2c_INT007"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3c_INT007"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="ie4c_INT007"><text>"Baixo0-22"</text></inputEntry>
        <inputEntry id="ie5c_INT007"><text>true</text></inputEntry>
        <outputEntry id="oe1c_INT007"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2c_INT007"><text>"PCI TCE indicado - SYNTAX baixo"</text></outputEntry>
        <outputEntry id="oe3c_INT007"><text>"EXCEL, NOBLE Trials - PCI equivalente CABG em SYNTAX baixo-intermediario"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_SyntaxBaixo_INT007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_SyntaxBaixo_INT007_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_SyntaxInt_INT007">
        <inputEntry id="ie1d_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie2d_INT007"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3d_INT007"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="ie4d_INT007"><text>"Intermediario23-32"</text></inputEntry>
        <inputEntry id="ie5d_INT007"><text>true</text></inputEntry>
        <outputEntry id="oe1d_INT007"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2d_INT007"><text>"PCI TCE razoavel - SYNTAX intermediario"</text></outputEntry>
        <outputEntry id="oe3d_INT007"><text>"SCAI 2023 - Heart Team decision"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_SyntaxInt_INT007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_SyntaxInt_INT007_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_Hist_INT007">
        <inputEntry id="ie1e_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie2e_INT007"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="ie3e_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie4e_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie5e_INT007"><text>-</text></inputEntry>
        <outputEntry id="oe1e_INT007"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2e_INT007"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="oe3e_INT007"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_INT007_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_INT007_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_INT007">
        <inputEntry id="ie1f_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie2f_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie3f_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie4f_INT007"><text>-</text></inputEntry>
        <inputEntry id="ie5f_INT007"><text>-</text></inputEntry>
        <outputEntry id="oe1f_INT007"><text>"Pendente"</text></outputEntry>
        <outputEntry id="oe2f_INT007"><text>"Heart Team obrigatorio para TCE"</text></outputEntry>
        <outputEntry id="oe3f_INT007"><text>"ACC/AHA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_INT007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_INT007_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
