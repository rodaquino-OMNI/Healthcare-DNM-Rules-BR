<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-IMG-013" name="Biópsia Endomiocárdica" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-IMG-013" name="Biópsia Endomiocárdica">
    <decisionTable id="decisionTable_CAR-IMG-013" hitPolicy="FIRST">
      <input id="input_1_car_img_013" label="Indicacao" camunda:inputVariable="indicacao">
        <inputExpression id="inputExpression_1_car_img_013" typeRef="string">
          <text>indicacao</text>
        </inputExpression>
      </input>
      <input id="input_2_car_img_013" label="Alternativasdiagnosticas" camunda:inputVariable="alternativasDiagnosticas">
        <inputExpression id="inputExpression_2_car_img_013" typeRef="string">
          <text>alternativasDiagnosticas</text>
        </inputExpression>
      </input>
      <input id="input_3_car_img_013" label="Riscoprocedimento" camunda:inputVariable="riscoProcedimento">
        <inputExpression id="inputExpression_3_car_img_013" typeRef="string">
          <text>riscoProcedimento</text>
        </inputExpression>
      </input>
      <input id="input_4_car_img_013" label="Impactoconduta" camunda:inputVariable="impactoConduta">
        <inputExpression id="inputExpression_4_car_img_013" typeRef="string">
          <text>impactoConduta</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-IMG-013" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-IMG-013">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-IMG-013" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-IMG-013" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_img_013_r1">
        <inputEntry id="inputEntry_car_img_013_r1_1">
          <text>rejeicao_transplante,miocardite_fulminante</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r1_2">
          <text>nao_definitivas</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r1_3">
          <text>aceitavel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r1_4">
          <text>mudanca_terapia</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_013_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_013_r1_2">
          <text>"BEM indicada: impacto terapêutico direto"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_013_r1_3">
          <text>"Classe I para rejeição Tx ou miocardite fulminante (ISHLT 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_013_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_013_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_013_r2">
        <inputEntry id="inputEntry_car_img_013_r2_1">
          <text>cardiomiopatia_restrictiva</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r2_2">
          <text>rmc_inconclusiva</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r2_4">
          <text>mudanca_terapia</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_013_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_013_r2_2">
          <text>"BEM para diferenciar amiloide vs sarcoidose vs HFEF"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_013_r2_3">
          <text>"Quando RMC + cintigrafia não-diagnóstica e tratamento específico disponível"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_013_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_013_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_013_r3">
        <inputEntry id="inputEntry_car_img_013_r3_1">
          <text>miocardite_cronica</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r3_2">
          <text>rmc_diagnostica</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_013_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_013_r3_2">
          <text>"RMC suficiente: BEM não adiciona informação"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_013_r3_3">
          <text>"Realce tardio + edema T2: diagnóstico Lake Louise sem necessidade biópsia"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_013_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_013_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_013_r4">
        <inputEntry id="inputEntry_car_img_013_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r4_3">
          <text>muito_alto</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_013_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_013_r4_2">
          <text>"Risco supera benefício potencial"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_013_r4_3">
          <text>"Plaquetas &lt;30k, anticoagulação irredutível: complicação &gt;5%"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_013_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_013_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-IMG-013_fallback">
        <inputEntry id="inputEntry_car_img_013_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_013_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-IMG-013_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-013_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-013_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-IMG-013_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-IMG-013_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-IMG-013">
      <dmndi:DMNShape id="DMNShape_CAR-IMG-013" dmnElementRef="CAR-IMG-013">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
