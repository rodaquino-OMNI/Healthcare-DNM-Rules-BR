<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_017"
             name="Left Bundle Branch Area Pacing (LBBAP)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-017
    Procedimento: Left Bundle Branch Area Pacing (LBBAP/LBBP)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    Alternativa a CRT e HBP, falha de HBP, bloqueio AV com FEVE reduzida

    CONTRAINDICACOES:
    Perfuracao septal previa, experiencia local limitada

    REFERENCIAS TECNICAS:
    HRS Conduction System Pacing 2024, ESC 2024

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_017" name="Regra Clinica - LBBAP">
    <decisionTable id="DecisionTable_CDQ_DEV_017" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_017" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_017" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_017">
          <text>"BloqueioAVFEVEReduzida","FalhaHBP","AlternativaCRT","UpgradeCRT"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_017" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_017" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_017">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_017" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_017" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_017">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_experienciaCentro_017" label="Centro com Experiencia LBBAP" camunda:inputVariable="experienciaCentro">
        <inputExpression id="InputExpression_experienciaCentro_017" typeRef="boolean">
          <text>experienciaCentro</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_017" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_017" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_017" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_017">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_017" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_017" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_017">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_017_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_017_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_017_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_017_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_017_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_017_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_017_observacao"><text>"LBBAP nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_017_fundamentacao"><text>"HRS CSP 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_017_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_017_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_017">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_017_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_017_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_017_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_017_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_017_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_017_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_017_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_017_fundamentacao"><text>"ESC 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_017_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_017_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_SemExperiencia_017">
        <description>Avaliar: Centro sem experiencia LBBAP</description>
        <inputEntry id="InputEntry_Contraind_Exp_017_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Exp_017_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Exp_017_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Exp_017_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Exp_017_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Exp_017_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Exp_017_observacao"><text>"Encaminhar para centro com experiencia LBBAP"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Exp_017_fundamentacao"><text>"HRS 2024 - Tecnica em evolucao, requer expertise"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SemExperiencia_017_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SemExperiencia_017_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_ClasseI_017">
        <description>Aprovado: Indicacao com experiencia</description>
        <inputEntry id="InputEntry_Aprovado_CI_017_1"><text>"BloqueioAVFEVEReduzida","FalhaHBP","AlternativaCRT"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_017_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_017_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_017_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_017_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_017_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_017_observacao"><text>"LBBAP indicado - alternativa a CRT/HBP"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_017_fundamentacao"><text>"HRS CSP 2024 - Taxa sucesso maior 90%, QRS mais estreito que HBP"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_017_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_017_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_017">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_017_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_017_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_017_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_017_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_017_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_017_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_017_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_017_fundamentacao"><text>"ESC 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_017_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_017_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_017">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_017_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_017_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_017_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_017_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_017_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_017_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_017_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_017_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_017_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_017_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_017">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_017_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_017_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_017_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_017_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_017_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_017_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_017_observacao"><text>"Analise por especialista em CSP"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_017_fundamentacao"><text>"HRS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_017_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_017_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
