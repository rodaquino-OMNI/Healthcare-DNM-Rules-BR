          <text>etiologiaIM</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_015" label="Dilatacaoanular" camunda:inputVariable="dilatacaoAnular">
        <inputExpression id="inputExpression_2_car_dev_015" typeRef="string">
          <text>dilatacaoAnular</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_015" label="Funcionamentofolhetos" camunda:inputVariable="funcionamentoFolhetos">
        <inputExpression id="inputExpression_3_car_dev_015" typeRef="string">
          <text>funcionamentoFolhetos</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_015" label="Calcificacaoanular" camunda:inputVariable="calcificacaoAnular">
        <inputExpression id="inputExpression_4_car_dev_015" typeRef="string">
          <text>calcificacaoAnular</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-015" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-015">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-015" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-015" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
