      <rule id="rule_CAR-DEV-015_fallback">
        <inputEntry id="inputEntry_car_dev_015_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-015_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-015_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-015_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-015_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-015_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
