          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_015_r1">
        <inputEntry id="inputEntry_car_dev_015_r1_1">
          <text>degenerativa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r1_2">
          <text>severa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r1_3">
          <text>prolapso</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r1_4">
          <text>minima</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_015_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_015_r1_2">
          <text>"Ring rígido/semirrígido: restaura geometria 3D"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_015_r1_3">
          <text>"IMD por prolapso: ring semi-rígido reduz recorrência IM"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_015_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_015_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_015_r2">
        <inputEntry id="inputEntry_car_dev_015_r2_1">
          <text>funcional</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r2_2">
          <text>moderada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r2_3">
          <text>restricao</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r2_4">
          <text>moderada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_015_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_015_r2_2">
          <text>"Ring completo: IM funcional com dilatação"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_015_r2_3">
          <text>"Downsizing 2 tamanhos: reduz diâmetro septo-lateral"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_015_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_015_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_015_r3">
        <inputEntry id="inputEntry_car_dev_015_r3_1">
          <text>reumatica</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r3_3">
          <text>espessamento</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r3_4">
          <text>severa</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_015_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_015_r3_2">
          <text>"Anuloplastia isolada inadequada: considerar troca valvar"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_015_r3_3">
          <text>"Doença reumática: anuloplastia alta taxa falência"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_015_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_015_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_015_r4">
        <inputEntry id="inputEntry_car_dev_015_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r4_2">
          <text>minima</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r4_3">
          <text>preservado</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_015_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_015_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_015_r4_2">
          <text>"IM leve: anuloplastia benefício incerto"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_015_r4_3">
          <text>"Considerar apenas se cirurgia concomitante (CABG)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_015_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_015_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

