<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_016"
             name="His Bundle Pacing - Estimulacao do Feixe de His"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-016
    Procedimento: His Bundle Pacing (HBP)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    Alternativa a CRT em pacientes com bloqueio AV e FEVE reduzida,
    upgrade de marcapasso VVI com cardiomiopatia induzida por estimulacao

    CONTRAINDICACOES:
    Doenca nodal His-Purkinje distal, experiencia local limitada

    REFERENCIAS TECNICAS:
    HRS Physiologic Pacing 2024, SBC/DECA 2023

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_016" name="Regra Clinica - His Bundle Pacing">
    <decisionTable id="DecisionTable_CDQ_DEV_016" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_016" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_016" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_016">
          <text>"BloqueioAV","FEVEReduzidaEstimulacao","AlternativaCRT","BRECorregivelHBP"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_016" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_016" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_016">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_016" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_016" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_016">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_experienciaCentro_016" label="Centro com Experiencia HBP" camunda:inputVariable="experienciaCentro">
        <inputExpression id="InputExpression_experienciaCentro_016" typeRef="boolean">
          <text>experienciaCentro</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_016" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_016" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_016" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_016">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_016" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_016" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_016">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_016_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_016_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_016_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_016_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_016_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_016_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_016_observacao"><text>"HBP nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_016_fundamentacao"><text>"HRS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_016_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_016_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_016">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_016_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_016_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_016_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_016_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_016_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_016_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_016_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_016_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_016_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_016_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_SemExperiencia_016">
        <description>Avaliar: Centro sem experiencia HBP</description>
        <inputEntry id="InputEntry_Contraind_Exp_016_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Exp_016_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Exp_016_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Exp_016_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Exp_016_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Exp_016_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Exp_016_observacao"><text>"Encaminhar para centro com experiencia em HBP"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Exp_016_fundamentacao"><text>"HRS 2024 - Curva de aprendizado significativa"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SemExperiencia_016_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SemExperiencia_016_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe I/IIa -->
      <rule id="Rule_Aprovado_ClasseI_016">
        <description>Aprovado: Indicacao Classe I/IIa com experiencia</description>
        <inputEntry id="InputEntry_Aprovado_CI_016_1"><text>"BloqueioAV","FEVEReduzidaEstimulacao","AlternativaCRT"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_016_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_016_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_016_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_016_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_016_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_016_observacao"><text>"HBP indicado - estimulacao fisiologica"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_016_fundamentacao"><text>"HRS Physiologic Pacing 2024 - Preserva sincronia ventricular"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_016_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_016_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_016">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_016_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_016_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_016_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_016_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_016_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_016_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_016_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_016_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_016_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_016_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_016">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_016_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_016_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_016_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_016_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_016_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_016_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_016_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_016_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_016_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_016_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_016">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_016_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_016_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_016_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_016_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_016_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_016_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_016_observacao"><text>"Analise por eletrofisiologista especializado"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_016_fundamentacao"><text>"HRS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_016_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_016_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
