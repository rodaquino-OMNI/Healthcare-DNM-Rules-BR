<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-020" name="Dispositivo Seio Coronário - CVRx/Cardiac Contractility" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-020" name="Dispositivo Seio Coronário - CVRx/Cardiac Contractility">
    <decisionTable id="decisionTable_CAR-DEV-020" hitPolicy="FIRST">
      <input id="input_1_car_dev_020" label="Classenyha" camunda:inputVariable="classeNYHA">
        <inputExpression id="inputExpression_1_car_dev_020" typeRef="string">
          <text>classeNYHA</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_020" label="Qrscomplexo" camunda:inputVariable="qrsComplexo">
        <inputExpression id="inputExpression_2_car_dev_020" typeRef="string">
          <text>qrsComplexo</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_020" label="Terapiaotimizada" camunda:inputVariable="terapiaOtimizada">
        <inputExpression id="inputExpression_3_car_dev_020" typeRef="string">
          <text>terapiaOtimizada</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_020" label="Anatomiaseiocoronario" camunda:inputVariable="anatomiaSeioCoronario">
        <inputExpression id="inputExpression_4_car_dev_020" typeRef="string">
          <text>anatomiaSeioCoronario</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-020" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-020">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-020" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-020" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_020_r1">
        <inputEntry id="inputEntry_car_dev_020_r1_1">
          <text>III,IV</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r1_2">
          <text>&lt; 130</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r1_3">
          <text>maxima</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r1_4">
          <text>adequada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_020_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_020_r1_2">
          <text>"Optimizer/CCM: IC QRS estreito"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_020_r1_3">
          <text>"Modulação contratilidade melhora sintomas sem prolongar QRS"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_020_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_020_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_020_r2">
        <inputEntry id="inputEntry_car_dev_020_r2_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r2_2">
          <text>&gt;= 130</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_020_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_020_r2_2">
          <text>"QRS largo: TRC indicada"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_020_r2_3">
          <text>"Ressincronização superior dispositivos seio coronário em BCRE"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_020_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_020_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_020_r3">
        <inputEntry id="inputEntry_car_dev_020_r3_1">
          <text>I,II</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_020_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_020_r3_2">
          <text>"Sintomas leves: dispositivo não justificado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_020_r3_3">
          <text>"Classe I-II: otimização medicamentosa suficiente"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_020_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_020_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_020_r4">
        <inputEntry id="inputEntry_car_dev_020_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r4_3">
          <text>submaxima</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_020_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_020_r4_2">
          <text>"Otimizar TMO antes considerar device"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_020_r4_3">
          <text>"Titular betabloqueador, ARNI, antagonista mineralocorticoide antes CCM"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_020_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_020_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-020_fallback">
        <inputEntry id="inputEntry_car_dev_020_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_020_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-020_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-020_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-020_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-020_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-020_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-020">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-020" dmnElementRef="CAR-DEV-020">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
