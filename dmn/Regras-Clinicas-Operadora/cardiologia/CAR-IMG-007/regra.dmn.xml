<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-IMG-007" name="RMC - Cardiomiopatias" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-IMG-007" name="RMC - Cardiomiopatias">
    <decisionTable id="decisionTable_CAR-IMG-007" hitPolicy="FIRST">
      <input id="input_1_car_img_007" label="Suspeitadiagnostica" camunda:inputVariable="suspeitaDiagnostica">
        <inputExpression id="inputExpression_1_car_img_007" typeRef="string">
          <text>suspeitaDiagnostica</text>
        </inputExpression>
      </input>
      <input id="input_2_car_img_007" label="Ecodiagnostica" camunda:inputVariable="ecodiagnostica">
        <inputExpression id="inputExpression_2_car_img_007" typeRef="string">
          <text>ecodiagnostica</text>
        </inputExpression>
      </input>
      <input id="input_3_car_img_007" label="Indicacaobiopsia" camunda:inputVariable="indicacaoBiopsia">
        <inputExpression id="inputExpression_3_car_img_007" typeRef="string">
          <text>indicacaoBiopsia</text>
        </inputExpression>
      </input>
      <input id="input_4_car_img_007" label="Contraindicacaormc" camunda:inputVariable="contraindicacaoRMC">
        <inputExpression id="inputExpression_4_car_img_007" typeRef="string">
          <text>contraindicacaoRMC</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-IMG-007" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-IMG-007">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-IMG-007" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-IMG-007" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_img_007_r1">
        <inputEntry id="inputEntry_car_img_007_r1_1">
          <text>miocardite,sarcoidose,amiloidose</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r1_2">
          <text>inconclusivo</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r1_3">
          <text>evitar</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r1_4">
          <text>nenhuma</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_007_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_007_r1_2">
          <text>"RMC diagnóstica: caracterização tecidual única"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_007_r1_3">
          <text>"Realce tardio + mapeamento T1/T2/ECV diferenciam etiologias"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_007_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_007_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_007_r2">
        <inputEntry id="inputEntry_car_img_007_r2_1">
          <text>chmc,cmd</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r2_2">
          <text>diagnostico</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r2_3">
          <text>nao_indicada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_007_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_007_r2_2">
          <text>"ECO suficiente: RMC não modifica conduta"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_007_r2_3">
          <text>"Diagnóstico firmado por eco + clínica: RMC não agrega"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_007_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_007_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_007_r3">
        <inputEntry id="inputEntry_car_img_007_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r3_3">
          <text>necessaria</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_007_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_007_r3_2">
          <text>"Biópsia se RMC inconclusiva ou contraindicada"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_007_r3_3">
          <text>"Miocardite fulminante, sarcoidose ativa: biópsia gold standard"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_007_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_007_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_007_r4">
        <inputEntry id="inputEntry_car_img_007_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_r4_4">
          <text>cdi,marcapasso_nao_condicional</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_007_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_007_r4_2">
          <text>"Dispositivos não-RMC-safe contraindicam exame"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_007_r4_3">
          <text>"CDI/MP pré-2008: risco aquecimento leads - considerar TC cardíaca"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_007_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_007_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-IMG-007_fallback">
        <inputEntry id="inputEntry_car_img_007_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_007_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-IMG-007_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-007_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-007_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-IMG-007_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-IMG-007_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-IMG-007">
      <dmndi:DMNShape id="DMNShape_CAR-IMG-007" dmnElementRef="CAR-IMG-007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
