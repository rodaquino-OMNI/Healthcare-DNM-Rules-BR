<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-IMG-011" name="PET Cardíaco - Viabilidade e Fluxo" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-IMG-011" name="PET Cardíaco - Viabilidade e Fluxo">
    <decisionTable id="decisionTable_CAR-IMG-011" hitPolicy="FIRST">
      <input id="input_1_car_img_011" label="Indicacao" camunda:inputVariable="indicacao">
        <inputExpression id="inputExpression_1_car_img_011" typeRef="string">
          <text>indicacao</text>
        </inputExpression>
      </input>
      <input id="input_2_car_img_011" label="Resultadospect" camunda:inputVariable="resultadoSPECT">
        <inputExpression id="inputExpression_2_car_img_011" typeRef="string">
          <text>resultadoSPECT</text>
        </inputExpression>
      </input>
      <input id="input_3_car_img_011" label="Disponibilidadepet" camunda:inputVariable="disponibilidadePET">
        <inputExpression id="inputExpression_3_car_img_011" typeRef="integer">
          <text>disponibilidadePET</text>
        </inputExpression>
      </input>
      <input id="input_4_car_img_011" label="Diabetesdescompensado" camunda:inputVariable="diabetesDescompensado">
        <inputExpression id="inputExpression_4_car_img_011" typeRef="string">
          <text>diabetesDescompensado</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-IMG-011" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-IMG-011">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-IMG-011" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-IMG-011" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_img_011_r1">
        <inputEntry id="inputEntry_car_img_011_r1_1">
          <text>viabilidade</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r1_2">
          <text>discordante</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r1_3">
          <text>disponivel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r1_4">
          <text>controlado</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_011_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_011_r1_2">
          <text>"PET viabilidade: gold standard"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_011_r1_3">
          <text>"Rubídio-82 ou F18-FDG: acurácia superior SPECT em viabilidade"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_011_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_011_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_011_r2">
        <inputEntry id="inputEntry_car_img_011_r2_1">
          <text>reserva_fluxo</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r2_3">
          <text>disponivel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_011_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_011_r2_2">
          <text>"PET quantifica reserva fluxo: DAC não obstrutiva"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_011_r2_3">
          <text>"CFR &lt;2.0 mesmo sem estenose: pior prognóstico - intensificar terapia"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_011_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_011_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_011_r3">
        <inputEntry id="inputEntry_car_img_011_r3_1">
          <text>viabilidade</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r3_3">
          <text>indisponivel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_011_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_011_r3_2">
          <text>"PET indisponível: usar RMC viabilidade"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_011_r3_3">
          <text>"RMC alternativa adequada se PET inacessível"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_011_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_011_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_011_r4">
        <inputEntry id="inputEntry_car_img_011_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_r4_4">
          <text>descompensado</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_011_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_011_r4_2">
          <text>"DM descompensado compromete PET-FDG"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_011_r4_3">
          <text>"Hiperglicemia &gt;200: inibe captação FDG miocárdica - controlar antes"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_011_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_011_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-IMG-011_fallback">
        <inputEntry id="inputEntry_car_img_011_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_011_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-IMG-011_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-011_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-011_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-IMG-011_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-IMG-011_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-IMG-011">
      <dmndi:DMNShape id="DMNShape_CAR-IMG-011" dmnElementRef="CAR-IMG-011">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
