<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-008" name="CTO - Oclusão Total Crônica" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-008" name="CTO - Oclusão Total Crônica">
    <decisionTable id="decisionTable_CAR-INT-008" hitPolicy="FIRST">
      <input id="input_1_car_int_008" label="Sintomas" camunda:inputVariable="sintomas">
        <inputExpression id="inputExpression_1_car_int_008" typeRef="string">
          <text>sintomas</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_008" label="Viabilidademiocardica" camunda:inputVariable="viabilidadeMiocardica">
        <inputExpression id="inputExpression_2_car_int_008" typeRef="integer">
          <text>viabilidadeMiocardica</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_008" label="J-Ctoscore" camunda:inputVariable="j-ctoScore">
        <inputExpression id="inputExpression_3_car_int_008" typeRef="integer">
          <text>j-ctoScore</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_008" label="Tentativasprevias" camunda:inputVariable="tentativasPrevias">
        <inputExpression id="inputExpression_4_car_int_008" typeRef="string">
          <text>tentativasPrevias</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-008" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-008">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-008" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-008" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_008_r1">
        <inputEntry id="inputEntry_car_int_008_r1_1">
          <text>anginaRefrataria</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r1_2">
          <text>presente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r1_3">
          <text>[0..2]</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r1_4">
          <text>0</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_008_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_008_r1_2">
          <text>"ICP-CTO aprovada: sintomas + viabilidade + baixa complexidade"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_008_r1_3">
          <text>"J-CTO ≤2 + isquemia &gt;10%: sucesso 85-90% (EuroCTO 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_008_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_008_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_008_r2">
        <inputEntry id="inputEntry_car_int_008_r2_1">
          <text>assintomatico</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_008_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_008_r2_2">
          <text>"ICP-CTO não indicada: ausência sintomas"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_008_r2_3">
          <text>"Sem evidência benefício em pacientes assintomáticos"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_008_r2_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_008_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_008_r3">
        <inputEntry id="inputEntry_car_int_008_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r3_2">
          <text>ausente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_008_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_008_r3_2">
          <text>"CTO não viável: ausência tecido recuperável"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_008_r3_3">
          <text>"Cintilografia/RMC sem viabilidade contraindica ICP"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_008_r3_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_008_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_008_r4">
        <inputEntry id="inputEntry_car_int_008_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r4_3">
          <text>&gt; 3</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_r4_4">
          <text>&gt;= 2</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_008_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_008_r4_2">
          <text>"CTO complexa: múltiplas falhas prévias"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_008_r4_3">
          <text>"J-CTO &gt;3 + falhas anteriores: considerar CABG ou TMO"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_008_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_008_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-008_fallback">
        <inputEntry id="inputEntry_car_int_008_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_008_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-008_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-008_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-008_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-008_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-008_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-008">
      <dmndi:DMNShape id="DMNShape_CAR-INT-008" dmnElementRef="CAR-INT-008">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
