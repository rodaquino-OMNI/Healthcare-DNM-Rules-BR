<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_019"
             name="WCD - Wearable Cardioverter Defibrillator"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-019
    Procedimento: WCD (Colete Desfibrilador Vestivel)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    Ponte para CDI, pos-revascularizacao com FEVE baixa, pos-IAM,
    aguardando recuperacao FEVE

    CONTRAINDICACOES:
    Indicacao definitiva CDI ja estabelecida, impossibilidade uso

    REFERENCIAS TECNICAS:
    HRS WCD 2024, SBC/DECA 2023

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_019" name="Regra Clinica - WCD">
    <decisionTable id="DecisionTable_CDQ_DEV_019" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_019" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_019" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_019">
          <text>"PosIAMFEVEBaixa","PosRevascularizacao","AguardandoRecuperacaoFEVE","ExtracaoDispositivo","PonteCDI"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_019" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_019" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_019">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_019" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_019" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_019">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_periodoTemporario_019" label="Periodo Temporario Definido" camunda:inputVariable="periodoTemporario">
        <inputExpression id="InputExpression_periodoTemporario_019" typeRef="boolean">
          <text>periodoTemporario</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_019" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_019" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_019" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_019">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_019" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_019" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_019">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_019_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_019_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_019_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_019_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_019_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_019_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_019_observacao"><text>"WCD nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_019_fundamentacao"><text>"HRS WCD 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_019_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_019_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_019">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_019_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_019_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_019_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_019_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_019_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_019_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_019_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_019_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_019_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_019_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_SemPeriodo_019">
        <description>Avaliar: Sem periodo temporario definido</description>
        <inputEntry id="InputEntry_Contraind_Per_019_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Per_019_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Per_019_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Per_019_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Per_019_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Per_019_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Per_019_observacao"><text>"Definir periodo de uso WCD e plano de reavaliacao"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Per_019_fundamentacao"><text>"HRS 2024 - WCD indicado por periodo limitado ate definicao CDI"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SemPeriodo_019_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_SemPeriodo_019_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_ClasseI_019">
        <description>Aprovado: Indicacao ponte para CDI</description>
        <inputEntry id="InputEntry_Aprovado_CI_019_1"><text>"PosIAMFEVEBaixa","PosRevascularizacao","AguardandoRecuperacaoFEVE","ExtracaoDispositivo"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_019_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_019_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_019_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_019_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_019_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_019_observacao"><text>"WCD indicado como ponte para decisao CDI"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_019_fundamentacao"><text>"HRS 2024, VEST Trial - Protecao temporaria em periodo vulneravel"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_019_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_019_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_019">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_019_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_019_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_019_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_019_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_019_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_019_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_019_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_019_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_019_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_019_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_019">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_019_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_019_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_019_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_019_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_019_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_019_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_019_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_019_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_019_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_019_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_019">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_019_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_019_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_019_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_019_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_019_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_019_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_019_observacao"><text>"Analise adicional necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_019_fundamentacao"><text>"HRS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_019_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_019_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
