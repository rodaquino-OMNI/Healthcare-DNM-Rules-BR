<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_007"
             name="Implante de CDI para Prevencao Primaria"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-007
    Procedimento: Implante de Cardiodesfibrilador Implantavel (CDI) - Prevencao Primaria
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS (DUT/ANS):
    Cardiomiopatia isquemica ou nao-isquemica com FEVE menor ou igual 35%,
    classe funcional NYHA II-III, terapia medica otimizada por 3 meses

    CONTRAINDICACOES:
    FEVE maior 35%, expectativa de vida menor 1 ano, classe NYHA IV refrataria,
    revascularizacao recente menor 40 dias, IAM menor 40 dias

    REFERENCIAS TECNICAS:
    SBC/DECA CDI 2023, HRS/EHRA Sudden Cardiac Death 2024, ACC/AHA HF 2023

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_007" name="Regra Clinica - CDI Prevencao Primaria">
    <decisionTable id="DecisionTable_CDQ_DEV_007" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_007" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_007" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_007">
          <text>"CMPIsquemica","CMPNaoIsquemica","CMPDilatada","CMPHipertrofica","Canalopatia"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_007" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_007" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_007">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_007" label="Classe de Indicacao (SBC/ACC/AHA)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_007" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_007">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_feVE_007" label="Fracao de Ejecao do VE (%)" camunda:inputVariable="feVE">
        <inputExpression id="InputExpression_feVE_007" typeRef="string">
          <text>feVE</text>
        </inputExpression>
        <inputValues id="InputValues_feVE_007">
          <text>"menor15%","15-30%","30-35%","35-40%","maior40%","NaoAvaliada"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_007" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_007" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_007" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_007">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_007" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_007" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_007">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_007_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_007_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_007_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_007_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_007_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_007_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_007_observacao"><text>"CDI nao indicado conforme classificacao Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_007_fundamentacao"><text>"SBC/DECA CDI 2023, HRS/EHRA 2024 - Classe III: CDI nao indicado ou potencialmente prejudicial"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_007_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_007_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpectativaVida_007">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_007_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_007_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_007_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_007_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_007_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_007_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_007_observacao"><text>"Expectativa de vida inferior a 1 ano - CDI nao indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_007_fundamentacao"><text>"SBC/DECA 2023, ACC/AHA 2023 - CDI requer expectativa de vida maior 1 ano para beneficio clinico"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpectativaVida_007_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpectativaVida_007_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_FEVEPreservada_007">
        <description>Contraindicacao: FEVE preservada - CDI prevencao primaria nao indicado</description>
        <inputEntry id="InputEntry_Contraind_FEVE_007_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_FEVE_007_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_FEVE_007_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_FEVE_007_4"><text>"maior40%"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_FEVE_007_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_FEVE_007_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_FEVE_007_observacao"><text>"FEVE maior 40%: CDI prevencao primaria nao indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_FEVE_007_fundamentacao"><text>"SBC/DECA 2023 - Prevencao primaria requer FEVE menor ou igual 35% (estudos MADIT-II, SCD-HeFT)"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_FEVEPreservada_007_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_FEVEPreservada_007_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe I -->
      <rule id="Rule_Aprovado_ClasseI_CMPIsq_007">
        <description>Aprovado: CMP Isquemica com FEVE baixa Classe I</description>
        <inputEntry id="InputEntry_Aprovado_CI_CMPIsq_007_1"><text>"CMPIsquemica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_CMPIsq_007_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_CMPIsq_007_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_CMPIsq_007_4"><text>"menor15%","15-30%","30-35%"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_CMPIsq_007_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_CMPIsq_007_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_CMPIsq_007_observacao"><text>"CMP Isquemica com FEVE reduzida - CDI prevencao primaria indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_CMPIsq_007_fundamentacao"><text>"SBC/DECA 2023, MADIT-II Trial - Reducao mortalidade 31% com CDI em CMP isquemica FEVE menor 30%"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_CMPIsq_007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_CMPIsq_007_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_CMPNaoIsq_007">
        <description>Aprovado: CMP Nao-Isquemica com FEVE baixa Classe I</description>
        <inputEntry id="InputEntry_Aprovado_CI_CMPNaoIsq_007_1"><text>"CMPNaoIsquemica","CMPDilatada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_CMPNaoIsq_007_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_CMPNaoIsq_007_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_CMPNaoIsq_007_4"><text>"menor15%","15-30%","30-35%"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_CMPNaoIsq_007_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_CMPNaoIsq_007_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_CMPNaoIsq_007_observacao"><text>"CMP Nao-Isquemica com FEVE reduzida - CDI indicado apos 3 meses TMO"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_CMPNaoIsq_007_fundamentacao"><text>"SBC/DECA 2023, SCD-HeFT Trial - Beneficio similar em CMP nao-isquemica apos otimizacao terapeutica"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_CMPNaoIsq_007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_CMPNaoIsq_007_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe IIa/IIb -->
      <rule id="Rule_Aprovado_ClasseIIa_007">
        <description>Aprovado: Indicacao Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_007_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_007_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_007_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_007_4"><text>"menor15%","15-30%","30-35%"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_007_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_007_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_007_observacao"><text>"Indicacao Classe IIa - CDI provavelmente util"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_007_fundamentacao"><text>"SBC/DECA 2023, HRS/EHRA 2024 - Classe IIa: Beneficio maior Risco"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_007_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_007">
        <description>Aprovado: Indicacao Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_007_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_007_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_007_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_007_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_007_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_007_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_007_observacao"><text>"Indicacao Classe IIb - CDI pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_007_fundamentacao"><text>"SBC/DECA 2023 - Classe IIb: Avaliacao individualizada recomendada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_007_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO - Documentacao Inadequada -->
      <rule id="Rule_Reprovado_HistoricoInsuficiente_007">
        <description>Reprovado: Historico clinico insuficiente</description>
        <inputEntry id="InputEntry_Reprovado_Hist_007_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_007_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_007_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_007_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_007_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_007_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_007_observacao"><text>"Documentacao clinica insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_007_fundamentacao"><text>"ANS RN 465/2021: Necessaria documentacao clinica adequada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_HistoricoInsuficiente_007_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_HistoricoInsuficiente_007_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_007">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_007_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_007_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_007_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_007_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_007_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_007_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_007_observacao"><text>"Caso requer analise clinica adicional"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_007_fundamentacao"><text>"Avaliacao individualizada necessaria conforme SBC/DECA"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_007_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_007_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
