<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-007" name="PCI Staged vs Complete Revascularization" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-007" name="PCI Staged vs Complete Revascularization">
    <decisionTable id="decisionTable_CAR-INT-007" hitPolicy="FIRST">
      <input id="input_1_car_int_007" label="Apresentacao" camunda:inputVariable="apresentacao">
        <inputExpression id="inputExpression_1_car_int_007" typeRef="string">
          <text>apresentacao</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_007" label="Numerovasos" camunda:inputVariable="numeroVasos">
        <inputExpression id="inputExpression_2_car_int_007" typeRef="integer">
          <text>numeroVasos</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_007" label="Sintaxscore" camunda:inputVariable="sintaxScore">
        <inputExpression id="inputExpression_3_car_int_007" typeRef="integer">
          <text>sintaxScore</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_007" label="Funcaorenalbasal" camunda:inputVariable="funcaoRenalBasal">
        <inputExpression id="inputExpression_4_car_int_007" typeRef="string">
          <text>funcaoRenalBasal</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-007" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-007">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-007" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-007" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_007_r1">
        <inputEntry id="inputEntry_car_int_007_r1_1">
          <text>STEMI</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r1_2">
          <text>&gt; 1</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r1_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r1_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_007_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_007_r1_2">
          <text>"ICP staged: culprit only em STEMI multiarterial"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_007_r1_3">
          <text>"Evidência PRAMI: não-culprit em segundo tempo (ESC 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_007_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_007_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_007_r2">
        <inputEntry id="inputEntry_car_int_007_r2_1">
          <text>angina_estavel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r2_2">
          <text>&gt; 2</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r2_3">
          <text>&lt; 22</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r2_4">
          <text>&gt; 60</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_007_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_007_r2_2">
          <text>"Revascularização completa ad hoc se SYNTAX baixo"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_007_r2_3">
          <text>"SYNTAX ≤22 + função renal OK: 1 procedimento viável"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_007_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_007_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_007_r3">
        <inputEntry id="inputEntry_car_int_007_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r3_2">
          <text>&gt; 2</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r3_3">
          <text>&gt; 33</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_007_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_007_r3_2">
          <text>"ICP não indicada: SYNTAX alto indica CABG"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_007_r3_3">
          <text>"SYNTAX &gt;32: mortalidade menor com cirurgia (SYNTAX trial)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_007_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_007_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_007_r4">
        <inputEntry id="inputEntry_car_int_007_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_r4_4">
          <text>&lt; 30</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_007_r4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_007_r4_2">
          <text>"ICP staged: proteção renal"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_007_r4_3">
          <text>"TFG &lt;30: risco nefropatia por contraste - fracionar procedimentos"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_007_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_007_r4_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-007_fallback">
        <inputEntry id="inputEntry_car_int_007_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_007_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-007_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-007_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-007_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-007_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-007_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-007">
      <dmndi:DMNShape id="DMNShape_CAR-INT-007" dmnElementRef="CAR-INT-007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
