<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_012"
             name="Implante de Marcapasso Sem Cabo (Leadless Pacemaker)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-012
    Procedimento: Marcapasso Sem Cabo (Leadless - Micra)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    Bradicardia com indicacao VVIR, ausencia/comprometimento acesso venoso,
    historia previa infeccao dispositivo, alto risco infeccao

    CONTRAINDICACOES:
    Necessidade estimulacao atrial/bicameral, trombo VD, valva tricuspide mecanica

    REFERENCIAS TECNICAS:
    SBC/DECA Leadless 2023, HRS/EHRA 2024

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_012" name="Regra Clinica - Leadless Pacemaker">
    <decisionTable id="DecisionTable_CDQ_DEV_012" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_012" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_012" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_012">
          <text>"BradicardiaVVIR","FAPermanenteBradi","AcessoVenosoComprometido","InfeccaoPrevia","AltoRiscoInfeccao"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_012" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_012" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_012">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_012" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_012" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_012">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_necessidadeAV_012" label="Necessidade Sincronia AV" camunda:inputVariable="necessidadeAV">
        <inputExpression id="InputExpression_necessidadeAV_012" typeRef="boolean">
          <text>necessidadeAV</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_012" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_012" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_012" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_012">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_012" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_012" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_012">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_012_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_012_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_012_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_012_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_012_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_012_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_012_observacao"><text>"Leadless nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_012_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_012_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_012_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_012">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_012_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_012_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_012_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_012_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_012_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_012_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_012_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_012_fundamentacao"><text>"HRS/EHRA 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_012_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_012_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_NecessidadeAV_012">
        <description>Contraindicacao: Necessita sincronia AV - leadless nao adequado</description>
        <inputEntry id="InputEntry_Contraind_AV_012_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_AV_012_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_AV_012_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_AV_012_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_AV_012_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_AV_012_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_AV_012_observacao"><text>"Necessidade sincronia AV - considerar DDD convencional"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_AV_012_fundamentacao"><text>"SBC/DECA 2023 - Leadless VR nao mantem sincronia AV (Micra AV em avaliacao)"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_NecessidadeAV_012_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_NecessidadeAV_012_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe I -->
      <rule id="Rule_Aprovado_ClasseI_AcessoComprometido_012">
        <description>Aprovado: Acesso venoso comprometido</description>
        <inputEntry id="InputEntry_Aprovado_CI_AV_012_1"><text>"AcessoVenosoComprometido"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_AV_012_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_AV_012_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_AV_012_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_AV_012_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_AV_012_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_AV_012_observacao"><text>"Leadless indicado - acesso venoso comprometido"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_AV_012_fundamentacao"><text>"SBC/DECA 2023, HRS 2024 - Leadless alternativa quando acesso venoso inviavel"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_AcessoComprometido_012_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_AcessoComprometido_012_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_InfeccaoPrevia_012">
        <description>Aprovado: Historia infeccao previa</description>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_012_1"><text>"InfeccaoPrevia","AltoRiscoInfeccao"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_012_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_012_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_012_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Inf_012_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Inf_012_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Inf_012_observacao"><text>"Leadless indicado - reducao risco infeccao"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Inf_012_fundamentacao"><text>"Micra Registry - Taxa infeccao 0.13% vs 1.7% convencional"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_InfeccaoPrevia_012_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_InfeccaoPrevia_012_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_FA_012">
        <description>Aprovado: FA permanente com bradicardia</description>
        <inputEntry id="InputEntry_Aprovado_CI_FA_012_1"><text>"FAPermanenteBradi"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_FA_012_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_FA_012_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_FA_012_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_FA_012_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_FA_012_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_FA_012_observacao"><text>"FA permanente - VVIR leadless adequado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_FA_012_fundamentacao"><text>"SBC/DECA 2023 - FA permanente nao requer sincronia AV"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_FA_012_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_FA_012_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_012">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_012_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_012_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_012_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_012_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_012_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_012_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_012_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_012_fundamentacao"><text>"HRS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_012_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_012_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_012">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_012_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_012_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_012_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_012_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_012_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_012_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_012_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_012_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_012_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_012_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_012">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_012_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_012_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_012_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_012_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_012_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_012_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_012_observacao"><text>"Analise adicional necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_012_fundamentacao"><text>"Avaliacao eletrofisiologista"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_012_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_012_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
