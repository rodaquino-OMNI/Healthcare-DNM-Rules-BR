      <input id="Input_003" label="DuracaoQRS" camunda:inputVariable="duracaoqrs">
        <inputExpression id="InputExpression_003" typeRef="string"><text>duracaoqrs</text></inputExpression>
        <inputValues id="InputValues_003"><text>"Maior150ms,130a149ms,120a129ms,Menor120ms"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_HF003" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
