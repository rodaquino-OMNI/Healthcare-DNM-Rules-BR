<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-014" name="Prótese Valvar Mecânica vs Biológica" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-014" name="Prótese Valvar Mecânica vs Biológica">
    <decisionTable id="decisionTable_CAR-DEV-014" hitPolicy="FIRST">
      <input id="input_1_car_dev_014" label="Idadepaciente" camunda:inputVariable="idadePaciente">
        <inputExpression id="inputExpression_1_car_dev_014" typeRef="integer">
          <text>idadePaciente</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_014" label="Contraindicacaoaco" camunda:inputVariable="contraindicacaoACO">
        <inputExpression id="inputExpression_2_car_dev_014" typeRef="string">
          <text>contraindicacaoACO</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_014" label="Reintervencao" camunda:inputVariable="reintervencao">
        <inputExpression id="inputExpression_3_car_dev_014" typeRef="string">
          <text>reintervencao</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_014" label="Preferenciapaciente" camunda:inputVariable="preferenciaPaciente">
        <inputExpression id="inputExpression_4_car_dev_014" typeRef="string">
          <text>preferenciaPaciente</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-014" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-014">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-014" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-014" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_014_r1">
        <inputEntry id="inputEntry_car_dev_014_r1_1">
          <text>&lt; 60</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r1_2">
          <text>nenhuma</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r1_3">
          <text>aceita</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r1_4">
          <text>anticoagulacao</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_014_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_014_r1_2">
          <text>"Mecânica recomendada: jovem sem contraindicação ACO"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_014_r1_3">
          <text>"Durabilidade vitalícia evita reoperações (ESC/EACTS 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_014_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_014_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_014_r2">
        <inputEntry id="inputEntry_car_dev_014_r2_1">
          <text>&gt;= 70</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_014_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_014_r2_2">
          <text>"Bioprótese recomendada: idosos"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_014_r2_3">
          <text>"Expectativa vida &lt; durabilidade bioprótese - evitar ACO"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_014_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_014_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_014_r3">
        <inputEntry id="inputEntry_car_dev_014_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r3_2">
          <text>presente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_014_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_014_r3_2">
          <text>"Bioprótese obrigatória: contraindicação ACO"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_014_r3_3">
          <text>"Sangramento recorrente, fragilidade, impossibilidade monitorização INR"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_014_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_014_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_014_r4">
        <inputEntry id="inputEntry_car_dev_014_r4_1">
          <text>[60..69]</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_014_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_014_r4_2">
          <text>"Decisão compartilhada: balanço ACO vs reintervenção"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_014_r4_3">
          <text>"Zona cinzenta: considerar estilo vida, comorbidades, preferência paciente"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_014_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_014_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-014_fallback">
        <inputEntry id="inputEntry_car_dev_014_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_014_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-014_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-014_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-014_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-014_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-014_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-014">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-014" dmnElementRef="CAR-DEV-014">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
