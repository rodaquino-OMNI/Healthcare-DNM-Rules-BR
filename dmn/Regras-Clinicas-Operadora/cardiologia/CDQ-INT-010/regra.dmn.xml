<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_INT_010"
             name="MitraClip - Reparo Percutaneo Valva Mitral"
             namespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_CDQ_INT_010" name="Regra Clinica - MitraClip - Reparo Percutaneo Valva Mitral">
    <decisionTable id="DecisionTable_CDQ_INT_010" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_INT010" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_INT010" typeRef="string"><text>condicaoPaciente</text></inputExpression>
      </input>

      <input id="Input_historicoClinico_INT010" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_INT010" typeRef="string"><text>historicoClinico</text></inputExpression>
        <inputValues id="InputValues_historicoClinico_INT010"><text>"Adequado","Incompleto","NaoDocumentado"</text></inputValues>
      </input>

      <input id="Input_classeIndicacao_INT010" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_INT010" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_INT010"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
      </input>

      <input id="Input_riscoProcedimento_INT010" label="Risco Procedimento" camunda:inputVariable="riscoProcedimento">
        <inputExpression id="InputExpression_riscoProcedimento_INT010" typeRef="string"><text>riscoProcedimento</text></inputExpression>
        <inputValues id="InputValues_riscoProcedimento_INT010"><text>"Baixo","Intermediario","Alto","Proibitivo"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_INT010" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_INT010" typeRef="boolean"><text>expectativaVidaSuperior1Ano</text></inputExpression>
      </input>

      <output id="Output_resultado_INT010" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_INT010"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_INT010" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao_INT010" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraind_ClasseIII_INT010">
        <inputEntry id="ie1_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie2_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie3_INT010"><text>"Classe III"</text></inputEntry>
        <inputEntry id="ie4_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie5_INT010"><text>-</text></inputEntry>
        <outputEntry id="oe1_INT010"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2_INT010"><text>"Procedimento nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="oe3_INT010"><text>"COAPT Trial 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_INT010_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_INT010_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_INT010">
        <inputEntry id="ie1b_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie2b_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie3b_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie4b_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie5b_INT010"><text>false</text></inputEntry>
        <outputEntry id="oe1b_INT010"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2b_INT010"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="oe3b_INT010"><text>"COAPT Trial 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_INT010_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_INT010_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_INT010">
        <inputEntry id="ie1d_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie2d_INT010"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3d_INT010"><text>"Classe I"</text></inputEntry>
        <inputEntry id="ie4d_INT010"><text>"Baixo","Intermediario"</text></inputEntry>
        <inputEntry id="ie5d_INT010"><text>true</text></inputEntry>
        <outputEntry id="oe1d_INT010"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2d_INT010"><text>"Procedimento indicado - Classe I"</text></outputEntry>
        <outputEntry id="oe3d_INT010"><text>"COAPT Trial 2024 - Indicacao Classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_INT010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_INT010_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_INT010">
        <inputEntry id="ie1e_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie2e_INT010"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3e_INT010"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="ie4e_INT010"><text>"Baixo","Intermediario"</text></inputEntry>
        <inputEntry id="ie5e_INT010"><text>true</text></inputEntry>
        <outputEntry id="oe1e_INT010"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2e_INT010"><text>"Procedimento razoavel - Classe IIa"</text></outputEntry>
        <outputEntry id="oe3e_INT010"><text>"COAPT Trial 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_INT010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_INT010_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_INT010">
        <inputEntry id="ie1f_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie2f_INT010"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3f_INT010"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="ie4f_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie5f_INT010"><text>true</text></inputEntry>
        <outputEntry id="oe1f_INT010"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2f_INT010"><text>"Procedimento pode ser considerado - Classe IIb"</text></outputEntry>
        <outputEntry id="oe3f_INT010"><text>"COAPT Trial 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_INT010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_INT010_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_Hist_INT010">
        <inputEntry id="ie1g_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie2g_INT010"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="ie3g_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie4g_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie5g_INT010"><text>-</text></inputEntry>
        <outputEntry id="oe1g_INT010"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2g_INT010"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="oe3g_INT010"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_INT010_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_INT010_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_INT010">
        <inputEntry id="ie1h_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie2h_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie3h_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie4h_INT010"><text>-</text></inputEntry>
        <inputEntry id="ie5h_INT010"><text>-</text></inputEntry>
        <outputEntry id="oe1h_INT010"><text>"Pendente"</text></outputEntry>
        <outputEntry id="oe2h_INT010"><text>"Analise adicional por especialista"</text></outputEntry>
        <outputEntry id="oe3h_INT010"><text>"COAPT Trial 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_INT010_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_INT010_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
