<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_EP_001"
             name="Ablacao de Fibrilacao Atrial (FA)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-EP-001
    Procedimento: Ablacao de Fibrilacao Atrial - Isolamento de Veias Pulmonares
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    FA paroxistica sintomatica refrataria a pelo menos 1 AAD,
    FA persistente sintomatica, FA com IC

    CONTRAINDICACOES:
    Trombo em AE, contraindicacao anticoagulacao, estenose VP previa

    REFERENCIAS TECNICAS:
    HRS/EHRA/ECAS Ablation 2023, SBC/SOBRAC 2023, CASTLE-AF Trial

    ===============================================================================
  -->

  <decision id="Decision_CDQ_EP_001" name="Regra Clinica - Ablacao FA">
    <decisionTable id="DecisionTable_CDQ_EP_001" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_EP001" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_EP001" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_EP001">
          <text>"FAParoxisticaSintomatica","FAPersistenteSintomatica","FALongstanding","FAComIC","FAPrimeiraLinha"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_EP001" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_EP001" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_EP001">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_EP001" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_EP001" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_EP001">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_falhaAAD_EP001" label="Falha Antiarritmico" camunda:inputVariable="falhaAAD">
        <inputExpression id="InputExpression_falhaAAD_EP001" typeRef="string">
          <text>falhaAAD</text>
        </inputExpression>
        <inputValues id="InputValues_falhaAAD_EP001">
          <text>"FalhaAADClasse1ou3","Intolerancia","PrimeiraLinha","NaoTentado"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_EP001" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_EP001" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_EP001" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_EP001">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_EP001" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_EP001" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_EP001">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP001_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP001_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP001_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP001_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP001_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP001_observacao"><text>"Ablacao FA nao indicada - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP001_fundamentacao"><text>"HRS/EHRA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_EP001_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_EP001_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_EP001">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP001_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP001_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP001_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP001_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP001_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP001_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP001_fundamentacao"><text>"SOBRAC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_EP001_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_EP001_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_ClasseI_EP001">
        <description>Aprovado: FA paroxistica sintomatica apos falha AAD</description>
        <inputEntry id="InputEntry_Aprovado_CI_EP001_1"><text>"FAParoxisticaSintomatica","FAPersistenteSintomatica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP001_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP001_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP001_4"><text>"FalhaAADClasse1ou3","Intolerancia"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP001_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP001_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP001_observacao"><text>"Ablacao FA indicada - falha AAD documentada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP001_fundamentacao"><text>"HRS/EHRA 2023, CABANA Trial - Ablacao superior a AAD em FA sintomatica"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_EP001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_EP001_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_FAComIC_EP001">
        <description>Aprovado: FA com IC - CASTLE-AF</description>
        <inputEntry id="InputEntry_Aprovado_IC_EP001_1"><text>"FAComIC"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_IC_EP001_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_IC_EP001_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_IC_EP001_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_IC_EP001_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_IC_EP001_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_IC_EP001_observacao"><text>"Ablacao FA indicada em IC - beneficio mortalidade"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_IC_EP001_fundamentacao"><text>"CASTLE-AF Trial - Reducao mortalidade e hospitalizacao IC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_FAComIC_EP001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_FAComIC_EP001_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_EP001">
        <description>Aprovado: Classe IIa - primeira linha selecionados</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP001_1"><text>"FAPrimeiraLinha"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP001_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP001_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP001_4"><text>"PrimeiraLinha"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP001_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP001_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP001_observacao"><text>"Ablacao FA primeira linha razoavel em casos selecionados"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP001_fundamentacao"><text>"EARLY-AF Trial - Ablacao primeira linha eficaz"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_EP001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_EP001_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_EP001">
        <description>Aprovado: Classe IIb - longstanding</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_EP001_1"><text>"FALongstanding"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_EP001_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_EP001_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_EP001_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_EP001_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_EP001_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_EP001_observacao"><text>"Classe IIb - FA longstanding pode ser considerada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_EP001_fundamentacao"><text>"SOBRAC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_EP001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_EP001_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_EP001">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_EP001_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP001_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP001_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP001_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP001_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP001_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP001_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_EP001_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_EP001_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_EP_001">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_EP001_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP001_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP001_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP001_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP001_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_EP001_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_EP001_observacao"><text>"Analise adicional necessaria - ETE pre-procedimento"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_EP001_fundamentacao"><text>"HRS/EHRA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_EP_001_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_EP_001_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
