<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_EP_002"
             name="Ablacao de Flutter Atrial Tipico"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-EP-002
    Procedimento: Ablacao de Flutter Atrial Tipico - Istmo Cavotricuspideo
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    Flutter atrial tipico sintomatico, Flutter recorrente,
    Flutter degenerado de FA

    CONTRAINDICACOES:
    Flutter atipico (avaliar mapeamento), infeccao ativa

    REFERENCIAS TECNICAS:
    HRS/EHRA SVT 2023, SOBRAC 2023

    ===============================================================================
  -->

  <decision id="Decision_CDQ_EP_002" name="Regra Clinica - Ablacao Flutter">
    <decisionTable id="DecisionTable_CDQ_EP_002" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_EP002" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_EP002" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_EP002">
          <text>"FlutterTipicoSintomatico","FlutterRecorrente","FlutterPosAblacaoFA","FlutterAtipico"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_EP002" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_EP002" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_EP002">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_EP002" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_EP002" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_EP002">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_tipoFlutter_EP002" label="Tipo de Flutter" camunda:inputVariable="tipoFlutter">
        <inputExpression id="InputExpression_tipoFlutter_EP002" typeRef="string">
          <text>tipoFlutter</text>
        </inputExpression>
        <inputValues id="InputValues_tipoFlutter_EP002">
          <text>"ICTDependente","NaoICTDependente","Indeterminado"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_EP002" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_EP002" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_EP002" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_EP002">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_EP002" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_EP002" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_EP002">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP002_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP002_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP002_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP002_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_EP002_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP002_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP002_observacao"><text>"Ablacao Flutter nao indicada - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_EP002_fundamentacao"><text>"HRS/EHRA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_EP002_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_EP002_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_EP002">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP002_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP002_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP002_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP002_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_EP002_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP002_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP002_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_EP002_fundamentacao"><text>"SOBRAC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_EP002_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_EP002_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_ClasseI_EP002">
        <description>Aprovado: Flutter tipico ICT-dependente</description>
        <inputEntry id="InputEntry_Aprovado_CI_EP002_1"><text>"FlutterTipicoSintomatico","FlutterRecorrente"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP002_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP002_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP002_4"><text>"ICTDependente"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_EP002_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP002_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP002_observacao"><text>"Ablacao Flutter tipico indicada - alta taxa sucesso"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_EP002_fundamentacao"><text>"HRS/EHRA 2023 - Ablacao curativa em Flutter tipico maior 95%"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_EP002_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_EP002_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_EP002">
        <description>Aprovado: Flutter pos-ablacao FA</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP002_1"><text>"FlutterPosAblacaoFA"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP002_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP002_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP002_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_EP002_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP002_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP002_observacao"><text>"Ablacao Flutter pos-ablacao FA razoavel"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_EP002_fundamentacao"><text>"SOBRAC 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_EP002_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_EP002_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Pendente_Atipico_EP002">
        <description>Pendente: Flutter atipico - avaliar mapeamento</description>
        <inputEntry id="InputEntry_Pend_Atip_EP002_1"><text>"FlutterAtipico"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Atip_EP002_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Atip_EP002_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Atip_EP002_4"><text>"NaoICTDependente","Indeterminado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Atip_EP002_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Atip_EP002_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Atip_EP002_observacao"><text>"Flutter atipico - requer mapeamento 3D"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Atip_EP002_fundamentacao"><text>"HRS/EHRA 2023 - Mapeamento eletroatanomico recomendado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Pendente_Atipico_EP002_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Pendente_Atipico_EP002_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_EP002">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_EP002_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP002_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP002_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP002_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_EP002_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP002_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP002_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_EP002_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_EP002_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_EP002_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_EP_002">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_EP002_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP002_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP002_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP002_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_EP002_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_EP002_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_EP002_observacao"><text>"Analise adicional necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_EP002_fundamentacao"><text>"HRS/EHRA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_EP_002_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_EP_002_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
