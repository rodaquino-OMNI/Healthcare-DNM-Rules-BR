<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-025" name="Reabilitação Cardíaca - Fases" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-025" name="Reabilitação Cardíaca - Fases">
    <decisionTable id="decisionTable_CAR-INT-025" hitPolicy="FIRST">
      <input id="input_1_car_int_025" label="Evento" camunda:inputVariable="evento">
        <inputExpression id="inputExpression_1_car_int_025" typeRef="string">
          <text>evento</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_025" label="Tempopos Evento" camunda:inputVariable="tempoPos Evento">
        <inputExpression id="inputExpression_2_car_int_025" typeRef="integer">
          <text>tempoPos Evento</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_025" label="Estratificacaorisco" camunda:inputVariable="estratificacaoRisco">
        <inputExpression id="inputExpression_3_car_int_025" typeRef="string">
          <text>estratificacaoRisco</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_025" label="Aderenciamedicacao" camunda:inputVariable="aderenciaMedicacao">
        <inputExpression id="inputExpression_4_car_int_025" typeRef="string">
          <text>aderenciaMedicacao</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-025" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-025">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-025" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-025" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_025_r1">
        <inputEntry id="inputEntry_car_int_025_r1_1">
          <text>iam,cirurgia_cardiaca</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r1_2">
          <text>[7..14]dias</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r1_3">
          <text>baixo</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r1_4">
          <text>adequada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_025_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_025_r1_2">
          <text>"Fase II (ambulatorial supervisionada) aprovada"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_025_r1_3">
          <text>"Início 1-2 semanas pós-evento: supervisionado 3x/semana por 12 semanas"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_025_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_025_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_025_r2">
        <inputEntry id="inputEntry_car_int_025_r2_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r2_2">
          <text>[3..6]meses</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r2_3">
          <text>intermediario</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r2_4">
          <text>adequada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_025_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_025_r2_2">
          <text>"Transição Fase II para Fase III"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_025_r2_3">
          <text>"Manutenção longo prazo: redução mortalidade 20-30% (AHA 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_025_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_025_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_025_r3">
        <inputEntry id="inputEntry_car_int_025_r3_1">
          <text>ic_descompensada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r3_3">
          <text>alto</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_025_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_025_r3_2">
          <text>"Reabilitação contraindicada: instabilidade"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_025_r3_3">
          <text>"Estabilização clínica necessária antes início exercícios"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_025_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_025_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_025_r4">
        <inputEntry id="inputEntry_car_int_025_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_r4_4">
          <text>inadequada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_025_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_025_r4_2">
          <text>"Otimizar aderência antes prescrição exercício"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_025_r4_3">
          <text>"Reabilitação eficaz requer otimização farmacológica concomitante"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_025_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_025_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-025_fallback">
        <inputEntry id="inputEntry_car_int_025_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_025_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-025_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-025_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-025_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-025_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-025_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-025">
      <dmndi:DMNShape id="DMNShape_CAR-INT-025" dmnElementRef="CAR-INT-025">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
