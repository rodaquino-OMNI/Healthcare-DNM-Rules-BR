<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-EP-002" name="Cardioversão Elétrica FA - Timing" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-EP-002" name="Cardioversão Elétrica FA - Timing">
    <decisionTable id="decisionTable_CAR-EP-002" hitPolicy="FIRST">
      <input id="input_1_car_ep_002" label="Duracaofa" camunda:inputVariable="duracaoFA">
        <inputExpression id="inputExpression_1_car_ep_002" typeRef="string">
          <text>duracaoFA</text>
        </inputExpression>
      </input>
      <input id="input_2_car_ep_002" label="Anticoagulacao" camunda:inputVariable="anticoagulacao">
        <inputExpression id="inputExpression_2_car_ep_002" typeRef="string">
          <text>anticoagulacao</text>
        </inputExpression>
      </input>
      <input id="input_3_car_ep_002" label="Ete Trombo" camunda:inputVariable="ete Trombo">
        <inputExpression id="inputExpression_3_car_ep_002" typeRef="string">
          <text>ete Trombo</text>
        </inputExpression>
      </input>
      <input id="input_4_car_ep_002" label="Instabilidadehemodinamica" camunda:inputVariable="instabilidadeHemodinamica">
        <inputExpression id="inputExpression_4_car_ep_002" typeRef="integer">
          <text>instabilidadeHemodinamica</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-EP-002" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-EP-002">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-EP-002" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-EP-002" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_ep_002_r1">
        <inputEntry id="inputEntry_car_ep_002_r1_1">
          <text>&lt; 48h</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r1_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r1_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r1_4">
          <text>nao</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_002_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_002_r1_2">
          <text>"Cardioversão imediata: FA &lt;48h"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_002_r1_3">
          <text>"Risco tromboembólico baixo se FA recente documentada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_002_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_002_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_002_r2">
        <inputEntry id="inputEntry_car_ep_002_r2_1">
          <text>&gt; 48h</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r2_2">
          <text>adequada_3semanas</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r2_3">
          <text>ausente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r2_4">
          <text>nao</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_002_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_002_r2_2">
          <text>"Cardioversão eletiva: ACO + ausência trombo"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_002_r2_3">
          <text>"Estratégia convencional: 3 semanas ACO pré + 4 semanas pós-cardioversão"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_002_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_002_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_002_r3">
        <inputEntry id="inputEntry_car_ep_002_r3_1">
          <text>&gt; 48h</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r3_2">
          <text>inadequada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r3_4">
          <text>sim</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_002_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_002_r3_2">
          <text>"Cardioversão emergencial + anticoagulação imediata"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_002_r3_3">
          <text>"Instabilidade: cardioversão sem delay + heparina + ACO longo prazo"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_002_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_002_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_002_r4">
        <inputEntry id="inputEntry_car_ep_002_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r4_3">
          <text>presente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_002_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_002_r4_2">
          <text>"Trombo AE: adiar cardioversão"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_002_r4_3">
          <text>"ACO 3-4 semanas + ETE controle: cardioversão após resolução trombo"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_002_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_002_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-EP-002_fallback">
        <inputEntry id="inputEntry_car_ep_002_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_002_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-EP-002_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-002_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-002_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-EP-002_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-EP-002_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-EP-002">
      <dmndi:DMNShape id="DMNShape_CAR-EP-002" dmnElementRef="CAR-EP-002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
