<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-011" name="MitraClip - Critérios Insuficiência Mitral" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-011" name="MitraClip - Critérios Insuficiência Mitral">
    <decisionTable id="decisionTable_CAR-DEV-011" hitPolicy="FIRST">
      <input id="input_1_car_dev_011" label="Tipoim" camunda:inputVariable="tipoIM">
        <inputExpression id="inputExpression_1_car_dev_011" typeRef="string">
          <text>tipoIM</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_011" label="Feve" camunda:inputVariable="feve">
        <inputExpression id="inputExpression_2_car_dev_011" typeRef="integer">
          <text>feve</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_011" label="Grauim" camunda:inputVariable="grauIM">
        <inputExpression id="inputExpression_3_car_dev_011" typeRef="string">
          <text>grauIM</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_011" label="Anatomiavalvar" camunda:inputVariable="anatomiaValvar">
        <inputExpression id="inputExpression_4_car_dev_011" typeRef="string">
          <text>anatomiaValvar</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-011" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-011">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-011" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-011" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_011_r1">
        <inputEntry id="inputEntry_car_dev_011_r1_1">
          <text>funcional</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r1_2">
          <text>&lt; 30</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r1_3">
          <text>severa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r1_4">
          <text>anatomia_favoravel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_011_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_011_r1_2">
          <text>"MitraClip aprovado IM funcional FEVE reduzida"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_011_r1_3">
          <text>"COAPT trial: benefício mortalidade em IC + IM funcional severa"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_011_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_011_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_011_r2">
        <inputEntry id="inputEntry_car_dev_011_r2_1">
          <text>degenerativa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r2_3">
          <text>severa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r2_4">
          <text>anatomia_favoravel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_011_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_011_r2_2">
          <text>"MitraClip aprovado IM degenerativa alto risco cirúrgico"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_011_r2_3">
          <text>"Alternativa EVEREST para STS &gt;8% ou contraindicação cirurgia"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_011_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_011_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_011_r3">
        <inputEntry id="inputEntry_car_dev_011_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r3_2">
          <text>&lt; 20</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_011_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_011_r3_2">
          <text>"MitraClip contraindicado: FEVE &lt;20%"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_011_r3_3">
          <text>"Risco elevado + benefício incerto em disfunção severa"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_011_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_011_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_011_r4">
        <inputEntry id="inputEntry_car_dev_011_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_r4_4">
          <text>anatomia_complexa</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_011_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_011_r4_2">
          <text>"Anatomia desfavorável: preferir cirurgia"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_011_r4_3">
          <text>"Flail gap &gt;10mm ou calcificação comissural contraindicam MitraClip"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_011_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_011_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-011_fallback">
        <inputEntry id="inputEntry_car_dev_011_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_011_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-011_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-011_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-011_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-011_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-011_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-011">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-011" dmnElementRef="CAR-DEV-011">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
