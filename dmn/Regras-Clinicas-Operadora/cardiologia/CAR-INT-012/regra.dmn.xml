<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-012" name="Litotripsia Intravascular (IVL) - Calcificação Severa" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-012" name="Litotripsia Intravascular (IVL) - Calcificação Severa">
    <decisionTable id="decisionTable_CAR-INT-012" hitPolicy="FIRST">
      <input id="input_1_car_int_012" label="Calcificacaotype" camunda:inputVariable="calcificacaoType">
        <inputExpression id="inputExpression_1_car_int_012" typeRef="string">
          <text>calcificacaoType</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_012" label="Comprimentolesao" camunda:inputVariable="comprimentoLesao">
        <inputExpression id="inputExpression_2_car_int_012" typeRef="string">
          <text>comprimentoLesao</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_012" label="Tentativarotablator" camunda:inputVariable="tentativaRotablator">
        <inputExpression id="inputExpression_3_car_int_012" typeRef="string">
          <text>tentativaRotablator</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_012" label="Disponibilidadedispositivo" camunda:inputVariable="disponibilidadeDispositivo">
        <inputExpression id="inputExpression_4_car_int_012" typeRef="integer">
          <text>disponibilidadeDispositivo</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-012" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-012">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-012" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-012" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_012_r1">
        <inputEntry id="inputEntry_car_int_012_r1_1">
          <text>concentric_severe</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r1_2">
          <text>&gt; 20mm</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r1_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_012_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_012_r1_2">
          <text>"IVL aprovada: calcificação concêntrica extensa"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_012_r1_3">
          <text>"Shockwave IVL para lesões longas calcificadas (FDA approved 2021)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_012_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_012_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_012_r2">
        <inputEntry id="inputEntry_car_int_012_r2_1">
          <text>superficial</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r2_2">
          <text>&lt;= 10mm</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_012_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_012_r2_2">
          <text>"IVL não necessária: calcificação superficial curta"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_012_r2_3">
          <text>"Balão scoring ou NC suficiente para calcificação leve"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_012_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_012_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_012_r3">
        <inputEntry id="inputEntry_car_int_012_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r3_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_012_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_012_r3_2">
          <text>"IVL como alternativa/complemento Rotablator"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_012_r3_3">
          <text>"Menor risco perfuração que aterectomia rotacional em calcificação profunda"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_012_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_012_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_012_r4">
        <inputEntry id="inputEntry_car_int_012_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_r4_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_012_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_012_r4_2">
          <text>"IVL indisponível: considerar transferência ou Rotablator"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_012_r4_3">
          <text>"Dispositivo não amplamente disponível: avaliar alternativas"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_012_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_012_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-012_fallback">
        <inputEntry id="inputEntry_car_int_012_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_012_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-012_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-012_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-012_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-012_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-012_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-012">
      <dmndi:DMNShape id="DMNShape_CAR-INT-012" dmnElementRef="CAR-INT-012">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
