<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_009"
             name="Implante de CRT-D (Ressincronizador com Desfibrilador)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-009
    Procedimento: Implante de CRT-D (Terapia de Ressincronizacao Cardiaca com CDI)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS (DUT/ANS):
    IC sintomatica NYHA II-IV, FEVE menor ou igual 35%, QRS maior ou igual 130ms
    com morfologia BRE ou maior ou igual 150ms sem BRE, ritmo sinusal

    CONTRAINDICACOES:
    QRS menor 130ms, expectativa vida menor 1 ano, NYHA I, comorbidades graves

    REFERENCIAS TECNICAS:
    SBC/DECA TRC 2023, ESC CRT Guidelines 2024, ACC/AHA HF 2023

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_009" name="Regra Clinica - CRT-D">
    <decisionTable id="DecisionTable_CDQ_DEV_009" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_009" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_009" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_009">
          <text>"ICFEReduzida","ICFEModerada","BRECompleto","QRSAlargado","DissincroniaSevera"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_009" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_009" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_009">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_009" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_009" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_009">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_duracaoQRS_009" label="Duracao QRS (ms)" camunda:inputVariable="duracaoQRS">
        <inputExpression id="InputExpression_duracaoQRS_009" typeRef="string">
          <text>duracaoQRS</text>
        </inputExpression>
        <inputValues id="InputValues_duracaoQRS_009">
          <text>"menor130ms","130-149ms","maior150ms","NaoAvaliado"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_009" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_009" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_009" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_009">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_009" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_009" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_009">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_009_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_009_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_009_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_009_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_009_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_009_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_009_observacao"><text>"CRT-D nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_009_fundamentacao"><text>"SBC/DECA TRC 2023 - Classe III: nao indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_009_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_009_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_009">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_009_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_009_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_009_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_009_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_009_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_009_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_009_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_009_fundamentacao"><text>"ESC CRT 2024 - Requer expectativa vida maior 1 ano"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_009_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_009_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_QRSEstreito_009">
        <description>Contraindicacao: QRS menor 130ms</description>
        <inputEntry id="InputEntry_Contraind_QRS_009_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_QRS_009_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_QRS_009_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_QRS_009_4"><text>"menor130ms"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_QRS_009_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_QRS_009_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_QRS_009_observacao"><text>"QRS menor 130ms - CRT nao indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_QRS_009_fundamentacao"><text>"ESC 2024, ECHO-CRT Trial - CRT em QRS estreito aumenta mortalidade"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_QRSEstreito_009_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_QRSEstreito_009_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe I -->
      <rule id="Rule_Aprovado_ClasseI_QRS150BRE_009">
        <description>Aprovado: QRS maior 150ms com BRE</description>
        <inputEntry id="InputEntry_Aprovado_CI_QRS150_009_1"><text>"BRECompleto","DissincroniaSevera"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_QRS150_009_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_QRS150_009_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_QRS150_009_4"><text>"maior150ms"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_QRS150_009_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_QRS150_009_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_QRS150_009_observacao"><text>"BRE com QRS maior 150ms - Classe I para CRT-D"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_QRS150_009_fundamentacao"><text>"SBC/DECA 2023, COMPANION/MADIT-CRT - Maior beneficio em BRE com QRS alargado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_QRS150BRE_009_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_QRS150BRE_009_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_IC_009">
        <description>Aprovado: IC com indicacao Classe I</description>
        <inputEntry id="InputEntry_Aprovado_CI_IC_009_1"><text>"ICFEReduzida"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_IC_009_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_IC_009_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_IC_009_4"><text>"130-149ms","maior150ms"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_IC_009_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_IC_009_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_IC_009_observacao"><text>"ICFER com QRS alargado - CRT-D indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_IC_009_fundamentacao"><text>"ACC/AHA HF 2023 - FEVE menor 35% + QRS maior 130ms: Classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_IC_009_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_IC_009_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_009">
        <description>Aprovado: Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_009_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_009_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_009_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_009_4"><text>"130-149ms","maior150ms"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_009_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_009_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_009_observacao"><text>"Classe IIa - CRT-D provavelmente util"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_009_fundamentacao"><text>"ESC 2024 - Beneficio maior Risco"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_009_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_009_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_009">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_009_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_009_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_009_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_009_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_009_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_009_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_009_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_009_fundamentacao"><text>"SBC/DECA 2023 - Avaliacao individualizada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_009_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_009_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_009">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_009_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_009_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_009_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_009_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_009_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_009_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_009_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_009_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_009_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_009_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_009">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_009_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_009_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_009_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_009_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_009_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_009_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_009_observacao"><text>"Analise adicional necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_009_fundamentacao"><text>"Avaliacao individualizada SBC/DECA/ESC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_009_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_009_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
