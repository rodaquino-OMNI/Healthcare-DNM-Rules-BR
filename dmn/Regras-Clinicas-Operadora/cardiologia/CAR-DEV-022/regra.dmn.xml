<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-022" name="Stent Carotídeo vs Endarterectomia" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-022" name="Stent Carotídeo vs Endarterectomia">
    <decisionTable id="decisionTable_CAR-DEV-022" hitPolicy="FIRST">
      <input id="input_1_car_dev_022" label="Grauestenose" camunda:inputVariable="grauEstenose">
        <inputExpression id="inputExpression_1_car_dev_022" typeRef="string">
          <text>grauEstenose</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_022" label="Sintomas" camunda:inputVariable="sintomas">
        <inputExpression id="inputExpression_2_car_dev_022" typeRef="string">
          <text>sintomas</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_022" label="Anatomiacarotida" camunda:inputVariable="anatomiaCarotida">
        <inputExpression id="inputExpression_3_car_dev_022" typeRef="string">
          <text>anatomiaCarotida</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_022" label="Comorbidades" camunda:inputVariable="comorbidades">
        <inputExpression id="inputExpression_4_car_dev_022" typeRef="integer">
          <text>comorbidades</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-022" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-022">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-022" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-022" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_022_r1">
        <inputEntry id="inputEntry_car_dev_022_r1_1">
          <text>&gt; 70</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r1_2">
          <text>recente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r1_3">
          <text>favoravel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r1_4">
          <text>baixas</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_022_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_022_r1_2">
          <text>"CEA primeira linha: estenose sintomática"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_022_r1_3">
          <text>"NASCET: CEA reduz AVC 17% vs 32% TMO em estenose &gt;70%"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_022_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_022_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_022_r2">
        <inputEntry id="inputEntry_car_dev_022_r2_1">
          <text>&gt; 70</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r2_2">
          <text>recente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r2_3">
          <text>desfavoravel</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r2_4">
          <text>altas</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_022_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_022_r2_2">
          <text>"CAS se anatomia ou comorbidade desfavorecem CEA"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_022_r2_3">
          <text>"Reestenose pós-CEA, radiação cervical, idade &gt;80: CAS razoável"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_022_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_022_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_022_r3">
        <inputEntry id="inputEntry_car_dev_022_r3_1">
          <text>&lt; 50</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_022_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_022_r3_2">
          <text>"Estenose &lt;50%: TMO suficiente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_022_r3_3">
          <text>"Risco procedimento supera benefício em estenose leve"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_022_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_022_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_022_r4">
        <inputEntry id="inputEntry_car_dev_022_r4_1">
          <text>[50..69]</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r4_2">
          <text>assintomatico</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_r4_4">
          <text>altas</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_022_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_022_r4_2">
          <text>"Estenose moderada assintomática: TMO preferível"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_022_r4_3">
          <text>"Risco-benefício desfavorável para revascularização"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_022_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_022_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-022_fallback">
        <inputEntry id="inputEntry_car_dev_022_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_022_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-022_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-022_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-022_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-022_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-022_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-022">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-022" dmnElementRef="CAR-DEV-022">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
