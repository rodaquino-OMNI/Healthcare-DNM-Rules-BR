<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_INT_008"
             name="Cirurgia de Revascularizacao Miocardica (CABG)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-INT-008
    Procedimento: CABG - Cirurgia de Revascularizacao Miocardica
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    Referencias: ACC/AHA CABG 2023, ESC Revascularization 2024
    ===============================================================================
  -->

  <decision id="Decision_CDQ_INT_008" name="Regra Clinica - CABG">
    <decisionTable id="DecisionTable_CDQ_INT_008" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_INT008" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_INT008" typeRef="string"><text>condicaoPaciente</text></inputExpression>
        <inputValues id="InputValues_condicaoPaciente_INT008"><text>"TriarterialDAP","TCESignificativo","DiabetesTriarterial","FEVEBaixaIsquemica"</text></inputValues>
      </input>

      <input id="Input_historicoClinico_INT008" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_INT008" typeRef="string"><text>historicoClinico</text></inputExpression>
        <inputValues id="InputValues_historicoClinico_INT008"><text>"Adequado","Incompleto","NaoDocumentado"</text></inputValues>
      </input>

      <input id="Input_classeIndicacao_INT008" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_INT008" typeRef="string"><text>classeIndicacao</text></inputExpression>
        <inputValues id="InputValues_classeIndicacao_INT008"><text>"Classe I","Classe IIa","Classe IIb","Classe III"</text></inputValues>
      </input>

      <input id="Input_riscoCircurgico_INT008" label="Risco Cirurgico (STS Score)" camunda:inputVariable="riscoCircurgico">
        <inputExpression id="InputExpression_riscoCircurgico_INT008" typeRef="string"><text>riscoCircurgico</text></inputExpression>
        <inputValues id="InputValues_riscoCircurgico_INT008"><text>"Baixo","Intermediario","Alto","Proibitivo"</text></inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_INT008" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_INT008" typeRef="boolean"><text>expectativaVidaSuperior1Ano</text></inputExpression>
      </input>

      <output id="Output_resultado_INT008" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_INT008"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao_INT008" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao_INT008" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraind_ClasseIII_INT008">
        <inputEntry id="ie1_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie2_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie3_INT008"><text>"Classe III"</text></inputEntry>
        <inputEntry id="ie4_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie5_INT008"><text>-</text></inputEntry>
        <outputEntry id="oe1_INT008"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2_INT008"><text>"CABG nao indicado"</text></outputEntry>
        <outputEntry id="oe3_INT008"><text>"ACC/AHA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_INT008_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_INT008_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_RiscoProibitivo_INT008">
        <inputEntry id="ie1b_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie2b_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie3b_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie4b_INT008"><text>"Proibitivo"</text></inputEntry>
        <inputEntry id="ie5b_INT008"><text>-</text></inputEntry>
        <outputEntry id="oe1b_INT008"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2b_INT008"><text>"Risco cirurgico proibitivo - considerar PCI ou tratamento clinico"</text></outputEntry>
        <outputEntry id="oe3b_INT008"><text>"STS Score - Mortalidade prevista proibitiva"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_RiscoProibitivo_INT008_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_RiscoProibitivo_INT008_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Triarterial_INT008">
        <inputEntry id="ie1c_INT008"><text>"TriarterialDAP","TCESignificativo","DiabetesTriarterial"</text></inputEntry>
        <inputEntry id="ie2c_INT008"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3c_INT008"><text>"Classe I"</text></inputEntry>
        <inputEntry id="ie4c_INT008"><text>"Baixo","Intermediario"</text></inputEntry>
        <inputEntry id="ie5c_INT008"><text>true</text></inputEntry>
        <outputEntry id="oe1c_INT008"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2c_INT008"><text>"CABG indicado - doenca multiarterial"</text></outputEntry>
        <outputEntry id="oe3c_INT008"><text>"FREEDOM Trial - CABG superior em DM triarterial"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_Triarterial_INT008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_Triarterial_INT008_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_CIIa_INT008">
        <inputEntry id="ie1d_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie2d_INT008"><text>"Adequado"</text></inputEntry>
        <inputEntry id="ie3d_INT008"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="ie4d_INT008"><text>"Baixo","Intermediario"</text></inputEntry>
        <inputEntry id="ie5d_INT008"><text>true</text></inputEntry>
        <outputEntry id="oe1d_INT008"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="oe2d_INT008"><text>"CABG razoavel"</text></outputEntry>
        <outputEntry id="oe3d_INT008"><text>"ESC 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_CIIa_INT008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_CIIa_INT008_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_Hist_INT008">
        <inputEntry id="ie1e_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie2e_INT008"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="ie3e_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie4e_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie5e_INT008"><text>-</text></inputEntry>
        <outputEntry id="oe1e_INT008"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="oe2e_INT008"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="oe3e_INT008"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_INT008_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_INT008_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_INT008">
        <inputEntry id="ie1f_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie2f_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie3f_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie4f_INT008"><text>-</text></inputEntry>
        <inputEntry id="ie5f_INT008"><text>-</text></inputEntry>
        <outputEntry id="oe1f_INT008"><text>"Pendente"</text></outputEntry>
        <outputEntry id="oe2f_INT008"><text>"Heart Team discussion"</text></outputEntry>
        <outputEntry id="oe3f_INT008"><text>"ACC/AHA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_INT008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_INT008_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
