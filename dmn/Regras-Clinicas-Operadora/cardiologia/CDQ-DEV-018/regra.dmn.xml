<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_018"
             name="CDI Subcutaneo (S-ICD)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-018
    Procedimento: CDI Subcutaneo (S-ICD)
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    Indicacao CDI sem necessidade estimulacao antibradicardia ou ATP,
    acesso venoso comprometido, alto risco infeccao endovascular

    CONTRAINDICACOES:
    Necessidade estimulacao bradicardia, necessidade ATP para TV,
    morfologia ECG incompativel

    REFERENCIAS TECNICAS:
    HRS S-ICD 2024, SBC/DECA 2023

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_018" name="Regra Clinica - S-ICD">
    <decisionTable id="DecisionTable_CDQ_DEV_018" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_018" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_018" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_018">
          <text>"IndicacaoCDISemPacing","AcessoVenosoComprometido","RiscoInfeccaoEndovascular","JovemAtivo"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_018" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_018" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_018">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_018" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_018" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_018">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_necessidadePacing_018" label="Necessidade Estimulacao/ATP" camunda:inputVariable="necessidadePacing">
        <inputExpression id="InputExpression_necessidadePacing_018" typeRef="boolean">
          <text>necessidadePacing</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_018" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_018" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_018" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_018">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_018" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_018" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_018">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_018_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_018_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_018_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_018_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_018_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_018_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_018_observacao"><text>"S-ICD nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_018_fundamentacao"><text>"HRS S-ICD 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_018_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_018_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_018">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_018_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_018_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_018_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_018_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_018_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_018_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_018_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_018_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_018_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_018_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_NecessitaPacing_018">
        <description>Contraindicacao: Necessita pacing/ATP</description>
        <inputEntry id="InputEntry_Contraind_Pac_018_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Pac_018_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Pac_018_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Pac_018_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Pac_018_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Pac_018_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Pac_018_observacao"><text>"Necessita pacing/ATP - indicar CDI transvenoso"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Pac_018_fundamentacao"><text>"HRS 2024 - S-ICD nao oferece pacing antibradicardia ou ATP"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_NecessitaPacing_018_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_NecessitaPacing_018_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_ClasseI_018">
        <description>Aprovado: Indicacao sem pacing</description>
        <inputEntry id="InputEntry_Aprovado_CI_018_1"><text>"IndicacaoCDISemPacing","AcessoVenosoComprometido","RiscoInfeccaoEndovascular"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_018_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_018_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_018_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_018_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_018_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_018_observacao"><text>"S-ICD indicado - sem necessidade pacing"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_018_fundamentacao"><text>"HRS 2024, PRAETORIAN Trial - Eficacia similar CDI transvenoso, menor risco infeccao"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_018_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_018_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_018">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_018_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_018_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_018_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_018_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_018_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_018_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_018_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_018_fundamentacao"><text>"SBC/DECA 2023"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_018_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_018_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_018">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_018_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_018_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_018_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_018_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_018_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_018_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_018_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_018_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_018_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_018_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_018">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_018_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_018_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_018_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_018_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_018_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_018_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_018_observacao"><text>"Analise adicional - screening ECG obrigatorio"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_018_fundamentacao"><text>"HRS 2024"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_018_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_018_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
