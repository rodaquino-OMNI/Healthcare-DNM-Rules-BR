<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-019" name="Ablação Nó AV - Controle Frequência Permanente" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-019" name="Ablação Nó AV - Controle Frequência Permanente">
    <decisionTable id="decisionTable_CAR-INT-019" hitPolicy="FIRST">
      <input id="input_1_car_int_019" label="Controlefrequencia" camunda:inputVariable="controleFrequencia">
        <inputExpression id="inputExpression_1_car_int_019" typeRef="string">
          <text>controleFrequencia</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_019" label="Qualidadevida" camunda:inputVariable="qualidadeVida">
        <inputExpression id="inputExpression_2_car_int_019" typeRef="integer">
          <text>qualidadeVida</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_019" label="Dependenciamarcapasso" camunda:inputVariable="dependenciaMarcapasso">
        <inputExpression id="inputExpression_3_car_int_019" typeRef="string">
          <text>dependenciaMarcapasso</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_019" label="Alternativasterapeuticas" camunda:inputVariable="alternativasTerapeuticas">
        <inputExpression id="inputExpression_4_car_int_019" typeRef="string">
          <text>alternativasTerapeuticas</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-019" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-019">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-019" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-019" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_019_r1">
        <inputEntry id="inputEntry_car_int_019_r1_1">
          <text>refratario_completo</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r1_2">
          <text>severamente_comprometida</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r1_3">
          <text>aceita</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r1_4">
          <text>esgotadas</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_019_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_019_r1_2">
          <text>"Ablação AV + MP aprovada: refratariedade completa"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_019_r1_3">
          <text>"Classe IIa para FA permanente não controlável (HRS 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_019_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_019_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_019_r2">
        <inputEntry id="inputEntry_car_int_019_r2_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r2_3">
          <text>recusa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_019_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_019_r2_2">
          <text>"Ablação AV sem aceite dependência MP"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_019_r2_3">
          <text>"Paciente deve compreender necessidade MP definitivo"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_019_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_019_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_019_r3">
        <inputEntry id="inputEntry_car_int_019_r3_1">
          <text>refratario_parcial</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r3_4">
          <text>nao_tentadas</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_019_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_019_r3_2">
          <text>"Esgotar alternativas antes ablação AV"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_019_r3_3">
          <text>"Tentar digoxina, betabloqueador, amiodarona, ablação FA antes"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_019_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_019_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_019_r4">
        <inputEntry id="inputEntry_car_int_019_r4_1">
          <text>controlado</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_019_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_019_r4_2">
          <text>"Controle adequado: ablação AV desnecessária"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_019_r4_3">
          <text>"Ablação AV último recurso - irreversível"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_019_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_019_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-019_fallback">
        <inputEntry id="inputEntry_car_int_019_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_019_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-019_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-019_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-019_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-019_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-019_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-019">
      <dmndi:DMNShape id="DMNShape_CAR-INT-019" dmnElementRef="CAR-INT-019">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
