<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-IMG-003" name="Stress Echo vs Medicina Nuclear" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-IMG-003" name="Stress Echo vs Medicina Nuclear">
    <decisionTable id="decisionTable_CAR-IMG-003" hitPolicy="FIRST">
      <input id="input_1_car_img_003" label="Imc" camunda:inputVariable="imc">
        <inputExpression id="inputExpression_1_car_img_003" typeRef="string">
          <text>imc</text>
        </inputExpression>
      </input>
      <input id="input_2_car_img_003" label="Qualidadejanelaeco" camunda:inputVariable="qualidadeJanelaEco">
        <inputExpression id="inputExpression_2_car_img_003" typeRef="integer">
          <text>qualidadeJanelaEco</text>
        </inputExpression>
      </input>
      <input id="input_3_car_img_003" label="Clearancerenal" camunda:inputVariable="clearanceRenal">
        <inputExpression id="inputExpression_3_car_img_003" typeRef="string">
          <text>clearanceRenal</text>
        </inputExpression>
      </input>
      <input id="input_4_car_img_003" label="Mobilidade Paciente" camunda:inputVariable="mobilidade Paciente">
        <inputExpression id="inputExpression_4_car_img_003" typeRef="integer">
          <text>mobilidade Paciente</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-IMG-003" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-IMG-003">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-IMG-003" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-IMG-003" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_img_003_r1">
        <inputEntry id="inputEntry_car_img_003_r1_1">
          <text>&lt; 30</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r1_2">
          <text>boa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r1_3">
          <text>&gt; 60</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r1_4">
          <text>adequada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_003_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_003_r1_2">
          <text>"Stress eco primeira linha: custo-efetivo"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_003_r1_3">
          <text>"Sensibilidade 85% para DAC se janela acústica adequada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_003_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_003_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_003_r2">
        <inputEntry id="inputEntry_car_img_003_r2_1">
          <text>&gt;= 35</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r2_2">
          <text>ruim</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_003_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_003_r2_2">
          <text>"SPECT preferencial: obesidade limita eco"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_003_r2_3">
          <text>"Medicina nuclear não depende janela acústica"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_003_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_003_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_003_r3">
        <inputEntry id="inputEntry_car_img_003_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r3_3">
          <text>&lt; 30</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_003_r3_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_003_r3_2">
          <text>"Avaliar risco contraste em IRC"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_003_r3_3">
          <text>"SPECT não requer contraste mas pode necessitar dipiridamol/regadenoson"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_003_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_003_r3_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_003_r4">
        <inputEntry id="inputEntry_car_img_003_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_r4_4">
          <text>limitada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_003_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_003_r4_2">
          <text>"Teste incapacidade exercício: farmacológico indicado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_003_r4_3">
          <text>"Dobutamina-eco ou SPECT-dipiridamol para pacientes acamados"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_003_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_003_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-IMG-003_fallback">
        <inputEntry id="inputEntry_car_img_003_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_003_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-IMG-003_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-003_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-003_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-IMG-003_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-IMG-003_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-IMG-003">
      <dmndi:DMNShape id="DMNShape_CAR-IMG-003" dmnElementRef="CAR-IMG-003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
