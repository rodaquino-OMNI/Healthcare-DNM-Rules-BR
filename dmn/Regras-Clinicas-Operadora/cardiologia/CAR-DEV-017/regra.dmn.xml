<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-017" name="ASD/VSD Device Closure - Defeitos Septais" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-017" name="ASD/VSD Device Closure - Defeitos Septais">
    <decisionTable id="decisionTable_CAR-DEV-017" hitPolicy="FIRST">
      <input id="input_1_car_dev_017" label="Tipodefeito" camunda:inputVariable="tipoDefeito">
        <inputExpression id="inputExpression_1_car_dev_017" typeRef="string">
          <text>tipoDefeito</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_017" label="Tamanhomm" camunda:inputVariable="tamanhoMm">
        <inputExpression id="inputExpression_2_car_dev_017" typeRef="string">
          <text>tamanhoMm</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_017" label="Bordasadequadas" camunda:inputVariable="bordasAdequadas">
        <inputExpression id="inputExpression_3_car_dev_017" typeRef="string">
          <text>bordasAdequadas</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_017" label="Hipertensaopulmonar" camunda:inputVariable="hipertensaoPulmonar">
        <inputExpression id="inputExpression_4_car_dev_017" typeRef="string">
          <text>hipertensaoPulmonar</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-017" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-017">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-017" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-017" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_017_r1">
        <inputEntry id="inputEntry_car_dev_017_r1_1">
          <text>asd_ostium_secundum</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r1_2">
          <text>&lt; 38</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r1_3">
          <text>adequadas</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r1_4">
          <text>ausente_ou_leve</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_017_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_017_r1_2">
          <text>"Fechamento percutâneo ASD: anatomia favorável"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_017_r1_3">
          <text>"Tamanho &lt;38mm + bordas ≥5mm: sucesso &gt;95%"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_017_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_017_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_017_r2">
        <inputEntry id="inputEntry_car_dev_017_r2_1">
          <text>vsd_perimembranoso</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r2_2">
          <text>&lt; 12</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r2_3">
          <text>adequadas</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r2_4">
          <text>ausente</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_017_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_017_r2_2">
          <text>"Fechamento VSD selecionado: anatomia específica"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_017_r2_3">
          <text>"VSD perimembranoso sem aneurisma septal: device closure possível"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_017_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_017_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_017_r3">
        <inputEntry id="inputEntry_car_dev_017_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r3_4">
          <text>severa_irreversivel</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_017_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_017_r3_2">
          <text>"HP severa irreversível contraindica fechamento"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_017_r3_3">
          <text>"RVP &gt;8 UW ou Qp:Qs &lt;1.5: síndrome Eisenmenger - não fechar"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_017_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_017_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_017_r4">
        <inputEntry id="inputEntry_car_dev_017_r4_1">
          <text>asd_ostium_primum,vsd_inlet</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_017_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_017_r4_2">
          <text>"Defeito tipo requer cirurgia"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_017_r4_3">
          <text>"ASD primum / VSD inlet: envolvimento estruturas AV - cirurgia obrigatória"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_017_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_017_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-017_fallback">
        <inputEntry id="inputEntry_car_dev_017_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_017_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-017_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-017_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-017_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-017_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-017_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-017">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-017" dmnElementRef="CAR-DEV-017">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
