<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-EP-009" name="Teste Provocação Ajmalina/Procainamida - Brugada" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-EP-009" name="Teste Provocação Ajmalina/Procainamida - Brugada">
    <decisionTable id="decisionTable_CAR-EP-009" hitPolicy="FIRST">
      <input id="input_1_car_ep_009" label="Ecgbasal" camunda:inputVariable="ecgBasal">
        <inputExpression id="inputExpression_1_car_ep_009" typeRef="string">
          <text>ecgBasal</text>
        </inputExpression>
      </input>
      <input id="input_2_car_ep_009" label="Historiasincope" camunda:inputVariable="historiaSincope">
        <inputExpression id="inputExpression_2_car_ep_009" typeRef="string">
          <text>historiaSincope</text>
        </inputExpression>
      </input>
      <input id="input_3_car_ep_009" label="Mortesubitafamiliar" camunda:inputVariable="morteSubitaFamiliar">
        <inputExpression id="inputExpression_3_car_ep_009" typeRef="string">
          <text>morteSubitaFamiliar</text>
        </inputExpression>
      </input>
      <input id="input_4_car_ep_009" label="Sintomasprovocar" camunda:inputVariable="sintomasProvocar">
        <inputExpression id="inputExpression_4_car_ep_009" typeRef="string">
          <text>sintomasProvocar</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-EP-009" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-EP-009">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-EP-009" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-EP-009" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_ep_009_r1">
        <inputEntry id="inputEntry_car_ep_009_r1_1">
          <text>tipo2_tipo3</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r1_2">
          <text>presente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r1_3">
          <text>positiva</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r1_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_009_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_009_r1_2">
          <text>"Teste provocação: ECG suspeito + história"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_009_r1_3">
          <text>"Unmask padrão tipo 1: diagnóstico Brugada - considerar CDI"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_009_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_009_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_009_r2">
        <inputEntry id="inputEntry_car_ep_009_r2_1">
          <text>tipo1_espontaneo</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_009_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_009_r2_2">
          <text>"ECG tipo 1 basal: diagnóstico confirmado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_009_r2_3">
          <text>"Teste provocação desnecessário - proceder estratificação risco"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_009_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_009_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_009_r3">
        <inputEntry id="inputEntry_car_ep_009_r3_1">
          <text>normal</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r3_2">
          <text>ausente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r3_3">
          <text>negativa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_009_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_009_r3_2">
          <text>"Baixa suspeita: teste não indicado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_009_r3_3">
          <text>"Probabilidade pré-teste baixa: teste provocação não recomendado"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_009_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_009_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_009_r4">
        <inputEntry id="inputEntry_car_ep_009_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_r4_4">
          <text>bradicardia_severa,alergiaAjmalina</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_009_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_009_r4_2">
          <text>"Contraindicação teste provocação"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_009_r4_3">
          <text>"FC &lt;50, BAV, alergia: evitar bloqueadores sódio"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_009_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_009_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-EP-009_fallback">
        <inputEntry id="inputEntry_car_ep_009_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_009_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-EP-009_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-009_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-009_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-EP-009_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-EP-009_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-EP-009">
      <dmndi:DMNShape id="DMNShape_CAR-EP-009" dmnElementRef="CAR-EP-009">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
