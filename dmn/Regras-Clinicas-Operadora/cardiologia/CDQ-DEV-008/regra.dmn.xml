<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_CDQ_DEV_008"
             name="Implante de CDI para Prevencao Secundaria"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    REGRA: CDQ-DEV-008
    Procedimento: Implante de CDI - Prevencao Secundaria
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS (DUT/ANS):
    Sobrevivente de parada cardiaca por FV/TV, TV sustentada sintomatica,
    sincope com TV induzida em EEF

    CONTRAINDICACOES:
    Causa reversivel identificada, expectativa vida menor 1 ano,
    arritmia de causa tratavel

    REFERENCIAS TECNICAS:
    SBC/DECA CDI 2023, HRS/EHRA SCD Prevention 2024

    ===============================================================================
  -->

  <decision id="Decision_CDQ_DEV_008" name="Regra Clinica - CDI Prevencao Secundaria">
    <decisionTable id="DecisionTable_CDQ_DEV_008" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente_008" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente_008" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente_008">
          <text>"PCRRecuperada","TVSustentada","FVDocumentada","SincopeTVInduzida","TVPolimorficaSustentada"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico_008" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico_008" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico_008">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao_008" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao_008" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao_008">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_causaReversivel_008" label="Causa Reversivel Identificada" camunda:inputVariable="causaReversivel">
        <inputExpression id="InputExpression_causaReversivel_008" typeRef="boolean">
          <text>causaReversivel</text>
        </inputExpression>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano_008" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano_008" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_008" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado_008">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao_008" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao_008" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICACOES -->
      <rule id="Rule_Contraind_ClasseIII_008">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_008_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_008_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_008_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_008_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_008_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_008_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_008_observacao"><text>"CDI nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_008_fundamentacao"><text>"SBC/DECA 2023 - Classe III: Procedimento nao indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_008_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ClasseIII_008_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpVida_008">
        <description>Contraindicacao: Expectativa vida menor 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_008_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_008_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_008_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_008_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_008_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_008_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_008_observacao"><text>"Expectativa vida insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_008_fundamentacao"><text>"HRS/EHRA 2024 - CDI requer expectativa vida maior 1 ano"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_008_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_ExpVida_008_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_CausaReversivel_008">
        <description>Contraindicacao: Causa reversivel identificada</description>
        <inputEntry id="InputEntry_Contraind_CausaRev_008_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CausaRev_008_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CausaRev_008_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CausaRev_008_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CausaRev_008_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_CausaRev_008_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CausaRev_008_observacao"><text>"Arritmia por causa reversivel - tratar causa base primeiro"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CausaRev_008_fundamentacao"><text>"SBC/DECA 2023 - Causas reversiveis: disturbio eletrolitico, isquemia aguda, farmacos pro-arritmicos"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_CausaReversivel_008_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Contraind_CausaReversivel_008_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES Classe I -->
      <rule id="Rule_Aprovado_ClasseI_PCR_008">
        <description>Aprovado: PCR recuperada sem causa reversivel</description>
        <inputEntry id="InputEntry_Aprovado_CI_PCR_008_1"><text>"PCRRecuperada","FVDocumentada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_PCR_008_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_PCR_008_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_PCR_008_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_PCR_008_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_PCR_008_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_PCR_008_observacao"><text>"PCR/FV recuperada - CDI prevencao secundaria indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_PCR_008_fundamentacao"><text>"SBC/DECA 2023, AVID Trial - Reducao mortalidade 27% com CDI em sobreviventes PCR"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_PCR_008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_PCR_008_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_TVSust_008">
        <description>Aprovado: TV sustentada sintomatica</description>
        <inputEntry id="InputEntry_Aprovado_CI_TVS_008_1"><text>"TVSustentada","TVPolimorficaSustentada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_TVS_008_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_TVS_008_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_TVS_008_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_TVS_008_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_TVS_008_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_TVS_008_observacao"><text>"TV sustentada sintomatica - CDI indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_TVS_008_fundamentacao"><text>"SBC/DECA 2023, HRS/EHRA 2024 - TV sustentada com comprometimento hemodinamico: Classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_TVSust_008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseI_TVSust_008_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa_008">
        <description>Aprovado: Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_008_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_008_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_008_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_008_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_008_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_008_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_008_observacao"><text>"Classe IIa - CDI provavelmente util"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_008_fundamentacao"><text>"SBC/DECA 2023 - Beneficio maior Risco"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIa_008_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb_008">
        <description>Aprovado: Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_008_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_008_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_008_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_008_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_008_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_008_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_008_observacao"><text>"Classe IIb - pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_008_fundamentacao"><text>"SBC/DECA 2023 - Avaliacao individualizada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Aprovado_ClasseIIb_008_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACAO -->
      <rule id="Rule_Reprovado_Hist_008">
        <description>Reprovado: Documentacao inadequada</description>
        <inputEntry id="InputEntry_Reprovado_Hist_008_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_008_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_008_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_008_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_008_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_008_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_008_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_008_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_008_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Reprovado_Hist_008_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_CDQ_DEV_008">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_008_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_008_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_008_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_008_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_008_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_008_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_008_observacao"><text>"Analise adicional necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_008_fundamentacao"><text>"Avaliacao individualizada SBC/DECA"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_008_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_CDQ_DEV_008_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
