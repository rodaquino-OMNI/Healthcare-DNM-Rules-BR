<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-IMG-017" name="MUGA Scan - Ventriculografia Radioisotópica" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-IMG-017" name="MUGA Scan - Ventriculografia Radioisotópica">
    <decisionTable id="decisionTable_CAR-IMG-017" hitPolicy="FIRST">
      <input id="input_1_car_img_017" label="Indicacao" camunda:inputVariable="indicacao">
        <inputExpression id="inputExpression_1_car_img_017" typeRef="string">
          <text>indicacao</text>
        </inputExpression>
      </input>
      <input id="input_2_car_img_017" label="Disponibilidadeeco" camunda:inputVariable="disponibilidadeEco">
        <inputExpression id="inputExpression_2_car_img_017" typeRef="integer">
          <text>disponibilidadeEco</text>
        </inputExpression>
      </input>
      <input id="input_3_car_img_017" label="Quimioterapiaemcurso" camunda:inputVariable="quimioterapiaEmCurso">
        <inputExpression id="inputExpression_3_car_img_017" typeRef="string">
          <text>quimioterapiaEmCurso</text>
        </inputExpression>
      </input>
      <input id="input_4_car_img_017" label="Contraindicacaormc" camunda:inputVariable="contraindicacaoRMC">
        <inputExpression id="inputExpression_4_car_img_017" typeRef="string">
          <text>contraindicacaoRMC</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-IMG-017" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-IMG-017">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-IMG-017" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-IMG-017" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_img_017_r1">
        <inputEntry id="inputEntry_car_img_017_r1_1">
          <text>cardiotoxicidade_antraciclina</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r1_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r1_3">
          <text>True</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r1_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_017_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_017_r1_2">
          <text>"MUGA seriada: monitorização FEVE em quimio"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_017_r1_3">
          <text>"Reprodutibilidade superior ECO para mudanças sutis FEVE (variabilidade <5%)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_017_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_017_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_017_r2">
        <inputEntry id="inputEntry_car_img_017_r2_1">
          <text>baseline_quimio</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r2_2">
          <text>True</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r2_3">
          <text>True</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_017_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_017_r2_2">
          <text>"ECO suficiente: custo-efetivo para screening"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_017_r2_3">
          <text>"MUGA reservado para seguimento se baseline eco subótimo"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_017_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_017_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_017_r3">
        <inputEntry id="inputEntry_car_img_017_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r3_2">
          <text>True</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_017_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_017_r3_2">
          <text>"Fora contexto quimio: ECO ou RMC preferíveis"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_017_r3_3">
          <text>"Exposição radiação MUGA: reservar para indicações específicas"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_017_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_017_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_img_017_r4">
        <inputEntry id="inputEntry_car_img_017_r4_1">
          <text>necessaria</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_r4_4">
          <text>True</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_img_017_r4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_017_r4_2">
          <text>"MUGA alternativa se ECO inadequado + RMC contraindicada"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_img_017_r4_3">
          <text>"Raro cenário: janela eco ruim + dispositivo não-RMC-safe"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_img_017_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_img_017_r4_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-IMG-017_fallback">
        <inputEntry id="inputEntry_car_img_017_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_img_017_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-IMG-017_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-017_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-IMG-017_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-IMG-017_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-IMG-017_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-IMG-017">
      <dmndi:DMNShape id="DMNShape_CAR-IMG-017" dmnElementRef="CAR-IMG-017">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
