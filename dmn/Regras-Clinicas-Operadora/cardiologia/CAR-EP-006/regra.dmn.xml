<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-EP-006" name="Teste Ergométrico - Indicação Isquemia" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-EP-006" name="Teste Ergométrico - Indicação Isquemia">
    <decisionTable id="decisionTable_CAR-EP-006" hitPolicy="FIRST">
      <input id="input_1_car_ep_006" label="Probabilidadedac" camunda:inputVariable="probabilidadeDAC">
        <inputExpression id="inputExpression_1_car_ep_006" typeRef="integer">
          <text>probabilidadeDAC</text>
        </inputExpression>
      </input>
      <input id="input_2_car_ep_006" label="Capacidadeexercicio" camunda:inputVariable="capacidadeExercicio">
        <inputExpression id="inputExpression_2_car_ep_006" typeRef="integer">
          <text>capacidadeExercicio</text>
        </inputExpression>
      </input>
      <input id="input_3_car_ep_006" label="Ecgbasal" camunda:inputVariable="ecgBasal">
        <inputExpression id="inputExpression_3_car_ep_006" typeRef="string">
          <text>ecgBasal</text>
        </inputExpression>
      </input>
      <input id="input_4_car_ep_006" label="Sintomas" camunda:inputVariable="sintomas">
        <inputExpression id="inputExpression_4_car_ep_006" typeRef="string">
          <text>sintomas</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-EP-006" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-EP-006">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-EP-006" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-EP-006" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_ep_006_r1">
        <inputEntry id="inputEntry_car_ep_006_r1_1">
          <text>intermediaria</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r1_2">
          <text>adequada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r1_3">
          <text>normal</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r1_4">
          <text>anginatipica</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_006_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_006_r1_2">
          <text>"TE primeira linha: probabilidade intermediária"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_006_r1_3">
          <text>"Sensibilidade 68% / Especificidade 77% para DAC (ACC/AHA 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_006_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_006_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_006_r2">
        <inputEntry id="inputEntry_car_ep_006_r2_1">
          <text>alta</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r2_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_006_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_006_r2_2">
          <text>"Probabilidade alta: teste anatomico/funcional direto"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_006_r2_3">
          <text>"TE valor limitado se pré-teste &gt;70%: cateterismo ou CCTA"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_006_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_006_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_006_r3">
        <inputEntry id="inputEntry_car_ep_006_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r3_2">
          <text>limitada</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_006_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_006_r3_2">
          <text>"Incapacidade exercício: teste farmacológico"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_006_r3_3">
          <text>"Claudicação, DPOC, ortopédico: SPECT-dipiridamol ou eco-dobutamina"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_006_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_006_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_006_r4">
        <inputEntry id="inputEntry_car_ep_006_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r4_3">
          <text>bnre,wpw,mp</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_006_r4_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_006_r4_2">
          <text>"ECG basal não interpretável: imaging stress"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_006_r4_3">
          <text>"BNRE, WPW, ritmo MP: alterações ST não-diagnósticas"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_006_r4_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_006_r4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-EP-006_fallback">
        <inputEntry id="inputEntry_car_ep_006_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_006_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-EP-006_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-006_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-006_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-EP-006_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-EP-006_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-EP-006">
      <dmndi:DMNShape id="DMNShape_CAR-EP-006" dmnElementRef="CAR-EP-006">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
