<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-INT-005" name="STEMI Door-to-Balloon - Tempo Crítico" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-INT-005" name="STEMI Door-to-Balloon - Tempo Crítico">
    <decisionTable id="decisionTable_CAR-INT-005" hitPolicy="FIRST">
      <input id="input_1_car_int_005" label="Temposintomas" camunda:inputVariable="tempoSintomas">
        <inputExpression id="inputExpression_1_car_int_005" typeRef="integer">
          <text>tempoSintomas</text>
        </inputExpression>
      </input>
      <input id="input_2_car_int_005" label="Disponibilidadehemodinamica" camunda:inputVariable="disponibilidadeHemodinamica">
        <inputExpression id="inputExpression_2_car_int_005" typeRef="integer">
          <text>disponibilidadeHemodinamica</text>
        </inputExpression>
      </input>
      <input id="input_3_car_int_005" label="Contraindicacaofibrinolitico" camunda:inputVariable="contraindicacaoFibrinolitico">
        <inputExpression id="inputExpression_3_car_int_005" typeRef="string">
          <text>contraindicacaoFibrinolitico</text>
        </inputExpression>
      </input>
      <input id="input_4_car_int_005" label="Choquecardiogenico" camunda:inputVariable="choqueCardiogenico">
        <inputExpression id="inputExpression_4_car_int_005" typeRef="string">
          <text>choqueCardiogenico</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-INT-005" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-INT-005">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-INT-005" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-INT-005" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_int_005_r1">
        <inputEntry id="inputEntry_car_int_005_r1_1">
          <text>&lt; 120</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r1_2">
          <text>imediata</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r1_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r1_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_005_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_005_r1_2">
          <text>"ICP primária imediata: janela terapêutica"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_005_r1_3">
          <text>"D2B &lt;90min meta para STEMI (ACC/AHA 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_005_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_005_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_005_r2">
        <inputEntry id="inputEntry_car_int_005_r2_1">
          <text>&gt; 120</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r2_2">
          <text>imediata</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r2_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r2_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_005_r2_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_005_r2_2">
          <text>"Considerar fibrinólise se D2B &gt;120min"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_005_r2_3">
          <text>"FMC-to-device &gt;120min: lise + resgate se necessário"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_005_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_005_r2_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_005_r3">
        <inputEntry id="inputEntry_car_int_005_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_005_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_005_r3_2">
          <text>"ICP urgente + suporte hemodinâmico"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_005_r3_3">
          <text>"Choque cardiogênico: prioridade absoluta D2B &lt;60min"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_005_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_005_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_int_005_r4">
        <inputEntry id="inputEntry_car_int_005_r4_1">
          <text>&lt; 12h</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r4_2">
          <text>diferida_24h</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r4_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_int_005_r4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_005_r4_2">
          <text>"ICP farmaco-invasiva: lise + ICP 3-24h"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_int_005_r4_3">
          <text>"Estratégia híbrida quando D2B &gt;120min inevitável"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_int_005_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_int_005_r4_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-INT-005_fallback">
        <inputEntry id="inputEntry_car_int_005_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_int_005_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-INT-005_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-005_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-INT-005_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-INT-005_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-INT-005_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-INT-005">
      <dmndi:DMNShape id="DMNShape_CAR-INT-005" dmnElementRef="CAR-INT-005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
