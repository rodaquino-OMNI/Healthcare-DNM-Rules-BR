<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-024" name="Drug-Coated Balloon - Femoropoplítea" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-024" name="Drug-Coated Balloon - Femoropoplítea">
    <decisionTable id="decisionTable_CAR-DEV-024" hitPolicy="FIRST">
      <input id="input_1_car_dev_024" label="Tipolesao" camunda:inputVariable="tipoLesao">
        <inputExpression id="inputExpression_1_car_dev_024" typeRef="string">
          <text>tipoLesao</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_024" label="Stentprevio" camunda:inputVariable="stentPrevio">
        <inputExpression id="inputExpression_2_car_dev_024" typeRef="string">
          <text>stentPrevio</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_024" label="Comprimento" camunda:inputVariable="comprimento">
        <inputExpression id="inputExpression_3_car_dev_024" typeRef="string">
          <text>comprimento</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_024" label="Diabetesmellitus" camunda:inputVariable="diabetesMellitus">
        <inputExpression id="inputExpression_4_car_dev_024" typeRef="string">
          <text>diabetesMellitus</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-024" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-024">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-024" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-024" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_024_r1">
        <inputEntry id="inputEntry_car_dev_024_r1_1">
          <text>novo</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r1_2">
          <text>ausente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r1_3">
          <text>&lt; 100mm</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r1_4">
          <text>presente</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_024_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_024_r1_2">
          <text>"DCB primeira linha: lesões focais SFA"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_024_r1_3">
          <text>"IN.PACT SFA: perviedade 82% vs 60% balão convencional"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_024_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_024_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_024_r2">
        <inputEntry id="inputEntry_car_dev_024_r2_1">
          <text>reestenose_intra_stent</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r2_2">
          <text>presente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_024_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_024_r2_2">
          <text>"DCB para reestenose intra-stent"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_024_r2_3">
          <text>"Alternativa re-stenting: evita múltiplas camadas metal"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_024_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_024_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_024_r3">
        <inputEntry id="inputEntry_car_dev_024_r3_1">
          <text>oclusao_total</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r3_3">
          <text>&gt; 250mm</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_024_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_024_r3_2">
          <text>"Lesão extensa: considerar stent ou bypass"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_024_r3_3">
          <text>"DCB ideal para lesões &lt;150mm - longas necessitam scaffolding"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_024_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_024_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_024_r4">
        <inputEntry id="inputEntry_car_dev_024_r4_1">
          <text>calcificacao_severa</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r4_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_024_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_024_r4_2">
          <text>"Preparação lesão necessária antes DCB"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_024_r4_3">
          <text>"Aterectomia ou litotripsia pré-DCB em calcificação severa"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_024_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_024_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-024_fallback">
        <inputEntry id="inputEntry_car_dev_024_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_024_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-024_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-024_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-024_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-024_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-024_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-024">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-024" dmnElementRef="CAR-DEV-024">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
