<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-DEV-019" name="Valva Pulmonar Percutânea - Melody/Sapien" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-DEV-019" name="Valva Pulmonar Percutânea - Melody/Sapien">
    <decisionTable id="decisionTable_CAR-DEV-019" hitPolicy="FIRST">
      <input id="input_1_car_dev_019" label="Condiçãocongênita" camunda:inputVariable="condiçãoCongênita">
        <inputExpression id="inputExpression_1_car_dev_019" typeRef="string">
          <text>condiçãoCongênita</text>
        </inputExpression>
      </input>
      <input id="input_2_car_dev_019" label="Conduiteprevio" camunda:inputVariable="conduitePrevio">
        <inputExpression id="inputExpression_2_car_dev_019" typeRef="string">
          <text>conduitePrevio</text>
        </inputExpression>
      </input>
      <input id="input_3_car_dev_019" label="Gradientervot" camunda:inputVariable="gradienteRVOT">
        <inputExpression id="inputExpression_3_car_dev_019" typeRef="string">
          <text>gradienteRVOT</text>
        </inputExpression>
      </input>
      <input id="input_4_car_dev_019" label="Insuficiênciapulmonar" camunda:inputVariable="insuficiênciaPulmonar">
        <inputExpression id="inputExpression_4_car_dev_019" typeRef="string">
          <text>insuficiênciaPulmonar</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-DEV-019" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-DEV-019">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-DEV-019" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-DEV-019" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_dev_019_r1">
        <inputEntry id="inputEntry_car_dev_019_r1_1">
          <text>tetraFallot,tgv</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r1_2">
          <text>presente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r1_3">
          <text>&gt; 40</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r1_4">
          <text>severa</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_019_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_019_r1_2">
          <text>"TPVI: disfunção conduite RVOT"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_019_r1_3">
          <text>"Melody/Sapien evita reoperação em conduite degenerado"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_019_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_019_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_019_r2">
        <inputEntry id="inputEntry_car_dev_019_r2_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r2_2">
          <text>ausente</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r2_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r2_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_019_r2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_019_r2_2">
          <text>"TPVI requer conduite ou anel prévio"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_019_r2_3">
          <text>"Anatomia nativa: não há local ancoragem - cirurgia necessária"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_019_r2_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_019_r2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_019_r3">
        <inputEntry id="inputEntry_car_dev_019_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r3_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r3_3">
          <text>&lt; 36</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r3_4">
          <text>moderada</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_019_r3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_019_r3_2">
          <text>"Disfunção não-severa: adiar intervenção"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_019_r3_3">
          <text>"Gradiente &lt;36 + IP moderada: aguardar progressão"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_019_r3_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_019_r3_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_dev_019_r4">
        <inputEntry id="inputEntry_car_dev_019_r4_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r4_2">
          <text>dilatado</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_r4_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_dev_019_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_019_r4_2">
          <text>"Conduite muito dilatado: risco embolização"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_dev_019_r4_3">
          <text>"Diâmetro &gt;28mm: avaliar anatomia - pode requerer prótese diferente"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_dev_019_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_dev_019_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-DEV-019_fallback">
        <inputEntry id="inputEntry_car_dev_019_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_dev_019_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-DEV-019_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-019_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-DEV-019_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-DEV-019_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-DEV-019_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-DEV-019">
      <dmndi:DMNShape id="DMNShape_CAR-DEV-019" dmnElementRef="CAR-DEV-019">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
