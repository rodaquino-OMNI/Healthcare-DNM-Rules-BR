<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="definitions_CAR-EP-003" name="Controle Frequência vs Ritmo FA" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.27.0">
  <decision id="CAR-EP-003" name="Controle Frequência vs Ritmo FA">
    <decisionTable id="decisionTable_CAR-EP-003" hitPolicy="FIRST">
      <input id="input_1_car_ep_003" label="Idadepaciente" camunda:inputVariable="idadePaciente">
        <inputExpression id="inputExpression_1_car_ep_003" typeRef="integer">
          <text>idadePaciente</text>
        </inputExpression>
      </input>
      <input id="input_2_car_ep_003" label="Sintomas" camunda:inputVariable="sintomas">
        <inputExpression id="inputExpression_2_car_ep_003" typeRef="string">
          <text>sintomas</text>
        </inputExpression>
      </input>
      <input id="input_3_car_ep_003" label="Primeiroepisodio" camunda:inputVariable="primeiroEpisodio">
        <inputExpression id="inputExpression_3_car_ep_003" typeRef="string">
          <text>primeiroEpisodio</text>
        </inputExpression>
      </input>
      <input id="input_4_car_ep_003" label="Cardiopatiaestrutura L" camunda:inputVariable="cardiopatiaEstrutura l">
        <inputExpression id="inputExpression_4_car_ep_003" typeRef="string">
          <text>cardiopatiaEstrutura l</text>
        </inputExpression>
      </input>
      <output id="output_resultado_CAR-EP-003" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado_CAR-EP-003">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao_CAR-EP-003" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao_CAR-EP-003" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdício" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Ação Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_car_ep_003_r1">
        <inputEntry id="inputEntry_car_ep_003_r1_1">
          <text>&lt; 65</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r1_2">
          <text>significativos</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r1_3">
          <text>sim</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r1_4">
          <text>minima</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_003_r1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_003_r1_2">
          <text>"Controle ritmo primeira linha: jovem sintomático"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_003_r1_3">
          <text>"Cardioversão + AAD ou ablação em FA recente sintomática (EAST-AFNET 4)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_003_r1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_003_r1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_003_r2">
        <inputEntry id="inputEntry_car_ep_003_r2_1">
          <text>&gt;= 75</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r2_2">
          <text>minimos</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r2_3">
          <text>nao</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r2_4">
          <text>significativa</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_003_r2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_003_r2_2">
          <text>"Controle frequência: idoso minimamente sintomático"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_003_r2_3">
          <text>"Rate control não-inferior em idosos + melhor tolerabilidade que AAD"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_003_r2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_003_r2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_003_r3">
        <inputEntry id="inputEntry_car_ep_003_r3_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r3_2">
          <text>refratarios</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r3_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r3_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_003_r3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_003_r3_2">
          <text>"Ablação FA: sintomas refratários a AAD"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_003_r3_3">
          <text>"Classe I para FA sintomática apesar ≥1 AAD (HRS/EHRA 2023)"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_003_r3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_003_r3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="rule_car_ep_003_r4">
        <inputEntry id="inputEntry_car_ep_003_r4_1">
          <text>[65..75]</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r4_2">
          <text>moderados</text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r4_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_r4_4">
          <text>presente</text>
        </inputEntry>
        <outputEntry id="outputEntry_car_ep_003_r4_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_003_r4_2">
          <text>"Decisão compartilhada: balanço benefício-risco"</text>
        </outputEntry>
        <outputEntry id="outputEntry_car_ep_003_r4_3">
          <text>"Zona cinzenta: considerar preferência paciente e comorbidades"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_car_ep_003_r4_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_car_ep_003_r4_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="rule_CAR-EP-003_fallback">
        <inputEntry id="inputEntry_car_ep_003_fb_1">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_fb_2">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_fb_3">
          <text></text>
        </inputEntry>
        <inputEntry id="inputEntry_car_ep_003_fb_4">
          <text></text>
        </inputEntry>
        <outputEntry id="outputEntry_CAR-EP-003_fb_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-003_fb_2">
          <text>"Caso requer avaliação adicional por cardiologista especializado"</text>
        </outputEntry>
        <outputEntry id="outputEntry_CAR-EP-003_fb_3">
          <text>"Documentação insuficiente para decisão automatizada"</text>
        </outputEntry>        <outputEntry id="OutputEntry_rule_CAR-EP-003_fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_rule_CAR-EP-003_fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CAR-EP-003">
      <dmndi:DMNShape id="DMNShape_CAR-EP-003" dmnElementRef="CAR-EP-003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
