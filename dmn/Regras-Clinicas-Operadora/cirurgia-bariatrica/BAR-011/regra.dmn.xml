<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_BAR011"
             name="Acompanhamento Nutricional Longo Prazo"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    TUSS: BAR-011
    Procedimento: Acompanhamento Nutricional Especializado Pos-Bariatrica
    Especialidade: Cirurgia Bariatrica / Nutrologia
    Versao: 3.0.0-TIER1
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS (SBCBM 2022, CFM 2.131/2015):
    - Seguimento nutricional obrigatorio pos-bariatrica
    - Deficiencias nutricionais documentadas
    - Reganho de peso em acompanhamento
    - Gravidez pos-bariatrica
    - Complicacoes nutricionais (anemia, osteoporose)

    FREQUENCIA RECOMENDADA:
    - Ano 1: Trimestral
    - Anos 2-5: Semestral
    - Apos 5 anos: Anual

    REFERENCIAS TECNICAS:
    - CFM 2.131/2015 - Equipe Multidisciplinar Obrigatoria
    - SBCBM 2022 - Protocolo de Seguimento Pos-Operatorio
    - ASMBS Nutrition Guidelines 2020

    ===============================================================================
  -->

  <decision id="Decision_BAR011" name="Regra Clinica - Acompanhamento Nutricional Longo Prazo">
    <decisionTable id="DecisionTable_BAR011" hitPolicy="FIRST">

      <!-- ========== INPUTS (5) ========== -->

      <input id="Input_tempoPosCirurgia" label="Tempo Pos-Cirurgia (meses)" camunda:inputVariable="tempoPosCirurgia">
        <inputExpression id="InputExpression_tempoPosCirurgia" typeRef="number">
          <text>tempoPosCirurgia</text>
        </inputExpression>
      </input>

      <input id="Input_indicacaoConsulta" label="Indicacao da Consulta" camunda:inputVariable="indicacaoConsulta">
        <inputExpression id="InputExpression_indicacaoConsulta" typeRef="string">
          <text>indicacaoConsulta</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoConsulta">
          <text>"SegmentoRotina","DeficienciaNutricional","ReganhoPeso","Gravidez","ComplicacaoNutricional"</text>
        </inputValues>
      </input>

      <input id="Input_ultimaConsultaNutricional" label="Ultima Consulta Nutricional (meses)" camunda:inputVariable="ultimaConsultaNutricional">
        <inputExpression id="InputExpression_ultimaConsultaNutricional" typeRef="number">
          <text>ultimaConsultaNutricional</text>
        </inputExpression>
      </input>

      <input id="Input_deficienciasDocumentadas" label="Deficiencias Documentadas" camunda:inputVariable="deficienciasDocumentadas">
        <inputExpression id="InputExpression_deficienciasDocumentadas" typeRef="string">
          <text>deficienciasDocumentadas</text>
        </inputExpression>
        <inputValues id="InputValues_deficienciasDocumentadas">
          <text>"Multiplas","VitaminaB12","Ferro","VitaminaD","Proteina","Nenhuma"</text>
        </inputValues>
      </input>

      <input id="Input_tipoCirurgiaRealizada" label="Tipo de Cirurgia Bariatrica" camunda:inputVariable="tipoCirurgiaRealizada">
        <inputExpression id="InputExpression_tipoCirurgiaRealizada" typeRef="string">
          <text>tipoCirurgiaRealizada</text>
        </inputExpression>
        <inputValues id="InputValues_tipoCirurgiaRealizada">
          <text>"Bypass","Sleeve","DS","SADI","BandaGastrica","Outro"</text>
        </inputValues>
      </input>

      <!-- ========== OUTPUTS (3 - Padrao TIER-1) ========== -->

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      <!-- Output 4: Alertas Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- ========== RULES ========== -->

      <!-- APROVACOES PRIORITARIAS -->
      <rule id="Rule_Aprovado_Gravidez">
        <description>Aprovado Prioritario: Gestante pos-bariatrica</description>
        <inputEntry id="InputEntry_GR_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_GR_2"><text>"Gravidez"</text></inputEntry>
        <inputEntry id="InputEntry_GR_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_GR_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_GR_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_GR_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_GR_observacao"><text>"Gestacao pos-bariatrica requer acompanhamento nutricional intensivo mensal"</text></outputEntry>
        <outputEntry id="OutputEntry_GR_fundamentacao"><text>"ASMBS 2020, SBCBM 2022 - Gestantes pos-bariatrica tem alto risco de deficiencias nutricionais fetais"</text></outputEntry>
      <outputEntry id="OE_Aprovado_Gravidez_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_Gravidez_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ComplicacaoNutricional">
        <description>Aprovado Urgente: Complicacao nutricional grave</description>
        <inputEntry id="InputEntry_CN_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_CN_2"><text>"ComplicacaoNutricional"</text></inputEntry>
        <inputEntry id="InputEntry_CN_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_CN_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_CN_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_CN_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_CN_observacao"><text>"Complicacao nutricional (anemia grave, osteoporose, encefalopatia) - acompanhamento intensivo"</text></outputEntry>
        <outputEntry id="OutputEntry_CN_fundamentacao"><text>"SBCBM 2022, ASMBS 2020 - Complicacoes nutricionais requerem intervencao nutricional especializada"</text></outputEntry>
      <outputEntry id="OE_Aprovado_ComplicacaoNutricional_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_ComplicacaoNutricional_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_DeficienciaMultipla">
        <description>Aprovado: Deficiencias nutricionais multiplas</description>
        <inputEntry id="InputEntry_DM_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_DM_2"><text>"DeficienciaNutricional"</text></inputEntry>
        <inputEntry id="InputEntry_DM_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_DM_4"><text>"Multiplas"</text></inputEntry>
        <inputEntry id="InputEntry_DM_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_DM_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_DM_observacao"><text>"Deficiencias nutricionais multiplas documentadas - acompanhamento intensivo indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_DM_fundamentacao"><text>"CFM 2.131/2015, SBCBM 2022 - Correcao de multiplas deficiencias requer acompanhamento especializado"</text></outputEntry>
      <outputEntry id="OE_Aprovado_DeficienciaMultipla_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_DeficienciaMultipla_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES ROTINA ANO 1 -->
      <rule id="Rule_Aprovado_Ano1_Trimestral">
        <description>Aprovado: Seguimento ano 1 - trimestral</description>
        <inputEntry id="InputEntry_A1_1"><text>&lt;= 12</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"SegmentoRotina"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_A1_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_observacao"><text>"Primeiro ano pos-operatorio - acompanhamento trimestral conforme protocolo"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_fundamentacao"><text>"CFM 2.131/2015, SBCBM 2022 - Seguimento trimestral obrigatorio no primeiro ano"</text></outputEntry>
      <outputEntry id="OE_Aprovado_Ano1_Trimestral_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_Ano1_Trimestral_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES ROTINA ANOS 2-5 -->
      <rule id="Rule_Aprovado_Anos2a5_Semestral">
        <description>Aprovado: Seguimento anos 2-5 - semestral</description>
        <inputEntry id="InputEntry_A25_1"><text>[13..60]</text></inputEntry>
        <inputEntry id="InputEntry_A25_2"><text>"SegmentoRotina"</text></inputEntry>
        <inputEntry id="InputEntry_A25_3"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="InputEntry_A25_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A25_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_A25_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_A25_observacao"><text>"Anos 2-5 pos-operatorio - acompanhamento semestral conforme protocolo"</text></outputEntry>
        <outputEntry id="OutputEntry_A25_fundamentacao"><text>"CFM 2.131/2015, SBCBM 2022 - Seguimento semestral anos 2 a 5"</text></outputEntry>
      <outputEntry id="OE_Aprovado_Anos2a5_Semestral_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_Anos2a5_Semestral_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES ROTINA APOS 5 ANOS -->
      <rule id="Rule_Aprovado_Apos5Anos_Anual">
        <description>Aprovado: Seguimento apos 5 anos - anual</description>
        <inputEntry id="InputEntry_A5_1"><text>&gt; 60</text></inputEntry>
        <inputEntry id="InputEntry_A5_2"><text>"SegmentoRotina"</text></inputEntry>
        <inputEntry id="InputEntry_A5_3"><text>&gt;= 12</text></inputEntry>
        <inputEntry id="InputEntry_A5_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A5_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_A5_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_A5_observacao"><text>"Apos 5 anos - acompanhamento anual vitalicio conforme protocolo"</text></outputEntry>
        <outputEntry id="OutputEntry_A5_fundamentacao"><text>"CFM 2.131/2015, SBCBM 2022 - Seguimento anual vitalicio obrigatorio"</text></outputEntry>
      <outputEntry id="OE_Aprovado_Apos5Anos_Anual_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_Apos5Anos_Anual_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES ESPECIAIS - CIRURGIAS MALABSORTIVAS -->
      <rule id="Rule_Aprovado_DS_SADI_Intensivo">
        <description>Aprovado: Cirurgias malabsortivas requerem acompanhamento mais frequente</description>
        <inputEntry id="InputEntry_DS_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_DS_2"><text>"SegmentoRotina","DeficienciaNutricional"</text></inputEntry>
        <inputEntry id="InputEntry_DS_3"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="InputEntry_DS_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_DS_5"><text>"DS","SADI"</text></inputEntry>
        <outputEntry id="OutputEntry_DS_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_DS_observacao"><text>"Cirurgia malabsortiva (DS/SADI) - acompanhamento nutricional intensificado"</text></outputEntry>
        <outputEntry id="OutputEntry_DS_fundamentacao"><text>"ASMBS 2020, SBCBM 2022 - DS/SADI tem maior risco de deficiencias proteicas e liposoluveis"</text></outputEntry>
      <outputEntry id="OE_Aprovado_DS_SADI_Intensivo_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_DS_SADI_Intensivo_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES - REGANHO -->
      <rule id="Rule_Aprovado_Reganho">
        <description>Aprovado: Reganho de peso em acompanhamento</description>
        <inputEntry id="InputEntry_RG_1"><text>&gt;= 18</text></inputEntry>
        <inputEntry id="InputEntry_RG_2"><text>"ReganhoPeso"</text></inputEntry>
        <inputEntry id="InputEntry_RG_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_RG_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_RG_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_RG_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_RG_observacao"><text>"Reganho de peso documentado - intensificar acompanhamento nutricional"</text></outputEntry>
        <outputEntry id="OutputEntry_RG_fundamentacao"><text>"SBCBM 2022 - Intervencao nutricional precoce no reganho de peso"</text></outputEntry>
      <outputEntry id="OE_Aprovado_Reganho_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprovado_Reganho_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- REPROVACOES -->
      <rule id="Rule_Reprovado_Recente">
        <description>Reprovado: Consulta nutricional recente</description>
        <inputEntry id="InputEntry_CR_1"><text>&gt; 12</text></inputEntry>
        <inputEntry id="InputEntry_CR_2"><text>"SegmentoRotina"</text></inputEntry>
        <inputEntry id="InputEntry_CR_3"><text>&lt; 3</text></inputEntry>
        <inputEntry id="InputEntry_CR_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_CR_5"><text>not("DS","SADI")</text></inputEntry>
        <outputEntry id="OutputEntry_CR_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_CR_observacao"><text>"Consulta nutricional realizada ha menos de 3 meses sem indicacao especifica"</text></outputEntry>
        <outputEntry id="OutputEntry_CR_fundamentacao"><text>"SBCBM 2022 - Frequencia semestral/anual para seguimento de rotina sem deficiencias"</text></outputEntry>
      <outputEntry id="OE_Reprovado_Recente_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Reprovado_Recente_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK RULE -->
      <rule id="Rule_Fallback_BAR011">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Avaliar indicacao especifica do acompanhamento nutricional"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"CFM 2.131/2015 - Acompanhamento nutricional vitalicio e obrigatorio pos-bariatrica"</text></outputEntry>
      <outputEntry id="OE_Fallback_BAR011_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Fallback_BAR011_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
