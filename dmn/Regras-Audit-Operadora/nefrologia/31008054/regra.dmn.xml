<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA004_31008054" name="DIA-004: Revisao Acesso HD - Estenose" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA004_31008054" name="DIA-004: Revisao Acesso HD Estenose">
    <decisionTable id="DecisionTable_DIA004_31008054" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Diagnostico (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"T82.858","I77.1","T82.898","I72.3","Z99.2"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="Grau de Estenose (%)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Tipo de Acesso Vascular">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"FAV_Nativa","Enxerto_PTFE","Cateter"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Fluxo Acesso (mL/min)">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Menor_400","400_500","500_600","Maior_600","Nao_Medido"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Fistulografia/Doppler com Estenose">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA004_Reprovado_SemImagem">
        <description>Reprovado - Sem imagem documentando estenose</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Fistulografia ou US Doppler obrigatorio para documentar estenose e planejar revisao cirurgica. Solicitar exame de imagem."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDOQI 2019 - Pre-procedural imaging mandatory; RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA004_Reprovado_EstenoseLeve">
        <description>Reprovado - Estenose menor que 50% (nao significativa)</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;lt; 50</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>not("Menor_400")</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Estenose menor que 50% nao e hemodinamicamente significativa. Manter vigilancia e reavaliar em 3 meses."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDOQI 2019 - Intervention only for stenosis maior que 50% with clinical/hemodynamic criteria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA004_Reprovado_FluxoAdequado">
        <description>Reprovado - Fluxo adequado sem criterio hemodinamico</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>[50..70]</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>"FAV_Nativa"</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>"Maior_600"</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"FAV com fluxo maior que 600 mL/min nao preenche criterio hemodinamico para intervencao. Manter vigilancia trimestral."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDOQI 2019 - Anatomic stenosis alone insufficient for intervention"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA004_Aprovado_EstenoseCritica_FAV">
        <description>Aprovado - Estenose critica FAV com fluxo baixo</description>
        <inputEntry id="IE_A1_1"><text>"T82.858","I77.1"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;gt;= 50</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"FAV_Nativa"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Menor_400","400_500"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Estenose maior ou igual a 50% em FAV nativa com fluxo menor que 500 mL/min - criterios hemodinamicos preenchidos. Revisao cirurgica ou angioplastia indicada."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDOQI 2019 - AVF stenosis maior que 50% + Qa menor que 500 mL/min indicates intervention"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA004_Aprovado_EstenoseCritica_Enxerto">
        <description>Aprovado - Estenose critica enxerto com fluxo baixo</description>
        <inputEntry id="IE_A2_1"><text>"T82.858","I77.1"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;gt;= 50</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Enxerto_PTFE"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Menor_400"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Estenose maior ou igual a 50% em enxerto com fluxo menor que 400 mL/min - criterios hemodinamicos preenchidos. Revisao ou angioplastia indicada."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDOQI 2019 - Graft stenosis maior que 50% + Qa menor que 400 mL/min indicates intervention"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA004_Aprovado_EstenoseSevera">
        <description>Aprovado - Estenose severa (maior que 70%)</description>
        <inputEntry id="IE_A3_1"><text>"T82.858","I77.1"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>&amp;gt; 70</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"FAV_Nativa","Enxerto_PTFE"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Estenose severa maior que 70% - alto risco de trombose. Intervencao indicada mesmo sem alteracao de fluxo significativa."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDOQI 2019 - Severe stenosis (maior que 70%) warrants preemptive intervention"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA004_Aprovado_Aneurisma">
        <description>Aprovado - Aneurisma com risco de ruptura</description>
        <inputEntry id="IE_A4_1"><text>"I72.3","T82.898"</text></inputEntry>
        <inputEntry id="IE_A4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_3"><text>"FAV_Nativa","Enxerto_PTFE"</text></inputEntry>
        <inputEntry id="IE_A4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A4_2"><text>"Aneurisma ou pseudoaneurisma de acesso vascular - revisao cirurgica indicada pelo risco de ruptura e sangramento."</text></outputEntry>
        <outputEntry id="OE_A4_3"><text>"KDOQI 2019 - Aneurysm repair for expanding or at-risk lesions"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA004_Pendente_FluxoNaoMedido">
        <description>Pendente - Fluxo nao medido, completar avaliacao</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>[50..70]</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Nao_Medido"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Fluxo do acesso nao foi medido. Completar avaliacao com medicao de Qa para determinar significancia hemodinamica da estenose."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDOQI 2019 - Qa measurement essential for clinical decision-making"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008054">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer analise por cirurgiao vascular conforme KDOQI 2019."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDOQI 2019 / SBN 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA004_31008054">
      <dmndi:DMNShape id="DMNShape_Decision_DIA004_31008054" dmnElementRef="Decision_DIA004_31008054">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>