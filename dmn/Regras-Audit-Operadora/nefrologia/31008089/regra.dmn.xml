<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA007_31008089" name="DIA-007: Adequacao Dialise - Kt/V Thresholds" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA007_31008089" name="DIA-007: Adequacao Dialise Kt/V">
    <decisionTable id="DecisionTable_DIA007_31008089" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Status Renal (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"Z99.2","N18.5","N19","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="spKt/V por Sessao (HD) ou Semanal (DP)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Modalidade de Dialise">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"HD_3x_semana","HD_4x_semana","HD_Diaria","DP_CAPD","DP_APD"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Fluxo do Acesso Adequado">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Adequado","Fluxo_Baixo","Recirculacao","Nao_Avaliado"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Kt/V Mensal Documentado">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA007_Reprovado_SemKtV">
        <description>Reprovado - Kt/V nao documentado</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Kt/V mensal obrigatorio para monitoramento de adequacao dialitica. Coletar ureia pre e pos-dialise para calculo."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDOQI 2015 - Monthly Kt/V monitoring required"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA007_Reprovado_KtV_Critico_HD">
        <description>Reprovado - Kt/V criticamente baixo HD (menor que 1.0)</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;lt; 1.0</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>"HD_3x_semana","HD_4x_semana"</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Kt/V criticamente baixo (menor que 1.0) - dialise grosseiramente inadequada. Investigar urgente: acesso vascular, tempo de sessao, recirculacao."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDOQI 2015 - spKt/V menor que 1.0 indicates grossly inadequate dialysis"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA007_Aprovado_KtV_Ideal_HD">
        <description>Aprovado - Kt/V ideal HD (maior ou igual a 1.4)</description>
        <inputEntry id="IE_A1_1"><text>"Z99.2","N18.5"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;gt;= 1.4</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"HD_3x_semana","HD_4x_semana","HD_Diaria"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Adequado"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"spKt/V maior ou igual a 1.4 - dialise adequada conforme meta KDOQI. Manter prescricao atual e monitorar mensalmente."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDOQI 2015 - Target spKt/V maior ou igual a 1.4 for thrice-weekly HD"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA007_Aprovado_KtV_Ideal_DP">
        <description>Aprovado - Kt/V semanal ideal DP (maior ou igual a 1.7)</description>
        <inputEntry id="IE_A2_1"><text>"Z99.2","N18.5"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;gt;= 1.7</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"DP_CAPD","DP_APD"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Kt/V semanal maior ou igual a 1.7 - dialise peritoneal adequada conforme ISPD. Manter prescricao e monitorar funcao residual."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"ISPD 2020 - Weekly Kt/V target maior ou igual a 1.7 for PD"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA007_Aprovado_KtV_Aceitavel_HD">
        <description>Aprovado - Kt/V aceitavel HD (1.2-1.4)</description>
        <inputEntry id="IE_A3_1"><text>"Z99.2","N18.5"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>[1.2..1.4[</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"HD_3x_semana"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"Adequado"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"spKt/V 1.2-1.4 - dialise aceitavel mas abaixo do ideal. Considerar otimizacao: aumentar tempo ou frequencia se sintomatico."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDOQI 2015 - Minimum spKt/V 1.2; target 1.4 recommended"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA007_Pendente_KtV_Baixo_HD">
        <description>Pendente - Kt/V baixo HD (1.0-1.2), otimizacao necessaria</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>[1.0..1.2[</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"HD_3x_semana"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"spKt/V 1.0-1.2 - dialise subotima. Otimizacao necessaria: avaliar acesso vascular, aumentar tempo de sessao (4h minimo), considerar HD 4x/semana."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDOQI 2015 - Below minimum target; intervention needed to increase dose"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_DIA007_Pendente_Fluxo_Baixo">
        <description>Pendente - Fluxo acesso baixo comprometendo adequacao</description>
        <inputEntry id="IE_P2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_2"><text>&amp;lt; 1.4</text></inputEntry>
        <inputEntry id="IE_P2_3"><text>"HD_3x_semana","HD_4x_semana"</text></inputEntry>
        <inputEntry id="IE_P2_4"><text>"Fluxo_Baixo","Recirculacao"</text></inputEntry>
        <inputEntry id="IE_P2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P2_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P2_2"><text>"Kt/V baixo associado a problema de acesso vascular (fluxo baixo ou recirculacao). Avaliar acesso antes de ajustar prescricao."</text></outputEntry>
        <outputEntry id="OE_P2_3"><text>"KDOQI 2015/2019 - Address access issues before modifying prescription"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_DIA007_Pendente_KtV_Baixo_DP">
        <description>Pendente - Kt/V baixo DP (menor que 1.7)</description>
        <inputEntry id="IE_P3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P3_2"><text>&amp;lt; 1.7</text></inputEntry>
        <inputEntry id="IE_P3_3"><text>"DP_CAPD","DP_APD"</text></inputEntry>
        <inputEntry id="IE_P3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_P3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P3_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P3_2"><text>"Kt/V semanal menor que 1.7 - dialise peritoneal inadequada. Aumentar volume ou frequencia de trocas. Avaliar funcao residual e PET."</text></outputEntry>
        <outputEntry id="OE_P3_3"><text>"ISPD 2020 - Inadequate PD requires prescription adjustment"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008089">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer avaliacao por nefrologista conforme KDOQI 2015 e ISPD 2020."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDOQI 2015 / ISPD 2020"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA007_31008089">
      <dmndi:DMNShape id="DMNShape_Decision_DIA007_31008089" dmnElementRef="Decision_DIA007_31008089">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>