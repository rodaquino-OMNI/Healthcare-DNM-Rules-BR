<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="DIA-002-Confeccao-FAV-Timing" name="Regra Confecção FAV - Timing e Indicação" namespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DIA002_FAV" name="DIA-002: Confecção Fístula Arteriovenosa (FAV) - Timing">
    <decisionTable id="DecisionTable_DIA002" hitPolicy="FIRST">
      <input id="Input_eGFR" label="Taxa Filtração Glomerular (eGFR)" camunda:inputVariable="taxaFiltracaoGlomerular">
        <inputExpression id="InputExpression_eGFR" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      <input id="Input_ProgressaoRapida" label="Progressão Rápida DRC" camunda:inputVariable="progressaoRapida">
        <inputExpression id="InputExpression_Progressao" typeRef="boolean">
          <text>progressaoRapida</text>
        </inputExpression>
        <inputValues id="InputValues_Progressao">
          <text>true,false</text>
        </inputValues>
      </input>

      <input id="Input_MesesPrevistoDialise" label="Previsão Início Diálise (meses)" camunda:inputVariable="mesesPrevistoInicioDialise">
        <inputExpression id="InputExpression_Previsao" typeRef="number">
          <text>mesesPrevistoInicioDialise</text>
        </inputExpression>
      </input>

      <input id="Input_QualidadeVasosAdequada" label="Mapeamento Vascular Adequado" camunda:inputVariable="qualidadeVasosAdequada">
        <inputExpression id="InputExpression_Vasos" typeRef="boolean">
          <text>qualidadeVasosAdequada</text>
        </inputExpression>
        <inputValues id="InputValues_Vasos">
          <text>true,false</text>
        </inputValues>
      </input>

      <input id="Input_TentativasFAVPrevias" label="Tentativas FAV Prévias (falhas)" camunda:inputVariable="tentativasFavPrevias">
        <inputExpression id="InputExpression_Tentativas" typeRef="number">
          <text>tentativasFavPrevias</text>
        </inputExpression>
      </input>

      <output id="Output_Resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_Resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_Observacao" label="Observação" name="observacao" typeRef="string" />

      <output id="Output_Fundamentacao" label="Fundamentação Legal" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA002_01">
        <inputEntry id="InputEntry_R1_eGFR">
          <text>&amp;gt; 30</text>
        </inputEntry>
        <inputEntry id="InputEntry_R1_Progressao">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_R1_Previsao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R1_Vasos">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R1_Tentativas">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R1_Resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R1_Obs">
          <text>"eGFR &amp;gt;30 ml/min/1.73m² sem progressão rápida. Confecção FAV prematura. Manter acompanhamento nefrológico e reavaliação quando eGFR &amp;lt;20."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R1_Fund">
          <text>"SBN Diretrizes - FAV quando eGFR &amp;lt;20 ou progressão rápida"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA002_02">
        <inputEntry id="InputEntry_R2_eGFR">
          <text>&amp;lt;= 30</text>
        </inputEntry>
        <inputEntry id="InputEntry_R2_Progressao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R2_Previsao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R2_Vasos">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_R2_Tentativas">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R2_Resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R2_Obs">
          <text>"Mapeamento vascular inadequado (veias/artérias calibre insuficiente). Considerar enxertia protética ou cateter tunelizado longa permanência."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R2_Fund">
          <text>"SBN/KDOQI - Mapeamento vascular pré-FAV obrigatório"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA002_03">
        <inputEntry id="InputEntry_R3_eGFR">
          <text>&amp;lt;= 20</text>
        </inputEntry>
        <inputEntry id="InputEntry_R3_Progressao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R3_Previsao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R3_Vasos">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R3_Tentativas">
          <text>&amp;gt;= 2</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R3_Resultado">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R3_Obs">
          <text>"≥2 tentativas FAV prévias falharam. Pendente reavaliação cirurgião vascular para FAV distal/proximal alternativa ou enxertia protética."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R3_Fund">
          <text>"KDOQI 2019 - Após 2 falhas FAV, considerar enxertia ou cateter"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_DIA002_04">
        <inputEntry id="InputEntry_R4_eGFR">
          <text>&amp;lt; 20</text>
        </inputEntry>
        <inputEntry id="InputEntry_R4_Progressao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R4_Previsao">
          <text>[6..12]</text>
        </inputEntry>
        <inputEntry id="InputEntry_R4_Vasos">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R4_Tentativas">
          <text>&amp;lt; 2</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R4_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R4_Obs">
          <text>"eGFR &amp;lt;20 com expectativa diálise 6-12 meses. Timing ideal confecção FAV (maturação 3-6 meses). Mapeamento vascular adequado."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R4_Fund">
          <text>"KDOQI 2019 - FAV quando diálise esperada em 6-12 meses"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA002_05">
        <inputEntry id="InputEntry_R5_eGFR">
          <text>[20..30]</text>
        </inputEntry>
        <inputEntry id="InputEntry_R5_Progressao">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R5_Previsao">
          <text>&amp;lt; 12</text>
        </inputEntry>
        <inputEntry id="InputEntry_R5_Vasos">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R5_Tentativas">
          <text>&amp;lt; 2</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R5_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R5_Obs">
          <text>"Progressão rápida DRC (eGFR declínio &amp;gt;5 ml/min/ano). Confecção FAV antecipada para garantir maturação antes início diálise previsto."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R5_Fund">
          <text>"KDOQI 2019 - FAV antecipada se progressão rápida"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA002_06">
        <inputEntry id="InputEntry_R6_eGFR">
          <text>&amp;lt; 15</text>
        </inputEntry>
        <inputEntry id="InputEntry_R6_Progressao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R6_Previsao">
          <text>&amp;lt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_R6_Vasos">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R6_Tentativas">
          <text>&amp;lt; 2</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R6_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R6_Obs">
          <text>"eGFR &amp;lt;15 com diálise iminente (&amp;lt;6 meses). Confecção FAV urgente. Se início &amp;lt;3 meses, considerar cateter temporário até maturação FAV."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R6_Fund">
          <text>"SBN/KDOQI - FAV preferencial mesmo com início diálise iminente"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA002_07">
        <inputEntry id="InputEntry_R7_eGFR">
          <text>[15..20]</text>
        </inputEntry>
        <inputEntry id="InputEntry_R7_Progressao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R7_Previsao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R7_Vasos">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R7_Tentativas">
          <text>&amp;lt; 2</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R7_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R7_Obs">
          <text>"DRC estágio 4 (eGFR 15-20). Timing apropriado confecção FAV para maturação adequada antes início diálise. Mapeamento vascular favorável."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R7_Fund">
          <text>"KDOQI 2019 - FAV quando eGFR 15-20 ml/min/1.73m²"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Default_Fallback">
        <inputEntry id="InputEntry_Fallback_eGFR">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_Progressao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_Previsao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_Vasos">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_Tentativas">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Fallback_Resultado">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Fallback_Obs">
          <text>"Caso não contemplado nas regras. Solicitar avaliação nefrologista e cirurgião vascular para decisão individualizada conforme KDOQI 2019."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Fallback_Fund">
          <text>"Análise caso a caso - KDOQI 2019"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>