<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30701023" name="Confeccao de Fistula Radiobraquial (Radiocefaica) para Hemodialise" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30701023" name="Regra Clinica - Confeccao Fistula Radiocefalica">
    <decisionTable id="DecisionTable_30701023" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"DRC4_PreDialise","DRC5_PreDialise","DRC5_EmDialise","PrimeiroAcesso","TesteAllenNegativo","TesteAllenPositivo"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (KDOQI/SBN)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"MapeamentoCompleto_AllenNegativo","MapeamentoCompleto_AllenPositivo","MapeamentoParcial","SemMapeamento"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida Superior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Procedimento Classe III - contraindicado. Avaliar alternativas: FAV braquiocefalica ou braquiobasilica."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDOQI 2024 - FAV radiocefalica contraindicada quando anatomia desfavoravel ou risco de isquemia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_observacao"><text>"Expectativa de vida inferior a 1 ano. FAV radiocefalica tem maior tempo de maturacao (8-12 semanas). Considerar cateter tunelizado."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_fundamentacao"><text>"KDOQI 2024 - FAV distal requer tempo adequado para maturacao e uso prolongado"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_TesteAllenPositivo">
        <description>Contraindicacao: Teste de Allen positivo - risco de isquemia</description>
        <inputEntry id="InputEntry_Contraind_Allen_1"><text>"TesteAllenPositivo"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Allen_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Allen_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Allen_4"><text>"MapeamentoCompleto_AllenPositivo"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Allen_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Allen_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Allen_observacao"><text>"Teste de Allen positivo indica circulacao palmar dependente da arteria radial. FAV radiocefalica contraindicada pelo risco de isquemia da mao."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Allen_fundamentacao"><text>"KDOQI 2024, ESVS 2023 - Teste de Allen obrigatorio; se positivo, contraindicacao absoluta para FAV radial"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_SemMapeamento">
        <description>Reprovacao: Ausencia de mapeamento vascular</description>
        <inputEntry id="InputEntry_SemMapeamento_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_4"><text>"SemMapeamento"</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_observacao"><text>"Mapeamento vascular pre-operatorio obrigatorio. FAV radiocefalica requer veia cefalica maior que 2.0mm e arteria radial patente."</text></outputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_fundamentacao"><text>"KDOQI 2024 - Mapeamento pre-operatorio aumenta taxa de sucesso de 34% para 54%"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      

      <rule id="Rule_Aprovado_ClasseI_PrimeiroAcesso">
        <description>Aprovado: Primeiro acesso vascular com anatomia favoravel</description>
        <inputEntry id="InputEntry_Aprovado_Primeiro_1"><text>"PrimeiroAcesso","DRC4_PreDialise","DRC5_PreDialise"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Primeiro_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Primeiro_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Primeiro_4"><text>"MapeamentoCompleto_AllenNegativo"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Primeiro_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Primeiro_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Primeiro_observacao"><text>"FAV radiocefalica e primeira opcao para acesso permanente. Estrategia Fistula First - preserva territorios proximais para futuros acessos."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Primeiro_fundamentacao"><text>"KDOQI 2024 Fistula First Initiative; SBN 2023 - Iniciar sempre pelo acesso mais distal possivel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_EmDialise">
        <description>Aprovado: Paciente em dialise necessitando FAV definitiva</description>
        <inputEntry id="InputEntry_Aprovado_HD_1"><text>"DRC5_EmDialise"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_HD_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_HD_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_HD_4"><text>"MapeamentoCompleto_AllenNegativo"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_HD_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_HD_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_HD_observacao"><text>"Conversao de cateter para FAV radiocefalica. Manter cateter durante periodo de maturacao (8-12 semanas)."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_HD_fundamentacao"><text>"KDOQI 2024 - Conversao para FAV reduz mortalidade e hospitalizacoes por infeccao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIa">
        <description>Aprovado: Indicacao Classe IIa com Allen negativo</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_4"><text>"MapeamentoCompleto_AllenNegativo"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"Indicacao Classe IIa - procedimento razoavel. Anatomia borderline pode exigir tecnica microcirurgica."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"KDOQI 2024 - Classe IIa: Beneficio supera risco em situacoes especificas"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      

      <rule id="Rule_Pendente_MapeamentoParcial">
        <description>Pendente: Mapeamento vascular incompleto</description>
        <inputEntry id="InputEntry_Pendente_Map_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_4"><text>"MapeamentoParcial"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_observacao"><text>"Mapeamento incompleto. Obrigatorio: Teste de Allen, Doppler de arteria radial e veia cefalica bilateral."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_fundamentacao"><text>"KDOQI 2024 - Mapeamento completo e Teste de Allen sao pre-requisitos para FAV radiocefalica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_HistoricoIncompleto">
        <description>Pendente: Historico clinico incompleto</description>
        <inputEntry id="InputEntry_Pendente_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_observacao"><text>"Documentacao clinica incompleta. Solicitar estagiamento DRC, eGFR, Teste de Allen e mapeamento vascular."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_fundamentacao"><text>"ANS RN 465/2021 - Documentacao clinica completa obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      

      <rule id="Rule_Fallback_30701023">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise individualizada por cirurgiao vascular e nefrologista auditor."</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao caso a caso - KDOQI 2024, SBN 2023, ESVS 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>