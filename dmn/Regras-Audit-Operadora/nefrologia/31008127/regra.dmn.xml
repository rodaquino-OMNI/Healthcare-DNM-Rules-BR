<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA011_31008127" name="DIA-011: Avaliacao Doador Vivo - Medical Criteria" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA011_31008127" name="DIA-011: Avaliacao Doador Vivo Renal">
    <decisionTable id="DecisionTable_DIA011_31008127" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Relacao Doador-Receptor">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"Parente_1_Grau","Parente_2_Grau","Parente_3_4_Grau","Conjuge","Nao_Aparentado","Desconhecido"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="eGFR Doador (mL/min/1.73m2)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Comorbidades do Doador">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"Nenhuma","HAS_Controlada","HAS_Nao_Controlada","DM","Obesidade_Leve","Obesidade_Grave","Doenca_Renal"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Avaliacao Psicossocial">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Aprovado","Pendente","Coercao_Identificada","Psiquiatrico_Grave","Nao_Realizada"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Avaliacao Clinica + Imagem Completas">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA011_Reprovado_SemDocumentacao">
        <description>Reprovado - Avaliacao incompleta</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Avaliacao do doador incompleta. Necessario: funcao renal, sorologias, tipagem HLA, TC abdome e avaliacao psicossocial."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDIGO 2017 - Comprehensive donor evaluation required; ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Reprovado_eGFR_Baixo">
        <description>Reprovado - eGFR baixo, risco para doador</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;lt; 60</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"eGFR menor que 60 mL/min - contraindicacao absoluta a doacao. Risco inaceitavel de DRC pos-nefrectomia para o doador."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDIGO 2017 - GFR menor que 60 is absolute contraindication to donation"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Reprovado_Diabetes">
        <description>Reprovado - Diabetes mellitus</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>"DM"</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Diabetes mellitus e contraindicacao absoluta a doacao renal pelo risco de nefropatia diabetica futura no doador."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDIGO 2017 - Diabetes is absolute contraindication; ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Reprovado_Coercao">
        <description>Reprovado - Coercao identificada</description>
        <inputEntry id="IE_R4_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R4_4"><text>"Coercao_Identificada"</text></inputEntry>
        <inputEntry id="IE_R4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R4_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R4_2"><text>"Coercao ou pressao identificada na avaliacao psicossocial - doacao nao pode prosseguir. Decisao deve ser autonoma e voluntaria."</text></outputEntry>
        <outputEntry id="OE_R4_3"><text>"KDIGO 2017 - Donor autonomy paramount; Lei 9434/97"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Reprovado_DoencaRenal">
        <description>Reprovado - Doenca renal conhecida</description>
        <inputEntry id="IE_R5_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R5_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R5_3"><text>"Doenca_Renal"</text></inputEntry>
        <inputEntry id="IE_R5_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R5_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R5_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R5_2"><text>"Doenca renal conhecida e contraindicacao a doacao. Proteger funcao renal do potencial doador."</text></outputEntry>
        <outputEntry id="OE_R5_3"><text>"KDIGO 2017 - Known kidney disease contraindicates donation"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Reprovado_NaoAparentado">
        <description>Reprovado - Nao aparentado sem justificativa legal</description>
        <inputEntry id="IE_R6_1"><text>"Nao_Aparentado","Desconhecido"</text></inputEntry>
        <inputEntry id="IE_R6_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R6_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R6_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R6_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R6_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R6_2"><text>"Doador nao aparentado requer autorizacao judicial especifica conforme Lei 9434/97. Encaminhar para processo legal antes de prosseguir."</text></outputEntry>
        <outputEntry id="OE_R6_3"><text>"Lei 9434/1997 Art. 9 - Doacao entre nao-parentes requer autorizacao judicial"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA011_Aprovado_Ideal">
        <description>Aprovado - Doador ideal</description>
        <inputEntry id="IE_A1_1"><text>"Parente_1_Grau","Parente_2_Grau","Conjuge"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;gt;= 90</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Aprovado"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Doador ideal: parente proximo, eGFR maior ou igual a 90, sem comorbidades e avaliacao psicossocial favoravel. Prosseguir com avaliacao final e crossmatch."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDIGO 2017 - Ideal donor criteria met; ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Aprovado_eGFR_Aceitavel">
        <description>Aprovado - eGFR aceitavel (80-90)</description>
        <inputEntry id="IE_A2_1"><text>"Parente_1_Grau","Parente_2_Grau","Parente_3_4_Grau","Conjuge"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>[80..90[</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Nenhuma","Obesidade_Leve"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Aprovado"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"eGFR 80-90 mL/min e aceitavel para doacao. Discutir com doador risco de DRC pos-nefrectomia (pequeno mas existente)."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDIGO 2017 - GFR maior ou igual a 80 acceptable with counseling"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Aprovado_HAS_Controlada">
        <description>Aprovado - HAS controlada com medicacao unica</description>
        <inputEntry id="IE_A3_1"><text>"Parente_1_Grau","Parente_2_Grau","Conjuge"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>&amp;gt;= 80</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"HAS_Controlada"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"Aprovado"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"HAS controlada com uma medicacao e aceitavel. Confirmar PA menor que 140/90 e ausencia de lesao de orgao-alvo antes de aprovar definitivamente."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDIGO 2017 - Well-controlled HTN on single agent may be acceptable"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA011_Pendente_eGFR_Limite">
        <description>Pendente - eGFR limite, avaliar caso a caso</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>[60..80[</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Aprovado"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"eGFR 60-80 mL/min e limite. Avaliar idade do doador, risco calculado de ESKD pos-doacao e discutir com equipe multidisciplinar."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDIGO 2017 - Individualized risk assessment for GFR 60-80"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Pendente_Psicossocial">
        <description>Pendente - Avaliacao psicossocial nao realizada</description>
        <inputEntry id="IE_P2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_2"><text>&amp;gt;= 80</text></inputEntry>
        <inputEntry id="IE_P2_3"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="IE_P2_4"><text>"Nao_Realizada","Pendente"</text></inputEntry>
        <inputEntry id="IE_P2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P2_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P2_2"><text>"Avaliacao psicossocial obrigatoria pendente. Encaminhar para psicologo/psiquiatra da equipe de transplante antes de prosseguir."</text></outputEntry>
        <outputEntry id="OE_P2_3"><text>"KDIGO 2017 - Psychosocial evaluation mandatory; ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_DIA011_Pendente_Obesidade">
        <description>Pendente - Obesidade grave, reducao de peso necessaria</description>
        <inputEntry id="IE_P3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P3_2"><text>&amp;gt;= 80</text></inputEntry>
        <inputEntry id="IE_P3_3"><text>"Obesidade_Grave"</text></inputEntry>
        <inputEntry id="IE_P3_4"><text>"Aprovado"</text></inputEntry>
        <inputEntry id="IE_P3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P3_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P3_2"><text>"IMC maior que 35 kg/m2 - reducao de peso necessaria antes da doacao pelo risco cirurgico e metabolico. Reavaliar apos perda de peso."</text></outputEntry>
        <outputEntry id="OE_P3_3"><text>"KDIGO 2017 - BMI maior que 35 requires weight loss before donation"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008127">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer avaliacao pela equipe multidisciplinar de transplante conforme KDIGO 2017 e ABTO 2023."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDIGO 2017 / ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA011_31008127">
      <dmndi:DMNShape id="DMNShape_Decision_DIA011_31008127" dmnElementRef="Decision_DIA011_31008127">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>