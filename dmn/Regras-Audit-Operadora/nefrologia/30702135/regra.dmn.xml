<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30702135" name="Angioplastia de Arteria Renal" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30702135" name="Regra Clinica - Angioplastia de Arteria Renal">
    <decisionTable id="DecisionTable_30702135" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"HASResistente","DRCProgressiva","DisplasiaFibromuscular","EdemaPulmonarFlash","AnginaRenal","EstenoseMenor50"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (ACC/AHA)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"AngioTCRenal","CintilografiaCaptopril","Completos","Parciais","Ausentes","RimAtrofico"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Angioplastia contraindicada - rim atrofico ou oclusao total"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"ACC/AHA 2016 - Contraindicacoes: rim menor 7cm, oclusao cronica, DRC terminal"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_observacao"><text>"Expectativa de vida limitada - tratamento clinico"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_fundamentacao"><text>"ACC/AHA 2016 - Intervencao requer expectativa de vida compativel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_EstenoseMenor50">
        <description>Contraindicacao: Estenose menor 50% assintomatica</description>
        <inputEntry id="InputEntry_Contraind_Est50_1"><text>"EstenoseMenor50"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Est50_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Est50_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Est50_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Est50_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Est50_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Est50_observacao"><text>"Estenose menor 50% - tratamento clinico indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Est50_fundamentacao"><text>"ACC/AHA 2016 - Estenose menor 50% nao tem indicacao de intervencao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_RimAtrofico">
        <description>Contraindicacao: Rim atrofico</description>
        <inputEntry id="InputEntry_Contraind_Atrof_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Atrof_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Atrof_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Atrof_4"><text>"RimAtrofico"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Atrof_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Atrof_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Atrof_observacao"><text>"Rim atrofico - revascularizacao nao indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Atrof_fundamentacao"><text>"ACC/AHA 2016 - Rim menor 7cm: revascularizacao nao recupera funcao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_DisplasiaFM">
        <description>Aprovado: Displasia fibromuscular sintomatica</description>
        <inputEntry id="InputEntry_Aprovado_DFM_1"><text>"DisplasiaFibromuscular"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_DFM_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_DFM_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_DFM_4"><text>"AngioTCRenal","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_DFM_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_DFM_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_DFM_observacao"><text>"Displasia fibromuscular - angioplastia primeira escolha"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_DFM_fundamentacao"><text>"ACC/AHA 2016 Classe I - Angioplastia e tratamento de escolha para displasia fibromuscular sintomatica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_EdemaPulmonar">
        <description>Aprovado: Edema pulmonar flash recorrente</description>
        <inputEntry id="InputEntry_Aprovado_EPA_1"><text>"EdemaPulmonarFlash"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_EPA_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_EPA_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_EPA_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_EPA_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_EPA_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EPA_observacao"><text>"Edema pulmonar flash com estenose bilateral - revascularizacao indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EPA_fundamentacao"><text>"ACC/AHA 2016 Classe I - Edema pulmonar recorrente com estenose bilateral: forte indicacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_Geral">
        <description>Aprovado: Indicacao Classe I geral</description>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_observacao"><text>"Angioplastia renal indicada - Classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_fundamentacao"><text>"ACC/AHA 2016 - Indicacao estabelecida"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseIIa_HAS">
        <description>Aprovado: Classe IIa - HAS resistente</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>"HASResistente"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_4"><text>"AngioTCRenal","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"HAS resistente com estenose significativa - revascularizacao razoavel"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"ACC/AHA 2016 Classe IIa - HAS resistente com estenose maior 70%: intervencao pode ser considerada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIa_DRC">
        <description>Aprovado: Classe IIa - DRC progressiva</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_DRC_1"><text>"DRCProgressiva"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_DRC_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_DRC_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_DRC_4"><text>"CintilografiaCaptopril","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_DRC_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_DRC_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_DRC_observacao"><text>"DRC progressiva atribuivel a estenose - revascularizacao para preservar funcao"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_DRC_fundamentacao"><text>"ACC/AHA 2016 Classe IIa - DRC progressiva com estenose bilateral ou rim unico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_ExamesParciais">
        <description>Pendente: Exames comprobatorios parciais</description>
        <inputEntry id="InputEntry_Pend_Exames_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_4"><text>"Parciais"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_observacao"><text>"Aguardando angioTC ou cintilografia"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_fundamentacao"><text>"ACC/AHA 2016 - Documentacao anatomica e funcional obrigatorias"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Reprovado_HistoricoInsuficiente">
        <description>Reprovado: Historico clinico insuficiente</description>
        <inputEntry id="InputEntry_Reprovado_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Reprovado_ExamesAusentes">
        <description>Reprovado: Exames comprobatorios ausentes</description>
        <inputEntry id="InputEntry_Reprovado_Exames_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_4"><text>"Ausentes"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_observacao"><text>"Exames obrigatorios nao apresentados"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_fundamentacao"><text>"ACC/AHA 2016 - Imagem vascular obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30702135">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise pela cardiologia intervencionista"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"ACC/AHA PAD 2016, SCAI 2014"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>