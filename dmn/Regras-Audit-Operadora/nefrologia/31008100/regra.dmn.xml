<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA009_31008100" name="DIA-009: Quelantes de Fosforo - Selection Criteria" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA009_31008100" name="DIA-009: Quelantes de Fosforo">
    <decisionTable id="DecisionTable_DIA009_31008100" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Estagio DRC (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"N18.3","N18.4","N18.5","Z99.2","N25.0","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="Fosforo Serico (mg/dL)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Quelante Solicitado">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"Carbonato_Calcio","Acetato_Calcio","Sevelamer","Lantanio","Aluminio","Sucroferrico"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Nivel Calcio Serico">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Normal","Hipocalcemia","Hipercalcemia","Calcificacao_Vascular","Nao_Dosado"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="P + Ca Documentados">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA009_Reprovado_SemExames">
        <description>Reprovado - Exames laboratoriais obrigatorios ausentes</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Fosforo e calcio sericos obrigatorios para prescricao de quelantes. Solicitar exames antes de autorizar."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDIGO 2017 - P and Ca monitoring required for phosphate binder therapy"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA009_Reprovado_Fosforo_Normal">
        <description>Reprovado - Fosforo normal, quelante nao indicado</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;lt;= 5.5</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Fosforo menor ou igual a 5.5 mg/dL dentro do alvo - quelante nao indicado neste momento. Manter restricao dietetica e monitorar."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDIGO 2017 - Binders indicated when P persistently above upper normal limit"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA009_Reprovado_Aluminio">
        <description>Reprovado - Quelante de aluminio (contraindicado uso cronico)</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>"Aluminio"</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Quelantes de aluminio contraindicados para uso cronico devido toxicidade (encefalopatia, osteomalacia). Uso maximo 4 semanas emergencial."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDIGO 2017 - Avoid aluminum-containing phosphate binders"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA009_Reprovado_Calcio_Hipercalcemia">
        <description>Reprovado - Quelante de calcio com hipercalcemia</description>
        <inputEntry id="IE_R4_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R4_2"><text>&amp;gt; 5.5</text></inputEntry>
        <inputEntry id="IE_R4_3"><text>"Carbonato_Calcio","Acetato_Calcio"</text></inputEntry>
        <inputEntry id="IE_R4_4"><text>"Hipercalcemia","Calcificacao_Vascular"</text></inputEntry>
        <inputEntry id="IE_R4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R4_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R4_2"><text>"Quelante a base de calcio contraindicado com hipercalcemia ou calcificacao vascular. Usar quelante sem calcio (sevelamer ou lantanio)."</text></outputEntry>
        <outputEntry id="OE_R4_3"><text>"KDIGO 2017 - Restrict calcium-based binders in hypercalcemia/vascular calcification"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA009_Aprovado_Calcio_PrimeiraLinha">
        <description>Aprovado - Quelante de calcio como primeira linha</description>
        <inputEntry id="IE_A1_1"><text>"N18.3","N18.4","N18.5","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;gt; 5.5</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"Carbonato_Calcio","Acetato_Calcio"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Normal","Hipocalcemia"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Hiperfosfatemia (P maior que 5.5) com calcio normal/baixo - quelante de calcio indicado como primeira linha. Monitorar calcemia mensalmente."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDIGO 2017 - Calcium-based binders acceptable first-line if Ca not elevated"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA009_Aprovado_Sevelamer">
        <description>Aprovado - Sevelamer para hiperfosfatemia com hipercalcemia</description>
        <inputEntry id="IE_A2_1"><text>"N18.3","N18.4","N18.5","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;gt; 5.5</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Sevelamer"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Hipercalcemia","Calcificacao_Vascular","Normal"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Sevelamer indicado para hiperfosfatemia, especialmente com hipercalcemia ou calcificacao vascular. Quelante sem calcio preferencial nestes casos."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDIGO 2017 - Non-calcium binders preferred in vascular calcification"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA009_Aprovado_Lantanio">
        <description>Aprovado - Lantanio como alternativa</description>
        <inputEntry id="IE_A3_1"><text>"N18.5","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>&amp;gt; 6.5</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"Lantanio"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Lantanio indicado para hiperfosfatemia severa (P maior que 6.5) ou refrataria a outros quelantes. Quelante potente sem calcio."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDIGO 2017 - Lanthanum effective non-calcium binder option"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA009_Aprovado_Sucroferrico">
        <description>Aprovado - Sucroferrico para hiperfosfatemia com deficiencia ferro</description>
        <inputEntry id="IE_A4_1"><text>"N18.5","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A4_2"><text>&amp;gt; 5.5</text></inputEntry>
        <inputEntry id="IE_A4_3"><text>"Sucroferrico"</text></inputEntry>
        <inputEntry id="IE_A4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A4_2"><text>"Oxihidroxido sucroferrico indicado para hiperfosfatemia, com beneficio adicional de suplementacao de ferro. Quelante sem calcio."</text></outputEntry>
        <outputEntry id="OE_A4_3"><text>"KDIGO 2017 - Sucroferric oxyhydroxide effective with iron supplementation benefit"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA009_Pendente_Calcio_NaoDosado">
        <description>Pendente - Calcio nao dosado, completar avaliacao</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>&amp;gt; 5.5</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"Carbonato_Calcio","Acetato_Calcio"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Nao_Dosado"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Calcio nao dosado - necessario antes de prescrever quelante de calcio. Dosar Ca para descartar hipercalcemia."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDIGO 2017 - Ca assessment needed before calcium-based binders"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008100">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer avaliacao por nefrologista conforme KDIGO 2017 CKD-MBD."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDIGO 2017 / SBN 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA009_31008100">
      <dmndi:DMNShape id="DMNShape_Decision_DIA009_31008100" dmnElementRef="Decision_DIA009_31008100">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>