<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30701031" name="Implante de Protese Vascular para Hemodialise (Enxerto PTFE)" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30701031" name="Regra Clinica - Implante Protese Vascular HD">
    <decisionTable id="DecisionTable_30701031" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"FalhaFAVMultiplas","VeiasInadequadas","UrgenciaAcesso","TromboseVeiasCentrais","InfeccaoAtiva"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (KDOQI/SBN)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"MapeamentoCompleto_FAVInviavel","MapeamentoParcial","SemMapeamento","FlebografiaRealizada"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida Superior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Procedimento Classe III - contraindicado. Infeccao ativa, arteriopatia severa ou coagulopatia nao corrigida."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDOQI 2024 - Enxerto PTFE contraindicado em infeccao ativa ou condicoes sistemicas desfavoraveis"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_observacao"><text>"Expectativa de vida inferior a 1 ano. Enxerto tem custo elevado e complicacoes frequentes. Manter cateter tunelizado."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_fundamentacao"><text>"KDOQI 2024 - Considerar expectativa de vida e qualidade de vida ao escolher modalidade de acesso"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_InfeccaoAtiva">
        <description>Contraindicacao: Infeccao ativa - risco de infeccao do enxerto</description>
        <inputEntry id="InputEntry_Contraind_Inf_1"><text>"InfeccaoAtiva"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_observacao"><text>"Infeccao ativa e contraindicacao absoluta para implante de protese. Tratar infeccao e reavaliar apos resolucao completa."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_fundamentacao"><text>"KDOQI 2024, IDSA Guidelines - Material protetico nao deve ser implantado na vigencia de infeccao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_SemMapeamento">
        <description>Reprovacao: Ausencia de mapeamento vascular</description>
        <inputEntry id="InputEntry_SemMapeamento_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_4"><text>"SemMapeamento"</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_observacao"><text>"Mapeamento vascular obrigatorio para confirmar inviabilidade de FAV nativa e planejar local do enxerto."</text></outputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_fundamentacao"><text>"KDOQI 2024 - Enxerto so indicado apos documentar impossibilidade de FAV nativa"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      

      <rule id="Rule_Aprovado_ClasseI_FalhaFAV">
        <description>Aprovado: Falha de FAV multiplas com anatomia para enxerto</description>
        <inputEntry id="InputEntry_Aprovado_Falha_1"><text>"FalhaFAVMultiplas","VeiasInadequadas"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_4"><text>"MapeamentoCompleto_FAVInviavel","FlebografiaRealizada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_observacao"><text>"FAV nativa inviavel documentada. Enxerto PTFE indicado como acesso secundario. Maturacao em 2-3 semanas."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_fundamentacao"><text>"KDOQI 2024 - Enxerto e segunda opcao apos esgotamento de opcoes de FAV nativa"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIa_Urgencia">
        <description>Aprovado: Urgencia de acesso com veias superficiais inadequadas</description>
        <inputEntry id="InputEntry_Aprovado_Urg_1"><text>"UrgenciaAcesso"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Urg_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Urg_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Urg_4"><text>"MapeamentoCompleto_FAVInviavel"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Urg_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Urg_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Urg_observacao"><text>"Urgencia de acesso permanente. Enxerto permite puncao mais precoce que FAV (2-3 semanas). Manter cateter ate maturacao."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Urg_fundamentacao"><text>"KDOQI 2024 - Enxerto precoce pode ser alternativa quando FAV inviavel e cateter prolongado e indesejavel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIb">
        <description>Aprovado: Indicacao Classe IIb com documentacao adequada</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_4"><text>"MapeamentoCompleto_FAVInviavel","FlebografiaRealizada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_observacao"><text>"Indicacao Classe IIb - pode ser considerado. Avaliar alternativas e discutir risco-beneficio com paciente."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_fundamentacao"><text>"KDOQI 2024 - Classe IIb: Beneficio possivelmente supera risco"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      

      <rule id="Rule_Pendente_MapeamentoParcial">
        <description>Pendente: Necessario mapeamento completo</description>
        <inputEntry id="InputEntry_Pendente_Map_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_4"><text>"MapeamentoParcial"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_observacao"><text>"Mapeamento incompleto. Necessario documentar inviabilidade de FAV nativa e avaliar veias centrais (flebografia se suspeita de estenose)."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_fundamentacao"><text>"KDOQI 2024 - Enxerto so apos confirmar impossibilidade de FAV; avaliar veias centrais"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_HistoricoIncompleto">
        <description>Pendente: Historico clinico incompleto</description>
        <inputEntry id="InputEntry_Pendente_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_observacao"><text>"Documentacao incompleta. Necessario: historico de acessos previos, motivo de falhas, mapeamento vascular completo."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_fundamentacao"><text>"ANS RN 465/2021 - Documentacao clinica completa obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      

      <rule id="Rule_Fallback_30701031">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise individualizada por cirurgiao vascular e nefrologista auditor."</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao caso a caso - KDOQI 2024, SBN 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>