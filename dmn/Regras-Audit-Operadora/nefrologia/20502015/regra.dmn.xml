<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="DIA-001-Inicio-Dialise-Criterios" name="Regra Início Diálise - Critérios KDIGO" namespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DIA001_Inicio" name="DIA-001: Critérios Início Diálise">
    <decisionTable id="DecisionTable_DIA001" hitPolicy="FIRST">
      <input id="Input_eGFR" label="Taxa Filtração Glomerular (eGFR)" camunda:inputVariable="taxaFiltracaoGlomerular">
        <inputExpression id="InputExpression_eGFR" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      <input id="Input_SintomasUremicos" label="Sintomas Urêmicos Presentes" camunda:inputVariable="possuiSintomasUremicos">
        <inputExpression id="InputExpression_Sintomas" typeRef="boolean">
          <text>possuiSintomasUremicos</text>
        </inputExpression>
        <inputValues id="InputValues_Sintomas">
          <text>true,false</text>
        </inputValues>
      </input>

      <input id="Input_HipercalemiaSevera" label="Hipercalemia Severa (K &gt;6.5)" camunda:inputVariable="hipercalemiaSevera">
        <inputExpression id="InputExpression_K" typeRef="boolean">
          <text>hipercalemiaSevera</text>
        </inputExpression>
        <inputValues id="InputValues_K">
          <text>true,false</text>
        </inputValues>
      </input>

      <input id="Input_SobrecargaVolume" label="Sobrecarga Volêmica Refratária" camunda:inputVariable="sobrecargaVolemicaRefrataria">
        <inputExpression id="InputExpression_Volume" typeRef="boolean">
          <text>sobrecargaVolemicaRefrataria</text>
        </inputExpression>
        <inputValues id="InputValues_Volume">
          <text>true,false</text>
        </inputValues>
      </input>

      <input id="Input_DesnutricaoProgressiva" label="Desnutrição Progressiva" camunda:inputVariable="desnutricaoProgressiva">
        <inputExpression id="InputExpression_Desnutricao" typeRef="boolean">
          <text>desnutricaoProgressiva</text>
        </inputExpression>
        <inputValues id="InputValues_Desnutricao">
          <text>true,false</text>
        </inputValues>
      </input>

      <output id="Output_Resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_Resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_Observacao" label="Observação" name="observacao" typeRef="string" />

      <output id="Output_Fundamentacao" label="Fundamentação Legal" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA001_01">
        <inputEntry id="InputEntry_R1_eGFR">
          <text>&amp;gt; 15</text>
        </inputEntry>
        <inputEntry id="InputEntry_R1_Sintomas">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_R1_K">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_R1_Volume">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_R1_Desnutricao">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R1_Resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R1_Obs">
          <text>"eGFR &amp;gt;15 ml/min/1.73m² sem sintomas urêmicos ou complicações. Manter tratamento conservador e reavaliação periódica."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R1_Fund">
          <text>"KDIGO 2012 - Início diálise quando eGFR 5-10 ou &amp;lt;15 com sintomas"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA001_02">
        <inputEntry id="InputEntry_R2_eGFR">
          <text>&amp;lt; 10</text>
        </inputEntry>
        <inputEntry id="InputEntry_R2_Sintomas">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R2_K">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R2_Volume">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R2_Desnutricao">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R2_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R2_Obs">
          <text>"eGFR &amp;lt;10 ml/min/1.73m² - Indicação absoluta diálise. Avaliar modalidade (HD ou DP) conforme preferência paciente e contraindicações."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R2_Fund">
          <text>"KDIGO 2012 CKD Stage 5 - Início diálise eGFR &amp;lt;10"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA001_03">
        <inputEntry id="InputEntry_R3_eGFR">
          <text>[10..15]</text>
        </inputEntry>
        <inputEntry id="InputEntry_R3_Sintomas">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R3_K">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R3_Volume">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R3_Desnutricao">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R3_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R3_Obs">
          <text>"eGFR 10-15 com sintomas urêmicos (náusea, vômito, confusão mental, pericardite, encefalopatia). Indicação início diálise."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R3_Fund">
          <text>"KDIGO 2012 - Diálise se sintomas urêmicos não controlados"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA001_04">
        <inputEntry id="InputEntry_R4_eGFR">
          <text>&amp;lt; 20</text>
        </inputEntry>
        <inputEntry id="InputEntry_R4_Sintomas">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R4_K">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R4_Volume">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R4_Desnutricao">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R4_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R4_Obs">
          <text>"Hipercalemia severa (K &amp;gt;6.5 mEq/L) refratária a tratamento conservador. Indicação emergencial diálise para controle metabólico."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R4_Fund">
          <text>"KDIGO 2012 - Diálise urgente para hipercalemia refratária"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA001_05">
        <inputEntry id="InputEntry_R5_eGFR">
          <text>&amp;lt; 20</text>
        </inputEntry>
        <inputEntry id="InputEntry_R5_Sintomas">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R5_K">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R5_Volume">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R5_Desnutricao">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R5_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R5_Obs">
          <text>"Sobrecarga volêmica refratária a diuréticos (edema pulmonar, congestão sistêmica). Indicação diálise para ultrafiltração."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R5_Fund">
          <text>"KDIGO 2012 - Diálise para controle volêmico refratário"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA001_06">
        <inputEntry id="InputEntry_R6_eGFR">
          <text>[10..15]</text>
        </inputEntry>
        <inputEntry id="InputEntry_R6_Sintomas">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R6_K">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R6_Volume">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R6_Desnutricao">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R6_Resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R6_Obs">
          <text>"Desnutrição proteico-calórica progressiva (albumina &amp;lt;3.0 g/dL) refratária a suporte nutricional. Indicação início diálise."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R6_Fund">
          <text>"KDIGO 2012 - Diálise para desnutrição refratária ao manejo conservador"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_DIA001_07">
        <inputEntry id="InputEntry_R7_eGFR">
          <text>[10..15]</text>
        </inputEntry>
        <inputEntry id="InputEntry_R7_Sintomas">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_R7_K">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R7_Volume">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_R7_Desnutricao">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_R7_Resultado">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R7_Obs">
          <text>"Indicação início diálise aprovada. Pendente confecção acesso vascular (FAV preferencial) ou inserção cateter temporário urgente."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_R7_Fund">
          <text>"SBN Diretrizes - Acesso vascular prévio preferencial"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Default_Fallback">
        <inputEntry id="InputEntry_Fallback_eGFR">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_Sintomas">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_K">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_Volume">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_Desnutricao">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Fallback_Resultado">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Fallback_Obs">
          <text>"Caso não contemplado nas regras. Solicitar avaliação nefrologista para decisão individualizada conforme KDIGO 2012."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Fallback_Fund">
          <text>"Análise caso a caso - KDIGO 2012"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>