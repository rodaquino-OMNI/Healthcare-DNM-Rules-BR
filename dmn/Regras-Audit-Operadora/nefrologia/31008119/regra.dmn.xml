<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA010_31008119" name="DIA-010: Inicio Avaliacao Transplante - eGFR Thresholds" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA010_31008119" name="DIA-010: Avaliacao Transplante Renal">
    <decisionTable id="DecisionTable_DIA010_31008119" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Etiologia DRC (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"N18.4","N18.5","E11.22","E10.22","I12.0","N04.9","Q61.2","Z99.2","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="eGFR (mL/min/1.73m2)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Status Atual de Dialise">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"Pre_Dialise","Em_HD","Em_DP","Tx_Previo_Falha","Nao_Informado"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Presenca de Contraindicacoes">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Nenhuma","Neoplasia_Ativa","Infeccao_Ativa","CV_Terminal","Psiquiatrica_Grave","Abuso_Substancias"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Documentacao Basica Completa">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA010_Reprovado_SemDocumentacao">
        <description>Reprovado - Documentacao basica ausente</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Documentacao basica incompleta para avaliacao de transplante. Necessario: funcao renal, sorologias, tipagem ABO e laudo nefrologista."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"ABTO 2023 - Documentacao minima para inscricao em lista de transplante"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA010_Reprovado_eGFR_Alto">
        <description>Reprovado - eGFR ainda alto para iniciar avaliacao</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;gt; 25</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>"Pre_Dialise"</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"eGFR maior que 25 mL/min - precoce para avaliacao de transplante. Iniciar quando eGFR menor que 20. Manter acompanhamento nefrologico."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDIGO 2009 - Referral for transplant evaluation when GFR menor que 20"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA010_Reprovado_Contraindicacao_Absoluta">
        <description>Reprovado - Contraindicacao absoluta para transplante</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>"Neoplasia_Ativa","CV_Terminal","Abuso_Substancias"</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Contraindicacao absoluta para transplante renal presente. Reavaliar apos tratamento/resolucao da condicao contraindicate."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"ABTO 2023 - Contraindicacoes absolutas ao transplante renal"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA010_Aprovado_PreDialise_eGFR_Baixo">
        <description>Aprovado - Pre-dialise com eGFR menor que 20, transplante preemptivo</description>
        <inputEntry id="IE_A1_1"><text>"N18.4","N18.5"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;lt; 20</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"Pre_Dialise"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"eGFR menor que 20 mL/min em pre-dialise - avaliacao para transplante preemptivo indicada. Transplante antes de dialise tem melhores outcomes."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDIGO 2009 - Preemptive transplantation preferred; ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA010_Aprovado_Em_Dialise">
        <description>Aprovado - Paciente em dialise, avaliacao para transplante</description>
        <inputEntry id="IE_A2_1"><text>"N18.5","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Em_HD","Em_DP"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Paciente em dialise sem contraindicacoes - avaliacao para transplante indicada. Listar o mais precocemente possivel para reduzir tempo em dialise."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDIGO 2009 - All dialysis patients should be considered for transplant; ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA010_Aprovado_Retransplante">
        <description>Aprovado - Retransplante apos falha de enxerto previo</description>
        <inputEntry id="IE_A3_1"><text>"Z99.2","T86.12"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"Tx_Previo_Falha","Em_HD","Em_DP"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Falha de transplante previo - avaliacao para retransplante indicada. Avaliar sensibilizacao HLA (PRA) e planejar dessensibilizacao se necessario."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDIGO 2009 - Retransplantation should be considered; ABTO 2023 - Criterios retransplante"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA010_Aprovado_eGFR_Critico">
        <description>Aprovado - eGFR critico, avaliacao urgente</description>
        <inputEntry id="IE_A4_1"><text>"N18.5"</text></inputEntry>
        <inputEntry id="IE_A4_2"><text>&amp;lt; 10</text></inputEntry>
        <inputEntry id="IE_A4_3"><text>"Pre_Dialise"</text></inputEntry>
        <inputEntry id="IE_A4_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="IE_A4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A4_2"><text>"eGFR critico menor que 10 mL/min - avaliacao urgente para transplante. Considerar dialise iminente; se doador vivo disponivel, acelerar avaliacao."</text></outputEntry>
        <outputEntry id="OE_A4_3"><text>"ABTO 2023 - Avaliacao urgente quando dialise iminente; KDIGO 2009"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA010_Pendente_Infeccao">
        <description>Pendente - Infeccao ativa, tratar antes</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>&amp;lt; 20</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Infeccao_Ativa"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Infeccao ativa presente - tratar antes de listar para transplante. HCV/HBV/HIV nao sao contraindicacoes absolutas se tratados/controlados."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"ABTO 2023 - Infeccoes devem estar controladas antes do transplante"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_DIA010_Pendente_Psiquiatrica">
        <description>Pendente - Doenca psiquiatrica, avaliacao especializada</description>
        <inputEntry id="IE_P2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_2"><text>&amp;lt; 20</text></inputEntry>
        <inputEntry id="IE_P2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_4"><text>"Psiquiatrica_Grave"</text></inputEntry>
        <inputEntry id="IE_P2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P2_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P2_2"><text>"Doenca psiquiatrica grave - encaminhar para avaliacao psiquiatrica especializada. Transplante possivel se controle adequado da condicao."</text></outputEntry>
        <outputEntry id="OE_P2_3"><text>"ABTO 2023 - Avaliacao psicossocial obrigatoria; doenca controlada nao e contraindicacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008119">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer avaliacao por equipe de transplante conforme ABTO 2023 e KDIGO 2009."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - ABTO 2023 / KDIGO 2009"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA010_31008119">
      <dmndi:DMNShape id="DMNShape_Decision_DIA010_31008119" dmnElementRef="Decision_DIA010_31008119">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>