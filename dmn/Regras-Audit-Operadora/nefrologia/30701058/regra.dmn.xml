<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30701058" name="Angioplastia de Fistula Arteriovenosa" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30701058" name="Regra Clinica - Angioplastia FAV">
    <decisionTable id="DecisionTable_30701058" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"EstenoseSintomatica","EstenoseMaior50_Disfuncao","FluxoReduzido","PressaoVenosaElevada","TromboseAguda","InfeccaoAtiva"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (KDOQI/SBN)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"FistulografiaComEstenose","DopplerComEstenose","MedicaoFluxo_Kt_V","AvaliacaoClinica","SemAvaliacao"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida Superior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Procedimento Classe III. Estenose menor que 50% sem disfuncao, infeccao ativa ou FAV imatura."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDOQI 2024 - Angioplastia profilatica sem criterios de estenose significativa nao indicada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_observacao"><text>"Expectativa de vida inferior a 1 ano. Avaliar custo-beneficio de intervencao endovascular vs cateter."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_fundamentacao"><text>"KDOQI 2024 - Considerar prognostico na decisao de intervencoes invasivas"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_TromboseAguda">
        <description>Contraindicacao relativa: Trombose aguda - trombectomia primeiro</description>
        <inputEntry id="InputEntry_Contraind_Tromb_1"><text>"TromboseAguda"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Tromb_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Tromb_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Tromb_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Tromb_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Tromb_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Tromb_observacao"><text>"Trombose aguda - realizar trombectomia primeiro. Angioplastia pode ser associada no mesmo tempo se estenose identificada."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Tromb_fundamentacao"><text>"KDOQI 2024 - Trombectomia precede angioplastia em FAV trombosada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_InfeccaoAtiva">
        <description>Contraindicacao: Infeccao ativa</description>
        <inputEntry id="InputEntry_Contraind_Inf_1"><text>"InfeccaoAtiva"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_observacao"><text>"Infeccao ativa contraindica procedimento endovascular. Tratar infeccao e reavaliar."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_fundamentacao"><text>"KDOQI 2024 - Intervencao endovascular contraindicada em infeccao ativa"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      

      <rule id="Rule_Aprovado_ClasseI_EstenoseSintomatica">
        <description>Aprovado: Estenose sintomatica maior que 50%</description>
        <inputEntry id="InputEntry_Aprovado_Esteno_1"><text>"EstenoseSintomatica","EstenoseMaior50_Disfuncao"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Esteno_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Esteno_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Esteno_4"><text>"FistulografiaComEstenose","DopplerComEstenose"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Esteno_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Esteno_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Esteno_observacao"><text>"Estenose maior que 50% com disfuncao de acesso documentada. Angioplastia indicada para preservar FAV."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Esteno_fundamentacao"><text>"KDOQI 2024 - Estenose hemodinamicamente significativa: indicacao Classe I para angioplastia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_FluxoReduzido">
        <description>Aprovado: Fluxo reduzido com estenose documentada</description>
        <inputEntry id="InputEntry_Aprovado_Fluxo_1"><text>"FluxoReduzido"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Fluxo_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Fluxo_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Fluxo_4"><text>"FistulografiaComEstenose","MedicaoFluxo_Kt_V"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Fluxo_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Fluxo_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Fluxo_observacao"><text>"Fluxo de acesso menor que 600mL/min com estenose. Angioplastia para prevenir trombose."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Fluxo_fundamentacao"><text>"KDOQI 2024 - Monitorizacao de fluxo com intervencao proativa prolonga patencia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_PressaoVenosa">
        <description>Aprovado: Pressao venosa elevada com estenose</description>
        <inputEntry id="InputEntry_Aprovado_PV_1"><text>"PressaoVenosaElevada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_PV_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_PV_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_PV_4"><text>"FistulografiaComEstenose","DopplerComEstenose"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_PV_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_PV_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_PV_observacao"><text>"Pressao venosa maior que 150mmHg indicativa de estenose de via de drenagem. Angioplastia indicada."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_PV_fundamentacao"><text>"KDOQI 2024 - Pressao venosa elevada e marcador de estenose e preditor de trombose"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      

      <rule id="Rule_Pendente_SemImagem">
        <description>Pendente: Sem fistulografia ou Doppler</description>
        <inputEntry id="InputEntry_Pendente_Img_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_4"><text>"AvaliacaoClinica","SemAvaliacao"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_observacao"><text>"Documentacao de estenose ausente. Solicitar Doppler de FAV ou fistulografia para quantificar estenose."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_fundamentacao"><text>"KDOQI 2024 - Angioplastia requer documentacao de estenose maior que 50%"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_HistoricoIncompleto">
        <description>Pendente: Historico clinico incompleto</description>
        <inputEntry id="InputEntry_Pendente_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_observacao"><text>"Documentacao incompleta. Informar: sintomas de disfuncao, medicoes de fluxo/pressao, exame de imagem."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_fundamentacao"><text>"ANS RN 465/2021 - Documentacao clinica completa obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      

      <rule id="Rule_Fallback_30701058">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise individualizada por radiologista intervencionista/cirurgiao vascular."</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao caso a caso - KDOQI 2024, SVS 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>