<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA006_31008070" name="DIA-006: Conversao Cateter para Fistula - Timing" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA006_31008070" name="DIA-006: Conversao Cateter para FAV">
    <decisionTable id="DecisionTable_DIA006_31008070" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Status Renal (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"Z99.2","N18.5","N19","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="Tempo Uso Cateter (dias)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Status FAV em Maturacao">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"FAV_Matura","FAV_Em_Maturacao","FAV_Falha_Maturacao","Sem_FAV","FAV_Planejada"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Tipo de Cateter em Uso">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Cateter_Temporario","Cateter_Tunelizado","Cateter_Infectado","Sem_Cateter"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Avaliacao Maturacao FAV Documentada">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA006_Reprovado_SemAvaliacao">
        <description>Reprovado - Sem avaliacao documentada de maturacao</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Avaliacao de maturacao da FAV obrigatoria antes de conversao. Documentar exame fisico (thrill, desenvolvimento venoso) e Doppler se necessario."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDOQI 2019 - Maturation assessment required before AVF use"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA006_Reprovado_FAV_Imatura">
        <description>Reprovado - FAV ainda imatura para conversao</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>"FAV_Em_Maturacao"</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"FAV ainda em maturacao - aguardar desenvolvimento adequado (minimo 6-8 semanas, fluxo maior que 600 mL/min) antes de conversao."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDOQI 2019 - AVF requires 6-8 weeks maturation; Rule of 6s"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA006_Reprovado_CateterInfectado">
        <description>Reprovado - Cateter infectado, tratar antes de conversao</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>"FAV_Matura"</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>"Cateter_Infectado"</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Cateter com infeccao ativa - remover cateter e tratar infeccao antes de procedimento eletivo. Usar FAV imediatamente se matura."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDOQI 2019 - Infected catheter requires removal and treatment"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA006_Aprovado_FAV_Matura_Conversao">
        <description>Aprovado - FAV matura, conversao indicada</description>
        <inputEntry id="IE_A1_1"><text>"Z99.2","N18.5"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;gt;= 42</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"FAV_Matura"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Cateter_Tunelizado","Cateter_Temporario"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"FAV matura (maior que 6 semanas) e cateter em uso - conversao indicada. Iniciar canulacao de FAV e remover cateter apos 2 semanas de uso bem-sucedido."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDOQI 2019 - Transition to AVF when mature; remove catheter within 2 weeks of successful cannulation"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA006_Aprovado_Cateter_Prolongado">
        <description>Aprovado - Uso cateter prolongado, conversao urgente</description>
        <inputEntry id="IE_A2_1"><text>"Z99.2","N18.5"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;gt; 90</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"FAV_Matura"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Cateter_Tunelizado"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Uso de cateter maior que 90 dias com FAV matura disponivel - conversao urgente indicada. Risco elevado de infeccao e estenose central com cateter prolongado."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDOQI 2019 - Prolonged catheter use maior que 90 days associated with complications; CMS Fistula First"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA006_Aprovado_Cateter_Temporario">
        <description>Aprovado - Cateter temporario com FAV matura</description>
        <inputEntry id="IE_A3_1"><text>"Z99.2","N18.5"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"FAV_Matura"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"Cateter_Temporario"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Cateter temporario com FAV matura - conversao imediata indicada. Cateter temporario nao deve permanecer maior que 2-3 semanas."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDOQI 2019 - Non-tunneled catheters should not exceed 2-3 weeks"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA006_Pendente_FAV_Planejada">
        <description>Pendente - FAV planejada mas nao confeccionada</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>&amp;gt; 30</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"FAV_Planejada","Sem_FAV"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Cateter_Tunelizado"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Cateter em uso maior que 30 dias sem FAV confeccionada. Agendar confeccao de FAV urgente ou justificar impossibilidade anatomica."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDOQI 2019 - AVF planning should begin before dialysis start; CMS metrics"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_DIA006_Pendente_Falha_Maturacao">
        <description>Pendente - Falha de maturacao, reintervencao necessaria</description>
        <inputEntry id="IE_P2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_3"><text>"FAV_Falha_Maturacao"</text></inputEntry>
        <inputEntry id="IE_P2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P2_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P2_2"><text>"FAV com falha de maturacao - avaliar fistulografia e considerar angioplastia ou revisao cirurgica. Converter cateter apos resolucao."</text></outputEntry>
        <outputEntry id="OE_P2_3"><text>"KDOQI 2019 - Non-maturing AVF requires intervention; salvage procedures recommended"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008070">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer avaliacao por nefrologista conforme KDOQI 2019 e CMS Fistula First Initiative."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDOQI 2019 / SBN 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA006_31008070">
      <dmndi:DMNShape id="DMNShape_Decision_DIA006_31008070" dmnElementRef="Decision_DIA006_31008070">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>