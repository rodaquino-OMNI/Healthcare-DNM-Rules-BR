<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA003_31008046" name="DIA-003: Trombectomia Acesso Hemodialise" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA003_31008046" name="DIA-003: Trombectomia Acesso HD">
    <decisionTable id="DecisionTable_DIA003_31008046" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Diagnostico (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"T82.868","I82.8","T82.818","Z99.2","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="Tempo desde Trombose (horas)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Tipo de Acesso Vascular">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"FAV_Nativa","Enxerto_PTFE","Cateter_Tunelizado","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Status Atual do Acesso">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Trombosado_Agudo","Trombosado_Subagudo","Estenose_Critica","Funcionante","Infectado"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="US Doppler Documentando Trombose">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA003_Reprovado_SemDoppler">
        <description>Reprovado - US Doppler obrigatorio ausente</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"US Doppler do acesso vascular obrigatorio para documentar trombose e planejar intervencao. Solicitar exame antes de autorizar procedimento."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDOQI 2019 - Pre-procedural imaging required; RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA003_Reprovado_AcessoInfectado">
        <description>Reprovado - Acesso infectado (contraindicacao absoluta)</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>"Infectado"</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Infeccao ativa do acesso vascular e contraindicacao absoluta para trombectomia. Tratar infeccao primeiro ou considerar novo acesso."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDOQI 2019 - Active infection contraindicates thrombectomy"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA003_Reprovado_TromboseTardia">
        <description>Reprovado - Trombose tardia (maior que 72h) - baixa chance sucesso</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>&amp;gt; 72</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Trombose com mais de 72 horas - chance de sucesso significativamente reduzida. Considerar novo acesso vascular em vez de trombectomia."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDOQI 2019 - Thrombectomy success decreases significantly after 72h"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA003_Aprovado_TromboseAguda_FAV">
        <description>Aprovado - Trombose aguda FAV (menor que 48h)</description>
        <inputEntry id="IE_A1_1"><text>"T82.868","I82.8"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;lt; 48</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"FAV_Nativa"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Trombosado_Agudo"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Trombose aguda de FAV nativa com menos de 48h - trombectomia indicada urgente. FAV e acesso preferencial e deve ser preservada quando possivel."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDOQI 2019 - Urgent thrombectomy for acute AVF thrombosis; Fistula-first strategy"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA003_Aprovado_TromboseAguda_Enxerto">
        <description>Aprovado - Trombose aguda enxerto (menor que 48h)</description>
        <inputEntry id="IE_A2_1"><text>"T82.868","I82.8"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;lt; 48</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Enxerto_PTFE"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Trombosado_Agudo"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Trombose aguda de enxerto PTFE com menos de 48h - trombectomia indicada. Avaliar estenose subjacente durante procedimento."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDOQI 2019 - Graft thrombectomy with angioplasty of underlying stenosis"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA003_Aprovado_TromboseSubaguda">
        <description>Aprovado - Trombose subaguda (48-72h)</description>
        <inputEntry id="IE_A3_1"><text>"T82.868","I82.8"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>[48..72]</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"FAV_Nativa","Enxerto_PTFE"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"Trombosado_Agudo","Trombosado_Subagudo"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Trombose subaguda (48-72h) - trombectomia ainda indicada, porem com menor taxa de sucesso. Paciente deve ser informado sobre alternativas."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDOQI 2019 - Attempt salvage within 72h window"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA003_Pendente_AcessoFuncionante">
        <description>Pendente - Acesso funcionante, reavaliar indicacao</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Funcionante","Estenose_Critica"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Acesso ainda funcionante ou com estenose. Trombectomia nao indicada - considerar angioplastia profilatica para estenose critica."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDOQI 2019 - Preemptive angioplasty for significant stenosis"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008046">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer analise por cirurgiao vascular e nefrologista conforme KDOQI 2019."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDOQI 2019 / SBACV 2022"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA003_31008046">
      <dmndi:DMNShape id="DMNShape_Decision_DIA003_31008046" dmnElementRef="Decision_DIA003_31008046">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>