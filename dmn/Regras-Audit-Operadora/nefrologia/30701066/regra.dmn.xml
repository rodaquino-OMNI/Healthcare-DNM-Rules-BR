<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30701066" name="Ligadura de Fistula Arteriovenosa" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30701066" name="Regra Clinica - Ligadura FAV">
    <decisionTable id="DecisionTable_30701066" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"TransplanteEstavel_Maior3m","TransplanteRecente_Menor3m","ICCAltoDebito","SindromeRoubo","HipertensaoVenosa","InfeccaoCronica","EmDialise"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (KDOQI/SBN)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"EcocardiogramaDisfuncao","DopplerFluxoAlto","CreatininaEstavel","AvaliacaoClinica","SemAvaliacao"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida Superior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Procedimento Classe III. FAV e unico acesso potencial ou transplante instavel."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDOQI 2024 - Preservar FAV se houver possibilidade de retorno a dialise"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_TransplanteRecente">
        <description>Contraindicacao: Transplante recente menor que 3 meses</description>
        <inputEntry id="InputEntry_Contraind_TxRec_1"><text>"TransplanteRecente_Menor3m"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_TxRec_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_TxRec_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_TxRec_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_TxRec_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_TxRec_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_TxRec_observacao"><text>"Transplante recente menor que 3 meses. Aguardar estabilizacao da funcao renal antes de fechar FAV."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_TxRec_fundamentacao"><text>"AST Guidelines 2023 - Aguardar pelo menos 3-6 meses pos-transplante com funcao estavel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_EmDialise">
        <description>Contraindicacao: Paciente ainda em terapia dialitica</description>
        <inputEntry id="InputEntry_Contraind_Dial_1"><text>"EmDialise"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Dial_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Dial_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Dial_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Dial_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Dial_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Dial_observacao"><text>"Paciente em dialise nao pode ter FAV ligada. Acesso vascular essencial para sobrevivencia."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Dial_fundamentacao"><text>"KDOQI 2024 - FAV funcionante e essencial para paciente em hemodialise"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      

      <rule id="Rule_Aprovado_ClasseI_TransplanteEstavel">
        <description>Aprovado: Transplante estavel maior que 3 meses sem necessidade de dialise</description>
        <inputEntry id="InputEntry_Aprovado_Tx_1"><text>"TransplanteEstavel_Maior3m"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Tx_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Tx_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Tx_4"><text>"CreatininaEstavel"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Tx_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Tx_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Tx_observacao"><text>"Transplante renal estavel maior que 3 meses. Ligadura de FAV indicada se desejo do paciente e baixo risco de retorno a dialise."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Tx_fundamentacao"><text>"AST 2023, KDOQI 2024 - FAV pode ser fechada apos estabilidade do enxerto; discussao com paciente"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_ICCAltoDebito">
        <description>Aprovado: ICC de alto debito por FAV hiperfuncionante</description>
        <inputEntry id="InputEntry_Aprovado_ICC_1"><text>"ICCAltoDebito"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_ICC_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_ICC_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_ICC_4"><text>"EcocardiogramaDisfuncao","DopplerFluxoAlto"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_ICC_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_ICC_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_ICC_observacao"><text>"ICC de alto debito documentada com fluxo de FAV maior que 2L/min. Ligadura indicada se alternativa de acesso disponivel."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_ICC_fundamentacao"><text>"KDOQI 2024 - ICC de alto debito e indicacao Classe I para ligadura ou reducao de fluxo"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_SindromeRoubo">
        <description>Aprovado: Sindrome do roubo arterial severa com isquemia</description>
        <inputEntry id="InputEntry_Aprovado_Roubo_1"><text>"SindromeRoubo"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Roubo_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Roubo_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Roubo_4"><text>"DopplerFluxoAlto","AvaliacaoClinica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Roubo_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Roubo_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Roubo_observacao"><text>"Sindrome do roubo severa com risco de perda de membro. Ligadura urgente indicada. Planejar acesso alternativo."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Roubo_fundamentacao"><text>"KDOQI 2024, ESVS 2023 - Isquemia severa e indicacao absoluta para ligadura ou revisao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIa_InfeccaoCronica">
        <description>Aprovado: Infeccao cronica refrataria</description>
        <inputEntry id="InputEntry_Aprovado_Inf_1"><text>"InfeccaoCronica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Inf_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Inf_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Inf_4"><text>"AvaliacaoClinica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Inf_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Inf_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Inf_observacao"><text>"Infeccao cronica de FAV refrataria a tratamento. Ligadura indicada apos estabelecimento de acesso alternativo."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Inf_fundamentacao"><text>"KDOQI 2024, IDSA - FAV cronicamente infectada deve ser removida"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      

      <rule id="Rule_Pendente_SemAvaliacao">
        <description>Pendente: Sem exames comprobatorios</description>
        <inputEntry id="InputEntry_Pendente_Img_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_4"><text>"SemAvaliacao"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_observacao"><text>"Documentacao insuficiente. Necessario: creatinina recente, ecocardiograma se ICC, Doppler se suspeita de alto debito."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_fundamentacao"><text>"KDOQI 2024 - Documentar indicacao e confirmar estabilidade antes de ligadura"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_HistoricoIncompleto">
        <description>Pendente: Historico clinico incompleto</description>
        <inputEntry id="InputEntry_Pendente_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_observacao"><text>"Historico incompleto. Informar: status de transplante, funcao renal atual, sintomas de ICC ou isquemia."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_fundamentacao"><text>"ANS RN 465/2021 - Documentacao clinica completa obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      

      <rule id="Rule_Fallback_30701066">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise multidisciplinar: nefrologia, cirurgia vascular, transplante."</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao caso a caso - KDOQI 2024, AST 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>