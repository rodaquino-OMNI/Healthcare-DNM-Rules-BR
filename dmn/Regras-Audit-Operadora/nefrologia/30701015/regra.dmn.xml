<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30701015" name="Confeccao de Fistula Braquiocefalica para Hemodialise" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30701015" name="Regra Clinica - Confeccao Fistula Braquiocefalica">
    <decisionTable id="DecisionTable_30701015" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"DRC4_PreDialise","DRC5_PreDialise","DRC5_EmDialise","FalhaFAVPrevia","SemAcessoVascular"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (KDOQI/SBN)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"MapeamentoVascularCompleto","MapeamentoParcial","SemMapeamento","NaoAplicavel"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida Superior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III (nao indicado)</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Procedimento nao indicado conforme classificacao Classe III. Contraindicacoes absolutas: ICC NYHA IV descompensada, infeccao ativa, sindrome do roubo previa."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDOQI Vascular Access 2024, SBN 2023 - Classe III: Procedimento contraindicado ou potencialmente prejudicial"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_observacao"><text>"Expectativa de vida inferior a 1 ano. FAV requer 6-8 semanas para maturacao. Considerar cateter tunelizado como alternativa."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_fundamentacao"><text>"KDOQI 2024 - Fistula AVF indicada para pacientes com expectativa de vida suficiente para maturacao e uso prolongado"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_SemMapeamento">
        <description>Reprovacao: Ausencia de mapeamento vascular pre-operatorio</description>
        <inputEntry id="InputEntry_SemMapeamento_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_4"><text>"SemMapeamento"</text></inputEntry>
        <inputEntry id="InputEntry_SemMapeamento_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_observacao"><text>"Mapeamento vascular com Doppler e obrigatorio antes da confeccao de FAV. Necessario avaliar calibre venoso (maior 2.5mm) e fluxo arterial."</text></outputEntry>
        <outputEntry id="OutputEntry_SemMapeamento_fundamentacao"><text>"KDOQI 2024 - Mapeamento vascular pre-operatorio aumenta sucesso primario da FAV; ESVS 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      

      <rule id="Rule_Aprovado_ClasseI_PreDialise">
        <description>Aprovado: DRC 4-5 pre-dialise com indicacao Classe I</description>
        <inputEntry id="InputEntry_Aprovado_CI_1"><text>"DRC4_PreDialise","DRC5_PreDialise"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_4"><text>"MapeamentoVascularCompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_observacao"><text>"Indicacao Classe I para confeccao de FAV braquiocefalica. Planejamento ideal: 3-6 meses antes do inicio previsto da hemodialise."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_fundamentacao"><text>"KDOQI 2024 - FAV e acesso de primeira escolha; SBN 2023 - Confeccionar precocemente para maturacao adequada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_EmDialise">
        <description>Aprovado: Paciente em dialise com cateter necessitando FAV definitiva</description>
        <inputEntry id="InputEntry_Aprovado_HD_1"><text>"DRC5_EmDialise","SemAcessoVascular"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_HD_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_HD_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_HD_4"><text>"MapeamentoVascularCompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_HD_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_HD_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_HD_observacao"><text>"Paciente em hemodialise com cateter - FAV braquiocefalica indicada para conversao de acesso. Prioridade para reduzir infeccoes relacionadas a cateter."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_HD_fundamentacao"><text>"KDOQI 2024 - Conversao de cateter para FAV reduz morbimortalidade; CDC Guidelines Prevenção Infecção"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIa_FalhaPrevia">
        <description>Aprovado: Falha de FAV previa com indicacao para nova confeccao</description>
        <inputEntry id="InputEntry_Aprovado_Falha_1"><text>"FalhaFAVPrevia"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_4"><text>"MapeamentoVascularCompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_observacao"><text>"Falha de FAV previa com territorio vascular braquiocefalico preservado. Nova confeccao indicada apos mapeamento confirmar viabilidade."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_fundamentacao"><text>"KDOQI 2024 - Reutilizacao de territorios vasculares; ESVS 2023 - Estrategia escalonada de acessos"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIb">
        <description>Aprovado: Indicacao Classe IIb com mapeamento completo</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_4"><text>"MapeamentoVascularCompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_observacao"><text>"Indicacao Classe IIb - procedimento pode ser considerado. Avaliar cuidadosamente risco-beneficio individual."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_fundamentacao"><text>"KDOQI 2024, SBN 2023 - Classe IIb: Beneficio igual ou superior ao risco"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      

      <rule id="Rule_Pendente_MapeamentoParcial">
        <description>Pendente: Mapeamento vascular incompleto</description>
        <inputEntry id="InputEntry_Pendente_Map_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_4"><text>"MapeamentoParcial"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Map_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_observacao"><text>"Mapeamento vascular incompleto. Solicitar complementacao com Doppler arterial e venoso bilateral de membros superiores."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Map_fundamentacao"><text>"KDOQI 2024 - Mapeamento completo reduz falha primaria e permite planejamento otimo do acesso"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_HistoricoIncompleto">
        <description>Pendente: Historico clinico incompleto</description>
        <inputEntry id="InputEntry_Pendente_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_observacao"><text>"Documentacao clinica incompleta. Solicitar: estagiamento DRC, eGFR recente, avaliacao cardiologica se indicada, historico de acessos previos."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_fundamentacao"><text>"ANS RN 465/2021 Art. 5 - Documentacao clinica completa para autorizacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      

      <rule id="Rule_Fallback_30701015">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise clinica individualizada por nefrologista/cirurgiao vascular auditor."</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao caso a caso conforme KDOQI 2024, SBN 2023, ESVS 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>