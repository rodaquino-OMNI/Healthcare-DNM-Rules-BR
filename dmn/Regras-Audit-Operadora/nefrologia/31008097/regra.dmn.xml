<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA008_31008097" name="DIA-008: Terapia EPO/ESA - Hemoglobin Targets" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA008_31008097" name="DIA-008: Terapia ESA Metas Hemoglobina">
    <decisionTable id="DecisionTable_DIA008_31008097" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Estagio DRC (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"N18.3","N18.4","N18.5","Z99.2","D63.1","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="Hemoglobina Atual (g/dL)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Status Depositos de Ferro">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"Ferro_Adequado","Deficiencia_Ferro","Ferro_Baixo_Limite","Sobrecarga_Ferro"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Situacao Clinica Atual">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Estavel","HAS_Nao_Controlada","AVC_Recente","TEV_Recente","Neoplasia_Ativa","Infeccao_Ativa"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Hemograma + Ferritina/TSAT Documentados">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA008_Reprovado_SemExames">
        <description>Reprovado - Exames laboratoriais obrigatorios ausentes</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Hemograma com Hb e perfil de ferro (ferritina, TSAT) obrigatorios para inicio ou ajuste de ESA. Solicitar exames."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDIGO 2012 - Iron status assessment required before ESA initiation"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA008_Reprovado_Hb_Alta">
        <description>Reprovado - Hb ja elevada, nao iniciar/aumentar ESA</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;gt; 11.5</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Hemoglobina maior que 11.5 g/dL - nao iniciar ou reduzir ESA. Manter Hb na faixa 10-11.5 g/dL. Risco CV aumentado com Hb maior que 13."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDIGO 2012 - Do not intentionally target Hb maior que 11.5 g/dL with ESA"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA008_Reprovado_Ferro_Depletado">
        <description>Reprovado - Deficiencia de ferro nao corrigida</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>&amp;lt; 10</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>"Deficiencia_Ferro"</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Deficiencia de ferro presente - corrigir antes de iniciar ESA. Repor ferro ate ferritina maior ou igual a 100 ng/mL e TSAT maior ou igual a 20%."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDIGO 2012 - Iron repletion before or concurrent with ESA initiation"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA008_Reprovado_Contraindicacao">
        <description>Reprovado - Contraindicacao clinica para ESA</description>
        <inputEntry id="IE_R4_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R4_4"><text>"AVC_Recente","TEV_Recente","Neoplasia_Ativa"</text></inputEntry>
        <inputEntry id="IE_R4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R4_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R4_2"><text>"Contraindicacao clinica para ESA: AVC recente, TEV recente ou neoplasia ativa. Avaliar transfusao como alternativa temporaria."</text></outputEntry>
        <outputEntry id="OE_R4_3"><text>"KDIGO 2012 - ESA caution in active malignancy, recent stroke/VTE"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA008_Aprovado_Inicio_ESA">
        <description>Aprovado - Inicio ESA com Hb baixa e ferro adequado</description>
        <inputEntry id="IE_A1_1"><text>"N18.3","N18.4","N18.5","Z99.2","D63.1"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;lt; 10</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"Ferro_Adequado"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Estavel"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Hb menor que 10 g/dL com depositos de ferro adequados - inicio de ESA indicado. Alvo Hb 10-11.5 g/dL. Monitorar Hb mensalmente."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDIGO 2012 - Start ESA when Hb menor que 10 g/dL and iron replete"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA008_Aprovado_Manutencao_ESA">
        <description>Aprovado - Manutencao ESA com Hb na faixa alvo</description>
        <inputEntry id="IE_A2_1"><text>"N18.3","N18.4","N18.5","Z99.2","D63.1"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>[10..11.5]</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Ferro_Adequado","Ferro_Baixo_Limite"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Estavel"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Hb 10-11.5 g/dL na faixa alvo - manter dose atual de ESA. Continuar monitoramento mensal de Hb e trimestral de ferro."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDIGO 2012 - Target Hb 10-11.5 g/dL; maintain current ESA dose"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA008_Aprovado_Ajuste_Dose">
        <description>Aprovado - Ajuste de dose ESA por Hb subotima</description>
        <inputEntry id="IE_A3_1"><text>"N18.3","N18.4","N18.5","Z99.2","D63.1"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>[9..10[</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"Ferro_Adequado"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"Estavel"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Hb 9-10 g/dL abaixo do alvo - aumento de dose ESA indicado (maximo 25% por ajuste). Monitorar resposta em 4 semanas."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDIGO 2012 - Increase ESA dose by up to 25% for suboptimal Hb"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA008_Pendente_HAS">
        <description>Pendente - HAS nao controlada, controlar antes de ESA</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>&amp;lt; 10</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"Ferro_Adequado"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"HAS_Nao_Controlada"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Hipertensao nao controlada - controlar PA antes de iniciar ESA. ESA pode piorar HAS. Iniciar apos PA menor que 140/90."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDIGO 2012 - Control BP before initiating or increasing ESA"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_DIA008_Pendente_Ferro_Limite">
        <description>Pendente - Ferro no limite, considerar reposicao</description>
        <inputEntry id="IE_P2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_2"><text>&amp;lt; 10</text></inputEntry>
        <inputEntry id="IE_P2_3"><text>"Ferro_Baixo_Limite"</text></inputEntry>
        <inputEntry id="IE_P2_4"><text>"Estavel"</text></inputEntry>
        <inputEntry id="IE_P2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P2_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P2_2"><text>"Depositos de ferro no limite inferior - considerar reposicao concomitante ao inicio de ESA para otimizar resposta."</text></outputEntry>
        <outputEntry id="OE_P2_3"><text>"KDIGO 2012 - Iron supplementation may enhance ESA response"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008097">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer avaliacao por nefrologista conforme KDIGO 2012 Anemia Guidelines."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDIGO 2012 / SBN 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA008_31008097">
      <dmndi:DMNShape id="DMNShape_Decision_DIA008_31008097" dmnElementRef="Decision_DIA008_31008097">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>