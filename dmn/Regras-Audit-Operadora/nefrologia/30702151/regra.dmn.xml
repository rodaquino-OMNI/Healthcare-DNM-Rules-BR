        <inputEntry id="InputEntry_Aprovado_CIIa_Enx_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_Enx_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_Enx_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_Enx_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_Enx_resultado"><text>"Aprovado"</text></outputEntry>
