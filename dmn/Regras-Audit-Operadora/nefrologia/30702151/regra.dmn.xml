        <outputEntry id="OutputEntry_Reprovado_Exames_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_observacao"><text>"Exames obrigatorios nao apresentados"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_fundamentacao"><text>"SIR/CIRSE 2019 - Imagem vascular obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30702151">
