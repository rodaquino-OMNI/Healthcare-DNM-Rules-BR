        <outputEntry id="OutputEntry_Aprovado_Aneu_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Aneu_observacao"><text>"Aneurisma de arteria renal - embolizacao ou stent"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Aneu_fundamentacao"><text>"SIR/CIRSE 2019 - Aneurisma renal maior 2cm ou sintomatico: tratamento endovascular"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_MAV">
        <description>Aprovado: Malformacao arteriovenosa renal</description>
        <inputEntry id="InputEntry_Aprovado_MAV_1"><text>"MAVRenal"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_MAV_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_MAV_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_MAV_4"><text>"AngiografiaRenal","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_MAV_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_MAV_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_MAV_observacao"><text>"MAV renal sintomatica - embolizacao terapeutica"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_MAV_fundamentacao"><text>"SIR/CIRSE 2019 - MAV renal: embolizacao e tratamento de escolha"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Fistula">
        <description>Aprovado: Fistula arteriovenosa renal</description>
        <inputEntry id="InputEntry_Aprovado_Fist_1"><text>"FistulaAV"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Fist_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Fist_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Fist_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Fist_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Fist_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Fist_observacao"><text>"Fistula AV renal - embolizacao superseletiva"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Fist_fundamentacao"><text>"SIR/CIRSE 2019 - FAV pos-biopsia ou traumatica: embolizacao eficaz"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

