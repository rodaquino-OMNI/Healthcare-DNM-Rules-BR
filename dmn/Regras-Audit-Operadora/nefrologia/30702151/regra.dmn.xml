        <outputEntry id="OutputEntry_Aprovado_CIIa_Tum_fundamentacao"><text>"AUA/ASCO 2017 - Embolizacao pre-nefrectomia em tumores grandes ou hipervascularizados"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

