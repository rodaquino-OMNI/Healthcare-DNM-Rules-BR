        <inputEntry id="InputEntry_Aprovado_CIIa_Tum_1"><text>"TumorRenal"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_Tum_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_Tum_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_Tum_4"><text>"Completos"</text></inputEntry>
