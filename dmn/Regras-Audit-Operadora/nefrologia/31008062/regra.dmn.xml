<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA005_31008062" name="DIA-005: Selecao AV Graft vs AVF - KDOQI Fistula-First" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA005_31008062" name="DIA-005: Selecao FAV vs Enxerto">
    <decisionTable id="DecisionTable_DIA005_31008062" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Diagnostico Renal (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"N18.5","N18.4","N19","Z99.2","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="Diametro Veia Mapeamento (mm)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Tipo de Acesso Solicitado">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"FAV_Radiocefálica","FAV_Braquiocefalica","FAV_Braquiobasilica","Enxerto_PTFE","Cateter_Tunelizado"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Acessos Vasculares Previos">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Nenhum","FAV_Previa_Falha","Multiplas_FAV_Falhas","Enxerto_Previo_Falha","Veias_Exauridas"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Mapeamento Vascular Doppler">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA005_Reprovado_SemMapeamento">
        <description>Reprovado - Mapeamento vascular obrigatorio ausente</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Mapeamento vascular com US Doppler obrigatorio antes de confeccao de acesso. Necessario para selecao adequada do tipo e local."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDOQI 2019 - Pre-operative vascular mapping mandatory; RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA005_Reprovado_EnxertoSemJustificativa">
        <description>Reprovado - Enxerto solicitado sem tentativa previa de FAV</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;gt;= 2.5</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>"Enxerto_PTFE"</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>"Nenhum"</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Enxerto solicitado sem falha previa de FAV e com anatomia venosa adequada (maior ou igual a 2.5mm). FAV nativa deve ser tentada primeiro (Fistula-First)."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDOQI 2019 Fistula-First - AVF preferred over graft when anatomically feasible"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA005_Reprovado_VeiaInadequada_FAV">
        <description>Reprovado - FAV solicitada mas veia inadequada</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>&amp;lt; 2</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>"FAV_Radiocefálica","FAV_Braquiocefalica"</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Veia com diametro menor que 2mm - inadequada para FAV. Reavaliar outras opcoes de mapeamento ou considerar enxerto."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDOQI 2019 - Minimum vein diameter 2.5mm for AVF maturation"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA005_Aprovado_FAV_RC_Ideal">
        <description>Aprovado - FAV radiocefálica com anatomia ideal</description>
        <inputEntry id="IE_A1_1"><text>"N18.5","N18.4","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;gt;= 2.5</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"FAV_Radiocefálica"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Nenhum","FAV_Previa_Falha"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"FAV radiocefálica com veia cefalica maior ou igual a 2.5mm - opcao de primeira linha conforme Fistula-First. Confeccao aprovada."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDOQI 2019 Fistula-First - Radiocephalic AVF is preferred first option"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA005_Aprovado_FAV_BC">
        <description>Aprovado - FAV braquiocefalica com anatomia adequada</description>
        <inputEntry id="IE_A2_1"><text>"N18.5","N18.4","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;gt;= 2.5</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"FAV_Braquiocefalica"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"FAV braquiocefalica com veia adequada - segunda opcao na sequencia Fistula-First quando FAV RC nao viavel. Confeccao aprovada."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDOQI 2019 - Brachiocephalic AVF second option in sequence"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA005_Aprovado_FAV_BB">
        <description>Aprovado - FAV braquiobasilica (transposicao)</description>
        <inputEntry id="IE_A3_1"><text>"N18.5","N18.4","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"FAV_Braquiobasilica"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"FAV_Previa_Falha","Multiplas_FAV_Falhas"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"FAV braquiobasilica com transposicao - veia basilica maior ou igual a 3mm adequada. Terceira opcao na sequencia quando FAV superficiais esgotadas."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDOQI 2019 - Basilic vein transposition third option; minimum 3mm diameter"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA005_Aprovado_Enxerto_Justificado">
        <description>Aprovado - Enxerto apos falha de FAV ou veias inadequadas</description>
        <inputEntry id="IE_A4_1"><text>"N18.5","N18.4","Z99.2"</text></inputEntry>
        <inputEntry id="IE_A4_2"><text>&amp;lt; 2.5</text></inputEntry>
        <inputEntry id="IE_A4_3"><text>"Enxerto_PTFE"</text></inputEntry>
        <inputEntry id="IE_A4_4"><text>"Multiplas_FAV_Falhas","Veias_Exauridas"</text></inputEntry>
        <inputEntry id="IE_A4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A4_2"><text>"Enxerto PTFE indicado apos multiplas falhas de FAV ou anatomia venosa inadequada. Todas opcoes de FAV esgotadas conforme documentacao."</text></outputEntry>
        <outputEntry id="OE_A4_3"><text>"KDOQI 2019 - Graft acceptable when AVF options exhausted"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA005_Pendente_AnatomiaLimite">
        <description>Pendente - Anatomia limite, reavaliar opcoes</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>[2..2.5]</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"FAV_Radiocefálica","FAV_Braquiocefalica"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Veia com diametro limite (2-2.5mm). Risco de falha de maturacao maior. Discutir com paciente e considerar outras opcoes anatomicas."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDOQI 2019 - Marginal vein diameter associated with higher primary failure"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008062">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer avaliacao conjunta nefrologista e cirurgiao vascular conforme KDOQI 2019 Fistula-First."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDOQI 2019 / SBACV 2022"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA005_31008062">
      <dmndi:DMNShape id="DMNShape_Decision_DIA005_31008062" dmnElementRef="Decision_DIA005_31008062">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>