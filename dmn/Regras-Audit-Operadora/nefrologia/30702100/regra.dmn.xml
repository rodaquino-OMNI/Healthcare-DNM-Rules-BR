<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30702100" name="Litotripsia Extracorporea por Ondas de Choque - LECO" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30702100" name="Regra Clinica - LECO">
    <decisionTable id="DecisionTable_30702100" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"CalculoRenalMenor10mm","CalculoRenal10a20mm","CalculoUreteral","CalculoCoraliforme","Gestante","InfeccaoAtiva"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (EAU)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"TCComCalculo","UrocultNegativa","Completos","Parciais","Ausentes","DensidadeMaior1000HU"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"LECO contraindicada - aneurisma, gestacao ou coagulopatia"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"EAU 2022 - Contraindicacoes absolutas: gestacao, aneurisma aortico/renal, coagulopatia grave"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_Gestante">
        <description>Contraindicacao: Gestacao</description>
        <inputEntry id="InputEntry_Contraind_Gest_1"><text>"Gestante"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Gest_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Gest_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Gest_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Gest_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Gest_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Gest_observacao"><text>"Gestacao - LECO absolutamente contraindicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Gest_fundamentacao"><text>"EAU 2022, AUA 2016 - LECO contraindicada na gestacao pelo risco fetal"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_Infeccao">
        <description>Contraindicacao: Infeccao urinaria ativa</description>
        <inputEntry id="InputEntry_Contraind_Infec_1"><text>"InfeccaoAtiva"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Infec_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Infec_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Infec_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Infec_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Infec_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Infec_observacao"><text>"Infeccao urinaria ativa - tratar antes do procedimento"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Infec_fundamentacao"><text>"EAU 2022 - ITU deve ser tratada antes de LECO pelo risco de sepse"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_CalculoMenor10mm">
        <description>Aprovado: Calculo renal menor 10mm - primeira escolha</description>
        <inputEntry id="InputEntry_Aprovado_Menor10_1"><text>"CalculoRenalMenor10mm"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Menor10_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Menor10_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Menor10_4"><text>"TCComCalculo","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Menor10_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Menor10_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Menor10_observacao"><text>"Calculo renal menor 10mm - LECO primeira escolha"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Menor10_fundamentacao"><text>"EAU 2022 Recomendacao Forte - LECO primeira escolha para calculos renais menores 10mm com densidade menor 1000 HU"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Calculo10a20mm">
        <description>Aprovado: Calculo renal 10-20mm</description>
        <inputEntry id="InputEntry_Aprovado_10a20_1"><text>"CalculoRenal10a20mm"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_10a20_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_10a20_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_10a20_4"><text>"TCComCalculo","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_10a20_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_10a20_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_10a20_observacao"><text>"Calculo 10-20mm - LECO ou ureteroscopia"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_10a20_fundamentacao"><text>"EAU 2022 - Calculos 10-20mm: LECO ou ureteroscopia, decisao compartilhada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_CalculoUreteral">
        <description>Aprovado: Calculo ureteral proximal</description>
        <inputEntry id="InputEntry_Aprovado_Uret_1"><text>"CalculoUreteral"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Uret_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Uret_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Uret_4"><text>"TCComCalculo","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Uret_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Uret_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Uret_observacao"><text>"Calculo ureteral proximal - LECO alternativa valida"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Uret_fundamentacao"><text>"EAU 2022 - Calculos ureterais proximais: LECO ou ureteroscopia conforme tamanho e disponibilidade"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_Geral">
        <description>Aprovado: Indicacao Classe I geral</description>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_observacao"><text>"LECO indicada - Classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_fundamentacao"><text>"EAU 2022, SBU 2021 - Indicacao estabelecida para litotripsia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseIIa">
        <description>Aprovado: Indicacao Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"LECO razoavel - considerar caracteristicas do calculo"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"EAU 2022 - Avaliar densidade do calculo e anatomia renal"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseIIb">
        <description>Aprovado: Indicacao Classe IIb - Calculo coraliforme pequeno</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_1"><text>"CalculoCoraliforme"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_observacao"><text>"Calculo coraliforme - LECO como parte do tratamento multimodal"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_fundamentacao"><text>"EAU 2022 - Calculos coraliformes: preferir nefrolitotomia percutanea, LECO adjuvante"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_DensidadeAlta">
        <description>Pendente: Calculo com densidade maior 1000 HU</description>
        <inputEntry id="InputEntry_Pend_Dens_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Dens_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Dens_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Dens_4"><text>"DensidadeMaior1000HU"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Dens_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Dens_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Dens_observacao"><text>"Densidade maior 1000 HU - avaliar ureteroscopia como alternativa"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Dens_fundamentacao"><text>"EAU 2022 - Calculos de alta densidade: menor sucesso com LECO, considerar abordagem endoscopica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_ExamesParciais">
        <description>Pendente: Exames comprobatorios parciais</description>
        <inputEntry id="InputEntry_Pend_Exames_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_4"><text>"Parciais"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_observacao"><text>"Aguardando TC e urocultura"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_fundamentacao"><text>"EAU 2022 - TC sem contraste e urocultura negativa obrigatorios antes de LECO"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Reprovado_HistoricoInsuficiente">
        <description>Reprovado: Historico clinico insuficiente</description>
        <inputEntry id="InputEntry_Reprovado_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_observacao"><text>"Documentacao insuficiente para autorizacao"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_fundamentacao"><text>"ANS RN 465/2021 - Documentacao clinica adequada obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Reprovado_ExamesAusentes">
        <description>Reprovado: Exames comprobatorios ausentes</description>
        <inputEntry id="InputEntry_Reprovado_Exames_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_4"><text>"Ausentes"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_observacao"><text>"Exames obrigatorios nao apresentados"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_fundamentacao"><text>"EAU 2022 - TC para caracterizacao do calculo obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30702100">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise pela equipe de urologia"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao conforme EAU Guidelines 2022 e SBU 2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>