<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA012_31008135" name="DIA-012: Protocolo Imunossupressao - Induction Selection" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA012_31008135" name="DIA-012: Protocolo Imunossupressao Transplante">
    <decisionTable id="DecisionTable_DIA012_31008135" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Tipo de Transplante">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"Tx_Vivo_Primeiro","Tx_Vivo_Retx","Tx_Falecido_Primeiro","Tx_Falecido_Retx","Tx_ABOi","Tx_DSA_Positivo"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="PRA - Panel Reactive Antibody (%)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Inducao Solicitada">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"rATG_Timoglobulina","Basiliximab","Alemtuzumab","Sem_Inducao","Rituximab_Plus"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Risco Imunologico">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Baixo","Medio","Alto","Muito_Alto","Nao_Classificado"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="HLA + PRA + Crossmatch Documentados">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA012_Reprovado_SemDocumentacao">
        <description>Reprovado - Documentacao imunologica incompleta</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Avaliacao imunologica incompleta. Obrigatorio: tipagem HLA doador/receptor, PRA, crossmatch CDC e citometria antes de definir protocolo."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"KDIGO 2009 - Pre-transplant immunological assessment mandatory"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA012_Reprovado_rATG_BaixoRisco">
        <description>Reprovado - rATG em baixo risco (excessivo)</description>
        <inputEntry id="IE_R2_1"><text>"Tx_Vivo_Primeiro"</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;lt; 20</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>"rATG_Timoglobulina"</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>"Baixo"</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"rATG nao indicado em primeiro transplante de doador vivo com baixo risco imunologico (PRA menor que 20%). Basiliximab e preferencial neste cenario."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDIGO 2009 - IL-2RA (Basiliximab) preferred for low-risk; reserve depleting agents for high-risk"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA012_Reprovado_SemInducao_AltoRisco">
        <description>Reprovado - Alto risco sem inducao</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>&amp;gt; 50</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>"Sem_Inducao"</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>"Alto","Muito_Alto"</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Alto risco imunologico (PRA maior que 50%) sem terapia de inducao - risco elevado de rejeicao aguda. Inducao com anticorpo antilinfocitario obrigatoria."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDIGO 2009 - Induction therapy recommended for all; mandatory for high-risk"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA012_Aprovado_Basiliximab_BaixoRisco">
        <description>Aprovado - Basiliximab para baixo risco</description>
        <inputEntry id="IE_A1_1"><text>"Tx_Vivo_Primeiro","Tx_Falecido_Primeiro"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;lt; 20</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"Basiliximab"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Baixo","Medio"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Basiliximab indicado para primeiro transplante com baixo risco imunologico. Protocolo: 20mg IV D0 e D4. Manutencao com tacrolimus + MMF + prednisona."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDIGO 2009 - IL-2RA is first-line induction for low-immunologic risk"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA012_Aprovado_rATG_AltoRisco">
        <description>Aprovado - rATG para alto risco imunologico</description>
        <inputEntry id="IE_A2_1"><text>"Tx_Falecido_Retx","Tx_Vivo_Retx","Tx_DSA_Positivo"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;gt;= 50</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"rATG_Timoglobulina"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Alto","Muito_Alto"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"rATG (Timoglobulina) indicada para alto risco imunologico. Protocolo: 1.5 mg/kg/dia x 5-7 dias. Pre-medicacao com corticoide e anti-histaminico."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDIGO 2009 - Lymphocyte-depleting agents for high-immunologic risk; AST 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA012_Aprovado_rATG_Retransplante">
        <description>Aprovado - rATG para retransplante</description>
        <inputEntry id="IE_A3_1"><text>"Tx_Vivo_Retx","Tx_Falecido_Retx"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>[20..50]</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"rATG_Timoglobulina"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"Medio","Alto"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"rATG indicada para retransplante com risco imunologico moderado-alto (PRA 20-50%). Historia de rejeicao previa aumenta beneficio de inducao depletora."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"KDIGO 2009 - Consider depleting agents for retransplant; AST 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA012_Aprovado_ABOi_Rituximab">
        <description>Aprovado - Protocolo ABOi com Rituximab</description>
        <inputEntry id="IE_A4_1"><text>"Tx_ABOi"</text></inputEntry>
        <inputEntry id="IE_A4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_3"><text>"Rituximab_Plus"</text></inputEntry>
        <inputEntry id="IE_A4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A4_2"><text>"Transplante ABO-incompativel requer protocolo de dessensibilizacao: Rituximab 375 mg/m2 + Plasmaferese + IVIG. Monitorar titulos de isoaglutininas."</text></outputEntry>
        <outputEntry id="OE_A4_3"><text>"AST 2023 - ABOi desensitization protocol; ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA012_Aprovado_Basiliximab_MedioRisco">
        <description>Aprovado - Basiliximab para risco intermediario</description>
        <inputEntry id="IE_A5_1"><text>"Tx_Falecido_Primeiro"</text></inputEntry>
        <inputEntry id="IE_A5_2"><text>[20..50]</text></inputEntry>
        <inputEntry id="IE_A5_3"><text>"Basiliximab"</text></inputEntry>
        <inputEntry id="IE_A5_4"><text>"Medio"</text></inputEntry>
        <inputEntry id="IE_A5_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A5_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A5_2"><text>"Basiliximab aceitavel para primeiro transplante de falecido com risco intermediario. Alternativa: rATG se HLA-DR mismatch completo."</text></outputEntry>
        <outputEntry id="OE_A5_3"><text>"KDIGO 2009 - IL-2RA acceptable for intermediate risk; individualize"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA012_Pendente_RiscoNaoClassificado">
        <description>Pendente - Risco imunologico nao classificado</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Nao_Classificado"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Risco imunologico nao classificado. Completar avaliacao com PRA classe I e II, DSA e crossmatch para definir estratificacao e protocolo adequado."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDIGO 2009 - Risk stratification required before protocol selection"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_DIA012_Pendente_DSA_Positivo">
        <description>Pendente - DSA positivo, protocolo especializado</description>
        <inputEntry id="IE_P2_1"><text>"Tx_DSA_Positivo"</text></inputEntry>
        <inputEntry id="IE_P2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_3"><text>"Basiliximab"</text></inputEntry>
        <inputEntry id="IE_P2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_P2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P2_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P2_2"><text>"DSA positivo requer protocolo de dessensibilizacao (IVIG, plasmaferese). Basiliximab isolado insuficiente. Revisar com equipe de transplante."</text></outputEntry>
        <outputEntry id="OE_P2_3"><text>"AST 2023 - DSA-positive requires desensitization protocol"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008135">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado. Requer discussao com equipe multidisciplinar de transplante conforme KDIGO 2009 e AST 2023."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDIGO 2009 / AST 2023 / ABTO 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA012_31008135">
      <dmndi:DMNShape id="DMNShape_Decision_DIA012_31008135" dmnElementRef="Decision_DIA012_31008135">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>