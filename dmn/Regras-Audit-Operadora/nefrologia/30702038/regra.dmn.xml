<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30702038" name="Nefrectomia do Doador Vivo" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30702038" name="Regra Clinica - Nefrectomia do Doador Vivo">
    <decisionTable id="DecisionTable_30702038" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Doador" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"DoadorSaudavelElegivel","DoadorIdosoMaior60","DoadorComComorbidadeLeve","DoadorComContraindicacao"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (KDIGO)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"Completos","Parciais","Ausentes","TFGInsuficiente","ProteinuriaElevada"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Consentimento e Parecer Etico" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Doador com contraindicacao absoluta</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>"DoadorComContraindicacao"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Doador apresenta contraindicacao absoluta para nefrectomia"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDIGO Living Donor Guidelines 2024 - Contraindicacoes: DM, HAS nao controlada, HIV/HCV/HBV, neoplasia, DRC"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ClasseIII_Indicacao">
        <description>Contraindicacao: Indicacao Classe III</description>
        <inputEntry id="InputEntry_Contraind_CIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_CIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CIII_observacao"><text>"Classe III - Doacao nao indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CIII_fundamentacao"><text>"KDIGO 2024, ABTO 2024 - Classe III: Risco para o doador supera beneficio para receptor"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_SemConsentimento">
        <description>Contraindicacao: Sem consentimento ou parecer etico</description>
        <inputEntry id="InputEntry_Contraind_Consent_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Consent_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Consent_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Consent_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Consent_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Consent_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Consent_observacao"><text>"Ausencia de consentimento livre ou parecer etico favoravel"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Consent_fundamentacao"><text>"CFM 2173/2017, ABTO 2024 - Doacao requer consentimento livre, avaliacao psicologica e parecer etico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_TFGInsuficiente">
        <description>Contraindicacao: TFG insuficiente para doacao</description>
        <inputEntry id="InputEntry_Contraind_TFG_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_TFG_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_TFG_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_TFG_4"><text>"TFGInsuficiente"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_TFG_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_TFG_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_TFG_observacao"><text>"TFG menor que 80 mL/min/1.73m2 - funcao renal insuficiente para doacao"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_TFG_fundamentacao"><text>"KDIGO Living Donor Guidelines 2024 - TFG minima de 80 mL/min/1.73m2 para elegibilidade"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_Proteinuria">
        <description>Contraindicacao: Proteinuria elevada</description>
        <inputEntry id="InputEntry_Contraind_Prot_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Prot_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Prot_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Prot_4"><text>"ProteinuriaElevada"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Prot_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Prot_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Prot_observacao"><text>"Proteinuria maior 300mg/24h - indica doenca renal subjacente"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Prot_fundamentacao"><text>"KDIGO 2024 - Proteinuria acima de 300mg/24h contraindica doacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseI_Elegivel">
        <description>Aprovado: Doador saudavel elegivel com Classe I</description>
        <inputEntry id="InputEntry_Aprovado_CI_1"><text>"DoadorSaudavelElegivel"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_observacao"><text>"Doador elegivel - nefrectomia aprovada para transplante"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_fundamentacao"><text>"KDIGO Living Donor Guidelines 2024, ABTO 2024 - Doador saudavel com avaliacao completa e favoravel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseIIa_Idoso">
        <description>Aprovado: Doador idoso com Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>"DoadorIdosoMaior60"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"Doador idoso elegivel com avaliacao favoravel"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"KDIGO 2024 - Doadores acima de 60 anos podem ser aceitos com avaliacao individualizada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIa_Geral">
        <description>Aprovado: Indicacao Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_G_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_G_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_G_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_G_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_G_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_G_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_G_observacao"><text>"Indicacao Classe IIa - Doacao razoavel"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_G_fundamentacao"><text>"KDIGO 2024 - Classe IIa: Beneficio supera risco para doador e receptor"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_ComorbidadeLeve">
        <description>Pendente: Doador com comorbidade leve - avaliar</description>
        <inputEntry id="InputEntry_Pend_Comorb_1"><text>"DoadorComComorbidadeLeve"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Comorb_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Comorb_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Comorb_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Comorb_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Comorb_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Comorb_observacao"><text>"Doador com comorbidade leve - avaliar risco individual"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Comorb_fundamentacao"><text>"KDIGO 2024 - Comorbidades leves requerem analise individualizada do risco-beneficio"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Reprovado_HistoricoInsuficiente">
        <description>Reprovado: Historico clinico insuficiente</description>
        <inputEntry id="InputEntry_Reprovado_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_observacao"><text>"Avaliacao do doador incompleta"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_fundamentacao"><text>"KDIGO 2024, ABTO 2024 - Protocolo completo obrigatorio para seguranca do doador"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30702038">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise pela equipe de transplante"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao individualizada conforme KDIGO Living Donor Guidelines 2024"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>