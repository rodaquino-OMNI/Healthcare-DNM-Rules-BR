<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA002_31008038" name="DIA-002: Complicacoes Cateter DP - Manejo Peritonite" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA002_31008038" name="DIA-002: Manejo Complicacoes Cateter DP">
    <decisionTable id="DecisionTable_DIA002_31008038" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Tipo de Complicacao (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"T85.71","K65.0","K65.9","B37.7","T82.7","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="Leucocitos Dializado (celulas/uL)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Intervencao Solicitada">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"Remocao_Cateter","Troca_Cateter","Revisao_Cateter","Tratamento_Conservador"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Episodios Peritonite em 12 meses">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Zero","Um","Dois","Tres_ou_Mais","Refrataria","Fungica"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Documentacao Completa (Cultura + Laudo)">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA002_Reprovado_SemDocumentacao">
        <description>Reprovado - Documentacao obrigatoria ausente (cultura/laudo)</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Documentacao obrigatoria ausente. Necessario: cultura de liquido peritoneal com antibiograma, contagem celular e laudo do nefrologista."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"ISPD 2022 - Diagnostic Criteria; RN 465/2021 Art. 5"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA002_Reprovado_SemPeritonite">
        <description>Reprovado - Leucocitos normais, sem evidencia de peritonite</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;lt; 100</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>"Remocao_Cateter","Troca_Cateter"</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>"Zero","Um"</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Contagem de leucocitos menor que 100/uL nao caracteriza peritonite. Manter cateter e investigar outras causas de sintomas."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"ISPD 2022 - Peritonitis defined as WBC maior que 100/uL with maior que 50% PMN"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA002_Aprovado_PeritoniteFungica">
        <description>Aprovado - Peritonite fungica (remocao obrigatoria)</description>
        <inputEntry id="IE_A1_1"><text>"B37.7"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;gt;= 100</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"Remocao_Cateter"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>"Fungica"</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Peritonite fungica confirmada - remocao imediata do cateter obrigatoria. Iniciar antifungico sistemico. Transicao temporaria para hemodialise."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"ISPD 2022 - Fungal peritonitis requires immediate catheter removal"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA002_Aprovado_PeritoniteRecorrente">
        <description>Aprovado - Peritonite recorrente (3+ episodios em 12 meses)</description>
        <inputEntry id="IE_A2_1"><text>"K65.0","K65.9","T85.71"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;gt;= 100</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Remocao_Cateter","Troca_Cateter"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Tres_ou_Mais"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Peritonite recorrente (3+ episodios/12 meses) - indicada remocao ou troca de cateter apos resolucao do episodio atual (2-3 semanas)."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"ISPD 2022 - Recurrent peritonitis (3+ episodes/year) indicates catheter removal"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA002_Aprovado_PeritoniteRefrataria">
        <description>Aprovado - Peritonite refrataria (falha apos 5 dias ATB)</description>
        <inputEntry id="IE_A3_1"><text>"K65.0","K65.9","T85.71"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>&amp;gt;= 100</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"Remocao_Cateter"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>"Refrataria"</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Peritonite refrataria (sem melhora apos 5 dias de ATB adequado) - remocao de cateter indicada para controle do foco infeccioso."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"ISPD 2022 - Refractory peritonitis requires catheter removal after 5 days"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA002_Aprovado_InfeccaoTunel">
        <description>Aprovado - Infeccao de tunel refrataria</description>
        <inputEntry id="IE_A4_1"><text>"T85.71"</text></inputEntry>
        <inputEntry id="IE_A4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_3"><text>"Remocao_Cateter","Troca_Cateter"</text></inputEntry>
        <inputEntry id="IE_A4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A4_2"><text>"Infeccao de tunel refrataria a tratamento antibiotico - remocao ou troca de cateter indicada para erradicacao do foco."</text></outputEntry>
        <outputEntry id="OE_A4_3"><text>"ISPD 2017 - Refractory tunnel infection requires catheter removal"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA002_Pendente_PrimeiroEpisodio">
        <description>Pendente - Primeiro/segundo episodio, aguardar resposta ATB</description>
        <inputEntry id="IE_P1_1"><text>"K65.0","K65.9"</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>&amp;gt;= 100</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"Remocao_Cateter","Troca_Cateter"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Um","Dois"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Primeiro ou segundo episodio de peritonite - aguardar resposta ao tratamento antibiotico por 5 dias antes de indicar remocao de cateter."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"ISPD 2022 - Trial of antibiotics before catheter removal for non-refractory cases"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008038">
        <description>Fallback - Caso nao contemplado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado nas regras automaticas. Requer analise por nefrologista auditor conforme ISPD 2022 e SBN 2023."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - ISPD 2022 / SBN 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA002_31008038">
      <dmndi:DMNShape id="DMNShape_Decision_DIA002_31008038" dmnElementRef="Decision_DIA002_31008038">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>