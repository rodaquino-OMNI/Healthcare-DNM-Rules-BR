<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30702054" name="Biopsia Renal Cirurgica" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30702054" name="Regra Clinica - Biopsia Renal Cirurgica">
    <decisionTable id="DecisionTable_30702054" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"FalhaPercutanea","ContraindicacaoPercutanea","RimUnico","ObesidadeMorbida","AnomaliasAnatomicas","BiopsiaInconclusiva"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"Completos","Parciais","Ausentes","RiscoCirurgicoElevado"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Condicoes para Anestesia Geral" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Classe III</description>
        <inputEntry id="InputEntry_Contraind_CIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_CIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_CIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CIII_observacao"><text>"Biopsia cirurgica contraindicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_CIII_fundamentacao"><text>"KDIGO 2024 - Risco cirurgico supera beneficio diagnostico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_SemCondicoesAnestesia">
        <description>Contraindicacao: Sem condicoes para anestesia geral</description>
        <inputEntry id="InputEntry_Contraind_Anest_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Anest_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Anest_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Anest_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Anest_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Anest_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Anest_observacao"><text>"Paciente sem condicoes para anestesia geral"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Anest_fundamentacao"><text>"ASA Guidelines - Avaliacao pre-anestesica desfavoravel contraindica procedimento eletivo"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_RiscoCirurgico">
        <description>Contraindicacao: Risco cirurgico muito elevado</description>
        <inputEntry id="InputEntry_Contraind_Risco_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Risco_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Risco_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Risco_4"><text>"RiscoCirurgicoElevado"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Risco_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Risco_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Risco_observacao"><text>"Risco cirurgico elevado - considerar alternativas"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Risco_fundamentacao"><text>"AUA 2022 - Risco cirurgico deve ser ponderado contra beneficio diagnostico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_FalhaPercutanea">
        <description>Aprovado: Falha de biopsia percutanea</description>
        <inputEntry id="InputEntry_Aprovado_Falha_1"><text>"FalhaPercutanea","BiopsiaInconclusiva"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_observacao"><text>"Via cirurgica indicada apos falha de via percutanea"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_fundamentacao"><text>"KDIGO 2024, AUA 2022 - Biopsia cirurgica alternativa apos falha percutanea"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ContraindicacaoPercutanea">
        <description>Aprovado: Contraindicacao a via percutanea</description>
        <inputEntry id="InputEntry_Aprovado_Contra_1"><text>"ContraindicacaoPercutanea","RimUnico","AnomaliasAnatomicas"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Contra_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Contra_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Contra_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Contra_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Contra_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Contra_observacao"><text>"Via cirurgica indicada por contraindicacao a via percutanea"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Contra_fundamentacao"><text>"KDIGO 2024 - Rim unico ou anomalias anatomicas indicam via cirurgica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Obesidade">
        <description>Aprovado: Obesidade morbida com rim inacessivel</description>
        <inputEntry id="InputEntry_Aprovado_Obes_1"><text>"ObesidadeMorbida"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Obes_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Obes_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Obes_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Obes_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Obes_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Obes_observacao"><text>"Via laparoscopica indicada por inacessibilidade percutanea"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Obes_fundamentacao"><text>"AUA 2022 - Laparoscopia preferivel em obesidade morbida"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseIIa">
        <description>Aprovado: Indicacao Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"Indicacao Classe IIa - Biopsia cirurgica razoavel"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"KDIGO 2024 - Beneficio diagnostico justifica via cirurgica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIb">
        <description>Aprovado: Indicacao Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_observacao"><text>"Indicacao Classe IIb - pode ser considerada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_fundamentacao"><text>"KDIGO 2024 - Analise individualizada de risco-beneficio"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Reprovado_HistoricoInsuficiente">
        <description>Reprovado: Historico insuficiente</description>
        <inputEntry id="InputEntry_Reprovado_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_observacao"><text>"Documentacao incompleta - justificativa para via cirurgica necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_fundamentacao"><text>"ANS RN 465/2021 - Via cirurgica requer justificativa documentada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30702054">
        <description>Regra padrao</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Analise adicional necessaria"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao individualizada conforme KDIGO 2024"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>