<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_DIA001_31008020" name="DIA-001: Insercao Cateter Dialise Peritoneal - Criterios Timing" namespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_DIA001_31008020" name="DIA-001: Criterios Insercao Cateter DP">
    <decisionTable id="DecisionTable_DIA001_31008020" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoRenal" label="Diagnostico Renal (CID-10)">
        <inputExpression id="InputExpr_diagnosticoRenal" typeRef="string">
          <text>diagnosticoRenal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoRenal">
          <text>"N18.5","N18.4","N18.3","N17.9","N19","Z99.2","Outro"</text>
        </inputValues>
      </input>

      
      <input id="Input_taxaFiltracaoGlomerular" label="Taxa Filtracao Glomerular (eGFR mL/min)">
        <inputExpression id="InputExpr_taxaFiltracaoGlomerular" typeRef="number">
          <text>taxaFiltracaoGlomerular</text>
        </inputExpression>
      </input>

      
      <input id="Input_modalidadeDialise" label="Modalidade Dialise Escolhida">
        <inputExpression id="InputExpr_modalidadeDialise" typeRef="string">
          <text>modalidadeDialise</text>
        </inputExpression>
        <inputValues id="InputValues_modalidadeDialise">
          <text>"DP_CAPD","DP_APD","Hemodialise","Nao_Definida"</text>
        </inputValues>
      </input>

      
      <input id="Input_acessoVascular" label="Status Acesso Vascular">
        <inputExpression id="InputExpr_acessoVascular" typeRef="string">
          <text>acessoVascular</text>
        </inputExpression>
        <inputValues id="InputValues_acessoVascular">
          <text>"Impossivel","Exaurido","Disponivel","Nao_Avaliado"</text>
        </inputValues>
      </input>

      
      <input id="Input_documentacaoClinica" label="Documentacao Clinica Completa">
        <inputExpression id="InputExpr_documentacaoClinica" typeRef="boolean">
          <text>documentacaoClinica</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Normativa" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_DIA001_Reprovado_DocumentacaoAusente">
        <description>Reprovado - Documentacao clinica obrigatoria ausente</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Documentacao clinica obrigatoria ausente. Necessario: laudo nefrologista com eGFR, avaliacao cirurgica, consentimento informado para DP e avaliacao social."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"RN 465/2021 Art. 5 - Documentacao Minima Obrigatoria; ISPD 2017 Pre-operative Assessment"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA001_Reprovado_eGFR_Alto">
        <description>Reprovado - eGFR acima do limiar para inicio dialise (maior que 20)</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>&amp;gt; 20</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"eGFR maior que 20 mL/min/1.73m2 - nao atende criterios para insercao de cateter de dialise. Manter tratamento conservador e reavaliar quando eGFR menor que 15."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"KDIGO 2012 - Inicio dialise recomendado quando eGFR 5-10 ou menor que 15 com sintomas"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_DIA001_Reprovado_ModalidadeHD">
        <description>Reprovado - Modalidade escolhida e hemodialise (cateter DP nao indicado)</description>
        <inputEntry id="IE_R3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R3_3"><text>"Hemodialise"</text></inputEntry>
        <inputEntry id="IE_R3_4"><text>not("Impossivel","Exaurido")</text></inputEntry>
        <inputEntry id="IE_R3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_R3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R3_2"><text>"Modalidade escolhida e hemodialise com acesso vascular disponivel. Cateter de dialise peritoneal nao indicado para esta modalidade."</text></outputEntry>
        <outputEntry id="OE_R3_3"><text>"KDOQI 2019 - Selecao modalidade conforme preferencia paciente e condicoes clinicas"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA001_Aprovado_DRC5_DP_Eletivo">
        <description>Aprovado - DRC estagio 5 com opcao por DP eletiva</description>
        <inputEntry id="IE_A1_1"><text>"N18.5"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&amp;lt; 15</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"DP_CAPD","DP_APD"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"DRC estagio 5 (eGFR menor que 15) com opcao por dialise peritoneal. Inserir cateter 2-4 semanas antes do inicio planejado da DP para maturacao."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"KDIGO 2012 CKD Stage 5; ISPD 2017 - Inserir cateter 2+ semanas antes uso"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA001_Aprovado_AcessoVascular_Impossivel">
        <description>Aprovado - Acesso vascular impossivel/exaurido, DP como unica opcao</description>
        <inputEntry id="IE_A2_1"><text>"N18.5","N18.4","N19"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&amp;lt;= 15</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>"Impossivel","Exaurido"</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Acesso vascular impossivel ou exaurido - dialise peritoneal indicada como unica opcao de terapia renal substitutiva. Proceder com insercao de cateter de DP."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"KDOQI 2019 - DP como alternativa quando acesso vascular nao disponivel; SBN 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA001_Aprovado_eGFR_10_15_Planejamento">
        <description>Aprovado - eGFR 10-15 com planejamento antecipado</description>
        <inputEntry id="IE_A3_1"><text>"N18.5","N18.4"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>[10..15]</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"DP_CAPD","DP_APD"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"eGFR 10-15 mL/min com planejamento para dialise peritoneal. Insercao de cateter eletiva recomendada para permitir cicatrizacao adequada antes do inicio da terapia."</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"ISPD 2017 - Planejamento antecipado de acesso; KDIGO 2012 Timing of Dialysis Initiation"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_DIA001_Aprovado_Urgencia">
        <description>Aprovado - Indicacao urgente com eGFR critico</description>
        <inputEntry id="IE_A4_1"><text>"N18.5","N17.9","N19"</text></inputEntry>
        <inputEntry id="IE_A4_2"><text>&amp;lt; 10</text></inputEntry>
        <inputEntry id="IE_A4_3"><text>"DP_CAPD","DP_APD","Nao_Definida"</text></inputEntry>
        <inputEntry id="IE_A4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_A4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A4_2"><text>"eGFR critico menor que 10 mL/min - indicacao urgente de acesso dialitico. Considerar inicio DP precoce (1-2 semanas) com baixos volumes se necessario."</text></outputEntry>
        <outputEntry id="OE_A4_3"><text>"KDIGO 2012 - Inicio urgente dialise; ISPD 2017 Urgent-start PD Protocol"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_DIA001_Pendente_ModalidadeNaoDefinida">
        <description>Pendente - Modalidade de dialise nao definida</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>&amp;lt;= 15</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"Nao_Definida"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>"Disponivel","Nao_Avaliado"</text></inputEntry>
        <inputEntry id="IE_P1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Modalidade de dialise nao definida. Encaminhar para educacao pre-dialise e definicao de modalidade (HD vs DP) antes de proceder com acesso."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"KDOQI 2019 - Shared Decision Making; SBN 2023 Educacao Pre-Dialise"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_Fallback_31008020">
        <description>Fallback - Caso nao contemplado, requer analise por nefrologista auditor</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso nao contemplado nas regras automaticas. Requer analise individualizada por nefrologista auditor conforme KDIGO 2012 e diretrizes SBN."</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Analise caso a caso - KDIGO 2012 / SBN 2023 / ISPD 2017"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DIA001_31008020">
      <dmndi:DMNShape id="DMNShape_Decision_DIA001_31008020" dmnElementRef="Decision_DIA001_31008020">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>