<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30702127" name="Nefrolitotomia Percutanea" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30702127" name="Regra Clinica - Nefrolitotomia Percutanea">
    <decisionTable id="DecisionTable_30702127" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"CalculoMaior20mm","CalculoCoraliforme","PoloInferiorDesfavoravel","CalculoCistina","FalhaOutrosTratamentos","CalculoDiverticulo"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (EAU)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"TCAnatomiaCompleta","UrocultNegativa","Completos","Parciais","Ausentes"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"PCNL contraindicada - coagulopatia grave ou tumor no trajeto"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"EAU 2022 - Contraindicacoes: coagulopatia nao corrigida, tumor renal, aneurisma arterial"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_observacao"><text>"Expectativa de vida limitada - considerar tratamento conservador"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_fundamentacao"><text>"EAU 2022 - Procedimento invasivo requer expectativa de vida compativel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_CalculoMaior20">
        <description>Aprovado: Calculo maior 20mm - primeira escolha PCNL</description>
        <inputEntry id="InputEntry_Aprovado_Maior20_1"><text>"CalculoMaior20mm"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Maior20_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Maior20_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Maior20_4"><text>"TCAnatomiaCompleta","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Maior20_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Maior20_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Maior20_observacao"><text>"Calculo maior 20mm - PCNL primeira escolha"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Maior20_fundamentacao"><text>"EAU 2022 Recomendacao Forte - PCNL primeira escolha para calculos renais maiores 20mm"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Coraliforme">
        <description>Aprovado: Calculo coraliforme</description>
        <inputEntry id="InputEntry_Aprovado_Coral_1"><text>"CalculoCoraliforme"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Coral_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Coral_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Coral_4"><text>"TCAnatomiaCompleta","Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Coral_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Coral_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Coral_observacao"><text>"Calculo coraliforme - PCNL com possivel segundo tempo"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Coral_fundamentacao"><text>"EAU 2022 - Calculos coraliformes: PCNL tratamento de escolha, pode necessitar multiplos acessos"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_PoloInferior">
        <description>Aprovado: Polo inferior com anatomia desfavoravel</description>
        <inputEntry id="InputEntry_Aprovado_PoloInf_1"><text>"PoloInferiorDesfavoravel"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_PoloInf_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_PoloInf_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_PoloInf_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_PoloInf_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_PoloInf_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_PoloInf_observacao"><text>"Polo inferior com angulo desfavoravel - PCNL preferivel"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_PoloInf_fundamentacao"><text>"EAU 2022 - Calculos em polo inferior com angulo maior 70 graus ou infundibulo estreito: considerar PCNL"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Cistina">
        <description>Aprovado: Calculo de cistina</description>
        <inputEntry id="InputEntry_Aprovado_Cist_1"><text>"CalculoCistina"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Cist_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Cist_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Cist_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Cist_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Cist_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Cist_observacao"><text>"Calculo de cistina - LECO ineficaz, preferir PCNL ou URS"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Cist_fundamentacao"><text>"EAU 2022 - Calculos de cistina resistentes a LECO: abordagem endourologica preferencial"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_FalhaTratamento">
        <description>Aprovado: Falha de outros tratamentos</description>
        <inputEntry id="InputEntry_Aprovado_Falha_1"><text>"FalhaOutrosTratamentos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Falha_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_observacao"><text>"Falha de LECO e/ou URS - PCNL indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Falha_fundamentacao"><text>"EAU 2022 - Apos falha de tratamentos menos invasivos, PCNL e alternativa adequada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_Geral">
        <description>Aprovado: Indicacao Classe I geral</description>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Geral_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_observacao"><text>"PCNL indicada - Classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Geral_fundamentacao"><text>"EAU 2022, SBU 2021 - Indicacao estabelecida"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseIIa">
        <description>Aprovado: Indicacao Classe IIa - Diverticulo calicinal</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>"CalculoDiverticulo"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"Calculo em diverticulo - PCNL permite ablacao do colo"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"EAU 2022 - Calculos em diverticulo calicinal: PCNL permite tratamento do calculo e ablacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_ExamesParciais">
        <description>Pendente: Exames comprobatorios parciais</description>
        <inputEntry id="InputEntry_Pend_Exames_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_4"><text>"Parciais"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Exames_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_observacao"><text>"Aguardando TC com anatomia e urocultura"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Exames_fundamentacao"><text>"EAU 2022 - Planejamento de acesso requer TC detalhada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Reprovado_HistoricoInsuficiente">
        <description>Reprovado: Historico clinico insuficiente</description>
        <inputEntry id="InputEntry_Reprovado_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_observacao"><text>"Documentacao insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_fundamentacao"><text>"ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Reprovado_ExamesAusentes">
        <description>Reprovado: Exames comprobatorios ausentes</description>
        <inputEntry id="InputEntry_Reprovado_Exames_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_4"><text>"Ausentes"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Exames_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_observacao"><text>"Exames obrigatorios nao apresentados"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Exames_fundamentacao"><text>"EAU 2022 - TC anatomica obrigatoria para PCNL"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30702127">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise pela urologia"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"EAU Guidelines 2022, CROES Study"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>