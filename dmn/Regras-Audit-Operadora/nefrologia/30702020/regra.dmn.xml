<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30702020" name="Transplante Renal de Doador Falecido" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30702020" name="Regra Clinica - Transplante Renal de Doador Falecido">
    <decisionTable id="DecisionTable_30702020" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"DRCDialiticaInscritoSNT","DRCDialiticaNaoInscrito","RetransplanteInscritoSNT","UrgenciaMedica"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (KDIGO/ABTO)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"Completos","Parciais","Ausentes","PRAElevado","CrossmatchPositivo"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida maior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Contraindicacao absoluta ao transplante renal"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDIGO Transplant Guidelines 2024, ABTO 2024 - Classe III: neoplasia ativa, infeccao nao controlada, cardiopatia grave"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_observacao"><text>"Expectativa de vida insuficiente para beneficio do transplante"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_fundamentacao"><text>"KDIGO Transplant Guidelines 2024 - Expectativa de vida deve ser compativel com beneficio do procedimento"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_NaoInscrito">
        <description>Contraindicacao: Paciente nao inscrito no SNT</description>
        <inputEntry id="InputEntry_Contraind_SNT_1"><text>"DRCDialiticaNaoInscrito"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_SNT_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_SNT_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_SNT_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_SNT_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_SNT_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_SNT_observacao"><text>"Paciente nao inscrito no Sistema Nacional de Transplantes"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_SNT_fundamentacao"><text>"Portaria MS 2600/2009, ABTO 2024 - Transplante de doador falecido requer inscricao ativa no SNT"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_CrossmatchPositivo">
        <description>Contraindicacao: Crossmatch positivo</description>
        <inputEntry id="InputEntry_Contraind_Cross_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cross_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cross_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cross_4"><text>"CrossmatchPositivo"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cross_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Cross_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Cross_observacao"><text>"Crossmatch positivo - incompatibilidade imunologica"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Cross_fundamentacao"><text>"AST/ASTS Guidelines 2023 - Crossmatch positivo contraindica transplante pelo risco de rejeicao hiperaguda"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseI_SNT">
        <description>Aprovado: Paciente inscrito no SNT com indicacao Classe I</description>
        <inputEntry id="InputEntry_Aprovado_CI_1"><text>"DRCDialiticaInscritoSNT"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_observacao"><text>"Transplante renal de doador falecido indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_fundamentacao"><text>"KDIGO Transplant Guidelines 2024, ABTO 2024 - Transplante renal e tratamento de escolha para DRC terminal"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_Retransplante">
        <description>Aprovado: Retransplante com indicacao Classe I</description>
        <inputEntry id="InputEntry_Aprovado_Retx_1"><text>"RetransplanteInscritoSNT"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Retx_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Retx_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Retx_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Retx_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Retx_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Retx_observacao"><text>"Retransplante renal indicado apos falencia do enxerto"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Retx_fundamentacao"><text>"KDIGO 2024 - Retransplante oferece melhor sobrevida que manutencao em dialise"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_Urgencia">
        <description>Aprovado: Urgencia medica com indicacao Classe I</description>
        <inputEntry id="InputEntry_Aprovado_Urg_1"><text>"UrgenciaMedica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Urg_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Urg_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Urg_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Urg_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Urg_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Urg_observacao"><text>"Transplante de urgencia indicado - esgotamento de acessos vasculares"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Urg_fundamentacao"><text>"ABTO 2024, Portaria MS 2600/2009 - Urgencia medica por esgotamento de acessos para dialise"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseIIa">
        <description>Aprovado: Indicacao Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"Indicacao Classe IIa - Transplante razoavel"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"KDIGO 2024 - Classe IIa: Beneficio supera risco na maioria dos cenarios"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIb">
        <description>Aprovado: Indicacao Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_observacao"><text>"Indicacao Classe IIb - Transplante pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_fundamentacao"><text>"KDIGO 2024 - Classe IIb: Analise individualizada de risco-beneficio"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_PRAElevado">
        <description>Pendente: PRA elevado - avaliar estrategia de dessensibilizacao</description>
        <inputEntry id="InputEntry_Pend_PRA_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_PRA_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_PRA_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_PRA_4"><text>"PRAElevado"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_PRA_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_PRA_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_PRA_observacao"><text>"PRA elevado - avaliar viabilidade com equipe de imunologia de transplante"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_PRA_fundamentacao"><text>"KDIGO 2024 - Pacientes hipersensibilizados requerem avaliacao especializada e possivelmente dessensibilizacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Reprovado_HistoricoInsuficiente">
        <description>Reprovado: Historico clinico insuficiente</description>
        <inputEntry id="InputEntry_Reprovado_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_observacao"><text>"Documentacao clinica insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_fundamentacao"><text>"ANS RN 465/2021, ABTO 2024 - Documentacao completa obrigatoria para autorizacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30702020">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise pela equipe de transplante"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao individualizada conforme KDIGO 2024 e protocolos ABTO"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>