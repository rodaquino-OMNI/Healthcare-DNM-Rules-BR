<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30702046" name="Biopsia Renal Percutanea" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30702046" name="Regra Clinica - Biopsia Renal Percutanea">
    <decisionTable id="DecisionTable_30702046" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"SindromeNefrotica","GNRapidamenteProgressiva","HematuriaGlomerular","ProteinuriaMaior1g","DisfuncaoRenalIndeterminada","NefriteLupica","Vasculite","RejeicaoEnxerto"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"Completos","Parciais","Ausentes","CoagulopatiaAtiva","HASGraveNaoControlada"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Condicoes Favoraveis para Procedimento" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Biopsia renal contraindicada nesta situacao"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDIGO Glomerulonephritis 2024 - Classe III: Risco supera beneficio diagnostico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_Coagulopatia">
        <description>Contraindicacao: Coagulopatia ativa nao corrigida</description>
        <inputEntry id="InputEntry_Contraind_Coag_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Coag_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Coag_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Coag_4"><text>"CoagulopatiaAtiva"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Coag_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Coag_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Coag_observacao"><text>"Coagulopatia ativa - alto risco de sangramento"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Coag_fundamentacao"><text>"SBN Manual de Nefropatias 2023 - Coagulopatia deve ser corrigida antes da biopsia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_HASGrave">
        <description>Contraindicacao: HAS grave nao controlada</description>
        <inputEntry id="InputEntry_Contraind_HAS_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_HAS_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_HAS_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_HAS_4"><text>"HASGraveNaoControlada"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_HAS_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_HAS_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_HAS_observacao"><text>"HAS grave nao controlada - risco de sangramento aumentado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_HAS_fundamentacao"><text>"ERA-EDTA 2022 - PA deve estar menor 160/100 mmHg antes da biopsia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_CondicoesDesfavoraveis">
        <description>Contraindicacao: Condicoes desfavoraveis para procedimento</description>
        <inputEntry id="InputEntry_Contraind_Cond_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cond_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cond_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cond_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cond_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Cond_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Cond_observacao"><text>"Condicoes desfavoraveis para procedimento invasivo"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Cond_fundamentacao"><text>"KDIGO 2024 - Biopsia renal requer condicoes adequadas: coagulacao, PA controlada, rim acessivel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_GNRP">
        <description>Aprovado: GN rapidamente progressiva - urgencia diagnostica</description>
        <inputEntry id="InputEntry_Aprovado_GNRP_1"><text>"GNRapidamenteProgressiva"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_GNRP_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_GNRP_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_GNRP_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_GNRP_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_GNRP_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_GNRP_observacao"><text>"Urgencia diagnostica - biopsia essencial para definir tratamento"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_GNRP_fundamentacao"><text>"KDIGO Glomerulonephritis 2024 - GNRP requer diagnostico histologico urgente para tratamento adequado"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_SindromeNefrotica">
        <description>Aprovado: Sindrome nefrotica para definicao etiologica</description>
        <inputEntry id="InputEntry_Aprovado_SN_1"><text>"SindromeNefrotica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_SN_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_SN_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_SN_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_SN_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_SN_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_SN_observacao"><text>"Biopsia indicada para definicao etiologica e tratamento"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_SN_fundamentacao"><text>"KDIGO 2024 - Sindrome nefrotica requer diagnostico histologico para tratamento especifico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_NefriteLupica">
        <description>Aprovado: Nefrite lupica para classificacao ISN/RPS</description>
        <inputEntry id="InputEntry_Aprovado_Lupus_1"><text>"NefriteLupica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Lupus_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Lupus_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Lupus_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Lupus_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Lupus_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Lupus_observacao"><text>"Biopsia para classificacao ISN/RPS e definicao terapeutica"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Lupus_fundamentacao"><text>"KDIGO Lupus Nephritis 2024 - Classificacao histologica essencial para tratamento"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_RejeicaoEnxerto">
        <description>Aprovado: Suspeita de rejeicao de enxerto renal</description>
        <inputEntry id="InputEntry_Aprovado_Rej_1"><text>"RejeicaoEnxerto"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Rej_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Rej_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Rej_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Rej_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Rej_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Rej_observacao"><text>"Biopsia de enxerto para diagnostico de rejeicao"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Rej_fundamentacao"><text>"Banff Classification 2022, KDIGO Transplant 2024 - Biopsia padrao-ouro para diagnostico de rejeicao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_Geral">
        <description>Aprovado: Indicacao Classe I geral</description>
        <inputEntry id="InputEntry_Aprovado_CI_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_observacao"><text>"Indicacao Classe I - Biopsia diagnostica indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_fundamentacao"><text>"KDIGO Glomerulonephritis 2024 - Classe I: Biopsia essencial para diagnostico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_ClasseIIa">
        <description>Aprovado: Indicacao Classe IIa</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"Indicacao Classe IIa - Biopsia provavelmente util"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"KDIGO 2024 - Classe IIa: Beneficio diagnostico supera risco"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIb">
        <description>Aprovado: Indicacao Classe IIb</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_4"><text>"Completos"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_observacao"><text>"Indicacao Classe IIb - Biopsia pode ser considerada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_fundamentacao"><text>"KDIGO 2024 - Classe IIb: Decisao individualizada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Reprovado_HistoricoInsuficiente">
        <description>Reprovado: Historico clinico insuficiente</description>
        <inputEntry id="InputEntry_Reprovado_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Reprovado_Hist_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_observacao"><text>"Documentacao clinica insuficiente"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Hist_fundamentacao"><text>"ANS RN 465/2021 - Indicacao clinica deve estar claramente documentada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30702046">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise do nefrologista"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao individualizada conforme KDIGO Guidelines 2024"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>