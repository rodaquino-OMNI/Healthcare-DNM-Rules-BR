<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30701040" name="Trombectomia de Fistula Arteriovenosa" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30701040" name="Regra Clinica - Trombectomia FAV">
    <decisionTable id="DecisionTable_30701040" hitPolicy="FIRST">

      

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"TromboseAguda_Menos24h","TromboseAguda_24_72h","TromboseSubaguda_72h_7d","TromboseCronica_Maior7d","InfeccaoAtiva"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (KDOQI/SBN)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"DopplerConfirmaTrombose","FistulografiaRealizada","AvaliacaoClinica","SemAvaliacao"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida Superior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_Contraind_ClasseIII_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ClasseIII_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_observacao"><text>"Procedimento Classe III. Trombectomia contraindicada: infeccao, FAV imatura ou estenose nao tratavel."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ClasseIII_fundamentacao"><text>"KDOQI 2024 - Trombectomia nao indicada quando causa subjacente nao corrigivel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_Contraind_ExpVida_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_ExpVida_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_observacao"><text>"Expectativa de vida inferior a 1 ano. Considerar cateter tunelizado como alternativa a procedimento intervencionista."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_ExpVida_fundamentacao"><text>"KDOQI 2024 - Considerar expectativa de vida na decisao terapeutica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_TromboseCronica">
        <description>Contraindicacao: Trombose cronica maior que 7 dias</description>
        <inputEntry id="InputEntry_Contraind_Cronica_1"><text>"TromboseCronica_Maior7d"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cronica_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cronica_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cronica_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Cronica_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Cronica_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Cronica_observacao"><text>"Trombose cronica maior que 7 dias - FAV provavelmente nao recuperavel. Indicar confeccao de novo acesso."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Cronica_fundamentacao"><text>"KDOQI 2024, SVS 2023 - Trombose com mais de 7 dias tem baixa taxa de resgate"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_InfeccaoAtiva">
        <description>Contraindicacao: Infeccao ativa</description>
        <inputEntry id="InputEntry_Contraind_Inf_1"><text>"InfeccaoAtiva"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Inf_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_observacao"><text>"Infeccao ativa - trombectomia contraindicada pelo risco de disseminacao. Tratar infeccao e confeccionar novo acesso."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Inf_fundamentacao"><text>"KDOQI 2024 - Manipulacao de FAV infectada aumenta risco de sepse"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      

      <rule id="Rule_Aprovado_ClasseI_Aguda24h">
        <description>Aprovado: Trombose aguda menos de 24h - melhor prognostico</description>
        <inputEntry id="InputEntry_Aprovado_24h_1"><text>"TromboseAguda_Menos24h"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_24h_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_24h_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_24h_4"><text>"DopplerConfirmaTrombose","FistulografiaRealizada","AvaliacaoClinica"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_24h_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_24h_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_24h_observacao"><text>"Trombose aguda menos de 24h - janela ideal para trombectomia. Taxa de sucesso 80-90%. Urgencia terapeutica."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_24h_fundamentacao"><text>"KDOQI 2024 - Intervencao precoce maximiza taxa de resgate da FAV"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseI_Aguda72h">
        <description>Aprovado: Trombose aguda 24-72h - janela aceitavel</description>
        <inputEntry id="InputEntry_Aprovado_72h_1"><text>"TromboseAguda_24_72h"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_72h_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_72h_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_72h_4"><text>"DopplerConfirmaTrombose","FistulografiaRealizada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_72h_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_72h_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_72h_observacao"><text>"Trombose 24-72h - janela aceitavel. Taxa de sucesso 60-70%. Associar angioplastia se estenose identificada."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_72h_fundamentacao"><text>"KDOQI 2024 - Trombectomia com tratamento de lesao subjacente melhora patencia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_ClasseIIb_Subaguda">
        <description>Aprovado: Trombose subaguda 72h-7d - caso selecionado</description>
        <inputEntry id="InputEntry_Aprovado_Sub_1"><text>"TromboseSubaguda_72h_7d"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Sub_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Sub_3"><text>"Classe IIa","Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Sub_4"><text>"FistulografiaRealizada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_Sub_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_Sub_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Sub_observacao"><text>"Trombose subaguda 72h-7d. Taxa de sucesso reduzida (40-50%). Tentar resgate se FAV madura e sem alternativas."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Sub_fundamentacao"><text>"KDOQI 2024 - Tentativa de resgate pode ser justificada em FAV madura com bom historico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      

      <rule id="Rule_Pendente_SemAvaliacao">
        <description>Pendente: Sem avaliacao de imagem</description>
        <inputEntry id="InputEntry_Pendente_Img_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_4"><text>"SemAvaliacao"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_observacao"><text>"Avaliacao de imagem ausente. Doppler ou fistulografia necessarios para confirmar trombose e avaliar estenose subjacente."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_fundamentacao"><text>"KDOQI 2024 - Imagem pre-procedimento essencial para planejamento"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_HistoricoIncompleto">
        <description>Pendente: Historico clinico incompleto</description>
        <inputEntry id="InputEntry_Pendente_Hist_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Hist_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_observacao"><text>"Historico incompleto. Informar: tempo de trombose, historico da FAV, intervencoes previas, causa suspeita."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Hist_fundamentacao"><text>"ANS RN 465/2021 - Documentacao clinica completa obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      

      <rule id="Rule_Fallback_30701040">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise individualizada por cirurgiao vascular e nefrologista."</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"Avaliacao caso a caso - KDOQI 2024, SVS 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>