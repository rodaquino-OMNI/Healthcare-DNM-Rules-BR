<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/DMN/20151101/dmn.xsd"
  id="Definitions_20104073"
  name="Crioterapia grupo de até 5 lesões"
  namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS: 20104073
    Procedimento: Crioterapia grupo de até 5 lesões
    Versão: 3.0.0 (2026-02-07)

    Indicações Clínicas (DUT/ANS):
    - Verrugas virais (HPV)
    - Ceratoses actínicas e seborreicas
    - Molusco contagioso
    - Lesões benignas limitadas (≤5 lesões por sessão)

    Contraindicações:
    - >5 lesões por sessão
    - Lesões malignas sem biópsia prévia
    - Infecções ativas no local
    - Crioglobulinemia

    Referências:
    - Sociedade Brasileira de Dermatologia (SBD) - Consenso Crioterapia 2025
    - ANS Resolução Normativa 465/2021
    - Rol de Procedimentos ANS 2024
  -->

  <decision id="Decision_20104073" name="Decisão de Autorização - Crioterapia">
    <decisionTable id="DecisionTable_20104073" hitPolicy="FIRST">

      <!-- INPUTS -->
      <input id="Input_1" label="Diagnóstico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tipo de Lesão">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>tipoLesao</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Número de Lesões">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>numeroLesoes</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Biópsia Prévia Realizada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>biopsiaRealizada</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Localização das Lesões">
        <inputExpression id="InputExpression_5" typeRef="string">
          <text>localizacao</text>
        </inputExpression>
      </input>

      <!-- OUTPUTS -->
      <output id="Output_1" label="Resultado" typeRef="string" name="resultado">
        <outputValues>
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_2" label="Observação" typeRef="string" name="observacao" />

      <output id="Output_3" label="Fundamentação Legal" typeRef="string" name="fundamentacao" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICATIONS -->
      <rule id="Rule_Contraind_20104073_1">
        <description>Reprovação: Mais de 5 lesões excede o limite do código</description>
        <inputEntry id="IE_C1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_C1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_C1_3"><text>&gt; 5</text></inputEntry>
        <inputEntry id="IE_C1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_C1_5"><text>-</text></inputEntry>
        <outputEntry id="OE_C1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_C1_2"><text>"Número de lesões excede o limite de 5 por sessão - utilize código apropriado ou multiple autorizações"</text></outputEntry>
        <outputEntry id="OE_C1_3"><text>"TUSS: código 20104073 limitado a grupo de até 5 lesões"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_20104073_1_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_20104073_1_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_20104073_2">
        <description>Reprovação: Lesões suspeitas de malignidade sem biópsia</description>
        <inputEntry id="IE_C2_1"><text>matches(diagnosticoPrincipal, "C43|C44|D03|D04")</text></inputEntry>
        <inputEntry id="IE_C2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_C2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_C2_4"><text>false</text></inputEntry>
        <inputEntry id="IE_C2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_C2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_C2_2"><text>"Lesões suspeitas de malignidade requerem biópsia prévia antes de crioterapia"</text></outputEntry>
        <outputEntry id="OE_C2_3"><text>"SBD Consenso: tratamento destrutivo de lesões malignas sem confirmação histológica é contraindicado"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_20104073_2_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_20104073_2_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APPROVALS -->
      <rule id="Rule_Approve_20104073_1">
        <description>Aprovação: Verrugas virais (HPV)</description>
        <inputEntry id="IE_A1_1"><text>matches(diagnosticoPrincipal, "B07")</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>"Verruga vulgar","Verruga plantar","Verruga plana"</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>&lt;= 5</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A1_5"><text>-</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Verrugas virais - crioterapia autorizada como tratamento de primeira linha"</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"SBD Consenso: crioterapia com nitrogênio líquido indicada para verrugas HPV-induzidas"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_1_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Approve_20104073_2">
        <description>Aprovação: Ceratoses actínicas</description>
        <inputEntry id="IE_A2_1"><text>matches(diagnosticoPrincipal, "L57")</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>"Ceratose actínica","Queratose solar"</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>&lt;= 5</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Ceratoses actínicas - crioterapia autorizada para prevenção de carcinoma espinocelular"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"ANS Rol 2024: tratamento de lesões pré-malignas. SBD: crioterapia tratamento eficaz"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_2_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_2_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Approve_20104073_3">
        <description>Aprovação: Molusco contagioso</description>
        <inputEntry id="IE_A3_1"><text>matches(diagnosticoPrincipal, "B08.1")</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>"Molusco contagioso"</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>&lt;= 5</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A3_5"><text>-</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Molusco contagioso - crioterapia autorizada como método destrutivo eficaz"</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"SBD Diretriz: crioterapia opção terapêutica para molusco contagioso"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_3_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_3_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Approve_20104073_4">
        <description>Aprovação: Ceratoses seborreicas sintomáticas</description>
        <inputEntry id="IE_A4_1"><text>matches(diagnosticoPrincipal, "L82")</text></inputEntry>
        <inputEntry id="IE_A4_2"><text>"Ceratose seborreica","Verruga seborréica"</text></inputEntry>
        <inputEntry id="IE_A4_3"><text>&lt;= 5</text></inputEntry>
        <inputEntry id="IE_A4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_A4_5"><text>-</text></inputEntry>
        <outputEntry id="OE_A4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A4_2"><text>"Ceratoses seborreicas - crioterapia autorizada quando sintomáticas ou com indicação estética funcional"</text></outputEntry>
        <outputEntry id="OE_A4_3"><text>"ANS Rol 2024: remoção de lesões benignas sintomáticas"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_4_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_4_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Approve_20104073_5">
        <description>Aprovação: Lesões benignas diversas</description>
        <inputEntry id="IE_A5_1"><text>matches(diagnosticoPrincipal, "L81|L90|D22|D23")</text></inputEntry>
        <inputEntry id="IE_A5_2"><text>"Lentigo solar","Lesão pigmentada benigna","Fibroma"</text></inputEntry>
        <inputEntry id="IE_A5_3"><text>&lt;= 5</text></inputEntry>
        <inputEntry id="IE_A5_4"><text>true</text></inputEntry>
        <inputEntry id="IE_A5_5"><text>-</text></inputEntry>
        <outputEntry id="OE_A5_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A5_2"><text>"Lesões benignas com biópsia prévia confirmando benignidade - crioterapia autorizada"</text></outputEntry>
        <outputEntry id="OE_A5_3"><text>"SBD: crioterapia segura para lesões benignas confirmadas histologicamente"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_5_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104073_5_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK RULE -->
      <rule id="Rule_Fallback_20104073">
        <description>Regra padrão: casos não especificados requerem análise de auditoria</description>
        <inputEntry id="IE_FB_1"><text>-</text></inputEntry>
        <inputEntry id="IE_FB_2"><text>-</text></inputEntry>
        <inputEntry id="IE_FB_3"><text>-</text></inputEntry>
        <inputEntry id="IE_FB_4"><text>-</text></inputEntry>
        <inputEntry id="IE_FB_5"><text>-</text></inputEntry>
        <outputEntry id="OE_FB_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_FB_2"><text>"Caso não contemplado pelas regras automáticas - encaminhado para análise de auditoria médica com documentação completa"</text></outputEntry>
        <outputEntry id="OE_FB_3"><text>"ANS RN 465/2021 Art. 18: análise técnica em 48h úteis"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_20104073_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_20104073_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
