<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/DMN/20151101/dmn.xsd"
  id="Definitions_20104090"
  name="Curativo de extremidades de origem vascular"
  namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS: 20104090
    Procedimento: Curativo de extremidades de origem vascular
    Versão: 3.0.0 (2026-02-07)

    Indicações Clínicas (DUT/ANS):
    - Procedimentos dermatológicos específicos conforme protocolo ANS
    - CID-10 relacionados: I83|I70|E11.5|E10.5

    Contraindicações:
    - Casos não previstos no protocolo ANS
    - Procedimentos estéticos sem indicação clínica

    Referências:
    - Sociedade Brasileira de Dermatologia (SBD) - Diretrizes 2025
    - ANS Resolução Normativa 465/2021
    - Rol de Procedimentos ANS 2024
  -->

  <decision id="Decision_20104090" name="Decisão de Autorização - Curativo de extremidades de origem vascular">
    <decisionTable id="DecisionTable_20104090" hitPolicy="FIRST">

      <!-- INPUTS -->
      <input id="Input_1" label="Diagnóstico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Classificação da Lesão">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>classificacaoLesao</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Biópsia Realizada">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>biopsiaRealizada</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo Dermatológico Anexado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoDermatologico</text>
        </inputExpression>
      </input>

      <!-- OUTPUTS -->
      <output id="Output_1" label="Resultado" typeRef="string" name="resultado">
        <outputValues>
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_2" label="Observação" typeRef="string" name="observacao" />

      <output id="Output_3" label="Fundamentação Legal" typeRef="string" name="fundamentacao" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICATIONS -->
      <rule id="Rule_Contraind_20104090_1">
        <description>Reprovação: Procedimento estético sem indicação clínica</description>
        <inputEntry id="IE_C1_1"><text>not(matches(diagnosticoPrincipal, "I83|I70|E11.5|E10.5"))</text></inputEntry>
        <inputEntry id="IE_C1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_C1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_C1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_C1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_C1_2"><text>"CID-10 não compatível com indicações clínicas do procedimento. Laudo dermatológico não apresentado."</text></outputEntry>
        <outputEntry id="OE_C1_3"><text>"ANS Rol 2024: cobertura limitada a procedimentos com indicação clínica documentada"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_20104090_1_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_20104090_1_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APPROVALS -->
      <rule id="Rule_Approve_20104090_1">
        <description>Aprovação: CID-10 compatível com laudo</description>
        <inputEntry id="IE_A1_1"><text>matches(diagnosticoPrincipal, "I83|I70|E11.5|E10.5")</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Procedimento autorizado conforme indicação clínica e laudo dermatológico anexado"</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"ANS Rol 2024: procedimento coberto. SBD Diretriz: indicação clínica adequada"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104090_1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104090_1_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Approve_20104090_2">
        <description>Aprovação: Lesões com biópsia confirmando indicação</description>
        <inputEntry id="IE_A2_1"><text>matches(diagnosticoPrincipal, "I83|I70|E11.5|E10.5")</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Lesão com biópsia prévia confirmando necessidade do procedimento - autorizado"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"SBD: biópsia confirma indicação terapêutica"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104090_2_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Approve_20104090_2_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK RULE -->
      <rule id="Rule_Fallback_20104090">
        <description>Regra padrão: casos não especificados requerem análise de auditoria</description>
        <inputEntry id="IE_FB_1"><text>-</text></inputEntry>
        <inputEntry id="IE_FB_2"><text>-</text></inputEntry>
        <inputEntry id="IE_FB_3"><text>-</text></inputEntry>
        <inputEntry id="IE_FB_4"><text>-</text></inputEntry>
        <outputEntry id="OE_FB_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_FB_2"><text>"Caso não contemplado pelas regras automáticas - encaminhado para análise de auditoria médica com documentação completa"</text></outputEntry>
        <outputEntry id="OE_FB_3"><text>"ANS RN 465/2021 Art. 18: análise técnica em 48h úteis"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_20104090_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_20104090_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>