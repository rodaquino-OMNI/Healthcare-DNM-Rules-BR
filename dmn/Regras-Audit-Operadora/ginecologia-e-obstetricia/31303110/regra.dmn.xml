        <inputEntry id="InputEntry_3_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_6">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_5_6">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_6">
          <text>"Aprovado"</text>
