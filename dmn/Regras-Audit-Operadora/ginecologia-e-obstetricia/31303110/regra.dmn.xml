        <description>Aprovado - Câncer cervical IA2-IB</description>
        <inputEntry id="InputEntry_1_4">
          <text>"C53"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>"IA2","IB1","IB2"</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"Indicacao: Cancer de colo uterino estadio IA2-IB"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"FIGO 2021 - Histerectomia radical Classe II ou III indicada para estadios iniciais"</text>
