<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_31303293" name="Implante de dispositivo intrauterino DIU" targetNamespace="http://camunda.org/schema/1.0/dmn">


  <decision id="Decision_31303293" name="Avaliação de Cobertura para Sedação no Implante de DIU">
    <decisionTable id="DecisionTable_31303293" hitPolicy="FIRST">
      <input id="Input_1" label="Solicitado com Histeroscopia">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>solicitadoComHisteroscopia</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Condição Clínica Justificada">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>condicaoClinica</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Status da Cobertura para Sedação" name="statusSedacao" typeRef="string" />
      <output id="Output_2" label="Justificativa da Decisão" name="justificativaDecisao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação Técnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="Rule_1_Histeroscopia">
        <inputEntry id="InputEntry_1_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Histeroscopia não possui cobertura para implante de DIU."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"ANS RN 465/2021 - Histeroscopia não indicada para implante de DIU"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_1">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_1">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>

      <rule id="Rule_2_Imperativo_Clinico">
        <inputEntry id="InputEntry_1_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>"Imperativo Clínico"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Cobertura para sedação autorizada devido a imperativo clínico."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"FEBRASGO 2023 - Sedação indicada em caso de imperativo clínico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_2">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_2">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>

      <rule id="Rule_3_Estreitamento_Colo">
        <inputEntry id="InputEntry_1_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>"Estreitamento do Colo Uterino"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"Cobertura para sedação autorizada devido a estreitamento do colo uterino."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"FEBRASGO 2023 - Estreitamento do colo é indicação formal para sedação"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_3">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_3">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>

      <rule id="Rule_4_Sem_Justificativa">
        <inputEntry id="InputEntry_1_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"Sedação não autorizada por falta de justificativa técnica (imperativo clínico ou estreitamento do colo)."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"FEBRASGO 2023 - Sedação requer justificativa técnica formal"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_4">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_4">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>


    <rule id="Rule_Default_31303293">
      <description>Regra padrao - Criterios nao atendidos</description>
      <inputEntry id="InputEntry_Default_31303293_1">
        <text />
      </inputEntry>
      <inputEntry id="InputEntry_Default_31303293_2">
        <text />
      </inputEntry>
      <outputEntry id="OutputEntry_Default_31303293_1">
        <text>"Pendente"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_31303293_2">
        <text>"Análise adicional necessária - Verificar indicação de sedação"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_31303293_3">
        <text>"ANS RN 465/2021 | FEBRASGO 2023"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_31303293_4">
        <text>"NENHUM"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_31303293_5">
        <text>"SOLICITAR_INFO"</text>
      </outputEntry>
    </rule>
  </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_31303293">
      <dmndi:DMNShape id="DMNShape_31303293" dmnElementRef="Decision_31303293">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
