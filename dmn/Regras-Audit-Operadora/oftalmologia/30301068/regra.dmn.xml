<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30301068" name="Cantoplastia_medial" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_30301068" name="Criterios de Autorizacao - Cantoplastia Medial">
    <decisionTable id="DecisionTable_30301068" hitPolicy="FIRST">

      
      <input id="Input_indicacaoPrincipal" label="Indicacao Clinica Principal">
        <inputExpression id="InputExpr_indicacaoPrincipal" typeRef="string">
          <text>indicacaoPrincipal</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoPrincipal">
          <text>"EPICANTO_BLOQUEIO_VISUAL","TELECANTO_TRAUMATICO","ECTROPIO_MEDIAL_EPIFORA","LAGOFTALMO_MEDIAL","TUMOR_RECONSTRUCAO","FINALIDADE_ESTETICA"</text>
        </inputValues>
      </input>

      
      <input id="Input_distanciaIntercantal" label="Distancia Intercantal (mm)">
        <inputExpression id="InputExpr_distanciaIntercantal" typeRef="string">
          <text>distanciaIntercantal</text>
        </inputExpression>
        <inputValues id="InputValues_distanciaIntercantal">
          <text>"NORMAL_CRIANCA_&lt;32MM","NORMAL_ADULTO_&lt;36MM","TELECANTO_LEVE_32_34MM","TELECANTO_MODERADO_35_38MM","TELECANTO_GRAVE_&gt;38MM"</text>
        </inputValues>
      </input>

      
      <input id="Input_exposicaoCorneana" label="Exposicao Corneana Medial Documentada">
        <inputExpression id="InputExpr_exposicaoCorneana" typeRef="boolean">
          <text>exposicaoCorneana</text>
        </inputExpression>
      </input>

      
      <input id="Input_tratamentoPrevio" label="Tratamento Conservador Previo (3+ meses)">
        <inputExpression id="InputExpr_tratamentoPrevio" typeRef="string">
          <text>tratamentoPrevio</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentoPrevio">
          <text>"NAO_APLICAVEL_TRAUMA_AGUDO","NAO_APLICAVEL_TUMOR","REALIZADO_SEM_SUCESSO","NAO_REALIZADO","CONTRAINDICACAO_CIRURGICA"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoOftalmologico" label="Laudo Oftalmologico com Indicacao Cirurgica">
        <inputExpression id="InputExpr_laudoOftalmologico" typeRef="boolean">
          <text>laudoOftalmologico</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />

      
      
      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_30301068_Contraindicacao_Estetica">
        <description>Contraindicacao absoluta - Finalidade exclusivamente estetica</description>
        <inputEntry id="IE_Contra1_1"><text>"FINALIDADE_ESTETICA"</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Procedimento negado - cantoplastia medial com finalidade exclusivamente estetica nao tem cobertura obrigatoria"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"DUT ANS 2024 Art. 12 - Exclusao de Procedimentos Esteticos"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301068_Contraindicacao_SemLaudo">
        <description>Contraindicacao - Documentacao obrigatoria ausente</description>
        <inputEntry id="IE_Contra2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>false</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Laudo oftalmologico com indicacao cirurgica fundamentada, medida de distancia intercantal e fotografia eh documentacao obrigatoria"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"RN 465/2021 Art. 5 - Documentacao Minima para Cirurgia Oftalmologica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301068_Aprovacao_EpicantoBloqueioVisual">
        <description>Aprovacao prioritaria - Epicanto com comprometimento do eixo visual em criancas</description>
        <inputEntry id="IE_Aprov1_1"><text>"EPICANTO_BLOQUEIO_VISUAL"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Indicacao cirurgica prioritaria - epicanto congenito com bloqueio do eixo visual documentado - prevencao de ambliopia em crianca"</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"DUT ANS 2024 - Cirurgia Reparadora Palpebral Pediatrica Art. 18"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301068_Aprovacao_LagoftalmoExposicao">
        <description>Aprovacao secundaria - Lagoftalmo medial com exposicao corneana</description>
        <inputEntry id="IE_Aprov2_1"><text>"LAGOFTALMO_MEDIAL"</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Indicacao cirurgica em lagoftalmo medial pos-paralitico com exposicao corneana medial documentada - prevencao de ceratite"</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"DUT ANS 2024 - Cirurgia Reparadora Palpebral Funcional"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301068_Aprovacao_TelecantoTraumatico">
        <description>Aprovacao terciaria - Telecanto traumatico ou congenito grave</description>
        <inputEntry id="IE_Aprov3_1"><text>"TELECANTO_TRAUMATICO"</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>"TELECANTO_MODERADO_35_38MM","TELECANTO_GRAVE_&gt;38MM"</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>"NAO_APLICAVEL_TRAUMA_AGUDO"</text></inputEntry>
        <inputEntry id="IE_Aprov3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Indicacao cirurgica em telecanto traumatico ou congenito com distancia intercantal &gt;35mm - correcao reparadora funcional"</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"DUT ANS 2024 - Cirurgia Reparadora Pos-Traumatica e Congenita"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301068_Aprovacao_EctropioEpiforaRefrataria">
        <description>Aprovacao quaternaria - Ectropio medial com epifora refrataria apos 3+ meses de tratamento conservador</description>
        <inputEntry id="IE_Aprov4_1"><text>"ECTROPIO_MEDIAL_EPIFORA"</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>"REALIZADO_SEM_SUCESSO"</text></inputEntry>
        <inputEntry id="IE_Aprov4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"Indicacao cirurgica em ectropio medial com epifora cronica refrataria apos minimo 3 meses de tratamento conservador documentado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"DUT ANS 2024 - Cirurgia apos Falha de Tratamento Clinico Documentada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301068_Aprovacao_TumorReconstrucao">
        <description>Aprovacao quinaria - Tumor palpebral medial requerendo resseccao e reconstrucao cantonal</description>
        <inputEntry id="IE_Aprov5_1"><text>"TUMOR_RECONSTRUCAO"</text></inputEntry>
        <inputEntry id="IE_Aprov5_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov5_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov5_4"><text>"NAO_APLICAVEL_TUMOR"</text></inputEntry>
        <inputEntry id="IE_Aprov5_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov5_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov5_2"><text>"Indicacao cirurgica oncologica - resseccao de lesao tumoral palpebral medial com necessidade de reconstrucao do canto medial"</text></outputEntry>
        <outputEntry id="OE_Aprov5_3"><text>"DUT ANS 2024 - Cirurgia Oncologica Palpebral com Reconstrucao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_30301068">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor oftalmologico</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso requer analise manual por auditor oftalmologico especialista em cirurgia plastica palpebral - criterios nao atendidos automaticamente"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Processo manual de auditoria oftalmologica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>