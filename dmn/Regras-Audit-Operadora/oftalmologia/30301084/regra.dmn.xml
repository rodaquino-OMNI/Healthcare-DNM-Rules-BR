<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30301084" name="Correcao_cirurgica_de_ectropio_ou_entropio" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_30301084" name="Criterios Autorizacao - Correcao Ectropio/Entropio">
    <decisionTable id="DecisionTable_30301084" hitPolicy="FIRST">

      <input id="Input_diagnosticoPrincipal" label="Diagnostico Principal">
        <inputExpression id="InputExpr_diagnosticoPrincipal" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoPrincipal">
          <text>"H02.0","H02.1","H02.00","H02.01","H02.02","H02.03","H02.10","H02.11","H02.12","H02.13"</text>
        </inputValues>
      </input>

      <input id="Input_tipoMalposicao" label="Tipo de Malposicao">
        <inputExpression id="InputExpr_tipoMalposicao" typeRef="string">
          <text>tipoMalposicao</text>
        </inputExpression>
        <inputValues id="InputValues_tipoMalposicao">
          <text>"Entropio involutivo","Entropio cicatricial","Entropio espastico","Ectropio involutivo","Ectropio cicatricial","Ectropio paralitico"</text>
        </inputValues>
      </input>

      <input id="Input_lesaoCorneana" label="Lesao Corneana">
        <inputExpression id="InputExpr_lesaoCorneana" typeRef="string">
          <text>lesaoCorneana</text>
        </inputExpression>
        <inputValues id="InputValues_lesaoCorneana">
          <text>"Ausente","Abrasao leve","Ceratite punctata","Ulcera corneana"</text>
        </inputValues>
      </input>

      <input id="Input_laudoOftalmologico" label="Laudo Oftalmologico">
        <inputExpression id="InputExpr_laudoOftalmologico" typeRef="boolean">
          <text>laudoOftalmologico</text>
        </inputExpression>
      </input>

      <output id="Output_resultado" name="resultado" label="Resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" name="observacao" label="Observacao" typeRef="string" />
      <output id="Output_fundamentacao" name="fundamentacao" label="Fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindication_SemLaudo">
        <inputEntry><text>-</text></inputEntry>
        <inputEntry><text>-</text></inputEntry>
        <inputEntry><text>-</text></inputEntry>
        <inputEntry><text>false</text></inputEntry>
        <outputEntry><text>"Reprovado"</text></outputEntry>
        <outputEntry><text>"Laudo oftalmologico obrigatorio"</text></outputEntry>
        <outputEntry><text>"DUT ANS 2024 Art. 18"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraindication_SemLesao">
        <inputEntry><text>-</text></inputEntry>
        <inputEntry><text>-</text></inputEntry>
        <inputEntry><text>"Ausente"</text></inputEntry>
        <inputEntry><text>true</text></inputEntry>
        <outputEntry><text>"Reprovado"</text></outputEntry>
        <outputEntry><text>"Malposicao sem lesao corneana nao tem indicacao funcional"</text></outputEntry>
        <outputEntry><text>"DUT ANS 2024"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Approval_UlceraCorneana">
        <inputEntry><text>"H02.0","H02.1"</text></inputEntry>
        <inputEntry><text>-</text></inputEntry>
        <inputEntry><text>"Ulcera corneana"</text></inputEntry>
        <inputEntry><text>true</text></inputEntry>
        <outputEntry><text>"Aprovado"</text></outputEntry>
        <outputEntry><text>"Ulcera corneana secundaria - urgencia cirurgica"</text></outputEntry>
        <outputEntry><text>"RN 465/2021 Anexo II"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Approval_CeratitePunctata">
        <inputEntry><text>"H02.0","H02.1","H02.01","H02.11"</text></inputEntry>
        <inputEntry><text>"Entropio cicatricial","Ectropio cicatricial","Ectropio paralitico"</text></inputEntry>
        <inputEntry><text>"Ceratite punctata"</text></inputEntry>
        <inputEntry><text>true</text></inputEntry>
        <outputEntry><text>"Aprovado"</text></outputEntry>
        <outputEntry><text>"Malposicao com ceratite - risco lesao corneana"</text></outputEntry>
        <outputEntry><text>"DUT ANS 2024"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Approval_AbrasaoLeve">
        <inputEntry><text>"H02.0","H02.1"</text></inputEntry>
        <inputEntry><text>"Entropio involutivo","Ectropio involutivo"</text></inputEntry>
        <inputEntry><text>"Abrasao leve"</text></inputEntry>
        <inputEntry><text>true</text></inputEntry>
        <outputEntry><text>"Aprovado"</text></outputEntry>
        <outputEntry><text>"Malposicao involutiva com abrasao corneana"</text></outputEntry>
        <outputEntry><text>"DUT ANS 2024"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pending_TratamentoConservador">
        <inputEntry><text>"H02.0","H02.1"</text></inputEntry>
        <inputEntry><text>"Entropio involutivo","Ectropio involutivo"</text></inputEntry>
        <inputEntry><text>"Abrasao leve"</text></inputEntry>
        <inputEntry><text>true</text></inputEntry>
        <outputEntry><text>"Pendente"</text></outputEntry>
        <outputEntry><text>"Solicitar documentacao tratamento conservador previo (minimo 3 meses)"</text></outputEntry>
        <outputEntry><text>"DUT ANS 2024"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30301084">
        <inputEntry><text /></inputEntry>
        <inputEntry><text /></inputEntry>
        <inputEntry><text /></inputEntry>
        <inputEntry><text /></inputEntry>
        <outputEntry><text>"Pendente"</text></outputEntry>
        <outputEntry><text>"Encaminhar auditoria medica"</text></outputEntry>
        <outputEntry><text>"RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>

</definitions>