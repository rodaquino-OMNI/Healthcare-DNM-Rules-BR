<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30301050" name="Cantoplastia_lateral" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_30301050" name="Criterios de Autorizacao - Cantoplastia Lateral">
    <decisionTable id="DecisionTable_30301050" hitPolicy="FIRST">

      
      <input id="Input_indicacaoFuncional" label="Indicacao Funcional Documentada">
        <inputExpression id="InputExpr_indicacaoFuncional" typeRef="string">
          <text>indicacaoFuncional</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoFuncional">
          <text>"ECTROPIO_COM_EXPOSICAO_CORNEANA","INSTABILIDADE_CANTO_LATERAL_EPIFORA","DISTOPIA_TRAUMATICA","DEFORMIDADE_CONGENITA","FINALIDADE_ESTETICA"</text>
        </inputValues>
      </input>

      
      <input id="Input_exposicaoCorneana" label="Documentacao de Exposicao Corneana (Lampada Fenda)">
        <inputExpression id="InputExpr_exposicaoCorneana" typeRef="boolean">
          <text>exposicaoCorneana</text>
        </inputExpression>
      </input>

      
      <input id="Input_infeccaoAtiva" label="Presenca de Infeccao Palpebral Ativa">
        <inputExpression id="InputExpr_infeccaoAtiva" typeRef="boolean">
          <text>infeccaoAtiva</text>
        </inputExpression>
      </input>

      
      <input id="Input_tratamentoPrevio" label="Tratamento Conservador Previo">
        <inputExpression id="InputExpr_tratamentoPrevio" typeRef="string">
          <text>tratamentoPrevio</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentoPrevio">
          <text>"NAO_APLICAVEL_TRAUMA_AGUDO","REALIZADO_SEM_SUCESSO","NAO_REALIZADO","NAO_INDICADO_CASO_CONGENITO"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoOftalmologico" label="Laudo Oftalmologico com Indicacao Cirurgica">
        <inputExpression id="InputExpr_laudoOftalmologico" typeRef="boolean">
          <text>laudoOftalmologico</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />

      
      
      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_30301050_Contraindicacao_Estetica">
        <description>Contraindicacao absoluta - Finalidade exclusivamente estetica</description>
        <inputEntry id="IE_Contra1_1"><text>"FINALIDADE_ESTETICA"</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Procedimento negado - finalidade exclusivamente estetica sem indicacao funcional documentada"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"DUT ANS 2024 Art. 12 - Exclusao de Procedimentos Esteticos"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301050_Contraindicacao_Infeccao">
        <description>Contraindicacao temporaria - Infeccao palpebral ativa requer tratamento previo</description>
        <inputEntry id="IE_Contra2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Infeccao palpebral ativa (blefarite/dacriocistite) requer tratamento e resolucao antes de cirurgia eletiva"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"DUT ANS 2024 - Contraindicacao de Cirurgia Eletiva com Infeccao Ativa"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301050_Contraindicacao_SemLaudo">
        <description>Contraindicacao - Documentacao obrigatoria ausente</description>
        <inputEntry id="IE_Contra3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_5"><text>false</text></inputEntry>
        <outputEntry id="OE_Contra3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra3_2"><text>"Laudo oftalmologico com indicacao cirurgica fundamentada eh documentacao obrigatoria"</text></outputEntry>
        <outputEntry id="OE_Contra3_3"><text>"RN 465/2021 Art. 5 - Documentacao Minima para Cirurgia Oftalmologica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301050_Aprovacao_EctropioExposicao">
        <description>Aprovacao prioritaria - Ectropio com exposicao corneana documentada</description>
        <inputEntry id="IE_Aprov1_1"><text>"ECTROPIO_COM_EXPOSICAO_CORNEANA"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Indicacao cirurgica prioritaria - ectropio com exposicao corneana documentada em lampada de fenda - risco de ceratite"</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"DUT ANS 2024 - Cirurgia Reparadora Palpebral com Indicacao Funcional"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301050_Aprovacao_DistopiaTraumatica">
        <description>Aprovacao secundaria - Distopia cantonal pos-traumatica</description>
        <inputEntry id="IE_Aprov2_1"><text>"DISTOPIA_TRAUMATICA"</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>"NAO_APLICAVEL_TRAUMA_AGUDO"</text></inputEntry>
        <inputEntry id="IE_Aprov2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Indicacao cirurgica em distopia cantonal pos-traumatica com comprometimento funcional - correcao reparadora"</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"DUT ANS 2024 - Cirurgia Reparadora Pos-Traumatica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301050_Aprovacao_DeformidadeCongenita">
        <description>Aprovacao terciaria - Deformidade congenita com indicacao cirurgica</description>
        <inputEntry id="IE_Aprov3_1"><text>"DEFORMIDADE_CONGENITA"</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>"NAO_INDICADO_CASO_CONGENITO"</text></inputEntry>
        <inputEntry id="IE_Aprov3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Indicacao cirurgica em deformidade congenita do canto lateral com comprometimento funcional documentado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"DUT ANS 2024 - Cirurgia Reparadora de Malformacao Congenita"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_30301050_Aprovacao_InstabilidadeEpiforaRefrataria">
        <description>Aprovacao quaternaria - Instabilidade cantonal lateral com falha de tratamento conservador</description>
        <inputEntry id="IE_Aprov4_1"><text>"INSTABILIDADE_CANTO_LATERAL_EPIFORA"</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>"REALIZADO_SEM_SUCESSO"</text></inputEntry>
        <inputEntry id="IE_Aprov4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"Indicacao cirurgica apos falha documentada de tratamento conservador para instabilidade cantonal e epifora cronica"</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"DUT ANS 2024 - Cirurgia apos Falha de Tratamento Clinico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_30301050">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor oftalmologico</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso requer analise manual por auditor oftalmologico - criterios nao atendidos automaticamente"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Processo manual de auditoria oftalmologica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>