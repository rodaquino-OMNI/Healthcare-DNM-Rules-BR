<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30301076" name="Coloboma_com_plastica" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_30301076" name="Criterios de Autorizacao - Coloboma com Plastica">
    <decisionTable id="DecisionTable_30301076" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoPrincipal" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpr_diagnosticoPrincipal" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoPrincipal">
          <text>"Q10.3","S01.1","H02.1","H02.8","C44.1"</text>
        </inputValues>
      </input>

      
      <input id="Input_tamanhoDefeito" label="Tamanho do Defeito (% margem palpebral)">
        <inputExpression id="InputExpr_tamanhoDefeito" typeRef="string">
          <text>tamanhoDefeito</text>
        </inputExpression>
        <inputValues id="InputValues_tamanhoDefeito">
          <text>"&amp;lt;10%","10-30%","30-50%","&amp;gt;50%"</text>
        </inputValues>
      </input>

      
      <input id="Input_exposicaoCorneana" label="Exposicao Corneana Documentada">
        <inputExpression id="InputExpr_exposicaoCorneana" typeRef="string">
          <text>exposicaoCorneana</text>
        </inputExpression>
        <inputValues id="InputValues_exposicaoCorneana">
          <text>"Ausente","Minima","Moderada","Grave com ceratite"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoOftalmologico" label="Laudo Oftalmologico com Indicacao Cirurgica">
        <inputExpression id="InputExpr_laudoOftalmologico" typeRef="boolean">
          <text>laudoOftalmologico</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" name="resultado" label="Resultado da Analise" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" name="observacao" label="Observacao Tecnica" typeRef="string" />

      
      <output id="Output_fundamentacao" name="fundamentacao" label="Fundamentacao Legal" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindication_SemLaudo">
        <description>Contraindicacao: Ausencia de laudo oftalmologico</description>
        <inputEntry id="InputEntry_Contraindication_SemLaudo_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Contraindication_SemLaudo_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Contraindication_SemLaudo_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Contraindication_SemLaudo_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Contraindication_SemLaudo_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Contraindication_SemLaudo_2">
          <text>"Laudo oftalmologico com indicacao cirurgica e obrigatorio"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Contraindication_SemLaudo_3">
          <text>"DUT ANS 2024 Art. 18 - Documentacao Obrigatoria"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraindication_DefeitoMinimo">
        <description>Contraindicacao: Defeito minimo sem indicacao funcional</description>
        <inputEntry id="InputEntry_Contraindication_DefeitoMinimo_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Contraindication_DefeitoMinimo_2">
          <text>"&amp;lt;10%"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Contraindication_DefeitoMinimo_3">
          <text>"Ausente"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Contraindication_DefeitoMinimo_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Contraindication_DefeitoMinimo_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Contraindication_DefeitoMinimo_2">
          <text>"Defeito palpebral minimo (&amp;lt;10%) sem exposicao corneana nao tem indicacao cirurgica funcional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Contraindication_DefeitoMinimo_3">
          <text>"DUT ANS 2024 - Cirurgia plastica palpebral apenas se indicacao funcional"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Approval_ColobomaGrave">
        <description>Aprovacao: Coloboma grave com exposicao corneana significativa</description>
        <inputEntry id="InputEntry_Approval_ColobomaGrave_1">
          <text>"Q10.3","S01.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_ColobomaGrave_2">
          <text>"&amp;gt;50%"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_ColobomaGrave_3">
          <text>"Grave com ceratite","Moderada"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_ColobomaGrave_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Approval_ColobomaGrave_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Approval_ColobomaGrave_2">
          <text>"Coloboma palpebral extenso (&amp;gt;50%) com exposicao corneana documentada - Indicacao cirurgica clara"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Approval_ColobomaGrave_3">
          <text>"RN 465/2021 Anexo II - Procedimento obrigatorio para colobomas com comprometimento funcional"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Approval_ColobomaModerado">
        <description>Aprovacao: Coloboma moderado com indicacao funcional</description>
        <inputEntry id="InputEntry_Approval_ColobomaModerado_1">
          <text>"Q10.3","S01.1","H02.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_ColobomaModerado_2">
          <text>"30-50%"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_ColobomaModerado_3">
          <text>"Moderada","Grave com ceratite"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_ColobomaModerado_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Approval_ColobomaModerado_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Approval_ColobomaModerado_2">
          <text>"Coloboma palpebral moderado (30-50%) com exposicao corneana moderada/grave - Risco de ceratite"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Approval_ColobomaModerado_3">
          <text>"DUT ANS 2024 - Reconstrucao palpebral reparadora obrigatoria"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Approval_PosOncologico">
        <description>Aprovacao: Defeito pos-resseccao oncologica</description>
        <inputEntry id="InputEntry_Approval_PosOncologico_1">
          <text>"C44.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_PosOncologico_2">
          <text>"10-30%","30-50%","&amp;gt;50%"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_PosOncologico_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_PosOncologico_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Approval_PosOncologico_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Approval_PosOncologico_2">
          <text>"Defeito palpebral pos-resseccao oncologica - Reconstrucao reparadora necessaria"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Approval_PosOncologico_3">
          <text>"RN 465/2021 - Reconstrucao pos-oncologica e procedimento obrigatorio"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Approval_DefeitoModeradoComExposicao">
        <description>Aprovacao: Defeito moderado com exposicao corneana minima</description>
        <inputEntry id="InputEntry_Approval_DefeitoModeradoComExposicao_1">
          <text>"Q10.3","S01.1","H02.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_DefeitoModeradoComExposicao_2">
          <text>"10-30%"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_DefeitoModeradoComExposicao_3">
          <text>"Minima","Moderada"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Approval_DefeitoModeradoComExposicao_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Approval_DefeitoModeradoComExposicao_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Approval_DefeitoModeradoComExposicao_2">
          <text>"Defeito palpebral moderado (10-30%) com exposicao corneana - Prevencao de complicacoes"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Approval_DefeitoModeradoComExposicao_3">
          <text>"DUT ANS 2024 - Cirurgia plastica palpebral reparadora"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pending_DefeitoModeradoSemExposicao">
        <description>Pendente: Defeito moderado sem exposicao corneana - Avaliar necessidade funcional</description>
        <inputEntry id="InputEntry_Pending_DefeitoModeradoSemExposicao_1">
          <text>"Q10.3","S01.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Pending_DefeitoModeradoSemExposicao_2">
          <text>"10-30%"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Pending_DefeitoModeradoSemExposicao_3">
          <text>"Ausente"</text>
        </inputEntry>
        <inputEntry id="InputEntry_Pending_DefeitoModeradoSemExposicao_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Pending_DefeitoModeradoSemExposicao_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Pending_DefeitoModeradoSemExposicao_2">
          <text>"Defeito palpebral moderado sem exposicao corneana atual - Solicitar avaliacao funcional complementar e teste de Schirmer/BUT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Pending_DefeitoModeradoSemExposicao_3">
          <text>"DUT ANS 2024 - Analise individualizada de indicacao funcional"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30301076">
        <description>Regra padrao: Casos nao contemplados nas regras anteriores</description>
        <inputEntry id="InputEntry_Fallback_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Fallback_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Fallback_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Fallback_2">
          <text>"Caso nao contemplado nas regras padrao - Encaminhar para auditoria medica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Fallback_3">
          <text>"RN 465/2021 - Analise individualizada por auditoria medica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>

</definitions>