        <outputEntry id="OE_Pend1_2"><text>"Exames incompletos para corpectomia. Necessario: TC 3D para planejamento de reconstrucao, RNM para avaliacao de partes moles/medula"</text></outputEntry>
        <outputEntry id="OE_Pend1_3"><text>"Planejamento cirurgico requer imagem multimodal"</text></outputEntry>
        <outputEntry id="OE_Rule_30902089_Pendente_1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_30902089_Pendente_1_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_30902089_Pendente_2">
        <description>Pendente - Historico incompleto</description>
        <inputEntry id="IE_Pend2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend2_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="IE_Pend2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend2_5"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend2_6"><text>-</text></inputEntry>
