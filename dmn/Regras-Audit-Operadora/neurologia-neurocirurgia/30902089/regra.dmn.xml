        <inputEntry id="IE_Aprov3_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>"TC 3D + RNM + labs completos","TC 3D + RNM"</text></inputEntry>
        <inputEntry id="IE_Aprov3_5"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov3_6"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"OPLL com ocupacao maior que 60% do canal cervical. Corpectomia anterior com resseccao direta do OPLL indicada para descompressao adequada"</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"NASS Myelopathy 2024 - corpectomia para OPLL K-line negativo ou occupancy &gt;60%"</text></outputEntry>
        <outputEntry id="OE_Rule_30902089_Aprovacao_3_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_30902089_Aprovacao_3_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_30902089_Aprovacao_4">
        <description>Aprovacao - Osteomielite/discite</description>
        <inputEntry id="IE_Aprov4_1"><text>"Osteomielite/discite"</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>"TC 3D + RNM + labs completos"</text></inputEntry>
        <inputEntry id="IE_Aprov4_5"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov4_6"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"Osteomielite/discite cervical com destruicao vertebral. Corpectomia permite desbridamento amplo e reconstrucao. Antibioticoterapia prolongada necessaria"</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"IDSA Vertebral Osteomyelitis Guidelines - desbridamento cirurgico em falha de ATB ou instabilidade"</text></outputEntry>
        <outputEntry id="OE_Rule_30902089_Aprovacao_4_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_30902089_Aprovacao_4_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- PENDING RULES -->
