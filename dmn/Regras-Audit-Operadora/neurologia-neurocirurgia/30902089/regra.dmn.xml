        <outputEntry id="OE_Pend2_2"><text>"Documentacao incompleta. Solicitar: escore mJOA/ASIA, etiologia confirmada, relatorio oncologico (se tumor), biopsia (se necessario)"</text></outputEntry>
        <outputEntry id="OE_Pend2_3"><text>"RN 428/2017 - documentacao complementar para procedimentos de alta complexidade"</text></outputEntry>
        <outputEntry id="OE_Rule_30902089_Pendente_2_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_30902089_Pendente_2_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_30902089">
