        <outputEntry id="OE_Fallback_2"><text>"Caso requer analise manual por auditor medico especializado em neurocirurgia/coluna. Documentacao adicional pode ser solicitada"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Auditoria medica conforme RN 428/2017 e DUT ANS 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_30902011_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_30902011_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
