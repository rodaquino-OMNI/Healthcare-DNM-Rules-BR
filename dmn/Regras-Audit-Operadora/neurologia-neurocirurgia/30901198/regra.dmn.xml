        <inputEntry id="InputEntry_6_1"><text>"HSDCronicoSintomatico"</text></inputEntry>
        <inputEntry id="InputEntry_6_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_6_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_6_4"><text>"TCCranioCoagulograma","TCCranio"</text></inputEntry>
