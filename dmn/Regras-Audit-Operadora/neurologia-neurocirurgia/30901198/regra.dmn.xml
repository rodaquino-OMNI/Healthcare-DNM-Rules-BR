        <inputEntry id="InputEntry_2_1"><text>"HSDCronicoPequenoAssintomatico"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_Assintomatico_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"HSD cronico pequeno assintomatico - observacao recomendada"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Neurosurgery cSDH 2023 - Observacao para HSD cronico assintomatico"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_Assintomatico_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_Assintomatico_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_SemImagem">
        <description>Reprovado: Sem imagem</description>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>"SemImagem"</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_SemImagem_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"TC de cranio obrigatoria para diagnostico"</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"BTF 2023 - Imagem essencial para diagnostico e planejamento"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- WASTE PREVENTION RULES -->
      <rule id="Rule_30901198_DUP_Reop">
        <description>DUP: Reoperacao craniana em menos de 30 dias sem complicacao documentada</description>
        <inputEntry id="IE_30901198_DUP_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_DUP_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="IE_30901198_DUP_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_DUP_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_DUP_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_DUP_6"><text>&lt; 30</text></inputEntry>
        <outputEntry id="OE_30901198_DUP_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30901198_DUP_2"><text>"Reoperacao craniana em menos de 30 dias requer justificativa de complicacao documentada"</text></outputEntry>
        <outputEntry id="OE_30901198_DUP_3"><text>"AANS/CNS 2024 - Reoperacao precoce requer documentacao de complicacao ou evento agudo"</text></outputEntry>
        <outputEntry id="OE_30901198_DUP_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_30901198_DUP_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_30901198_DOC_Imagem">
        <description>DOC: Procedimento neurocirurgico sem imagem atual</description>
        <inputEntry id="IE_30901198_DOC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_DOC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_DOC_3"><text>"Classe I","Classe IIa"</text></inputEntry>
