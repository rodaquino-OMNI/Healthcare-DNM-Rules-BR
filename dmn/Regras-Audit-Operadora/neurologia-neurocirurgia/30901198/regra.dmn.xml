        <outputEntry id="OutputEntry_9_2"><text>"Documentar Glasgow, sintomas e medicacoes anticoagulantes"</text></outputEntry>
        <outputEntry id="OutputEntry_9_3"><text>"ANS RN 465/2021 - Documentacao adequada necessaria"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_DocIncompleta_4"><text>"DOC"</text></outputEntry>
