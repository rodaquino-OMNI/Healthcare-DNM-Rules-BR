        <inputEntry id="IE_30901198_DOC_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_DOC_6"><text>-</text></inputEntry>
        <outputEntry id="OE_30901198_DOC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30901198_DOC_2"><text>"TC de cranio obrigatoria para planejamento cirurgico"</text></outputEntry>
        <outputEntry id="OE_30901198_DOC_3"><text>"AANS/CNS 2024, ANS RN 465/2021 - Imagem atual obrigatoria"</text></outputEntry>
        <outputEntry id="OE_30901198_DOC_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_30901198_DOC_5"><text>"SOLICITAR_INFO"</text></outputEntry>
