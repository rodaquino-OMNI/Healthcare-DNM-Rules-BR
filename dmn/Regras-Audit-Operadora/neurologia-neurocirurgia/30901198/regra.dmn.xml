        <outputEntry id="OutputEntry_6_2"><text>"HSD cronico sintomatico - drenagem por trepanacao indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_6_3"><text>"Neurosurgery cSDH 2023 - Trepanacao com dreno para HSD cronico"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Cronico_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Cronico_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Deterioracao">
        <description>Aprovado: Deterioracao neurologica</description>
        <inputEntry id="InputEntry_7_1"><text>"DeterioracaoNeurologica"</text></inputEntry>
        <inputEntry id="InputEntry_7_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_7_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_7_4"><text>"TCCranioCoagulograma","TCCranio"</text></inputEntry>
        <inputEntry id="InputEntry_7_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901198_Deterioracao_6"><text>-</text></inputEntry>
