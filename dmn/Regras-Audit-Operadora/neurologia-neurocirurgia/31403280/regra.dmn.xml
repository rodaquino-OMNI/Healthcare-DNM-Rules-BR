<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             id="Definitions_31403280" name="Tratamento_sindromes_compressivas" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 31403280 - Tratamento de síndromes compressivas convencional
    Versao: 3.1.0-LEAN-TIER2 (2026-02-08)

    TIER-2 ENRICHMENT:
    - Input 5: examesComprobatorios (documentation quality)
    - Input 6: ultimoProcedimentoDias (frequency control)
    - DUP rule: Cirurgia repetida < 180 dias
    - ESC rule: Sem tratamento conservador previo
    - 5-output format: resultado, observacao, fundamentacao, alertasDesperdicio, acaoRecomendada

    Indicacoes Clinicas: Síndrome do túnel do carpo, Síndrome do canal de Guyon, Neuropatia cubital
    Referencias: RN ANS 465/2021, SBOT 2024, SBN 2023
  -->
  <decision id="Decision_31403280" name="Indicacao para Tratamento_sindromes_compressivas">
    <decisionTable id="DecisionTable_31403280" hitPolicy="FIRST">
      <input id="Input_1" label="Sindrome">
        <inputExpression id="InputExpression_1" typeRef="string"><text>sindrome</text></inputExpression>
        <inputValues id="InputValues_1">
          <text>"tunel_carpo","guyon","cubital","outra"</text>
        </inputValues>
      </input>
      <input id="Input_2" label="Gravidadesintomas">
        <inputExpression id="InputExpression_2" typeRef="string"><text>gravidadeSintomas</text></inputExpression>
        <inputValues id="InputValues_2">
          <text>"leve","moderado","grave"</text>
        </inputValues>
      </input>
      <input id="Input_3" label="Tempotratamentoconservador">
        <inputExpression id="InputExpression_3" typeRef="number"><text>tempoTratamentoConservador</text></inputExpression>
      </input>
      <input id="Input_4" label="Enmgconfirma">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>enmgConfirma</text></inputExpression>
      </input>
      <input id="Input_5" label="Exames Comprobatorios">
        <inputExpression id="InputExpression_5" typeRef="string"><text>examesComprobatorios</text></inputExpression>
        <inputValues id="InputValues_5">
          <text>"Completo","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string"/>
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraindicacao_SemENMG">
        <inputEntry id="IE_1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_1_4"><text>false</text></inputEntry>
        <inputEntry id="IE_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_1_2"><text>"ENMG não confirma síndrome compressiva."</text></outputEntry>
        <outputEntry id="OE_1_3"><text>"SBOT 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SemENMG_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SemENMG_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- WASTE PREVENTION RULES -->
      <rule id="Rule_31403280_DUP_Compressiva">
        <description>DUP: Cirurgia compressiva repetida em menos de 180 dias</description>
        <inputEntry id="IE_31403280_DUP_1"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_DUP_2"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_DUP_3"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_DUP_4"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_DUP_5"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_DUP_6"><text>&lt; 180</text></inputEntry>
        <outputEntry id="OE_31403280_DUP_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_31403280_DUP_2"><text>"Cirurgia compressiva repetida em menos de 180 dias. Aguardar recuperacao"</text></outputEntry>
        <outputEntry id="OE_31403280_DUP_3"><text>"SBN 2023 - Intervalo minimo 180 dias"</text></outputEntry>
        <outputEntry id="OE_31403280_DUP_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_31403280_DUP_5"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <rule id="Rule_31403280_ESC_Conservador">
        <description>ESC: Sem tratamento conservador previo</description>
        <inputEntry id="IE_31403280_ESC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_ESC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_ESC_3"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_ESC_4"><text>-</text></inputEntry>
        <inputEntry id="IE_31403280_ESC_5"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="IE_31403280_ESC_6"><text>-</text></inputEntry>
        <outputEntry id="OE_31403280_ESC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_31403280_ESC_2"><text>"Documentar falha de tratamento conservador"</text></outputEntry>
        <outputEntry id="OE_31403280_ESC_3"><text>"SBN 2023 - Tratamento conservador obrigatorio"</text></outputEntry>
        <outputEntry id="OE_31403280_ESC_4"><text>"ESC"</text></outputEntry>
        <outputEntry id="OE_31403280_ESC_5"><text>"SUGERIR_ALTERNATIVA"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Moderado_Grave">
        <inputEntry id="IE_2_1"><text>"tunel_carpo","guyon","cubital"</text></inputEntry>
        <inputEntry id="IE_2_2"><text>"moderado","grave"</text></inputEntry>
        <inputEntry id="IE_2_3"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_2_4"><text>true</text></inputEntry>
        <inputEntry id="IE_2_5"><text>"Completo"</text></inputEntry>
        <inputEntry id="IE_2_6"><text>-</text></inputEntry>
        <outputEntry id="OE_2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_2_2"><text>"Síndrome compressiva moderada/grave confirmada (≥6 meses conservador). Descompressão cirúrgica indicada."</text></outputEntry>
        <outputEntry id="OE_2_3"><text>"SBOT 2024 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Moderado_Grave_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Moderado_Grave_5"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_31403280">
        <inputEntry id="IE_3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_3_5"><text>-</text></inputEntry>
        <inputEntry id="IE_3_6"><text>-</text></inputEntry>
        <outputEntry id="OE_3_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_3_2"><text>"Requer avaliação por especialista e auditor médico."</text></outputEntry>
        <outputEntry id="OE_3_3"><text>"Análise caso a caso conforme RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_31403280_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_31403280_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
