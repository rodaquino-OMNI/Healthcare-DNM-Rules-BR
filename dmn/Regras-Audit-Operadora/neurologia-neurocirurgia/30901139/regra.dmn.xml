<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_30901139"
             name="Implante de Eletrodo para DBS"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    TUSS: 30901139
    Procedimento: Implante de Eletrodo para Estimulacao Cerebral Profunda (DBS)
    Versao: 3.1.0-LEAN-TIER2
    Data: 2026-02-08
    Enriquecido: LEAN TIER-2 Anti-Desperdicio (2026-02-08)
    ===============================================================================

    INDICACOES CLINICAS (DUT/ANS):
    Doenca de Parkinson avancada refrataria a medicacao,
    tremor essencial incapacitante, distonia primaria generalizada,
    TOC grave refratario (uso compassivo)

    CONTRAINDICACOES:
    Demencia moderada-grave, psicose ativa, depressao grave nao tratada,
    expectativa de vida limitada, coagulopatia, atrofia cerebral grave

    DOCUMENTACAO OBRIGATORIA:
    Avaliacao neurologica especializada, escala UPDRS,
    avaliacao neuropsicologica, RM de cranio de alta resolucao,
    teste de levodopa (para Parkinson)

    REFERENCIAS TECNICAS:
    MDS Guidelines DBS 2023, AANS/CNS Functional 2024,
    ABN Parkinson 2023, SBN 2023, ANS RN 465/2021, DUT 2024

    ===============================================================================
  -->

  <decision id="Decision_30901139" name="Regra Clinica - Implante de Eletrodo para DBS">
    <decisionTable id="DecisionTable_30901139" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"ParkinsonAvancado","TremorEssencialIncapacitante","DistoniaPrimaria","TOCRefratario","DemenciaModeradaGrave"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (MDS/AANS)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"AvaliacaoCompletaUPDRSNeuropsico","RMNeuropsico","ApenasRM","SemImagem","Sem exames"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida Superior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- REPROVACOES -->
      <rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_1_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_1_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Procedimento nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"MDS 2023 - Classe III: Sem indicacao"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_ClasseIII_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_ClasseIII_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_Demencia">
        <description>Contraindicacao: Demencia moderada-grave</description>
        <inputEntry id="InputEntry_2_1"><text>"DemenciaModeradaGrave"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_2_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Demencia moderada-grave contraindica DBS"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"MDS 2023 - DBS contraindicado em demencia por risco de piora cognitiva"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_Demencia_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_Demencia_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano</description>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>false</text></inputEntry>
        <inputEntry id="IE_30901139_3_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"Expectativa de vida insuficiente para beneficio do DBS"</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"MDS 2023 - DBS requer expectativa de vida adequada"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_ExpectativaVida_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_ExpectativaVida_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_SemImagem">
        <description>Reprovado: Sem imagem</description>
        <inputEntry id="InputEntry_4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>"SemImagem","Sem exames"</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_4_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_4_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_4_2"><text>"RM de cranio obrigatoria para planejamento"</text></outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"AANS/CNS 2024 - RM essencial para planejamento estereotaxico"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- WASTE PREVENTION RULES -->
      <rule id="Rule_30901139_ESC_Neuromod">
        <description>ESC: DBS sem falha de tratamento medicamentoso documentada</description>
        <inputEntry id="IE_30901139_ESC_NM_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_ESC_NM_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="IE_30901139_ESC_NM_3"><text>"Classe IIa","Classe IIb"</text></inputEntry>
        <inputEntry id="IE_30901139_ESC_NM_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_ESC_NM_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_ESC_NM_6"><text>-</text></inputEntry>
        <outputEntry id="OE_30901139_ESC_NM_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30901139_ESC_NM_2"><text>"DBS requer documentacao de falha terapeutica medicamentosa"</text></outputEntry>
        <outputEntry id="OE_30901139_ESC_NM_3"><text>"AANS/CNS 2024 - DBS indicado apos falha de terapia medicamentosa otimizada"</text></outputEntry>
        <outputEntry id="OE_30901139_ESC_NM_4"><text>"ESC"</text></outputEntry>
        <outputEntry id="OE_30901139_ESC_NM_5"><text>"SUGERIR_ALTERNATIVA"</text></outputEntry>
      </rule>

      <rule id="Rule_30901139_DOC_Neurofunc">
        <description>DOC: DBS sem laudo neurofisiologico</description>
        <inputEntry id="IE_30901139_DOC_NF_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_DOC_NF_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_DOC_NF_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="IE_30901139_DOC_NF_4"><text>"SemImagem","Sem exames"</text></inputEntry>
        <inputEntry id="IE_30901139_DOC_NF_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_DOC_NF_6"><text>-</text></inputEntry>
        <outputEntry id="OE_30901139_DOC_NF_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30901139_DOC_NF_2"><text>"DBS requer laudo neurofisiologico e escala funcional"</text></outputEntry>
        <outputEntry id="OE_30901139_DOC_NF_3"><text>"AANS/CNS 2024 - Avaliacao neurofisiologica obrigatoria antes de DBS"</text></outputEntry>
        <outputEntry id="OE_30901139_DOC_NF_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_30901139_DOC_NF_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_Parkinson">
        <description>Aprovado: Doenca de Parkinson avancada</description>
        <inputEntry id="InputEntry_5_1"><text>"ParkinsonAvancado"</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>"AvaliacaoCompletaUPDRSNeuropsico"</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901139_5_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_5_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_5_2"><text>"Parkinson avancado refratario - DBS indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_5_3"><text>"MDS DBS 2023 - Classe I para Parkinson avancado com flutuacoes motoras"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Parkinson_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Parkinson_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_TremorEssencial">
        <description>Aprovado: Tremor essencial incapacitante</description>
        <inputEntry id="InputEntry_6_1"><text>"TremorEssencialIncapacitante"</text></inputEntry>
        <inputEntry id="InputEntry_6_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_6_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_6_4"><text>"AvaliacaoCompletaUPDRSNeuropsico","RMNeuropsico"</text></inputEntry>
        <inputEntry id="InputEntry_6_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901139_6_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_6_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_6_2"><text>"Tremor essencial incapacitante - DBS Vim indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_6_3"><text>"MDS 2023 - DBS Vim para tremor essencial refratario"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_TremorEssencial_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_TremorEssencial_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Distonia">
        <description>Aprovado: Distonia primaria generalizada</description>
        <inputEntry id="InputEntry_7_1"><text>"DistoniaPrimaria"</text></inputEntry>
        <inputEntry id="InputEntry_7_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_7_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_7_4"><text>"AvaliacaoCompletaUPDRSNeuropsico","RMNeuropsico"</text></inputEntry>
        <inputEntry id="InputEntry_7_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901139_7_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_7_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_7_2"><text>"Distonia primaria generalizada - DBS GPi indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_7_3"><text>"MDS 2023 - DBS GPi para distonia primaria refrataria"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Distonia_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Distonia_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_TOC">
        <description>Aprovado: TOC grave refratario (uso compassivo)</description>
        <inputEntry id="InputEntry_8_1"><text>"TOCRefratario"</text></inputEntry>
        <inputEntry id="InputEntry_8_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_8_3"><text>"Classe IIa","Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_8_4"><text>"AvaliacaoCompletaUPDRSNeuropsico"</text></inputEntry>
        <inputEntry id="InputEntry_8_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901139_8_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_8_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_8_2"><text>"TOC grave refratario - DBS em uso compassivo"</text></outputEntry>
        <outputEntry id="OutputEntry_8_3"><text>"FDA HDE, ABP 2023 - DBS para TOC grave refratario a tratamento"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_TOC_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_TOC_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- PENDENTES -->
      <rule id="Rule_Pendente_SemAvaliacaoCompleta">
        <description>Pendente: Sem avaliacao neuropsicologica</description>
        <inputEntry id="InputEntry_9_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_9_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_9_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_9_4"><text>"ApenasRM"</text></inputEntry>
        <inputEntry id="InputEntry_9_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901139_9_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_9_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_9_2"><text>"Necessario avaliacao neuropsicologica completa"</text></outputEntry>
        <outputEntry id="OutputEntry_9_3"><text>"MDS 2023 - Avaliacao neuropsicologica obrigatoria antes de DBS"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_SemAvaliacaoCompleta_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_SemAvaliacaoCompleta_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Pendente_DocIncompleta">
        <description>Pendente: Documentacao incompleta</description>
        <inputEntry id="InputEntry_10_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_10_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_10_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_10_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_10_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901139_10_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_10_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_10_2"><text>"Documentar UPDRS, teste levodopa, avaliacao cognitiva"</text></outputEntry>
        <outputEntry id="OutputEntry_10_3"><text>"ANS RN 465/2021 - Documentacao adequada necessaria"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_DocIncompleta_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_DocIncompleta_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_30901139">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_11_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_11_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_11_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_11_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_11_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901139_11_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_11_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_11_2"><text>"Caso requer avaliacao em centro especializado em disturbios do movimento"</text></outputEntry>
        <outputEntry id="OutputEntry_11_3"><text>"Avaliacao conforme MDS DBS Guidelines 2023"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_30901139_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_30901139_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
