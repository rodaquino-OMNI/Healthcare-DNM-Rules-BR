        <outputEntry id="OE_Contra1_3"><text>"NASS/AOSpine 2024 - Classe III: nao realizar"</text></outputEntry>
        <outputEntry id="OE_Rule_30902046_Contraindicacao_1_4"><text>"PROT"</text></outputEntry>
