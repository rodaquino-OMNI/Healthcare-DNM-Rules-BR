        <outputEntry id="OE_Contra3_2"><text>"OPLL extensa detectada em TC. Via anterior com risco elevado de lesao dural/medular. Via posterior (laminoplastia ou laminectomia com fusao) mais segura"</text></outputEntry>
        <outputEntry id="OE_Contra3_3"><text>"NASS 2024 - OPLL com ocupacao >60% do canal: via posterior recomendada"</text></outputEntry>
        <outputEntry id="OE_Rule_30902046_Contraindicacao_3_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_30902046_Contraindicacao_3_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_30902046_Contraindicacao_4">
