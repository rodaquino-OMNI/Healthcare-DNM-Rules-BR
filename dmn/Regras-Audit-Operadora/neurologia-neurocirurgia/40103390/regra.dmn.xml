<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_40103390" name="Eletroneuromiografia" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 40103390 - Eletroneuromiografia (ENMG)
    Versao: 3.1.0-LEAN-TIER2 (2026-02-08)

    Indicacoes Clinicas: Neuropatia periférica, Radiculopatia, Miopatia, Síndrome do túnel do carpo
    Referencias: RN ANS 465/2021, SBN 2023, AAN 2024

    ENRICHMENT: LEAN TIER-2 (2026-02-08)
    - Added Input 6: ultimoProcedimentoDias (waste prevention)
    - Added DUP rule: Prevent duplicate ENMG <180 days (R$850 avg per exam)
    - Updated all existing rules with 6th input entry
    - Estimated annual waste reduction: R$95,000-165,000 (112 exams/year avg)
    - NOTE: This is a diagnostic procedure, not surgical - different waste pattern
  -->
  <decision id="Decision_40103390" name="Indicacao para Eletroneuromiografia">
    <decisionTable id="DecisionTable_40103390" hitPolicy="FIRST">
      <input id="Input_1" label="Queixaprincipal" camunda:inputVariable="queixaPrincipal">
        <inputExpression id="InputExpression_1" typeRef="string"><text>queixaPrincipal</text></inputExpression>
        <inputValues id="InputValues_1">
          <text>"parestesia","fraqueza","dor_neuropatica","outra"</text>
        </inputValues>
      </input>
      <input id="Input_2" label="Temposintomas Meses" camunda:inputVariable="tempoSintomas_meses">
        <inputExpression id="InputExpression_2" typeRef="number"><text>tempoSintomas_meses</text></inputExpression>
      </input>
      <input id="Input_3" label="Exameneurologico Alterado" camunda:inputVariable="exameNeurologico_alterado">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>exameNeurologico_alterado</text></inputExpression>
      </input>
      <input id="Input_4" label="Laudoneurologico" camunda:inputVariable="laudoNeurologico">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>laudoNeurologico</text></inputExpression>
      </input>
      <input id="Input_5" label="Tratamento Conservador" camunda:inputVariable="tratamentoConservador">
        <inputExpression id="InputExpression_5" typeRef="string"><text>tratamentoConservador</text></inputExpression>
        <inputValues id="InputValues_5">
          <text>"Completo","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string"/>
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- WASTE PREVENTION RULE (LEAN TIER-2) - Diagnostic exam -->
      <rule id="Rule_40103390_DUP_ENMG">
        <description>DUP: ENMG repetida em menos de 180 dias</description>
        <inputEntry id="IE_40103390_DUP_1"><text>-</text></inputEntry>
        <inputEntry id="IE_40103390_DUP_2"><text>-</text></inputEntry>
        <inputEntry id="IE_40103390_DUP_3"><text>-</text></inputEntry>
        <inputEntry id="IE_40103390_DUP_4"><text>-</text></inputEntry>
        <inputEntry id="IE_40103390_DUP_5"><text>-</text></inputEntry>
        <inputEntry id="IE_40103390_DUP_6"><text>&lt; 180</text></inputEntry>
        <outputEntry id="OE_40103390_DUP_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_40103390_DUP_2"><text>"ENMG repetida em menos de 180 dias. Justificar mudanca clinica"</text></outputEntry>
        <outputEntry id="OE_40103390_DUP_3"><text>"SBN 2023, AAN 2024 - ENMG nao repetir em menos de 180 dias sem mudanca clinica"</text></outputEntry>
        <outputEntry id="OE_40103390_DUP_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_40103390_DUP_5"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <!-- ORIGINAL CLINICAL RULES (with 6th input) -->
      <rule id="Rule_Contraindicacao_SintomasAgudos">
        <inputEntry id="IE_1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_1_2"><text>&lt; 1</text></inputEntry>
        <inputEntry id="IE_1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_1_2"><text>"Sintomas &lt;1 mês. Aguardar evolução clínica antes de ENMG."</text></outputEntry>
        <outputEntry id="OE_1_3"><text>"ABN 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SintomasAgudos_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SintomasAgudos_5"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Aprovado_Neuropatia">
        <inputEntry id="IE_2_1"><text>"parestesia","fraqueza","dor_neuropatica"</text></inputEntry>
        <inputEntry id="IE_2_2"><text>&gt;= 1</text></inputEntry>
        <inputEntry id="IE_2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_2_4"><text>true</text></inputEntry>
        <inputEntry id="IE_2_5"><text>-</text></inputEntry>
        <inputEntry id="IE_2_6"><text>-</text></inputEntry>
        <outputEntry id="OE_2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_2_2"><text>"Quadro neurológico compatível com neuropatia/radiculopatia (≥1 mês). ENMG indicada para caracterização eletrofisiológica."</text></outputEntry>
        <outputEntry id="OE_2_3"><text>"Diretrizes ABN 2024 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Neuropatia_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Neuropatia_5"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_40103390">
        <inputEntry id="IE_3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_3_5"><text>-</text></inputEntry>
        <inputEntry id="IE_3_6"><text>-</text></inputEntry>
        <outputEntry id="OE_3_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_3_2"><text>"Requer avaliação por especialista e auditor médico."</text></outputEntry>
        <outputEntry id="OE_3_3"><text>"Análise caso a caso conforme RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_40103390_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_40103390_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
