<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             id="Definitions_31403379" name="Simpatectomia_videotoracoscopia" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 31403379 - Simpatectomia por videotoracoscopia
    Versao: 3.1.0-LEAN-TIER2 (2026-02-08)

    TIER-2 ENRICHMENT:
    - Input 5: examesComprobatorios (documentation quality)
    - Input 6: ultimoProcedimentoDias (frequency control)
    - PROT rule: Simpatectomia sem indicacao refrataria
    - ESC rule: Sem tratamento clinico previo
    - 5-output format: resultado, observacao, fundamentacao, alertasDesperdicio, acaoRecomendada

    Indicacoes Clinicas: Hiperidrose palmar/axilar primária refratária, Similar to TUSS 30729117
    Referencias: RN ANS 465/2021, SBC Torax 2023
  -->
  <decision id="Decision_31403379" name="Indicacao para Simpatectomia_videotoracoscopia">
    <decisionTable id="DecisionTable_31403379" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnostico</text></inputExpression>
        <inputValues id="InputValues_1">
          <text>"hiperidrose_primaria","eritrofobia","outra"</text>
        </inputValues>
      </input>
      <input id="Input_2" label="Tempotratamentoconservador">
        <inputExpression id="InputExpression_2" typeRef="number"><text>tempoTratamentoConservador</text></inputExpression>
      </input>
      <input id="Input_3" label="Riscocardiopulmonar">
        <inputExpression id="InputExpression_3" typeRef="string"><text>riscoCardiopulmonar</text></inputExpression>
        <inputValues id="InputValues_3">
          <text>"alto","medio","baixo"</text>
        </inputValues>
      </input>
      <input id="Input_4" label="Laudoespecializado">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>laudoEspecializado</text></inputExpression>
      </input>
      <input id="Input_5" label="Exames Comprobatorios">
        <inputExpression id="InputExpression_5" typeRef="string"><text>examesComprobatorios</text></inputExpression>
        <inputValues id="InputValues_5">
          <text>"Completo","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string"/>
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraindicacao_RiscoAlto">
        <inputEntry id="IE_1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_1_3"><text>"alto"</text></inputEntry>
        <inputEntry id="IE_1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_1_2"><text>"Risco cardiopulmonar alto. Contraindicação para toracoscopia."</text></outputEntry>
        <outputEntry id="OE_1_3"><text>"SBACV 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_RiscoAlto_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_RiscoAlto_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- WASTE PREVENTION RULES -->
      <rule id="Rule_31403379_PROT_Simpatectomia">
        <description>PROT: Simpatectomia sem indicacao de hiperidrose refrataria</description>
        <inputEntry id="IE_31403379_PROT_1"><text>-</text></inputEntry>
        <inputEntry id="IE_31403379_PROT_2"><text>-</text></inputEntry>
        <inputEntry id="IE_31403379_PROT_3"><text>-</text></inputEntry>
        <inputEntry id="IE_31403379_PROT_4"><text>-</text></inputEntry>
        <inputEntry id="IE_31403379_PROT_5"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="IE_31403379_PROT_6"><text>-</text></inputEntry>
        <outputEntry id="OE_31403379_PROT_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_31403379_PROT_2"><text>"Simpatectomia sem indicacao documentada de hiperidrose refrataria"</text></outputEntry>
        <outputEntry id="OE_31403379_PROT_3"><text>"SBC Torax 2023 - Simpatectomia apenas para hiperidrose primaria refrataria"</text></outputEntry>
        <outputEntry id="OE_31403379_PROT_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_31403379_PROT_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_31403379_ESC_Conservador">
        <description>ESC: Simpatectomia sem tratamento clinico previo</description>
        <inputEntry id="IE_31403379_ESC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_31403379_ESC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_31403379_ESC_3"><text>-</text></inputEntry>
        <inputEntry id="IE_31403379_ESC_4"><text>-</text></inputEntry>
        <inputEntry id="IE_31403379_ESC_5"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="IE_31403379_ESC_6"><text>-</text></inputEntry>
        <outputEntry id="OE_31403379_ESC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_31403379_ESC_2"><text>"Documentar falha de tratamento clinico (toxina botulinica, anticolinergicos)"</text></outputEntry>
        <outputEntry id="OE_31403379_ESC_3"><text>"SBC Torax 2023 - Tratamento clinico antes de simpatectomia"</text></outputEntry>
        <outputEntry id="OE_31403379_ESC_4"><text>"ESC"</text></outputEntry>
        <outputEntry id="OE_31403379_ESC_5"><text>"SUGERIR_ALTERNATIVA"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_HiperidrosePrimaria">
        <inputEntry id="IE_2_1"><text>"hiperidrose_primaria","eritrofobia"</text></inputEntry>
        <inputEntry id="IE_2_2"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_2_3"><text>"baixo","medio"</text></inputEntry>
        <inputEntry id="IE_2_4"><text>true</text></inputEntry>
        <inputEntry id="IE_2_5"><text>"Completo"</text></inputEntry>
        <inputEntry id="IE_2_6"><text>-</text></inputEntry>
        <outputEntry id="OE_2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_2_2"><text>"Hiperidrose primária refratária (≥6 meses). Simpatectomia toracoscópica indicada."</text></outputEntry>
        <outputEntry id="OE_2_3"><text>"SBACV 2024 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_HiperidrosePrimaria_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_HiperidrosePrimaria_5"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_31403379">
        <inputEntry id="IE_3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_3_5"><text>-</text></inputEntry>
        <inputEntry id="IE_3_6"><text>-</text></inputEntry>
        <outputEntry id="OE_3_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_3_2"><text>"Requer avaliação por especialista e auditor médico."</text></outputEntry>
        <outputEntry id="OE_3_3"><text>"Análise caso a caso conforme RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_31403379_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_31403379_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
