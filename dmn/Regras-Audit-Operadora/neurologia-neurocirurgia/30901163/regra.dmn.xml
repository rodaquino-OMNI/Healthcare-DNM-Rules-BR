        <outputEntry id="OE_Rule_Contraind_Bitemporal_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_Bitemporal_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_Memoria">
