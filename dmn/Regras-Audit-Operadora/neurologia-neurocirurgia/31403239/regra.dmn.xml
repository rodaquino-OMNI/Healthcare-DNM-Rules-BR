<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_31403239" name="Microneurolise_unica" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 31403239 - Microneurólise única
    Versao: 3.1.0-LEAN-TIER2 (2026-02-08)

    Indicacoes Clinicas: Lesão nervosa focal única, Neuroma sintomático
    Referencias: RN ANS 465/2021, SBN 2023

    ENRICHMENT: LEAN TIER-2 (2026-02-08)
    - Added Input 6: ultimoProcedimentoDias (waste prevention)
    - Added DUP rule: Prevent duplicate surgery <180 days (R$2,800 avg)
    - Added DOC rule: Require mandatory ENMG (avoid failed surgery R$2,400)
    - Added ESC rule: Require conservative treatment first (prevent premature surgery)
    - Updated all existing rules with 6th input entry
    - Estimated annual waste reduction: R$160,000-280,000 (64 cases/year avg)
  -->
  <decision id="Decision_31403239" name="Indicacao para Microneurolise_unica">
    <decisionTable id="DecisionTable_31403239" hitPolicy="FIRST">
      <input id="Input_1" label="Tipolesao" camunda:inputVariable="tipoLesao">
        <inputExpression id="InputExpression_1" typeRef="string"><text>tipoLesao</text></inputExpression>
        <inputValues id="InputValues_1">
          <text>"neuroma","compressao","outra"</text>
        </inputValues>
      </input>
      <input id="Input_2" label="Enmgconfirma" camunda:inputVariable="enmgConfirma">
        <inputExpression id="InputExpression_2" typeRef="boolean"><text>enmgConfirma</text></inputExpression>
      </input>
      <input id="Input_3" label="Tempoevolucao" camunda:inputVariable="tempoEvolucao">
        <inputExpression id="InputExpression_3" typeRef="number"><text>tempoEvolucao</text></inputExpression>
      </input>
      <input id="Input_4" label="Laudoneurologico" camunda:inputVariable="laudoNeurologico">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>laudoNeurologico</text></inputExpression>
      </input>
      <input id="Input_5" label="Tratamento Conservador" camunda:inputVariable="tratamentoConservador">
        <inputExpression id="InputExpression_5" typeRef="string"><text>tratamentoConservador</text></inputExpression>
        <inputValues id="InputValues_5">
          <text>"Completo","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string"/>
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- WASTE PREVENTION RULES (LEAN TIER-2) -->
      <rule id="Rule_31403239_DUP_NervoPerif">
        <description>DUP: Cirurgia repetida em menos de 180 dias</description>
        <inputEntry id="IE_31403239_DUP_1"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DUP_2"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DUP_3"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DUP_4"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DUP_5"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DUP_6"><text>&lt; 180</text></inputEntry>
        <outputEntry id="OE_31403239_DUP_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_31403239_DUP_2"><text>"Cirurgia repetida em menos de 180 dias. Aguardar recuperacao neural"</text></outputEntry>
        <outputEntry id="OE_31403239_DUP_3"><text>"SBN 2023 - Intervalo minimo 180 dias"</text></outputEntry>
        <outputEntry id="OE_31403239_DUP_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_31403239_DUP_5"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <rule id="Rule_31403239_DOC_ENMG">
        <description>DOC: Sem ENMG previa</description>
        <inputEntry id="IE_31403239_DOC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DOC_2"><text>false</text></inputEntry>
        <inputEntry id="IE_31403239_DOC_3"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DOC_4"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DOC_5"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_DOC_6"><text>-</text></inputEntry>
        <outputEntry id="OE_31403239_DOC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_31403239_DOC_2"><text>"ENMG obrigatoria antes de cirurgia de nervo periferico"</text></outputEntry>
        <outputEntry id="OE_31403239_DOC_3"><text>"SBN 2023, ANS RN 465/2021 - ENMG obrigatoria"</text></outputEntry>
        <outputEntry id="OE_31403239_DOC_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_31403239_DOC_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_31403239_ESC_Conservador">
        <description>ESC: Sem tratamento conservador</description>
        <inputEntry id="IE_31403239_ESC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_ESC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_ESC_3"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_ESC_4"><text>-</text></inputEntry>
        <inputEntry id="IE_31403239_ESC_5"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="IE_31403239_ESC_6"><text>-</text></inputEntry>
        <outputEntry id="OE_31403239_ESC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_31403239_ESC_2"><text>"Documentar falha de tratamento conservador"</text></outputEntry>
        <outputEntry id="OE_31403239_ESC_3"><text>"SBN 2023 - Conservador 4-12 semanas antes de cirurgia"</text></outputEntry>
        <outputEntry id="OE_31403239_ESC_4"><text>"ESC"</text></outputEntry>
        <outputEntry id="OE_31403239_ESC_5"><text>"SUGERIR_ALTERNATIVA"</text></outputEntry>
      </rule>

      <!-- ORIGINAL CLINICAL RULES (with 6th input) -->
      <rule id="Rule_Contraindicacao_SemENMG">
        <inputEntry id="IE_1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_1_2"><text>"ENMG não confirma lesão."</text></outputEntry>
        <outputEntry id="OE_1_3"><text>"SBNC 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SemENMG_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SemENMG_5"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Aprovado_LesaoUnica">
        <inputEntry id="IE_2_1"><text>"neuroma","compressao"</text></inputEntry>
        <inputEntry id="IE_2_2"><text>true</text></inputEntry>
        <inputEntry id="IE_2_3"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="IE_2_4"><text>true</text></inputEntry>
        <inputEntry id="IE_2_5"><text>"Completo"</text></inputEntry>
        <inputEntry id="IE_2_6"><text>-</text></inputEntry>
        <outputEntry id="OE_2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_2_2"><text>"Lesão única confirmada. Microneurólise indicada."</text></outputEntry>
        <outputEntry id="OE_2_3"><text>"SBNC 2024 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_LesaoUnica_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_LesaoUnica_5"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_31403239">
        <inputEntry id="IE_3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_3_5"><text>-</text></inputEntry>
        <inputEntry id="IE_3_6"><text>-</text></inputEntry>
        <outputEntry id="OE_3_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_3_2"><text>"Requer avaliação por especialista e auditor médico."</text></outputEntry>
        <outputEntry id="OE_3_3"><text>"Análise caso a caso conforme RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_31403239_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_31403239_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
