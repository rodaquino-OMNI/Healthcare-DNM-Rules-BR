<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_30901104"
             name="Embolizacao de Aneurisma Cerebral"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    TUSS: 30901104
    Procedimento: Embolizacao de Aneurisma Cerebral
    Versao: 3.1.0-LEAN-TIER2
    Data: 2026-02-08
    Enrichment: Added waste prevention rules (DUP+DOC)
    ===============================================================================

    INDICACOES CLINICAS (DUT/ANS):
    Aneurisma roto com HSA (alternativa a clipagem), aneurisma de circulacao posterior,
    aneurisma em localizacao cirurgica desfavoravel, paciente de alto risco cirurgico,
    aneurisma nao-roto com indicacao de tratamento

    CONTRAINDICACOES:
    Anatomia desfavoravel para cateterismo, alergia grave a contraste,
    coagulopatia grave, aneurisma com colo muito largo sem dispositivo adequado

    DOCUMENTACAO OBRIGATORIA:
    Angiografia digital diagnostica, avaliacao de colo e domo,
    escala de Hunt-Hess/WFNS se roto, avaliacao de opcoes terapeuticas

    REFERENCIAS TECNICAS:
    AHA/ASA Aneurysm Guidelines 2023, AANS/CNS Endovascular 2024,
    SNIS Standards 2023, SBN 2023, ANS RN 465/2021, DUT 2024

    ===============================================================================
  -->

  <decision id="Decision_30901104" name="Regra Clinica - Embolizacao de Aneurisma Cerebral">
    <decisionTable id="DecisionTable_30901104" hitPolicy="FIRST">

      <input id="Input_condicaoPaciente" label="Condicao do Paciente" camunda:inputVariable="condicaoPaciente">
        <inputExpression id="InputExpression_condicaoPaciente" typeRef="string">
          <text>condicaoPaciente</text>
        </inputExpression>
        <inputValues id="InputValues_condicaoPaciente">
          <text>"AneurismaRotoHSA","AneurismaCirculacaoPosterior","AneurismaLocalizacaoDesfavoravel","AltoRiscoCirurgico","AnatomiaDesfavoravelCateterismo"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (AHA/SNIS)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_examesComprobatorios" label="Exames Comprobatorios" camunda:inputVariable="examesComprobatorios">
        <inputExpression id="InputExpression_examesComprobatorios" typeRef="string">
          <text>examesComprobatorios</text>
        </inputExpression>
        <inputValues id="InputValues_examesComprobatorios">
          <text>"AngiografiaDigitalCompleta","AngioTCRM","ApenasTC","SemImagem"</text>
        </inputValues>
      </input>

      <input id="Input_expectativaVidaSuperior1Ano" label="Expectativa de Vida Superior 1 Ano" camunda:inputVariable="expectativaVidaSuperior1Ano">
        <inputExpression id="InputExpression_expectativaVidaSuperior1Ano" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>

      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- REPROVACOES -->
      <rule id="Rule_Contraind_ClasseIII">
        <description>Contraindicacao: Procedimento Classe III</description>
        <inputEntry id="InputEntry_1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_1_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_ClasseIII_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Procedimento nao indicado - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"AHA/ASA 2023 - Classe III: Sem indicacao"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_ClasseIII_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_ClasseIII_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_AnatomiaDesfavoravel">
        <description>Contraindicacao: Anatomia desfavoravel para cateterismo</description>
        <inputEntry id="InputEntry_2_1"><text>"AnatomiaDesfavoravelCateterismo"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_AnatomiaDesfavoravel_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Anatomia vascular desfavoravel - considerar clipagem"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"SNIS 2023 - Via endovascular nao viavel, considerar abordagem cirurgica"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_AnatomiaDesfavoravel_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_AnatomiaDesfavoravel_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Contraind_ExpectativaVida">
        <description>Contraindicacao: Expectativa de vida menor que 1 ano (eletivo)</description>
        <inputEntry id="InputEntry_3_1"><text>"AltoRiscoCirurgico"</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>false</text></inputEntry>
        <inputEntry id="IE_30901104_ExpectativaVida_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"Expectativa de vida insuficiente para tratamento eletivo"</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"AHA/ASA 2023 - Tratamento eletivo requer expectativa adequada"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_ExpectativaVida_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_ExpectativaVida_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_SemImagem">
        <description>Reprovado: Sem imagem vascular</description>
        <inputEntry id="InputEntry_4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>"SemImagem"</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_SemImagem_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_4_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_4_2"><text>"Angiografia obrigatoria para planejamento endovascular"</text></outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"SNIS 2023 - Angiografia diagnostica essencial"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- WASTE PREVENTION RULES -->
      <rule id="Rule_30901104_DUP_Vascular">
        <description>DUP: Procedimento neurovascular repetido em menos de 90 dias</description>
        <inputEntry id="IE_30901104_DUP_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_DUP_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_DUP_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_DUP_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_DUP_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_DUP_6"><text>&lt; 90</text></inputEntry>
        <outputEntry id="OE_30901104_DUP_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_30901104_DUP_2"><text>"Procedimento neurovascular repetido em menos de 90 dias. Justificar evento agudo ou aguardar intervalo"</text></outputEntry>
        <outputEntry id="OE_30901104_DUP_3"><text>"SBNCV 2023, AANS/CNS 2024 - Intervalo minimo 90 dias entre procedimentos vasculares cerebrais eletivos"</text></outputEntry>
        <outputEntry id="OE_30901104_DUP_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_30901104_DUP_5"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <rule id="Rule_30901104_DOC_Imagem">
        <description>DOC: Procedimento neurocirurgico sem imagem atual</description>
        <inputEntry id="IE_30901104_DOC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_DOC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_DOC_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="IE_30901104_DOC_4"><text>"ApenasTC","SemImagem"</text></inputEntry>
        <inputEntry id="IE_30901104_DOC_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_DOC_6"><text>-</text></inputEntry>
        <outputEntry id="OE_30901104_DOC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30901104_DOC_2"><text>"Angiografia digital obrigatoria para planejamento cirurgico"</text></outputEntry>
        <outputEntry id="OE_30901104_DOC_3"><text>"AANS/CNS 2024, ANS RN 465/2021 - Imagem atual obrigatoria"</text></outputEntry>
        <outputEntry id="OE_30901104_DOC_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_30901104_DOC_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->
      <rule id="Rule_Aprovado_Roto">
        <description>Aprovado: Aneurisma roto com HSA</description>
        <inputEntry id="InputEntry_5_1"><text>"AneurismaRotoHSA"</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>"AngiografiaDigitalCompleta","AngioTCRM"</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_Roto_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_5_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_5_2"><text>"HSA por aneurisma roto - embolizacao de emergencia indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_5_3"><text>"AHA/ASA 2023, ISAT Trial - Embolizacao equivalente ou superior em muitos casos"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Roto_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Roto_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_CirculacaoPosterior">
        <description>Aprovado: Aneurisma de circulacao posterior</description>
        <inputEntry id="InputEntry_6_1"><text>"AneurismaCirculacaoPosterior"</text></inputEntry>
        <inputEntry id="InputEntry_6_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_6_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_6_4"><text>"AngiografiaDigitalCompleta"</text></inputEntry>
        <inputEntry id="InputEntry_6_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901104_CirculacaoPosterior_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_6_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_6_2"><text>"Aneurisma de circulacao posterior - embolizacao preferencial"</text></outputEntry>
        <outputEntry id="OutputEntry_6_3"><text>"AHA/ASA 2023 - Via endovascular preferida para circulacao posterior"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_CirculacaoPosterior_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_CirculacaoPosterior_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_LocalDesfavoravel">
        <description>Aprovado: Localizacao cirurgica desfavoravel</description>
        <inputEntry id="InputEntry_7_1"><text>"AneurismaLocalizacaoDesfavoravel"</text></inputEntry>
        <inputEntry id="InputEntry_7_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_7_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_7_4"><text>"AngiografiaDigitalCompleta"</text></inputEntry>
        <inputEntry id="InputEntry_7_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901104_LocalDesfavoravel_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_7_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_7_2"><text>"Localizacao cirurgica desfavoravel - embolizacao indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_7_3"><text>"AHA/ASA 2023 - Via endovascular para aneurismas de dificil acesso cirurgico"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_LocalDesfavoravel_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_LocalDesfavoravel_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_AltoRisco">
        <description>Aprovado: Alto risco cirurgico</description>
        <inputEntry id="InputEntry_8_1"><text>"AltoRiscoCirurgico"</text></inputEntry>
        <inputEntry id="InputEntry_8_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_8_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_8_4"><text>"AngiografiaDigitalCompleta"</text></inputEntry>
        <inputEntry id="InputEntry_8_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901104_AltoRisco_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_8_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_8_2"><text>"Alto risco cirurgico - embolizacao como alternativa menos invasiva"</text></outputEntry>
        <outputEntry id="OutputEntry_8_3"><text>"AHA/ASA 2023 - Via endovascular para pacientes de alto risco"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_AltoRisco_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_AltoRisco_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- PENDENTES -->
      <rule id="Rule_Pendente_SemAngiografiaDigital">
        <description>Pendente: Sem angiografia digital</description>
        <inputEntry id="InputEntry_9_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_9_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_9_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_9_4"><text>"AngioTCRM","ApenasTC"</text></inputEntry>
        <inputEntry id="InputEntry_9_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901104_SemAngiografiaDigital_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_9_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_9_2"><text>"Necessario angiografia digital para planejamento endovascular"</text></outputEntry>
        <outputEntry id="OutputEntry_9_3"><text>"SNIS 2023 - Angiografia digital obrigatoria para embolizacao"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_SemAngiografiaDigital_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_SemAngiografiaDigital_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Pendente_DocIncompleta">
        <description>Pendente: Documentacao incompleta</description>
        <inputEntry id="InputEntry_10_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_10_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_10_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_10_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_10_5"><text>true</text></inputEntry>
        <inputEntry id="IE_30901104_DocIncompleta_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_10_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_10_2"><text>"Documentar morfologia do aneurisma (colo, domo, relacao)"</text></outputEntry>
        <outputEntry id="OutputEntry_10_3"><text>"ANS RN 465/2021 - Documentacao adequada necessaria"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_DocIncompleta_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_DocIncompleta_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_30901104">
        <description>Regra padrao - Analise adicional necessaria</description>
        <inputEntry id="InputEntry_11_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_11_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_11_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_11_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_11_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901104_Fallback_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_11_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_11_2"><text>"Caso requer avaliacao neurointervencionista"</text></outputEntry>
        <outputEntry id="OutputEntry_11_3"><text>"Avaliacao conforme AHA/ASA e SNIS 2023"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_30901104_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_30901104_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
