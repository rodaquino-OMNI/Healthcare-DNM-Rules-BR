<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             id="Definitions_40814220" name="Trombectomia_AVC_isquemico" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 40814220 - Trombectomia mecânica no AVC isquêmico agudo
    Versao: 3.1.0-LEAN-TIER2 (2026-02-08)

    Indicacoes Clinicas (DUT/ANS):
    - AVC isquêmico agudo com oclusão de grande vaso intracraniano (ACI, ACM-M1/M2)
    - NIHSS ≥6 (déficit neurológico moderado a grave)
    - Janela terapêutica <24h do início dos sintomas (<6h ideal)
    - ASPECTS ≥6 em TC/RM (tecido cerebral viável)
    - mRS pré-mórbido 0-1 (independente funcional)

    Contraindicacoes:
    - Hemorragia intracraniana na TC
    - ASPECTS <6 (infarto extenso)
    - Oclusão de pequenos vasos distais
    - >24h do início dos sintomas sem penumbra isquêmica

    Referencias:
    - Diretrizes ABN/SBAN (Sociedade Brasileira de Doenças Cerebrovasculares) 2024
    - AHA/ASA Stroke Guidelines 2024
    - RN ANS 465/2021
  -->
  <decision id="Decision_40814220" name="Indicacao para Trombectomia AVC isquemico">
    <decisionTable id="DecisionTable_40814220" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
        <inputValues id="InputValues_1">
          <text>"avc_grande_vaso","avc_pequeno_vaso","hemorragico","outra"</text>
        </inputValues>
      </input>
      <input id="Input_2" label="NIHSS Score">
        <inputExpression id="InputExpression_2" typeRef="number"><text>nihssScore</text></inputExpression>
      </input>
      <input id="Input_3" label="Tempo Sintomas (horas)">
        <inputExpression id="InputExpression_3" typeRef="number"><text>tempoSintomas</text></inputExpression>
      </input>
      <input id="Input_4" label="ASPECTS Score">
        <inputExpression id="InputExpression_4" typeRef="number"><text>aspectsScore</text></inputExpression>
      </input>
      <input id="Input_5" label="Exames Comprobatorios">
        <inputExpression id="InputExpression_5" typeRef="string"><text>examesComprobatorios</text></inputExpression>
        <inputValues id="InputValues_5">
          <text>"TCComLaudo","RMDisponivel","SemImagem","Sem exames"</text>
        </inputValues>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string"/>
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraindicacao_Hemorragico">
        <inputEntry id="IE_1_1"><text>"hemorragico"</text></inputEntry>
        <inputEntry id="IE_1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_1_2"><text>"AVC hemorrágio. Contraindicação absoluta para trombectomia."</text></outputEntry>
        <outputEntry id="OE_1_3"><text>"AHA/ASA Guidelines 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_Hemorragico_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_Hemorragico_5"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Contraindicacao_PequenoVaso">
        <inputEntry id="IE_2_1"><text>"avc_pequeno_vaso"</text></inputEntry>
        <inputEntry id="IE_2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_2_5"><text>-</text></inputEntry>
        <inputEntry id="IE_2_6"><text>-</text></inputEntry>
        <outputEntry id="OE_2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2_2"><text>"Oclusão de pequeno vaso distal. Trombectomia não indicada (risco > benefício)."</text></outputEntry>
        <outputEntry id="OE_2_3"><text>"Diretrizes ABN/SBAN 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_PequenoVaso_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_PequenoVaso_5"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Contraindicacao_InfartoExtensivo">
        <inputEntry id="IE_3_1"><text>"avc_grande_vaso"</text></inputEntry>
        <inputEntry id="IE_3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_3_4"><text>&lt; 6</text></inputEntry>
        <inputEntry id="IE_3_5"><text>-</text></inputEntry>
        <inputEntry id="IE_3_6"><text>-</text></inputEntry>
        <outputEntry id="OE_3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_3_2"><text>"ASPECTS &lt;6 indica infarto extenso estabelecido. Alto risco de transformação hemorrágica com trombectomia."</text></outputEntry>
        <outputEntry id="OE_3_3"><text>"AHA/ASA Guidelines 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_InfartoExtensivo_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_InfartoExtensivo_5"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Contraindicacao_NIHSS_Baixo">
        <inputEntry id="IE_4_1"><text>"avc_grande_vaso"</text></inputEntry>
        <inputEntry id="IE_4_2"><text>&lt; 6</text></inputEntry>
        <inputEntry id="IE_4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_4_5"><text>-</text></inputEntry>
        <inputEntry id="IE_4_6"><text>-</text></inputEntry>
        <outputEntry id="OE_4_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_4_2"><text>"NIHSS &lt;6 indica déficit leve. Trombectomia não indicada (risco procedimento > benefício esperado)."</text></outputEntry>
        <outputEntry id="OE_4_3"><text>"Diretrizes ABN/SBAN 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_NIHSS_Baixo_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_NIHSS_Baixo_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_40814220_DOC_Imagem">
        <description>DOC: Procedimento sem imagem atual documentada</description>
        <inputEntry id="IE_40814220_DOC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_40814220_DOC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_40814220_DOC_3"><text>-</text></inputEntry>
        <inputEntry id="IE_40814220_DOC_4"><text>-</text></inputEntry>
        <inputEntry id="IE_40814220_DOC_5"><text>"SemImagem","Sem exames"</text></inputEntry>
        <inputEntry id="IE_40814220_DOC_6"><text>-</text></inputEntry>
        <outputEntry id="OE_40814220_DOC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_40814220_DOC_2"><text>"TC ou RNM obrigatoria para documentacao do procedimento"</text></outputEntry>
        <outputEntry id="OE_40814220_DOC_3"><text>"AANS/CNS 2024, ANS RN 465/2021 - Imagem obrigatoria"</text></outputEntry>
        <outputEntry id="OE_40814220_DOC_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_40814220_DOC_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_JanelaPrecoce">
        <inputEntry id="IE_5_1"><text>"avc_grande_vaso"</text></inputEntry>
        <inputEntry id="IE_5_2"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_5_3"><text>&lt;= 6</text></inputEntry>
        <inputEntry id="IE_5_4"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_5_5"><text>-</text></inputEntry>
        <inputEntry id="IE_5_6"><text>-</text></inputEntry>
        <outputEntry id="OE_5_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5_2"><text>"AVC isquêmico agudo com oclusão de grande vaso (≤6h). NIHSS ≥6, ASPECTS ≥6. Trombectomia mecânica URGENTE indicada."</text></outputEntry>
        <outputEntry id="OE_5_3"><text>"Diretrizes ABN/SBAN 2024 + AHA/ASA 2024 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_JanelaPrecoce_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_JanelaPrecoce_5"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Aprovado_JanelaTardia">
        <inputEntry id="IE_6_1"><text>"avc_grande_vaso"</text></inputEntry>
        <inputEntry id="IE_6_2"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_6_3"><text>&gt; 6, &lt;= 24</text></inputEntry>
        <inputEntry id="IE_6_4"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_6_5"><text>-</text></inputEntry>
        <inputEntry id="IE_6_6"><text>-</text></inputEntry>
        <outputEntry id="OE_6_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_6_2"><text>"AVC isquêmico 6-24h com perfil perfusional favorável (ASPECTS ≥6, penumbra salvável). Trombectomia indicada após avaliação por angioTC/RM."</text></outputEntry>
        <outputEntry id="OE_6_3"><text>"AHA/ASA Guidelines 2024 (DEFUSE-3, DAWN trials)"</text></outputEntry>
        <outputEntry id="OE_6_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_6_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_40814220">
        <inputEntry id="IE_7_1"><text>-</text></inputEntry>
        <inputEntry id="IE_7_2"><text>-</text></inputEntry>
        <inputEntry id="IE_7_3"><text>-</text></inputEntry>
        <inputEntry id="IE_7_4"><text>-</text></inputEntry>
        <inputEntry id="IE_7_5"><text>-</text></inputEntry>
        <inputEntry id="IE_7_6"><text>-</text></inputEntry>
        <outputEntry id="OE_7_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_7_2"><text>"Requer avaliação URGENTE por neurologista vascular/neurorradiologista intervencionista. Tempo é cérebro!"</text></outputEntry>
        <outputEntry id="OE_7_3"><text>"Análise emergencial conforme RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_40814220_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_40814220_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
