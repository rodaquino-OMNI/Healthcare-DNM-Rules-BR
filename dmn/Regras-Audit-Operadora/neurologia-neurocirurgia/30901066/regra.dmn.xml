        <inputEntry id="InputEntry_7_1"><text>"DefeitoCongenito"</text></inputEntry>
        <inputEntry id="InputEntry_7_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_7_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_7_4"><text>"TC3DCompleta"</text></inputEntry>
