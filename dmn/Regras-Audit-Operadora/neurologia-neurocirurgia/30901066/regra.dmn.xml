        <inputEntry id="InputEntry_6_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_6_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_6_2"><text>"Defeito traumatico - reconstrucao craniana indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_6_3"><text>"AANS/CNS 2023 - Cranioplastia para defeitos traumaticos"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Traumatico_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Traumatico_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Congenito">
