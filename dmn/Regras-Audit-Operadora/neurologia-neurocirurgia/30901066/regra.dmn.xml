        <outputEntry id="OutputEntry_4_2"><text>"TC craniana obrigatoria para planejamento de cranioplastia"</text></outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"AANS/CNS 2023 - TC 3D essencial para confeccao de protese"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_30901066_DUP_Reop">
        <description>DUP: Reoperacao cranioplastia em menos de 90 dias</description>
        <inputEntry id="IE_30901066_DUP_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901066_DUP_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30901066_DUP_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30901066_DUP_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30901066_DUP_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901066_DUP_6"><text>&lt; 90</text></inputEntry>
        <outputEntry id="OE_30901066_DUP_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30901066_DUP_2"><text>"Cranioplastia repetida em menos de 90 dias. Documentar complicacao que justifique reoperacao"</text></outputEntry>
        <outputEntry id="OE_30901066_DUP_3"><text>"AANS/CNS 2024 - Reoperacao precoce requer justificativa documentada"</text></outputEntry>
        <outputEntry id="OE_30901066_DUP_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_30901066_DUP_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_30901066_DOC_Imagem">
        <description>DOC: Procedimento sem imagem atual documentada</description>
        <inputEntry id="IE_30901066_DOC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901066_DOC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30901066_DOC_3"><text>"Classe I","Classe IIa"</text></inputEntry>
