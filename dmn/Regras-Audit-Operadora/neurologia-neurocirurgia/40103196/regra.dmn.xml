<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             id="Definitions_40103196" name="EEGq_mapeamento_cerebral" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 40103196 - EEG quantitativo com mapeamento cerebral
    Versao: 3.1.0-LEAN-TIER2 (2026-02-08)

    TIER-2 ENRICHMENT:
    - Input 5: examesComprobatorios (documentation quality)
    - Input 6: ultimoProcedimentoDias (frequency control)
    - FREQ rule: EEGq repetido < 180 dias
    - 5-output format: resultado, observacao, fundamentacao, alertasDesperdicio, acaoRecomendada

    Indicacoes Clinicas: Epilepsia refratária (investigação pré-cirúrgica), Encefalopatia de causa indeterminada, Disfunção cognitiva progressiva
    Referencias: RN ANS 465/2021, ABN 2024, SBN 2023, AAN 2024
  -->
  <decision id="Decision_40103196" name="Indicacao para EEGq_mapeamento_cerebral">
    <decisionTable id="DecisionTable_40103196" hitPolicy="FIRST">
      <input id="Input_1" label="Indicacao">
        <inputExpression id="InputExpression_1" typeRef="string"><text>indicacao</text></inputExpression>
        <inputValues id="InputValues_1">
          <text>"epilepsia_refrataria","encefalopatia","disfuncao_cognitiva","outra"</text>
        </inputValues>
      </input>
      <input id="Input_2" label="Eegconvencional Realizado">
        <inputExpression id="InputExpression_2" typeRef="boolean"><text>eegConvencional_realizado</text></inputExpression>
      </input>
      <input id="Input_3" label="Laudoneurologico">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>laudoNeurologico</text></inputExpression>
      </input>
      <input id="Input_4" label="Objetivo">
        <inputExpression id="InputExpression_4" typeRef="string"><text>objetivo</text></inputExpression>
        <inputValues id="InputValues_4">
          <text>"pre_cirurgico","diagnostico","seguimento"</text>
        </inputValues>
      </input>
      <input id="Input_5" label="Exames Comprobatorios">
        <inputExpression id="InputExpression_5" typeRef="string"><text>examesComprobatorios</text></inputExpression>
        <inputValues id="InputValues_5">
          <text>"Completo","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string"/>
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="Rule_Contraindicacao_SemEEGConvencional">
        <inputEntry id="IE_1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_1_2"><text>"EEG convencional não realizado. Requisito obrigatório antes do EEGq."</text></outputEntry>
        <outputEntry id="OE_1_3"><text>"ABN 2024"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SemEEGConvencional_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SemEEGConvencional_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- WASTE PREVENTION RULES -->
      <rule id="Rule_40103196_FREQ_EEG">
        <description>FREQ: EEGq repetido em menos de 180 dias</description>
        <inputEntry id="IE_40103196_FREQ_1"><text>-</text></inputEntry>
        <inputEntry id="IE_40103196_FREQ_2"><text>-</text></inputEntry>
        <inputEntry id="IE_40103196_FREQ_3"><text>-</text></inputEntry>
        <inputEntry id="IE_40103196_FREQ_4"><text>-</text></inputEntry>
        <inputEntry id="IE_40103196_FREQ_5"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="IE_40103196_FREQ_6"><text>&lt; 180</text></inputEntry>
        <outputEntry id="OE_40103196_FREQ_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_40103196_FREQ_2"><text>"EEGq repetido em menos de 180 dias. Limite 2x/ano"</text></outputEntry>
        <outputEntry id="OE_40103196_FREQ_3"><text>"SBN 2023, AAN 2024 - EEG limitado a 2x/ano sem indicacao especifica"</text></outputEntry>
        <outputEntry id="OE_40103196_FREQ_4"><text>"FREQ"</text></outputEntry>
        <outputEntry id="OE_40103196_FREQ_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_EpilepsiaRefrataria">
        <inputEntry id="IE_2_1"><text>"epilepsia_refrataria"</text></inputEntry>
        <inputEntry id="IE_2_2"><text>true</text></inputEntry>
        <inputEntry id="IE_2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_2_4"><text>"pre_cirurgico"</text></inputEntry>
        <inputEntry id="IE_2_5"><text>"Completo"</text></inputEntry>
        <inputEntry id="IE_2_6"><text>-</text></inputEntry>
        <outputEntry id="OE_2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_2_2"><text>"Epilepsia refratária com EEG convencional prévio. EEGq para mapeamento pré-cirúrgico indicado."</text></outputEntry>
        <outputEntry id="OE_2_3"><text>"Diretrizes ABN 2024 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_EpilepsiaRefrataria_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_EpilepsiaRefrataria_5"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_40103196">
        <inputEntry id="IE_3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_3_5"><text>-</text></inputEntry>
        <inputEntry id="IE_3_6"><text>-</text></inputEntry>
        <outputEntry id="OE_3_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_3_2"><text>"Requer avaliação por especialista e auditor médico."</text></outputEntry>
        <outputEntry id="OE_3_3"><text>"Análise caso a caso conforme RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_40103196_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_40103196_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
