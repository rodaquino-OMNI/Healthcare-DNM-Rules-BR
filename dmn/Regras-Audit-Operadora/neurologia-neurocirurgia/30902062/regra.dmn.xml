        <outputEntry id="OE_30902062_DOC1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_30902062_DOC1_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APPROVAL RULES -->
