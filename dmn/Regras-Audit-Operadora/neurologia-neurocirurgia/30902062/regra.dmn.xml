        <outputEntry id="OE_Contra3_3"><text>"AOSpine 2023 - tratamento de osteoporose obrigatorio pre-operatorio"</text></outputEntry>
        <outputEntry id="OE_Rule_30902062_Contraindicacao_3_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_30902062_Contraindicacao_3_5"><text>"NEGAR"</text></outputEntry>
