        <inputEntry id="IE_30902062_DUP1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30902062_DUP1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30902062_DUP1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30902062_DUP1_6"><text>&lt; 365</text></inputEntry>
        <outputEntry id="OE_30902062_DUP1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_30902062_DUP1_2"><text>"PLIF no mesmo segmento realizado ha menos de 365 dias. Fusao requer 6-12 meses para consolidacao. Verificar pseudoartrose documentada em TC"</text></outputEntry>
        <outputEntry id="OE_30902062_DUP1_3"><text>"NASS 2024 - pseudoartrose requer TC confirmatorio antes de revisao"</text></outputEntry>
