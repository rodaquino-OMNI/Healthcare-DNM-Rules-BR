<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             id="Definitions_30737079" name="Descompressao_Tunel_Carpo_Videoartroscopica" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 30737079 - Descompressão do túnel do carpo por videoartroscopia
    Versao: 3.1.0-LEAN-TIER2 (2026-02-08)
    Enriquecido com: Input 6 (ultimoProcedimentoDias), DUP/DOC/ESC waste rules

    Indicacoes Clinicas (DUT/ANS):
    - Síndrome do túnel do carpo (STC) confirmada por eletroneuromiografia
    - Sintomas persistentes após 6 meses de tratamento conservador
    - Sintomas moderados a graves (parestesias noturnas, atrofia tenar, perda de força)
    - Falha de tratamento com imobilização e corticoide

    Contraindicacoes:
    - STC leve sem falha de tratamento conservador
    - Anatomia atípica do nervo mediano (bifurcação alta)
    - Infecção local ativa
    - Artrite reumatoide grave do punho com deformidades
    - Tenossinovite flexora não tratada

    Referencias:
    - Diretrizes SBOT (Sociedade Brasileira de Ortopedia e Traumatologia) 2024
    - RN ANS 465/2021
    - American Academy of Orthopaedic Surgeons (AAOS) Clinical Practice Guideline 2023
  -->
  <decision id="Decision_30737079" name="Indicacao para Descompressao do Tunel do Carpo Videoartroscopica">
    <decisionTable id="DecisionTable_30737079" hitPolicy="FIRST">
      <!-- Input 1: Diagnóstico confirmado por ENMG -->
      <input id="Input_1" label="ENMG Confirmando STC">
        <inputExpression id="InputExpression_1" typeRef="boolean"><text>enmgConfirmaSTC</text></inputExpression>
      </input>
      <!-- Input 2: Gravidade dos sintomas -->
      <input id="Input_2" label="Gravidade Sintomas">
        <inputExpression id="InputExpression_2" typeRef="string"><text>gravidadeSintomas</text></inputExpression>
        <inputValues id="InputValues_2">
          <text>"leve","moderado","grave"</text>
        </inputValues>
      </input>
      <!-- Input 3: Tempo de tratamento conservador (meses) -->
      <input id="Input_3" label="Tempo Tratamento Conservador (meses)">
        <inputExpression id="InputExpression_3" typeRef="number"><text>tempoTratamentoConservador</text></inputExpression>
      </input>
      <!-- Input 4: Laudo ortopédico/neurológico -->
      <input id="Input_4" label="Laudo Especializado">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>laudoEspecializado</text></inputExpression>
      </input>
      <!-- Input 5: Classificação de qualidade de evidência -->
      <input id="Input_5" label="Classificacao Evidencia">
        <inputExpression id="InputExpression_5" typeRef="string"><text>classificacaoEvidencia</text></inputExpression>
        <inputValues id="InputValues_5">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>
      <!-- Input 6: Dias desde último procedimento -->
      <input id="Input_6" label="Exames Comprobatorios">
        <inputExpression id="InputExpression_6" typeRef="string"><text>examesComprobatorios</text></inputExpression>
        <inputValues id="InputValues_6">
          <text>"ENMG","RNM","USG","Completo","Parcial","SemImagem","Sem exames"</text>
        </inputValues>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <!-- Output 1: Resultado da avaliação -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <!-- Output 2: Observação clínica -->
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <!-- Output 3: Fundamentação legal/técnica -->
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string"/>
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- CONTRAINDICATIONS -->
      <rule id="Rule_Contraindicacao_SemENMG">
        <inputEntry id="IE_1_1"><text>false</text></inputEntry>
        <inputEntry id="IE_1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_1_6"><text>-</text></inputEntry>
        <inputEntry id="IE_1_7"><text>-</text></inputEntry>
        <outputEntry id="OE_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_1_2"><text>"Diagnóstico de STC não confirmado por eletroneuromiografia. Exame obrigatório para indicação cirúrgica."</text></outputEntry>
        <outputEntry id="OE_1_3"><text>"Diretrizes SBOT 2024 + AAOS 2023 - Requisito diagnóstico"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SemENMG_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_SemENMG_5"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Contraindicacao_Leve_TratInsuficiente">
        <inputEntry id="IE_2_1"><text>true</text></inputEntry>
        <inputEntry id="IE_2_2"><text>"leve"</text></inputEntry>
        <inputEntry id="IE_2_3"><text>&lt; 6</text></inputEntry>
        <inputEntry id="IE_2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_2_5"><text>-</text></inputEntry>
        <inputEntry id="IE_2_6"><text>-</text></inputEntry>
        <inputEntry id="IE_2_7"><text>-</text></inputEntry>
        <outputEntry id="OE_2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2_2"><text>"STC leve com tratamento conservador insuficiente. Necessário mínimo 6 meses de imobilização noturna e infiltração com corticoide antes de indicação cirúrgica."</text></outputEntry>
        <outputEntry id="OE_2_3"><text>"AAOS Clinical Practice Guideline 2023"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_Leve_TratInsuficiente_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraindicacao_Leve_TratInsuficiente_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- WASTE RULES -->
      <rule id="Rule_30737079_DUP_NervoPerif">
        <description>DUP: Cirurgia de nervo periferico repetida em menos de 180 dias</description>
        <inputEntry id="IE_30737079_DUP_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DUP_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DUP_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DUP_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DUP_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DUP_6"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DUP_7"><text>&lt; 180</text></inputEntry>
        <outputEntry id="OE_30737079_DUP_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_30737079_DUP_2"><text>"Cirurgia de nervo periferico repetida em menos de 180 dias. Aguardar recuperacao neural"</text></outputEntry>
        <outputEntry id="OE_30737079_DUP_3"><text>"SBN 2023 - Intervalo minimo 180 dias para reavaliar neurolise/neurorrafia"</text></outputEntry>
        <outputEntry id="OE_30737079_DUP_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_30737079_DUP_5"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <rule id="Rule_30737079_DOC_ENMG">
        <description>DOC: Cirurgia sem ENMG previa</description>
        <inputEntry id="IE_30737079_DOC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DOC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DOC_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DOC_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_DOC_5"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="IE_30737079_DOC_6"><text>"SemImagem","Sem exames"</text></inputEntry>
        <inputEntry id="IE_30737079_DOC_7"><text>-</text></inputEntry>
        <outputEntry id="OE_30737079_DOC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30737079_DOC_2"><text>"ENMG obrigatoria para documentar lesao neural antes de cirurgia"</text></outputEntry>
        <outputEntry id="OE_30737079_DOC_3"><text>"SBN 2023, ANS RN 465/2021 - ENMG obrigatoria para cirurgia de nervo periferico"</text></outputEntry>
        <outputEntry id="OE_30737079_DOC_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_30737079_DOC_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_30737079_ESC_Conservador">
        <description>ESC: Cirurgia sem tratamento conservador previo</description>
        <inputEntry id="IE_30737079_ESC_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_ESC_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_ESC_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_ESC_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_ESC_5"><text>"Classe IIa","Classe IIb"</text></inputEntry>
        <inputEntry id="IE_30737079_ESC_6"><text>-</text></inputEntry>
        <inputEntry id="IE_30737079_ESC_7"><text>-</text></inputEntry>
        <outputEntry id="OE_30737079_ESC_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30737079_ESC_2"><text>"Documentar falha de tratamento conservador (imobilizacao, fisioterapia, corticoide)"</text></outputEntry>
        <outputEntry id="OE_30737079_ESC_3"><text>"SBN 2023, AAN 2024 - Tratamento conservador 4-12 semanas antes de cirurgia"</text></outputEntry>
        <outputEntry id="OE_30737079_ESC_4"><text>"ESC"</text></outputEntry>
        <outputEntry id="OE_30737079_ESC_5"><text>"SUGERIR_ALTERNATIVA"</text></outputEntry>
      </rule>

      <!-- APPROVALS -->
      <rule id="Rule_Aprovado_Moderado">
        <inputEntry id="IE_3_1"><text>true</text></inputEntry>
        <inputEntry id="IE_3_2"><text>"moderado"</text></inputEntry>
        <inputEntry id="IE_3_3"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_3_4"><text>true</text></inputEntry>
        <inputEntry id="IE_3_5"><text>-</text></inputEntry>
        <inputEntry id="IE_3_6"><text>-</text></inputEntry>
        <inputEntry id="IE_3_7"><text>-</text></inputEntry>
        <outputEntry id="OE_3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_3_2"><text>"STC moderada confirmada por ENMG, refratária a 6+ meses de tratamento conservador. Descompressão videoartroscópica indicada com menor agressão tecidual."</text></outputEntry>
        <outputEntry id="OE_3_3"><text>"Diretrizes SBOT 2024 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Moderado_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Moderado_5"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Aprovado_Grave">
        <inputEntry id="IE_4_1"><text>true</text></inputEntry>
        <inputEntry id="IE_4_2"><text>"grave"</text></inputEntry>
        <inputEntry id="IE_4_3"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="IE_4_4"><text>true</text></inputEntry>
        <inputEntry id="IE_4_5"><text>-</text></inputEntry>
        <inputEntry id="IE_4_6"><text>-</text></inputEntry>
        <inputEntry id="IE_4_7"><text>-</text></inputEntry>
        <outputEntry id="OE_4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_4_2"><text>"STC grave confirmada por ENMG (atrofia tenar, perda de força, parestesias contínuas). Descompressão cirúrgica urgente indicada após mínimo 3 meses de tratamento para evitar lesão permanente do nervo mediano."</text></outputEntry>
        <outputEntry id="OE_4_3"><text>"Diretrizes SBOT 2024 + AAOS 2023 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Grave_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Grave_5"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Aprovado_Leve_TratProlongado">
        <inputEntry id="IE_5_1"><text>true</text></inputEntry>
        <inputEntry id="IE_5_2"><text>"leve"</text></inputEntry>
        <inputEntry id="IE_5_3"><text>&gt;= 12</text></inputEntry>
        <inputEntry id="IE_5_4"><text>true</text></inputEntry>
        <inputEntry id="IE_5_5"><text>-</text></inputEntry>
        <inputEntry id="IE_5_6"><text>-</text></inputEntry>
        <inputEntry id="IE_5_7"><text>-</text></inputEntry>
        <outputEntry id="OE_5_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5_2"><text>"STC leve confirmada por ENMG, porém refratária a 12+ meses de tratamento conservador intensivo. Descompressão videoartroscópica indicada para sintomas persistentes."</text></outputEntry>
        <outputEntry id="OE_5_3"><text>"Diretrizes SBOT 2024 + RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Leve_TratProlongado_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Leve_TratProlongado_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK RULE -->
      <rule id="Rule_Fallback_30737079">
        <inputEntry id="IE_6_1"><text>-</text></inputEntry>
        <inputEntry id="IE_6_2"><text>-</text></inputEntry>
        <inputEntry id="IE_6_3"><text>-</text></inputEntry>
        <inputEntry id="IE_6_4"><text>-</text></inputEntry>
        <inputEntry id="IE_6_5"><text>-</text></inputEntry>
        <inputEntry id="IE_6_6"><text>-</text></inputEntry>
        <inputEntry id="IE_6_7"><text>-</text></inputEntry>
        <outputEntry id="OE_6_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_6_2"><text>"Caso não enquadrado nas regras automáticas. Requer avaliação por ortopedista/neurocirurgião e auditor médico."</text></outputEntry>
        <outputEntry id="OE_6_3"><text>"Análise caso a caso conforme RN ANS 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_30737079_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Fallback_30737079_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
