      <rule id="Rule_30901082_DUP_Tumor">
        <description>DUP: Reoperacao de tumor cerebral em menos de 90 dias sem recidiva documentada</description>
        <inputEntry id="IE_30901082_DUP_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30901082_DUP_2"><text>"Incompleto","NaoDocumentado"</text></inputEntry>
        <inputEntry id="IE_30901082_DUP_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30901082_DUP_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30901082_DUP_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30901082_DUP_6"><text>&lt; 90</text></inputEntry>
        <outputEntry id="OE_30901082_DUP_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30901082_DUP_2"><text>"Reoperacao tumoral em menos de 90 dias. Documentar recidiva ou progressao com RNM comparativa"</text></outputEntry>
        <outputEntry id="OE_30901082_DUP_3"><text>"AANS/CNS Brain Tumors 2024, NCCN 2024 - Reoperacao precoce requer evidencia de progressao tumoral"</text></outputEntry>
