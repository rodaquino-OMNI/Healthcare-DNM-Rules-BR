        <inputEntry id="InputEntry_8_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_8_4"><text>"RMContrasteAngioRM","RMContraste"</text></inputEntry>
        <inputEntry id="InputEntry_8_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_6"><text>-</text></inputEntry>
