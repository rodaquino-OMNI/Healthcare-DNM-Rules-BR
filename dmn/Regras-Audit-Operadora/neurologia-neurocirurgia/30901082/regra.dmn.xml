        <outputEntry id="OE_Rule_Aprovado_Sintomatico_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_Sintomatico_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Crescimento">
        <description>Aprovado: Meningioma com crescimento documentado</description>
        <inputEntry id="InputEntry_7_1"><text>"MeningiomaCrescimento"</text></inputEntry>
        <inputEntry id="InputEntry_7_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_7_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_7_4"><text>"RMContrasteAngioRM","RMContraste"</text></inputEntry>
        <inputEntry id="InputEntry_7_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_6"><text>-</text></inputEntry>
