        <outputEntry id="OE_Aprov3_3"><text>"NASS 2024 - TLIF preferido por menor taxa de retropulsao de cage e lesao neural"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Aprovacao_3_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Aprovacao_3_5"><text>"APROVAR"</text></outputEntry>
