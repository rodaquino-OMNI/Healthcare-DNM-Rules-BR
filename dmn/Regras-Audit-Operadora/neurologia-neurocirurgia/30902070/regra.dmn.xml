        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Procedimento classificado como Classe III - contraindicado pelas diretrizes atuais"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"NASS 2024, AOSpine 2023 - Classe III: nao recomendado"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Contraindicacao_1_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Contraindicacao_1_5"><text>"NEGAR"</text></outputEntry>
      </rule>

