        <inputEntry id="IE_30902070_DOC1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_30902070_DOC1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30902070_DOC1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30902070_DOC1_4"><text>"Sem exames"</text></inputEntry>
        <inputEntry id="IE_30902070_DOC1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30902070_DOC1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_30902070_DOC1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_30902070_DOC1_2"><text>"Documentacao obrigatoria ausente: TLIF requer RNM, TC (anatomia do forame e pediculo) e RX dinamico para planejamento cirurgico"</text></outputEntry>
        <outputEntry id="OE_30902070_DOC1_3"><text>"NASS 2024 - TC fundamental para dimensionamento de cage e pediculos"</text></outputEntry>
        <outputEntry id="OE_30902070_DOC1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_30902070_DOC1_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- APPROVAL RULES -->
      <rule id="Rule_30902070_Aprovacao_1">
