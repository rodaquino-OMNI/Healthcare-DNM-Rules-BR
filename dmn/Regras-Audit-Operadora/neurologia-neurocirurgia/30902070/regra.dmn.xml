        <inputEntry id="IE_Contra2_1"><text>"Espondilolistese alto grau"</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_6"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Espondilolistese grau III-IV: TLIF isolado e insuficiente. Requer tecnica de reducao e possivelmente via anterior combinada (ALIF + posterior)"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"AOSpine 2023 - alto grau requer abordagem especifica com reducao"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Contraindicacao_2_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Contraindicacao_2_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_30902070_Contraindicacao_3">
