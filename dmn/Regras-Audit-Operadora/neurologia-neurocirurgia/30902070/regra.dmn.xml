        <inputEntry id="IE_Contra4_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra4_5"><text>false</text></inputEntry>
        <inputEntry id="IE_Contra4_6"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra4_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra4_2"><text>"Expectativa de vida limitada. Fusao requer 6-12 meses para consolidacao. Preferir tratamento conservador ou descompressao isolada"</text></outputEntry>
        <outputEntry id="OE_Contra4_3"><text>"Principios de medicina baseada em valor - cirurgia de fusao contraindicada"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Contraindicacao_4_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Contraindicacao_4_5"><text>"NEGAR"</text></outputEntry>
