        <inputEntry id="IE_30902070_DUP1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_30902070_DUP1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_30902070_DUP1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_30902070_DUP1_5"><text>-</text></inputEntry>
        <inputEntry id="IE_30902070_DUP1_6"><text>&lt; 365</text></inputEntry>
        <outputEntry id="OE_30902070_DUP1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_30902070_DUP1_2"><text>"TLIF no mesmo segmento realizado ha menos de 365 dias. Fusao requer 6-12 meses para consolidacao. Documentar pseudoartrose em TC antes de revisao"</text></outputEntry>
        <outputEntry id="OE_30902070_DUP1_3"><text>"AOSpine 2023 - revisao de fusao requer TC confirmatorio de pseudoartrose"</text></outputEntry>
        <outputEntry id="OE_30902070_DUP1_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_30902070_DUP1_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_30902070_ESC_1">
