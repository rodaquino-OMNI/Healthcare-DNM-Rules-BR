        <inputEntry id="IE_Aprov1_1"><text>"Espondilolistese I-II com radiculopatia"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>"RNM + TC + RX dinamico","RNM + TC"</text></inputEntry>
        <inputEntry id="IE_Aprov1_5"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_6"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Espondilolistese degenerativa com radiculopatia comprovada. TLIF permite descompressao foraminal e fusao com acesso unilateral, menor morbidade"</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"NASS 2024 - TLIF indicado para patologia foraminal unilateral com instabilidade"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Aprovacao_1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_30902070_Aprovacao_1_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_30902070_Aprovacao_2">
