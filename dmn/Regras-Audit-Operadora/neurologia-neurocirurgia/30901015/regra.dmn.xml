        <inputEntry id="InputEntry_Aprovado_CIIa_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_observacao"><text>"Indicacao Classe IIa - procedimento razoavel e provavelmente util"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIa_fundamentacao"><text>"AANS/CNS 2024 - Classe IIa: Beneficio provavel supera riscos"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseIIa_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseIIa_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIb">
        <description>Aprovado: Indicacao Classe IIb com documentacao completa</description>
        <inputEntry id="InputEntry_Aprovado_CIIb_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_4"><text>"RMComContrasteLaudada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIb_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_observacao"><text>"Indicacao Classe IIb - procedimento pode ser considerado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CIIb_fundamentacao"><text>"AANS/CNS 2024 - Classe IIb: Utilidade nao bem estabelecida mas pode ser considerado"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseIIb_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseIIb_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ========== RULES - PENDENTES ========== -->

      <rule id="Rule_Pendente_ImagemSemContraste">
        <description>Pendente: Imagem sem contraste - necessita complementacao</description>
        <inputEntry id="InputEntry_Pendente_Img_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Pendente_Img_3"><text>"Classe I","Classe IIa"</text></inputEntry>
