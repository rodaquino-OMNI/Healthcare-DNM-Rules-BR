      <rule id="Rule_Aprovado_ClasseI_TumorPrimario">
        <description>Aprovado: Tumor primario ressecavel com Classe I</description>
        <inputEntry id="InputEntry_Aprovado_CI_Prim_1"><text>"TumorPrimarioRessecavel"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Prim_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Prim_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Prim_4"><text>"RMComContrasteLaudada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Prim_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Prim_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Prim_observacao"><text>"Tumor primario ressecavel com indicacao Classe I - cirurgia indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Prim_fundamentacao"><text>"AANS/CNS Brain Tumors 2024, NCCN 2024 - Classe I: Resseccao maxima segura e indicada para tumores primarios"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseI_TumorPrimario_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseI_TumorPrimario_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_Metastase">
        <description>Aprovado: Metastase cerebral unica com Classe I</description>
        <inputEntry id="InputEntry_Aprovado_CI_Met_1"><text>"TumorMetastaticoUnico"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Met_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Met_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Met_4"><text>"RMComContrasteLaudada"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Met_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Met_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Met_observacao"><text>"Metastase unica ressecavel - indicacao cirurgica classe I"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Met_fundamentacao"><text>"AANS/CNS 2024, NCCN CNS Cancers 2024 - Metastase unica acessivel com doenca sistemica controlada"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseI_Metastase_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseI_Metastase_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseI_EfeitoMassa">
        <description>Aprovado: Tumor com efeito de massa sintomatico</description>
        <inputEntry id="InputEntry_Aprovado_CI_Massa_1"><text>"TumorComEfeitoMassa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Massa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Massa_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CI_Massa_4"><text>"RMComContrasteLaudada","TCComContraste"</text></inputEntry>
