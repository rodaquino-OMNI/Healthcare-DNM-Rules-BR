        <outputEntry id="OutputEntry_Pendente_Doc_observacao"><text>"Documentacao clinica incompleta - solicitar complementacao"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Doc_fundamentacao"><text>"ANS RN 465/2021 - Documentacao adequada necessaria para autorizacao"</text></outputEntry>
        <outputEntry id="OE_Rule_Pendente_DocIncompleta_4"><text>"DOC"</text></outputEntry>
