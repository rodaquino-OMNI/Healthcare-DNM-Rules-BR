        <outputEntry id="OutputEntry_Pendente_Img_observacao"><text>"Necessaria imagem com contraste para adequada avaliacao tumoral"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_Img_fundamentacao"><text>"AANS/CNS 2024 - RM com gadolinio padrao ouro para avaliacao de tumores cerebrais"</text></outputEntry>
