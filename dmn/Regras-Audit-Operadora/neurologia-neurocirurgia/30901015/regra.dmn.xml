        <outputEntry id="OutputEntry_Reprovado_Img_observacao"><text>"Obrigatorio exame de imagem para planejamento cirurgico"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Img_fundamentacao"><text>"AANS/CNS 2024, ANS RN 465/2021 - RM ou TC com contraste obrigatoria para cirurgia de tumor cerebral"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Reprovado_SemImagem_5"><text>"NEGAR"</text></outputEntry>
      </rule>

