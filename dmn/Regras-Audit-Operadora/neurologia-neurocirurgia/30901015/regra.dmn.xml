        <inputEntry id="InputEntry_Contraind_Irres_1"><text>"TumorIrressecavel"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Irres_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Irres_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Irres_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Irres_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_Irres_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_Irres_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Irres_observacao"><text>"Tumor classificado como irressecavel - considerar biopsia ou tratamento alternativo"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_Irres_fundamentacao"><text>"AANS/CNS 2024 - Tumores irressecaveis em areas eloquentes nao sao candidatos a craniotomia ressectiva"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_TumorIrressecavel_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_Contraind_TumorIrressecavel_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_SemImagem">
        <description>Reprovado: Ausencia de exame de imagem</description>
        <inputEntry id="InputEntry_Reprovado_Img_1"><text>-</text></inputEntry>
