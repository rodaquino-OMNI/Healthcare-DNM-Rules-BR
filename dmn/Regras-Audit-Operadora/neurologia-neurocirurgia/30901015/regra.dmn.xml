        <outputEntry id="OutputEntry_Aprovado_CI_Massa_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Massa_observacao"><text>"Tumor com efeito de massa - descompressao cirurgica indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_CI_Massa_fundamentacao"><text>"AANS/CNS 2024, SBN 2023 - Efeito de massa com sintomas neurologicos requer descompressao"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseI_EfeitoMassa_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_Aprovado_ClasseI_EfeitoMassa_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_ClasseIIa">
        <description>Aprovado: Indicacao Classe IIa com documentacao adequada</description>
        <inputEntry id="InputEntry_Aprovado_CIIa_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_CIIa_3"><text>"Classe IIa"</text></inputEntry>
