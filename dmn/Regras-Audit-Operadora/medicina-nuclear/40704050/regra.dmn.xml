<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_40704050"
             name="Cistografia radioisotópica direta"
             namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 40704050 - Cistografia radioisotópica direta
    Versao: 2.0.0 (2026-02-06)

    Indicacoes: Conforme protocolo ANS/TISS
    Contraindicacoes: Verificar regras especificas
    Referencias: ANS/TISS 2024, Resolucao Normativa
  -->
  <decision id="Decision_40704050" name="Decisão de Elegibilidade para Cistografia Radioisotópica Direta">
    <decisionTable id="DecisionTable_40704050" hitPolicy="FIRST">
      <input id="Input_1" label="Presença de Infecção do Trato Urinário (ITU)">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>presencaITU</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Indicação para Refluxo Vesicoureteral">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>indicacaoRefluxo</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" name="resultado" typeRef="string">
        <outputValues><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" name="fundamentacao" typeRef="string"/>
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues>
      </output>
      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>
      <rule id="Rule_40704050_1">
        <description>Paciente com ITU ativa não é elegível.</description>
        <inputEntry id="InputEntry_40704050_1_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_40704050_1_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_1_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_1_3">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_1_4">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_1_5">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_40704050_2">
        <description>Paciente sem indicação clínica para refluxo vesicoureteral.</description>
        <inputEntry id="InputEntry_40704050_2_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_40704050_2_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_2_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_2_3">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_2_4">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_2_5">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_40704050_3">
        <description>Paciente elegível para o procedimento.</description>
        <inputEntry id="InputEntry_40704050_3_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_40704050_3_2">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_3_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_3_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_3_4">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40704050_3_5">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
    
    <rule id="Rule_Default_40704050">
      <description>Regra padrao - Criterios nao atendidos</description>
      <inputEntry id="InputEntry_Default_40704050_1">
        <text></text>
      </inputEntry>
      <inputEntry id="InputEntry_Default_40704050_2">
        <text></text>
      </inputEntry>
      <outputEntry id="OutputEntry_Rule_Default_40704050_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40704050_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40704050_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40704050_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40704050_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
    </rule>
  </decisionTable>
  </decision>
</definitions>