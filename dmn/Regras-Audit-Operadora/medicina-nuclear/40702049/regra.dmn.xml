  <decision id="Decision_40702049" name="Escolha do Radiofármaco para Cintilografia de Sangramento Intestinal">
    <decisionTable id="DecisionTable_40702049" hitPolicy="FIRST">
      <input id="Input_1" label="Tipo de Sangramento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoSangramento</text>
        </inputExpression>
      </input>
      
      <output id="Output_resultado" name="resultado" typeRef="string">
        <outputValues><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" name="fundamentacao" typeRef="string"/>
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues>
      </output>
      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>
      
      <rule id="Rule_40702049_1">
        <inputEntry id="InputEntry_40702049_1_1">
          <text>"Ativo (&gt; 0.1 ml/minuto)"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_1_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_1_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_1_3">
          <text>"99mTc-enxofre coloidal"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_1_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_1_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      
      <rule id="Rule_40702049_2">
        <inputEntry id="InputEntry_40702049_2_1">
          <text>"Intermitente (&gt; 0.3 ml/minuto)"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_2_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_2_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_2_3">
          <text>"99mTc-hemácias"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_2_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40702049_2_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      
    
    <rule id="Rule_Default_40702049">
      <description>Regra padrao - Criterios nao atendidos</description>
      <inputEntry id="InputEntry_Default_40702049_1">
