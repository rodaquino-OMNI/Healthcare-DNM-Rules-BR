<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             id="Definitions_40701140"
             name="Cintilografia do miocardio perfusao - estresse fisico"
             namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 40701140 - Cintilografia do miocardio perfusao - estresse fisico
    Versao: 2.0.0 (2026-02-08 LEAN TIER-2)

    Indicacoes: Avaliacao de isquemia miocardica, perfusao coronariana
    Contraindicacoes: Instabilidade hemodinamica, arritmias graves
    Referencias: ANS/TISS 2024, Diretriz SBC Cardiologia Nuclear
  -->
  <decision id="Decision_40701140" name="Auditoria Cintilografia Miocardio Perfusao">
    <decisionTable id="DecisionTable_40701140" hitPolicy="FIRST">
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpression_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Suspeita_Isquemia","Avaliacao_Viabilidade","Pos_IAM","Estratificacao_Risco","Nenhuma"</text>
        </inputValues>
      </input>
      <input id="Input_documentacaoCompleta" label="Documentacao Completa">
        <inputExpression id="InputExpression_documentacaoCompleta" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>
      <input id="Input_contraindicacaoPresente" label="Contraindicacao Presente">
        <inputExpression id="InputExpression_contraindicacao" typeRef="boolean">
          <text>contraindicacaoPresente</text>
        </inputExpression>
      </input>
      <input id="Input_examesPrevios" label="Exames Previos Realizados">
        <inputExpression id="InputExpression_examesPrevios" typeRef="boolean">
          <text>examesPreviosRealizados</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="Rule_Contraindicacao">
        <description>Contraindicacao presente</description>
        <inputEntry id="InputEntry_1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Contraindicacao presente: instabilidade hemodinamica, arritmias graves ou incapacidade de realizar teste ergometrico."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"Protocolo SBC: contraindicacoes absolutas para teste de estresse."</text></outputEntry>
        <outputEntry id="OutputEntry_4_1"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_1"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_SemDocumentacao">
        <description>Documentacao ausente</description>
        <inputEntry id="InputEntry_1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Documentacao incompleta: necessario laudo medico com indicacao, ECG, exames laboratoriais."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"ANS RN 465: documentacao obrigatoria para procedimentos de media complexidade."</text></outputEntry>
        <outputEntry id="OutputEntry_4_2"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_5_2"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_SemIndicacao">
        <description>Sem indicacao clinica</description>
        <inputEntry id="InputEntry_1_3"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Ausencia de indicacao clinica documentada para cintilografia miocardica."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"DUT ANS: exame indicado para suspeita de isquemia, avaliacao pos-IAM ou estratificacao de risco."</text></outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_3"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_RepeticaoFrequente">
        <description>Exames previos recentes</description>
        <inputEntry id="InputEntry_1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Exame previo recente identificado. Avaliar justificativa para repeticao."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"Protocolo: intervalo minimo 6 meses salvo mudanca de quadro clinico."</text></outputEntry>
        <outputEntry id="OutputEntry_4_4"><text>"FREQ"</text></outputEntry>
        <outputEntry id="OutputEntry_5_4"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado">
        <description>Criterios atendidos</description>
        <inputEntry id="InputEntry_1_5"><text>"Suspeita_Isquemia","Avaliacao_Viabilidade","Pos_IAM","Estratificacao_Risco"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Procedimento aprovado. Indicacao clinica documentada e criterios atendidos."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"Conforme Diretriz SBC Cardiologia Nuclear e DUT ANS para cintilografia miocardica."</text></outputEntry>
        <outputEntry id="OutputEntry_4_5"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Default_40701140">
        <description>Regra padrao - Criterios nao atendidos</description>
        <inputEntry id="InputEntry_Default_1"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_3"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_4"><text></text></inputEntry>
        <outputEntry id="OutputEntry_Default_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Default_2"><text>"Analise adicional necessaria. Criterios de auditoria incompletos."</text></outputEntry>
        <outputEntry id="OutputEntry_Default_3"><text>"Necessario completar documentacao clinica e verificar indicacoes."</text></outputEntry>
        <outputEntry id="OutputEntry_Default_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Default_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
