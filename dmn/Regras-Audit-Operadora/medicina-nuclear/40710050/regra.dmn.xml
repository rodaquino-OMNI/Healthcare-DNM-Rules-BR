<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             id="Definitions_40710050"
             name="Tratamento de hipertireoidismo (Graves)"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="DMNAgent"
             exporterVersion="1.0">
  <!--
    TUSS 40710050 - Tratamento de hipertireoidismo (Graves)
    Versao: 2.0.0 (2026-02-06)

    Indicacoes: Conforme protocolo ANS/TISS
    Contraindicacoes: Verificar regras especificas
    Referencias: ANS/TISS 2024, Resolucao Normativa
  -->
  <decision id="Decision_40710050" name="Elegibilidade para Tratamento de Hipertireoidismo (Graves)">
    <decisionTable id="DecisionTable_40710050" hitPolicy="FIRST">
      <input id="Input_1" label="Preparo Completo (Dieta e Jejum)">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>preparoCompleto</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Diagnóstico">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>diagnostico</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Status" name="resultado" typeRef="string" />
      <output id="Output_2" label="Dose Iodo-131" name="doseIodo131" typeRef="string" />
      <output id="Output_3" label="Instruções" name="instrucoes" typeRef="string" />
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues>
      </output>
      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>
      <rule id="Rule_Reprovado_Preparo">
        <description>Reprova se o preparo (dieta pobre em iodo e jejum de 6h) não for cumprido.</description>
        <inputEntry id="InputEntry_1_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text></text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Não aplicável"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"Preparo incompleto. Paciente deve seguir dieta pobre em iodo e realizar jejum absoluto de 6 horas."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_1"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_5_1"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Reprovado_Diagnostico">
        <description>Reprova se o diagnóstico não for compatível, mesmo com o preparo correto.</description>
        <inputEntry id="InputEntry_1_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>not("Doenças graves", "bócio difuso tóxico")</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Não aplicável"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"Diagnóstico não elegível para este protocolo de tratamento."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_2"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_2"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_Aprovado">
        <description>Aprova o procedimento se o preparo estiver completo e o diagnóstico for compatível.</description>
        <inputEntry id="InputEntry_1_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>"Doenças graves", "bócio difuso tóxico"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"10 a 15 mCi"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"Procedimento aprovado. Administrar dose de Iodo-131 conforme especificado."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_3"><text>"APROVAR"</text></outputEntry>
      </rule>
    
    <rule id="Rule_Default_40710050">
      <description>Regra padrao - Criterios nao atendidos</description>
      <inputEntry id="InputEntry_Default_40710050_1">
        <text></text>
      </inputEntry>
      <inputEntry id="InputEntry_Default_40710050_2">
        <text></text>
      </inputEntry>
      <outputEntry id="OutputEntry_Default_40710050_1">
        <text>"Pendente"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_40710050_2">
        <text>"Não aplicável"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_40710050_3">
        <text>"Critérios não atendidos - avaliar manualmente"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_40710050_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Default_40710050_5"><text>"SOLICITAR_INFO"</text></outputEntry>
    </rule>
  </decisionTable>
  </decision>
</definitions>