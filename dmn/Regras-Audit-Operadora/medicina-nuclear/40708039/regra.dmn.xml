  <!--
    TUSS 40708039 - Cintilografia_com_leucocitos_marcados
    Versao: 2.0.0 (2026-02-06)

    Indicacoes: Conforme protocolo ANS/TISS
    Contraindicacoes: Verificar regras especificas
    Referencias: ANS/TISS 2024, Resolucao Normativa
  -->

  <decision id="Decision_40708039" name="Decisão Cintilografia com Leucócitos Marcados">
    <decisionTable id="DecisionTable_40708039" hitPolicy="FIRST">
      <input id="Input_1" label="Radiofármaco Utilizado">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>radiofarmacoUtilizado</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Ambiente de Manipulação">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>ambienteManipulacao</text>
        </inputExpression>
      </input>
      
      <output id="Output_resultado" name="resultado" typeRef="string">
        <outputValues><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" name="fundamentacao" typeRef="string"/>
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues>
      </output>
      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>
      
      <rule id="Rule_1_40708039_Fail_Radiofarmaco">
        <description>Verifica se o radiofármaco utilizado não é o especificado.</description>
        <inputEntry id="InputEntry_1_1">
          <text><![CDATA[not("Leucócitos marcados com 99mTc-HMPAO")]]></text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_1_40708039_Fail_Radiofarmaco_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_1_40708039_Fail_Radiofarmaco_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_1_40708039_Fail_Radiofarmaco_3">
          <text>"Não Conforme - Radiofármaco Incorreto"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_1_40708039_Fail_Radiofarmaco_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_1_40708039_Fail_Radiofarmaco_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      
      <rule id="Rule_2_40708039_Fail_Ambiente">
        <description>Verifica se o ambiente de manipulação é inadequado.</description>
        <inputEntry id="InputEntry_1_2">
          <text>"Leucócitos marcados com 99mTc-HMPAO"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text><![CDATA[not("Capela de fluxo laminar")]]></text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_2_40708039_Fail_Ambiente_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_2_40708039_Fail_Ambiente_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_2_40708039_Fail_Ambiente_3">
          <text>"Não Conforme - Ambiente de Manipulação Inadequado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_2_40708039_Fail_Ambiente_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_2_40708039_Fail_Ambiente_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>

      <rule id="Rule_3_40708039_Success">
        <description>Aprova o procedimento se todas as condições forem atendidas.</description>
        <inputEntry id="InputEntry_1_3">
          <text>"Leucócitos marcados com 99mTc-HMPAO"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>"Capela de fluxo laminar"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_3_40708039_Success_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_3_40708039_Success_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_3_40708039_Success_3">
          <text>"Conforme"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_3_40708039_Success_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_3_40708039_Success_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      
    
    <rule id="Rule_Default_40708039">
      <description>Regra padrao - Criterios nao atendidos</description>
      <inputEntry id="InputEntry_Default_40708039_1">
        <text></text>
      </inputEntry>
      <inputEntry id="InputEntry_Default_40708039_2">
        <text></text>
      </inputEntry>
      <outputEntry id="OutputEntry_Rule_Default_40708039_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40708039_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40708039_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40708039_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40708039_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
    </rule>
  </decisionTable>
  </decision>
  
</definitions>
