<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_40702057"
             name="Cintilografia para pesquisa de sangramento intestinal intermitente"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    TUSS 40702057 - Cintilografia para pesquisa de sangramento intestinal intermitente
    Versao: 2.0.0 (2026-02-06)

    Indicacoes: Conforme protocolo ANS/TISS
    Contraindicacoes: Verificar regras especificas
    Referencias: ANS/TISS 2024, Resolucao Normativa
  -->

  <decision id="Decision_40702057" name="Análise de Elegibilidade para Cintilografia de Sangramento Intestinal">
    <decisionTable id="DecisionTable_40702057" hitPolicy="FIRST">
      <input id="Input_1" label="Tipo de Sangramento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoSangramento</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Exame Contratado Recente (24h)">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>exameContrastadoRecente</text>
        </inputExpression>
      </input>
      
      <output id="Output_1" label="Status Aprovação" name="status_aprovacao" typeRef="string"/>
      <output id="Output_2" label="Radiofármaco Indicado" name="radiofarmaco_indicado" typeRef="string"/>
      <output id="Output_3" label="Justificativa" name="observacao" typeRef="string"/>
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues>
      </output>
      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>
      
      <rule id="Rule_40702057_1">
        <inputEntry id="InputEntry_1_1">
          <text>"Intermitente"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>"Não"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"99mTc-hemácias"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"Indicado para sangramentos intermitentes (> 0.3 ml/minuto). As hemácias marcadas permanecem em circulação por até 24 horas."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_1"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_1"><text>"APROVAR"</text></outputEntry>
      </rule>
      
      <rule id="Rule_40702057_2">
        <inputEntry id="InputEntry_1_2">
          <text>"Ativo"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>"Não"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"99mTc-enxofre coloidal"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"Indicado para sangramentos em atividade (> 0.1 ml/minuto). O enxofre coloidal é rapidamente depurado, permitindo detecção de sangramento ativo."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_2"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_2"><text>"APROVAR"</text></outputEntry>
      </rule>
      
      <rule id="Rule_40702057_3">
        <inputEntry id="InputEntry_1_3">
          <text>-"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>"Sim"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"N/A"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"Contraindicado. Exames contrastados via oral nas últimas 24 horas podem interferir no resultado do exame."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_3"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_40702057_4">
        <inputEntry id="InputEntry_1_4">
          <text>"Não Identificado"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>"Não"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"N/A"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"O tipo de sangramento (ativo ou intermitente) deve ser especificado para a correta seleção do radiofármaco."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_5_4"><text>"NEGAR"</text></outputEntry>
      </rule>
      
    
    <rule id="Rule_Default_40702057">
      <description>Regra padrao - Criterios nao atendidos</description>
      <inputEntry id="InputEntry_Default_40702057_1">
        <text></text>
      </inputEntry>
      <inputEntry id="InputEntry_Default_40702057_2">
        <text></text>
      </inputEntry>
      <outputEntry id="OutputEntry_Default_40702057_1">
        <text>"Pendente"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_40702057_2">
        <text>"N/A"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_40702057_3">
        <text>"Critérios não atendidos - avaliar manualmente"</text>
      </outputEntry>
      <outputEntry id="OutputEntry_Default_40702057_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Default_40702057_5"><text>"SOLICITAR_INFO"</text></outputEntry>
    </rule>
  </decisionTable>
  </decision>
</definitions>
