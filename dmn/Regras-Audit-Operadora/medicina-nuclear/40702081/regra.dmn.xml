<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             id="Definitions_40702081"
             name="Cintilografia para o estudo do trânsito esofágico semissólido"
             namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 40702081 - Cintilografia para o estudo do trânsito esofágico semissólido
    Versao: 2.0.0 (2026-02-08 LEAN TIER-2)

    Indicacoes: Disfagia, suspeita de dismotilidade esofagica
    Contraindicacoes: Obstrucao esofagica completa, impossibilidade de deglutir
    Referencias: ANS/TISS 2024, Protocolo Medicina Nuclear SBMN
  -->
  <decision id="Decision_40702081" name="Auditoria Cintilografia Transito Esofagico">
    <decisionTable id="DecisionTable_40702081" hitPolicy="FIRST">
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpression_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Disfagia","Dismotilidade","Pos_Cirurgia","Acalasia","Nenhuma"</text>
        </inputValues>
      </input>
      <input id="Input_jejum4h" label="Paciente em jejum de 4 horas">
        <inputExpression id="InputExpression_jejum4h" typeRef="boolean">
          <text>jejum4h</text>
        </inputExpression>
      </input>
      <input id="Input_capacidadeDegluticao" label="Capacidade de deglutição do radiofármaco">
        <inputExpression id="InputExpression_capacidadeDegluticao" typeRef="boolean">
          <text>capacidadeDegluticao</text>
        </inputExpression>
      </input>
      <input id="Input_documentacaoCompleta" label="Documentacao Completa">
        <inputExpression id="InputExpression_documentacaoCompleta" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="Rule_SemDocumentacao">
        <description>Documentacao ausente</description>
        <inputEntry id="InputEntry_1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Documentacao incompleta: necessario laudo medico com indicacao clinica, exames previos (EDA)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"ANS RN 465: documentacao obrigatoria para procedimentos de media complexidade."</text></outputEntry>
        <outputEntry id="OutputEntry_4_1"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_5_1"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_SemCapacidadeDegluticao">
        <description>Paciente sem capacidade de degluticao</description>
        <inputEntry id="InputEntry_1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Paciente nao possui capacidade de deglutir radiofarmaco. Procedimento inviavel."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"Protocolo SBMN: capacidade de degluticao e pre-requisito para cintilografia de transito esofagico."</text></outputEntry>
        <outputEntry id="OutputEntry_4_2"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_2"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_SemJejum">
        <description>Paciente nao cumpriu jejum</description>
        <inputEntry id="InputEntry_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Paciente nao cumpriu jejum de 4 horas. Procedimento nao pode ser realizado."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"Protocolo SBMN: jejum de 4 horas obrigatorio para estudo de transito esofagico."</text></outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_3"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_SemIndicacao">
        <description>Sem indicacao clinica</description>
        <inputEntry id="InputEntry_1_4"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Ausencia de indicacao clinica documentada para cintilografia de transito esofagico."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"DUT ANS: exame indicado para disfagia, dismotilidade, acalasia ou pos-cirurgia esofagica."</text></outputEntry>
        <outputEntry id="OutputEntry_4_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_4"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado">
        <description>Criterios atendidos</description>
        <inputEntry id="InputEntry_1_5"><text>"Disfagia","Dismotilidade","Pos_Cirurgia","Acalasia"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Procedimento aprovado. Indicacao clinica documentada e criterios tecnicos atendidos."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"Conforme Protocolo SBMN e DUT ANS para cintilografia de transito esofagico."</text></outputEntry>
        <outputEntry id="OutputEntry_4_5"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Default_40702081">
        <description>Regra padrao - Criterios nao atendidos</description>
        <inputEntry id="InputEntry_Default_1"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_3"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_4"><text></text></inputEntry>
        <outputEntry id="OutputEntry_Default_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Default_2"><text>"Analise adicional necessaria. Criterios de auditoria incompletos."</text></outputEntry>
        <outputEntry id="OutputEntry_Default_3"><text>"Necessario completar documentacao clinica e verificar criterios tecnicos."</text></outputEntry>
        <outputEntry id="OutputEntry_Default_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Default_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
