  <decision id="Decision_40707091" name="Cintilografia cerebral com Trodat (transportadores de dopamina)">
    <decisionTable id="DecisionTable_40707091" hitPolicy="FIRST">
      <input id="Input_1" label="Aplicação Clínica">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>aplicacaoClinica</text>
        </inputExpression>
      </input>
      
      <output id="Output_resultado" name="resultado" typeRef="string">
