        <outputEntry id="OutputEntry_Rule_40707091_1_4">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_40707091_1_5">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>

      <rule id="Rule_40707091_2">
        <description>Outras aplicações não são cobertas por esta regra.</description>
        <inputEntry id="InputEntry_40707091_2">
          <text><![CDATA[not("Diagnóstico diferencial de demência (Corpus de Lewy e Alzheimer)")]]></text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_40707091_2_1">
