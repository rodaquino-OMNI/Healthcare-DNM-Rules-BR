<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             id="Definitions_40708020"
             name="Cintilografia com Gálio-67"
             namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 40708020 - Cintilografia com Gálio-67
    Versao: 2.0.0 (2026-02-08 LEAN TIER-2)

    Indicacoes: Linfomas, processos inflamatorios, tumores especificos
    Contraindicacoes: Gravidez, lactacao, hipersensibilidade ao radiofarmaco
    Referencias: ANS/TISS 2024, Protocolo SBMN Oncologia
  -->
  <decision id="Decision_40708020" name="Auditoria Cintilografia Galio-67">
    <decisionTable id="DecisionTable_40708020" hitPolicy="FIRST">
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpression_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Linfoma","Hepatocarcinoma","Melanoma","Processo_Inflamatorio","Febre_Origem_Indeterminada","Nenhuma"</text>
        </inputValues>
      </input>
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpression_diagnosticoConfirmado" typeRef="boolean">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
      </input>
      <input id="Input_documentacaoCompleta" label="Documentacao Completa">
        <inputExpression id="InputExpression_documentacaoCompleta" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>
      <input id="Input_contraindicacaoPresente" label="Contraindicacao Presente">
        <inputExpression id="InputExpression_contraindicacao" typeRef="boolean">
          <text>contraindicacaoPresente</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="Rule_Contraindicacao">
        <description>Contraindicacao presente</description>
        <inputEntry id="InputEntry_1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Contraindicacao presente: gravidez, lactacao ou hipersensibilidade ao radiofarmaco."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"Protocolo SBMN: contraindicacoes absolutas para uso de Galio-67."</text></outputEntry>
        <outputEntry id="OutputEntry_4_1"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_1"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_SemDocumentacao">
        <description>Documentacao ausente</description>
        <inputEntry id="InputEntry_1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Documentacao incompleta: necessario laudo medico, exames histopatologicos ou de imagem previos."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"ANS RN 465: documentacao obrigatoria para procedimentos de alta complexidade."</text></outputEntry>
        <outputEntry id="OutputEntry_4_2"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_5_2"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_SemIndicacao">
        <description>Sem indicacao clinica</description>
        <inputEntry id="InputEntry_1_3"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Ausencia de indicacao clinica documentada para cintilografia com Galio-67."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"DUT ANS: exame indicado para linfomas, tumores especificos, processos inflamatorios ou febre de origem indeterminada."</text></outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_3"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_SemDiagnostico">
        <description>Diagnostico nao confirmado</description>
        <inputEntry id="InputEntry_1_4"><text>"Linfoma","Hepatocarcinoma","Melanoma"</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Diagnostico nao confirmado por histopatologia. Necessario laudo anatomopatologico."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"Protocolo SBMN: diagnostico histologico e pre-requisito para estadiamento por Galio-67."</text></outputEntry>
        <outputEntry id="OutputEntry_4_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_5_4"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Linfoma_Aprovado">
        <description>Linfoma confirmado</description>
        <inputEntry id="InputEntry_1_5"><text>"Linfoma"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Procedimento aprovado. Indicacao para estadiamento ou controle pos-terapia de linfoma."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"Protocolo SBMN: alta sensibilidade para linfomas Hodgkin, nao-Hodgkin histiocitico e Burkitt."</text></outputEntry>
        <outputEntry id="OutputEntry_4_5"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Tumor_Aprovado">
        <description>Tumor especifico confirmado</description>
        <inputEntry id="InputEntry_1_6"><text>"Hepatocarcinoma","Melanoma"</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Procedimento aprovado. Indicacao para estadiamento ou diagnostico diferencial."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"Protocolo SBMN: Galio-67 util para hepatocarcinoma e estadiamento de melanoma."</text></outputEntry>
        <outputEntry id="OutputEntry_4_6"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_6"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Inflamatorio_Aprovado">
        <description>Processo inflamatorio ou febre</description>
        <inputEntry id="InputEntry_1_7"><text>"Processo_Inflamatorio","Febre_Origem_Indeterminada"</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Procedimento aprovado. Indicacao para localizacao de foco infeccioso/inflamatorio."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"Protocolo SBMN: Galio-67 util para processos inflamatorios pulmonares, abdominais e osteoarticulares."</text></outputEntry>
        <outputEntry id="OutputEntry_4_7"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_7"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Default_40708020">
        <description>Regra padrao - Criterios nao atendidos</description>
        <inputEntry id="InputEntry_Default_1"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_2"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_3"><text></text></inputEntry>
        <inputEntry id="InputEntry_Default_4"><text></text></inputEntry>
        <outputEntry id="OutputEntry_Default_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Default_2"><text>"Analise adicional necessaria. Criterios de auditoria incompletos."</text></outputEntry>
        <outputEntry id="OutputEntry_Default_3"><text>"Necessario completar documentacao clinica e confirmar diagnostico."</text></outputEntry>
        <outputEntry id="OutputEntry_Default_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Default_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
