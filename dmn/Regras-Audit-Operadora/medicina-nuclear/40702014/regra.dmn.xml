<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             id="Definitions_40702014"
             name="Cintilografia das glândulas salivares"
             targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 40702014 - Cintilografia das glândulas salivares
    Versao: 2.0.0 (2026-02-06)

    Indicacoes: Conforme protocolo ANS/TISS
    Contraindicacoes: Verificar regras especificas
    Referencias: ANS/TISS 2024, Resolucao Normativa
  -->

  <decision id="Decision_40702014" name="Avaliação da Cintilografia das Glândulas Salivares">
    <decisionTable id="DecisionTable_40702014" hitPolicy="FIRST">
      <input id="Input_1" label="Paciente em Jejum">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>pacienteEmJejum</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Estímulo Salivar Realizado">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>estimuloSalivarRealizado</text>
        </inputExpression>
      </input>
      
      <output id="Output_resultado" name="resultado" typeRef="string">
        <outputValues><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" name="fundamentacao" typeRef="string"/>
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues>
      </output>
      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>
      
      <rule id="Rule_1_Aprovado">
        <description>Paciente atende a todos os critérios para o procedimento.</description>
        <inputEntry id="InputEntry_1_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_1_Aprovado_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_1_Aprovado_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_1_Aprovado_3">
          <text>"Critérios Atendidos"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_1_Aprovado_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_1_Aprovado_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      
      <rule id="Rule_2_Jejum">
        <description>Paciente não estava em jejum, critério obrigatório.</description>
        <inputEntry id="InputEntry_1_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_2_Jejum_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_2_Jejum_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_2_Jejum_3">
          <text>"Critério de Jejum Não Atendido"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_2_Jejum_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_2_Jejum_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>

      <rule id="Rule_3_Estimulo">
        <description>Estímulo salivar não foi realizado.</description>
        <inputEntry id="InputEntry_1_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_3_Estimulo_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_3_Estimulo_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_3_Estimulo_3">
          <text>"Critério de Estímulo Não Atendido"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_3_Estimulo_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_3_Estimulo_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      
    
    <rule id="Rule_Default_40702014">
      <description>Regra padrao - Criterios nao atendidos</description>
      <inputEntry id="InputEntry_Default_40702014_1">
        <text></text>
      </inputEntry>
      <inputEntry id="InputEntry_Default_40702014_2">
        <text></text>
      </inputEntry>
      <outputEntry id="OutputEntry_Rule_Default_40702014_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40702014_2">
          <text>"Auto-migrated from 1-output"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40702014_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40702014_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Rule_Default_40702014_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
    </rule>
  </decisionTable>
  </decision>
</definitions>
