      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>
      
      <rule id="Rule_40702090_1">
        <description>Paciente realizou o jejum necessário de 2 horas.</description>
        <inputEntry id="InputEntry_40702090_1">
          <text>"Sim"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_Rule_40702090_1_1">
