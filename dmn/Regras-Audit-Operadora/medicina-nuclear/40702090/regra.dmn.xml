  <decision id="Decision_40702090" name="Decisão para Cintilografia de Meckel">
    <decisionTable id="DecisionTable_40702090" hitPolicy="FIRST">
      <input id="Input_1" label="Jejum de 2 horas realizado">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>jejumRealizado</text>
        </inputExpression>
      </input>
      
      <output id="Output_resultado" name="resultado" typeRef="string">
