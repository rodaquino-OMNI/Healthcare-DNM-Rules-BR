<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_DUP_008"
             name="Prevencao de Duplicidade - Procedimentos Esteticos Repetidos"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    DUP-008 - Prevencao de Duplicidade de Procedimentos Esteticos Repetidos
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Controle de Desperdicio
    Categoria: Duplicate Procedure Prevention

    Objetivo:
    - Detectar repeticao de procedimentos com fins esteticos
    - Verificar cobertura conforme contrato
    - Bloquear procedimentos nao cobertos pelo ROL ANS

    Indicacoes para Bloqueio:
    - Procedimento primariamente estetico sem cobertura
    - Repeticao de tratamento estetico sem intervalo adequado
    - Ausencia de documentacao de indicacao funcional

    Excecoes Permitidas:
    - Cirurgia reparadora pos-mastectomia
    - Reconstrucao por trauma ou queimadura
    - Correcao de malformacao congenita

    Referencias Normativas:
    - ANS - RN 465/2021 - Exclusoes de Cobertura
    - CFM - Resolucao sobre Cirurgia Plastica
    - SBCP - Diretrizes de Cirurgia Reparadora 2023
  -->

  <decision id="Decision_DUP_008" name="Prevencao Duplicidade - Procedimentos Esteticos Repetidos">
    <decisionTable id="DecisionTable_DUP_008" hitPolicy="FIRST">

      <!-- Input 1: Procedimento primariamente estetico -->
      <input id="Input_procedimentoPrimariamenteEstetico" label="Procedimento Primariamente Estetico">
        <inputExpression id="InputExpr_procedimentoPrimariamenteEstetico" typeRef="boolean">
          <text>procedimentoPrimariamenteEstetico</text>
        </inputExpression>
      </input>

      <!-- Input 2: Indicacao funcional documentada -->
      <input id="Input_indicacaoFuncionalDocumentada" label="Indicacao Funcional Documentada">
        <inputExpression id="InputExpr_indicacaoFuncionalDocumentada" typeRef="boolean">
          <text>indicacaoFuncionalDocumentada</text>
        </inputExpression>
      </input>

      <!-- Input 3: Reconstrucao pos-mastectomia -->
      <input id="Input_reconstrucaoPosMastectomia" label="Reconstrucao Pos-Mastectomia">
        <inputExpression id="InputExpr_reconstrucaoPosMastectomia" typeRef="boolean">
          <text>reconstrucaoPosMastectomia</text>
        </inputExpression>
      </input>

      <!-- Input 4: Trauma ou malformacao congenita -->
      <input id="Input_traumaMalformacao" label="Trauma, Queimadura ou Malformacao Congenita">
        <inputExpression id="InputExpr_traumaMalformacao" typeRef="boolean">
          <text>traumaMalformacao</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- ============================================== -->
      <!-- REPROVADO: Estetico sem indicacao funcional   -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_008_Reprovado_SemIndicacao">
        <description>Reprovado - Procedimento estetico sem indicacao funcional</description>
        <inputEntry id="IE_Rep1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"Procedimento primariamente estetico sem cobertura pelo ROL ANS. Ausencia de indicacao funcional ou reparadora."</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"ANS RN 465/2021 Art. 20 - Exclusoes de Cobertura"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Reprovado_SemIndicacao_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Reprovado_SemIndicacao_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Reconstrucao pos-mastectomia        -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_008_Aprovado_PosMastectomia">
        <description>Aprovado - Cirurgia reparadora pos-mastectomia</description>
        <inputEntry id="IE_Aprov1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Procedimento autorizado - Reconstrucao mamaria pos-mastectomia conforme Lei 9.797/1999."</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"Lei 9.797/1999 - Cirurgia Plastica Reparadora"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Aprovado_PosMastectomia_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Aprovado_PosMastectomia_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Trauma ou malformacao               -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_008_Aprovado_TraumaMalformacao">
        <description>Aprovado - Reconstrucao por trauma ou malformacao</description>
        <inputEntry id="IE_Aprov2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Procedimento autorizado - Cirurgia reparadora por trauma, queimadura ou malformacao congenita."</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"SBCP Diretrizes 2023 - Cirurgia Reparadora"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Aprovado_TraumaMalformacao_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Aprovado_TraumaMalformacao_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Indicacao funcional documentada     -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_008_Aprovado_IndicacaoFuncional">
        <description>Aprovado - Indicacao funcional documentada</description>
        <inputEntry id="IE_Aprov3_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Procedimento autorizado - Indicacao funcional documentada transforma em procedimento reparador coberto."</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"Indicacao funcional aceita - ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Aprovado_IndicacaoFuncional_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Aprovado_IndicacaoFuncional_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Nao estetico                        -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_008_Aprovado_NaoEstetico">
        <description>Aprovado - Procedimento nao classificado como estetico</description>
        <inputEntry id="IE_Aprov4_1"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"Procedimento autorizado - Nao classificado como primariamente estetico."</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"Verificacao de classificacao - OK"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Aprovado_NaoEstetico_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Aprovado_NaoEstetico_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- PENDENTE: Fallback                            -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_008_Pendente_Fallback">
        <description>Pendente - Requer analise manual por auditor</description>
        <inputEntry id="IE_Pend_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Pend_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend_2"><text>"Caso requer analise manual para verificar carater reparador versus estetico."</text></outputEntry>
        <outputEntry id="OE_Pend_3"><text>"Auditoria medica - Processo manual"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Pendente_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_008_Pendente_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
