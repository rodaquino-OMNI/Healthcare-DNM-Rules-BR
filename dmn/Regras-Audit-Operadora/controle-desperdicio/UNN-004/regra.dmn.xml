<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_UNN_004"
             name="Prevencao de Procedimento Desnecessario - TC de Cranio para Cefaleia Simples"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    UNN-004 - Prevencao de Procedimento Desnecessario - TC de Cranio para Cefaleia Simples
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Controle de Desperdicio
    Categoria: Unnecessary Procedure Prevention

    Objetivo:
    - Evitar TC de cranio para cefaleia tensional ou enxaqueca tipica
    - Reduzir exposicao desnecessaria a radiacao
    - Seguir criterios de adequacao de imagem neurologica

    Indicacoes para Bloqueio (Choosing Wisely):
    - Cefaleia tensional recorrente sem mudanca de padrao
    - Enxaqueca tipica com aura classica
    - Exame neurologico normal

    Excecoes Permitidas (Red Flags):
    - Cefaleia subita e intensa (thunderclap)
    - Deficit neurologico focal
    - Papiledema ou sinais de hipertensao intracraniana
    - Imunossupressao ou coagulopatia

    Referencias Normativas:
    - ABM Choosing Wisely Brasil 2024 - Neurologia
    - ACR Head CT Appropriateness Criteria
    - IHS - International Headache Society Guidelines
  -->

  <decision id="Decision_UNN_004" name="Prevencao Desnecessario - TC Cranio para Cefaleia Simples">
    <decisionTable id="DecisionTable_UNN_004" hitPolicy="FIRST">

      <!-- Input 1: Cefaleia tipica sem red flags -->
      <input id="Input_cefaleiaTipicaSemRedFlags" label="Cefaleia Tipica (tensional/enxaqueca) sem Red Flags">
        <inputExpression id="InputExpr_cefaleiaTipicaSemRedFlags" typeRef="boolean">
          <text>cefaleiaTipicaSemRedFlags</text>
        </inputExpression>
      </input>

      <!-- Input 2: Cefaleia subita e intensa -->
      <input id="Input_cefaleiaSubitaIntensa" label="Cefaleia Subita e Intensa (Thunderclap)">
        <inputExpression id="InputExpr_cefaleiaSubitaIntensa" typeRef="boolean">
          <text>cefaleiaSubitaIntensa</text>
        </inputExpression>
      </input>

      <!-- Input 3: Deficit neurologico focal -->
      <input id="Input_deficitNeurologicoFocal" label="Deficit Neurologico Focal">
        <inputExpression id="InputExpr_deficitNeurologicoFocal" typeRef="boolean">
          <text>deficitNeurologicoFocal</text>
        </inputExpression>
      </input>

      <!-- Input 4: Sinais de hipertensao intracraniana -->
      <input id="Input_sinaisHipertensaoIntracraniana" label="Papiledema ou Sinais de Hipertensao Intracraniana">
        <inputExpression id="InputExpr_sinaisHipertensaoIntracraniana" typeRef="boolean">
          <text>sinaisHipertensaoIntracraniana</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- ============================================== -->
      <!-- REPROVADO: Cefaleia tipica sem alarmes        -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_004_Reprovado_CefaleiaTipica">
        <description>Reprovado - TC de cranio para cefaleia tipica sem sinais de alarme</description>
        <inputEntry id="IE_Rep1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"TC de cranio nao indicada para cefaleia tensional ou enxaqueca tipica sem sinais de alarme. Exposicao desnecessaria a radiacao."</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"ABM Choosing Wisely 2024; ACR Appropriateness Criteria"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Reprovado_CefaleiaTipica_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Reprovado_CefaleiaTipica_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Cefaleia subita intensa             -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_004_Aprovado_Thunderclap">
        <description>Aprovado - Cefaleia subita e intensa (thunderclap) requer TC urgente</description>
        <inputEntry id="IE_Aprov1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"TC autorizada - Cefaleia subita e intensa (thunderclap) requer exclusao de hemorragia subaracnoidea."</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"Red Flag - Suspeita de HSA"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Aprovado_Thunderclap_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Aprovado_Thunderclap_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Deficit neurologico focal           -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_004_Aprovado_DeficitFocal">
        <description>Aprovado - Deficit neurologico focal requer investigacao</description>
        <inputEntry id="IE_Aprov2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"TC autorizada - Deficit neurologico focal associado a cefaleia requer investigacao de lesao estrutural."</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"Red Flag - Deficit focal"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Aprovado_DeficitFocal_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Aprovado_DeficitFocal_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Hipertensao intracraniana           -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_004_Aprovado_HIC">
        <description>Aprovado - Sinais de hipertensao intracraniana</description>
        <inputEntry id="IE_Aprov3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"TC autorizada - Papiledema ou sinais de hipertensao intracraniana requerem investigacao urgente."</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"Red Flag - Hipertensao intracraniana"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Aprovado_HIC_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Aprovado_HIC_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Nao e cefaleia tipica               -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_004_Aprovado_NaoTipica">
        <description>Aprovado - Cefaleia atipica ou com mudanca de padrao</description>
        <inputEntry id="IE_Aprov4_1"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"TC autorizada - Cefaleia atipica ou com mudanca de padrao justifica investigacao."</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"Cefaleia atipica - Investigacao indicada"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Aprovado_NaoTipica_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Aprovado_NaoTipica_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- PENDENTE: Fallback                            -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_004_Pendente_Fallback">
        <description>Pendente - Requer analise manual por auditor</description>
        <inputEntry id="IE_Pend_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Pend_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend_2"><text>"Caso requer analise manual para avaliacao de indicacao de TC de cranio."</text></outputEntry>
        <outputEntry id="OE_Pend_3"><text>"Auditoria medica - Processo manual"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Pendente_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_004_Pendente_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
