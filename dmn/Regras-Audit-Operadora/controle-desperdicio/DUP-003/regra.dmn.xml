<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_DUP_003"
             name="Prevencao de Duplicidade - Consultas Multiplas Mesmo Dia"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    DUP-003 - Prevencao de Duplicidade de Consultas Multiplas no Mesmo Dia
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Controle de Desperdicio
    Categoria: Duplicate Procedure Prevention

    Objetivo:
    - Identificar consultas duplicadas da mesma especialidade no mesmo dia
    - Bloquear cobrancas multiplas indevidas
    - Permitir consultas legitimas em diferentes especialidades

    Indicacoes para Bloqueio:
    - Duas ou mais consultas da mesma especialidade no mesmo dia
    - Consulta de retorno no mesmo dia da consulta inicial
    - Consultas com mesmo CRM em horarios proximos

    Excecoes Permitidas:
    - Especialidades diferentes no mesmo dia
    - Urgencia/emergencia documentada
    - Interconsulta solicitada pelo medico assistente

    Referencias Normativas:
    - ANS - RN 465/2021 Art. 8 - Regras de Cobranca
    - ABRAMGE - Diretrizes de Consultas 2023
    - CFM - Resolucao 1958/2010 - Consulta Medica
  -->

  <decision id="Decision_DUP_003" name="Prevencao Duplicidade - Consultas Multiplas Mesmo Dia">
    <decisionTable id="DecisionTable_DUP_003" hitPolicy="FIRST">

      <!-- Input 1: Consulta mesma especialidade mesmo dia -->
      <input id="Input_consultaMesmaEspecialidadeMesmoDia" label="Consulta Mesma Especialidade no Mesmo Dia">
        <inputExpression id="InputExpr_consultaMesmaEspecialidadeMesmoDia" typeRef="boolean">
          <text>consultaMesmaEspecialidadeMesmoDia</text>
        </inputExpression>
      </input>

      <!-- Input 2: Urgencia ou emergencia -->
      <input id="Input_urgenciaEmergencia" label="Urgencia ou Emergencia Documentada">
        <inputExpression id="InputExpr_urgenciaEmergencia" typeRef="boolean">
          <text>urgenciaEmergencia</text>
        </inputExpression>
      </input>

      <!-- Input 3: Interconsulta solicitada -->
      <input id="Input_interconsultaSolicitada" label="Interconsulta Formalmente Solicitada">
        <inputExpression id="InputExpr_interconsultaSolicitada" typeRef="boolean">
          <text>interconsultaSolicitada</text>
        </inputExpression>
      </input>

      <!-- Input 4: Especialidades diferentes -->
      <input id="Input_especialidadesDiferentes" label="Especialidades Diferentes">
        <inputExpression id="InputExpr_especialidadesDiferentes" typeRef="boolean">
          <text>especialidadesDiferentes</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- ============================================== -->
      <!-- REPROVADO: Mesma especialidade sem excecao    -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_003_Reprovado_MesmaEspecialidade">
        <description>Reprovado - Consulta duplicada da mesma especialidade no mesmo dia</description>
        <inputEntry id="IE_Rep1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"Consulta duplicada detectada - Mesma especialidade no mesmo dia sem justificativa de urgencia ou interconsulta."</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"ANS RN 465/2021 Art. 8; CFM Resolucao 1958/2010"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Reprovado_MesmaEspecialidade_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Reprovado_MesmaEspecialidade_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Urgencia/emergencia                 -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_003_Aprovado_Urgencia">
        <description>Aprovado - Atendimento de urgencia/emergencia</description>
        <inputEntry id="IE_Aprov1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Consulta adicional autorizada - Urgencia ou emergencia documentada."</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"Excecao por urgencia - ANS RN 259/2011"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Aprovado_Urgencia_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Aprovado_Urgencia_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Interconsulta formal                -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_003_Aprovado_Interconsulta">
        <description>Aprovado - Interconsulta formalmente solicitada</description>
        <inputEntry id="IE_Aprov2_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Consulta adicional autorizada - Interconsulta solicitada pelo medico assistente."</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"Interconsulta legitima - CFM Resolucao 1958/2010"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Aprovado_Interconsulta_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Aprovado_Interconsulta_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Especialidades diferentes           -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_003_Aprovado_EspecDiferentes">
        <description>Aprovado - Consultas de especialidades diferentes</description>
        <inputEntry id="IE_Aprov3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Consultas autorizadas - Especialidades diferentes permitem multiplos atendimentos."</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"Especialidades distintas - Permitido"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Aprovado_EspecDiferentes_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Aprovado_EspecDiferentes_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Sem duplicidade                     -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_003_Aprovado_SemDuplicidade">
        <description>Aprovado - Nenhuma duplicidade detectada</description>
        <inputEntry id="IE_Aprov4_1"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"Consulta autorizada - Nenhuma duplicidade identificada."</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"Verificacao de duplicidade - OK"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Aprovado_SemDuplicidade_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Aprovado_SemDuplicidade_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- PENDENTE: Fallback                            -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_003_Pendente_Fallback">
        <description>Pendente - Requer analise manual por auditor</description>
        <inputEntry id="IE_Pend_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Pend_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend_2"><text>"Caso requer analise manual por auditor para validacao de duplicidade de consultas."</text></outputEntry>
        <outputEntry id="OE_Pend_3"><text>"Auditoria medica - Processo manual"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Pendente_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_003_Pendente_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
