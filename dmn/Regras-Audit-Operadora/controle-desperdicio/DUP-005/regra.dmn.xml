<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_DUP_005"
             name="Prevencao de Duplicidade - Fisioterapia Sessoes Excedentes"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    DUP-005 - Prevencao de Duplicidade de Sessoes de Fisioterapia Excedentes
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Controle de Desperdicio
    Categoria: Duplicate Procedure Prevention

    Objetivo:
    - Controlar numero de sessoes de fisioterapia por guia/periodo
    - Bloquear sessoes alem do limite autorizado
    - Exigir nova avaliacao para continuidade

    Indicacoes para Bloqueio:
    - Sessoes excedentes ao limite da guia (tipicamente 10-20 sessoes)
    - Mais de 2 sessoes por dia para mesma patologia
    - Ausencia de reavaliacao apos periodo de tratamento

    Excecoes Permitidas:
    - Autorizacao de prorrogacao aprovada
    - Protocolo de reabilitacao intensiva (pos-operatorio)
    - Tratamento de multiplas patologias documentadas

    Referencias Normativas:
    - ANS - RN 465/2021 - Cobertura de Fisioterapia
    - COFFITO - Diretrizes de Fisioterapia 2023
    - ABRAMGE - Controle de Sessoes 2023
  -->

  <decision id="Decision_DUP_005" name="Prevencao Duplicidade - Fisioterapia Sessoes Excedentes">
    <decisionTable id="DecisionTable_DUP_005" hitPolicy="FIRST">

      <!-- Input 1: Sessoes excedentes ao limite -->
      <input id="Input_sessoesExcedentesLimite" label="Sessoes Excedentes ao Limite Autorizado">
        <inputExpression id="InputExpr_sessoesExcedentesLimite" typeRef="boolean">
          <text>sessoesExcedentesLimite</text>
        </inputExpression>
      </input>

      <!-- Input 2: Prorrogacao autorizada -->
      <input id="Input_prorrogacaoAutorizada" label="Prorrogacao Autorizada pela Operadora">
        <inputExpression id="InputExpr_prorrogacaoAutorizada" typeRef="boolean">
          <text>prorrogacaoAutorizada</text>
        </inputExpression>
      </input>

      <!-- Input 3: Reabilitacao pos-operatoria -->
      <input id="Input_reabilitacaoPosOperatoria" label="Protocolo de Reabilitacao Pos-Operatoria">
        <inputExpression id="InputExpr_reabilitacaoPosOperatoria" typeRef="boolean">
          <text>reabilitacaoPosOperatoria</text>
        </inputExpression>
      </input>

      <!-- Input 4: Multiplas patologias documentadas -->
      <input id="Input_multiplasPatologias" label="Multiplas Patologias Documentadas">
        <inputExpression id="InputExpr_multiplasPatologias" typeRef="boolean">
          <text>multiplasPatologias</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- ============================================== -->
      <!-- REPROVADO: Excedente sem autorizacao          -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_005_Reprovado_SemAutorizacao">
        <description>Reprovado - Sessoes excedentes sem autorizacao de prorrogacao</description>
        <inputEntry id="IE_Rep1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"Sessoes de fisioterapia excedentes ao limite autorizado. Necessaria solicitacao de prorrogacao com reavaliacao clinica."</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"ANS RN 465/2021; COFFITO Diretrizes 2023"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Reprovado_SemAutorizacao_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Reprovado_SemAutorizacao_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Prorrogacao autorizada              -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_005_Aprovado_Prorrogacao">
        <description>Aprovado - Prorrogacao de sessoes autorizada</description>
        <inputEntry id="IE_Aprov1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Sessoes adicionais autorizadas - Prorrogacao aprovada pela operadora."</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"Prorrogacao autorizada - ANS RN 465/2021"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Aprovado_Prorrogacao_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Aprovado_Prorrogacao_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Reabilitacao pos-operatoria         -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_005_Aprovado_PosOp">
        <description>Aprovado - Protocolo de reabilitacao pos-operatoria</description>
        <inputEntry id="IE_Aprov2_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Sessoes adicionais autorizadas - Protocolo de reabilitacao pos-operatoria intensiva."</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"Protocolo Reabilitacao Intensiva - Excecao aplicavel"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Aprovado_PosOp_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Aprovado_PosOp_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Multiplas patologias                -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_005_Aprovado_MultiplasPatologias">
        <description>Aprovado - Tratamento de multiplas patologias documentadas</description>
        <inputEntry id="IE_Aprov3_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Sessoes adicionais autorizadas - Multiplas patologias em tratamento documentadas."</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"Multiplas patologias - COFFITO Diretrizes 2023"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Aprovado_MultiplasPatologias_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Aprovado_MultiplasPatologias_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Dentro do limite                    -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_005_Aprovado_DentroLimite">
        <description>Aprovado - Sessoes dentro do limite autorizado</description>
        <inputEntry id="IE_Aprov4_1"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"Sessao de fisioterapia autorizada - Dentro do limite da guia."</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"Verificacao de limite - OK"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Aprovado_DentroLimite_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Aprovado_DentroLimite_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- PENDENTE: Fallback                            -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_005_Pendente_Fallback">
        <description>Pendente - Requer analise manual por auditor</description>
        <inputEntry id="IE_Pend_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Pend_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend_2"><text>"Caso requer analise manual por auditor para validacao de limite de sessoes."</text></outputEntry>
        <outputEntry id="OE_Pend_3"><text>"Auditoria medica - Processo manual"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Pendente_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_005_Pendente_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
