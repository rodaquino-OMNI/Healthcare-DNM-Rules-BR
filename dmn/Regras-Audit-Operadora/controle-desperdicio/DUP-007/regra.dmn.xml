<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_DUP_007"
             name="Prevencao de Duplicidade - OPME Mesma Regiao Anatomica"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    DUP-007 - Prevencao de Duplicidade de OPME na Mesma Regiao Anatomica
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Controle de Desperdicio
    Categoria: Duplicate Procedure Prevention

    Objetivo:
    - Detectar solicitacoes duplicadas de OPME para mesma regiao anatomica
    - Verificar historico de implantes anteriores
    - Bloquear cobrancas indevidas de materiais ja implantados

    Indicacoes para Bloqueio:
    - OPME solicitado para regiao com implante recente (menos de 1 ano)
    - Mesmo codigo OPME em solicitacoes separadas
    - Quantidade de material incompativel com procedimento

    Excecoes Permitidas:
    - Revisao cirurgica documentada
    - Falha de material com laudo tecnico
    - Infeccao com necessidade de troca

    Referencias Normativas:
    - ANS - RN 465/2021 - Cobertura de OPME
    - ANVISA - Rastreabilidade de Implantes
    - ABRAMGE - Gestao de OPME 2023
  -->

  <decision id="Decision_DUP_007" name="Prevencao Duplicidade - OPME Mesma Regiao Anatomica">
    <decisionTable id="DecisionTable_DUP_007" hitPolicy="FIRST">

      <!-- Input 1: OPME em regiao com implante recente -->
      <input id="Input_opmeRegiaoImplanteRecente" label="OPME para Regiao com Implante Recente (menos de 1 ano)">
        <inputExpression id="InputExpr_opmeRegiaoImplanteRecente" typeRef="boolean">
          <text>opmeRegiaoImplanteRecente</text>
        </inputExpression>
      </input>

      <!-- Input 2: Revisao cirurgica documentada -->
      <input id="Input_revisaoCirurgicaDocumentada" label="Revisao Cirurgica Documentada">
        <inputExpression id="InputExpr_revisaoCirurgicaDocumentada" typeRef="boolean">
          <text>revisaoCirurgicaDocumentada</text>
        </inputExpression>
      </input>

      <!-- Input 3: Falha de material com laudo -->
      <input id="Input_falhaMaterialLaudo" label="Falha de Material com Laudo Tecnico">
        <inputExpression id="InputExpr_falhaMaterialLaudo" typeRef="boolean">
          <text>falhaMaterialLaudo</text>
        </inputExpression>
      </input>

      <!-- Input 4: Infeccao com necessidade de troca -->
      <input id="Input_infeccaoNecessidadeTroca" label="Infeccao Documentada com Necessidade de Troca">
        <inputExpression id="InputExpr_infeccaoNecessidadeTroca" typeRef="boolean">
          <text>infeccaoNecessidadeTroca</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- ============================================== -->
      <!-- REPROVADO: Duplicidade sem justificativa      -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_007_Reprovado_SemJustificativa">
        <description>Reprovado - OPME duplicado para mesma regiao sem justificativa</description>
        <inputEntry id="IE_Rep1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"OPME bloqueado - Regiao anatomica ja possui implante recente sem justificativa para nova intervencao."</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"ANS RN 465/2021; ANVISA Rastreabilidade"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Reprovado_SemJustificativa_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Reprovado_SemJustificativa_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Revisao cirurgica                   -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_007_Aprovado_Revisao">
        <description>Aprovado - Revisao cirurgica documentada</description>
        <inputEntry id="IE_Aprov1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Novo OPME autorizado - Revisao cirurgica documentada com indicacao clinica."</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"Revisao cirurgica - Indicacao aceita"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Aprovado_Revisao_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Aprovado_Revisao_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Falha de material                   -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_007_Aprovado_FalhaMaterial">
        <description>Aprovado - Falha de material com laudo tecnico</description>
        <inputEntry id="IE_Aprov2_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Novo OPME autorizado - Falha de material documentada com laudo tecnico do fabricante."</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"Falha tecnica - ANVISA Tecnovigilancia"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Aprovado_FalhaMaterial_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Aprovado_FalhaMaterial_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Infeccao com troca                  -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_007_Aprovado_Infeccao">
        <description>Aprovado - Infeccao documentada com necessidade de troca</description>
        <inputEntry id="IE_Aprov3_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Novo OPME autorizado - Infeccao de sitio cirurgico documentada requer troca do implante."</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"Complicacao infecciosa - Indicacao medica"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Aprovado_Infeccao_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Aprovado_Infeccao_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Sem duplicidade                     -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_007_Aprovado_SemDuplicidade">
        <description>Aprovado - Nenhum implante anterior na regiao</description>
        <inputEntry id="IE_Aprov4_1"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"OPME autorizado - Nenhum implante anterior identificado na regiao anatomica."</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"Verificacao de historico - OK"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Aprovado_SemDuplicidade_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Aprovado_SemDuplicidade_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- PENDENTE: Fallback                            -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_007_Pendente_Fallback">
        <description>Pendente - Requer analise manual por auditor</description>
        <inputEntry id="IE_Pend_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Pend_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend_2"><text>"Caso requer analise manual por auditor especialista em OPME."</text></outputEntry>
        <outputEntry id="OE_Pend_3"><text>"Auditoria OPME - Processo manual"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Pendente_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_007_Pendente_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
