<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_UNN_002"
             name="Prevencao de Procedimento Desnecessario - Antibiotico para Infeccao Viral"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    UNN-002 - Prevencao de Procedimento Desnecessario - Antibiotico para Infeccao Viral
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Controle de Desperdicio
    Categoria: Unnecessary Procedure Prevention

    Objetivo:
    - Evitar prescricao de antibioticos para infeccoes virais
    - Combater resistencia antimicrobiana
    - Reduzir custos com medicamentos inapropriados

    Indicacoes para Bloqueio (Choosing Wisely):
    - Antibiotico para resfriado comum (CID J00)
    - Antibiotico para gripe sem complicacao (CID J11)
    - Antibiotico para bronquite aguda viral (CID J20.9)

    Excecoes Permitidas:
    - Infeccao bacteriana secundaria documentada
    - Paciente imunossuprimido
    - Comorbidades de alto risco (DPOC, cardiopatia)

    Referencias Normativas:
    - ABM Choosing Wisely Brasil 2024 - Recomendacao 1
    - OMS - Antimicrobial Stewardship 2023
    - CFM/ANVISA - Uso Racional de Antimicrobianos
  -->

  <decision id="Decision_UNN_002" name="Prevencao Desnecessario - Antibiotico para Infeccao Viral">
    <decisionTable id="DecisionTable_UNN_002" hitPolicy="FIRST">

      <!-- Input 1: CID compativel com infeccao viral -->
      <input id="Input_cidCompativelInfeccaoViral" label="CID Compativel com Infeccao Viral (J00, J11, J20.9)">
        <inputExpression id="InputExpr_cidCompativelInfeccaoViral" typeRef="boolean">
          <text>cidCompativelInfeccaoViral</text>
        </inputExpression>
      </input>

      <!-- Input 2: Infeccao bacteriana secundaria documentada -->
      <input id="Input_infeccaoBacterianaSecundaria" label="Infeccao Bacteriana Secundaria Documentada">
        <inputExpression id="InputExpr_infeccaoBacterianaSecundaria" typeRef="boolean">
          <text>infeccaoBacterianaSecundaria</text>
        </inputExpression>
      </input>

      <!-- Input 3: Paciente imunossuprimido -->
      <input id="Input_pacienteImunossuprimido" label="Paciente Imunossuprimido">
        <inputExpression id="InputExpr_pacienteImunossuprimido" typeRef="boolean">
          <text>pacienteImunossuprimido</text>
        </inputExpression>
      </input>

      <!-- Input 4: Comorbidades de alto risco -->
      <input id="Input_comorbidadesAltoRisco" label="Comorbidades de Alto Risco (DPOC, Cardiopatia, DM)">
        <inputExpression id="InputExpr_comorbidadesAltoRisco" typeRef="boolean">
          <text>comorbidadesAltoRisco</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- ============================================== -->
      <!-- REPROVADO: ATB para infeccao viral            -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_002_Reprovado_ATBViral">
        <description>Reprovado - Antibiotico para infeccao viral sem indicacao</description>
        <inputEntry id="IE_Rep1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"Antibiotico nao indicado para infeccao viral sem complicacao bacteriana. Contribui para resistencia antimicrobiana."</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"ABM Choosing Wisely 2024; OMS Antimicrobial Stewardship 2023"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Reprovado_ATBViral_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Reprovado_ATBViral_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Infeccao bacteriana secundaria      -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_002_Aprovado_InfeccaoBacteriana">
        <description>Aprovado - Infeccao bacteriana secundaria documentada</description>
        <inputEntry id="IE_Aprov1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Antibiotico autorizado - Infeccao bacteriana secundaria documentada (sinusite, otite, pneumonia)."</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"Complicacao bacteriana - Indicacao aceita"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Aprovado_InfeccaoBacteriana_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Aprovado_InfeccaoBacteriana_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Imunossuprimido                     -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_002_Aprovado_Imunossuprimido">
        <description>Aprovado - Paciente imunossuprimido requer profilaxia</description>
        <inputEntry id="IE_Aprov2_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Antibiotico autorizado - Paciente imunossuprimido com risco elevado de complicacao."</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"Imunossupressao - Profilaxia justificada"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Aprovado_Imunossuprimido_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Aprovado_Imunossuprimido_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Comorbidades alto risco             -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_002_Aprovado_Comorbidades">
        <description>Aprovado - Comorbidades de alto risco justificam ATB precoce</description>
        <inputEntry id="IE_Aprov3_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Antibiotico autorizado - Comorbidades de alto risco (DPOC, cardiopatia, DM) justificam tratamento precoce."</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"Alto risco - Tratamento preventivo"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Aprovado_Comorbidades_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Aprovado_Comorbidades_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Infeccao nao viral                  -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_002_Aprovado_NaoViral">
        <description>Aprovado - Diagnostico nao e infeccao viral</description>
        <inputEntry id="IE_Aprov4_1"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"Antibiotico autorizado - Diagnostico nao compativel com infeccao viral."</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"Indicacao bacteriana - Prescricao adequada"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Aprovado_NaoViral_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Aprovado_NaoViral_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- PENDENTE: Fallback                            -->
      <!-- ============================================== -->
      <rule id="Rule_UNN_002_Pendente_Fallback">
        <description>Pendente - Requer analise manual por auditor</description>
        <inputEntry id="IE_Pend_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Pend_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend_2"><text>"Caso requer analise manual para avaliacao de indicacao de antibioticoterapia."</text></outputEntry>
        <outputEntry id="OE_Pend_3"><text>"Auditoria farmaceutica - Processo manual"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Pendente_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_UNN_002_Pendente_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
