<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_DUP_006"
             name="Prevencao de Duplicidade - Medicamentos em Uso Continuo"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    DUP-006 - Prevencao de Duplicidade de Medicamentos em Uso Continuo
    Versao: 1.0.0 (2026-02-07)
    Especialidade: Controle de Desperdicio
    Categoria: Duplicate Procedure Prevention

    Objetivo:
    - Detectar solicitacoes duplicadas de medicamentos de uso continuo
    - Verificar intervalo adequado entre dispensacoes
    - Bloquear dispensacao antecipada sem justificativa

    Indicacoes para Bloqueio:
    - Nova dispensacao antes do termino da posologia anterior
    - Solicitacao do mesmo medicamento por prescritores diferentes
    - Quantidade superior ao tratamento de 30 dias

    Excecoes Permitidas:
    - Ajuste de dose documentado
    - Perda ou extravio justificado
    - Viagem prolongada documentada

    Referencias Normativas:
    - ANS - RN 465/2021 - Cobertura Farmaceutica
    - ANVISA - RDC 344/1998 - Controle de Medicamentos
    - ABRAMGE - Gestao de Uso Continuo 2023
  -->

  <decision id="Decision_DUP_006" name="Prevencao Duplicidade - Medicamentos em Uso Continuo">
    <decisionTable id="DecisionTable_DUP_006" hitPolicy="FIRST">

      <!-- Input 1: Dispensacao antecipada -->
      <input id="Input_dispensacaoAntecipada" label="Dispensacao Antecipada (antes do termino da anterior)">
        <inputExpression id="InputExpr_dispensacaoAntecipada" typeRef="boolean">
          <text>dispensacaoAntecipada</text>
        </inputExpression>
      </input>

      <!-- Input 2: Ajuste de dose documentado -->
      <input id="Input_ajusteDoseDocumentado" label="Ajuste de Dose Documentado">
        <inputExpression id="InputExpr_ajusteDoseDocumentado" typeRef="boolean">
          <text>ajusteDoseDocumentado</text>
        </inputExpression>
      </input>

      <!-- Input 3: Justificativa de perda/extravio -->
      <input id="Input_justificativaPerdaExtravio" label="Justificativa de Perda ou Extravio">
        <inputExpression id="InputExpr_justificativaPerdaExtravio" typeRef="boolean">
          <text>justificativaPerdaExtravio</text>
        </inputExpression>
      </input>

      <!-- Input 4: Viagem prolongada documentada -->
      <input id="Input_viagemProlongadaDocumentada" label="Viagem Prolongada Documentada">
        <inputExpression id="InputExpr_viagemProlongadaDocumentada" typeRef="boolean">
          <text>viagemProlongadaDocumentada</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- ============================================== -->
      <!-- REPROVADO: Antecipada sem justificativa       -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_006_Reprovado_SemJustificativa">
        <description>Reprovado - Dispensacao antecipada sem justificativa</description>
        <inputEntry id="IE_Rep1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Rep1_2"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Rep1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Rep1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Rep1_2"><text>"Dispensacao antecipada bloqueada. Periodo de uso da dispensacao anterior ainda nao foi concluido."</text></outputEntry>
        <outputEntry id="OE_Rep1_3"><text>"ANVISA RDC 344/1998; ABRAMGE Gestao Uso Continuo 2023"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Reprovado_SemJustificativa_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Reprovado_SemJustificativa_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Ajuste de dose                      -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_006_Aprovado_AjusteDose">
        <description>Aprovado - Ajuste de dose documentado pelo prescritor</description>
        <inputEntry id="IE_Aprov1_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Nova dispensacao autorizada - Ajuste de dose documentado pelo prescritor."</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"Ajuste terapeutico - Excecao aplicavel"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Aprovado_AjusteDose_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Aprovado_AjusteDose_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Perda ou extravio                   -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_006_Aprovado_PerdaExtravio">
        <description>Aprovado - Perda ou extravio justificado</description>
        <inputEntry id="IE_Aprov2_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Nova dispensacao autorizada - Perda ou extravio justificado (BO ou declaracao anexa)."</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"Perda/extravio documentado - Excecao unica"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Aprovado_PerdaExtravio_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Aprovado_PerdaExtravio_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Viagem prolongada                   -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_006_Aprovado_Viagem">
        <description>Aprovado - Viagem prolongada documentada</description>
        <inputEntry id="IE_Aprov3_1"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Dispensacao antecipada autorizada - Viagem prolongada documentada com comprovantes."</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"Viagem documentada - Excecao aplicavel"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Aprovado_Viagem_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Aprovado_Viagem_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APROVADO: Intervalo adequado                  -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_006_Aprovado_IntervaloAdequado">
        <description>Aprovado - Intervalo adequado entre dispensacoes</description>
        <inputEntry id="IE_Aprov4_1"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"Dispensacao autorizada - Intervalo adequado desde ultima retirada."</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"Verificacao de intervalo - OK"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Aprovado_IntervaloAdequado_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Aprovado_IntervaloAdequado_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- PENDENTE: Fallback                            -->
      <!-- ============================================== -->
      <rule id="Rule_DUP_006_Pendente_Fallback">
        <description>Pendente - Requer analise manual por auditor</description>
        <inputEntry id="IE_Pend_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Pend_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Pend_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Pend_2"><text>"Caso requer analise manual por auditor para validacao de intervalo de dispensacao."</text></outputEntry>
        <outputEntry id="OE_Pend_3"><text>"Auditoria farmaceutica - Processo manual"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Pendente_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_DUP_006_Pendente_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
