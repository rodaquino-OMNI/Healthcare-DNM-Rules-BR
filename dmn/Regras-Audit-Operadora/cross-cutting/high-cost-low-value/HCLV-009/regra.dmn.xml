<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_HCLV009" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_HCLV009" name="HCLV-009: Observação vs Internação">
    <decisionTable id="DecisionTable_HCLV009" hitPolicy="FIRST">
      <input id="Input_diagnostico" label="Diagnóstico"><inputExpression id="InputExpression_diagnostico" typeRef="string"><text>diagnostico</text></inputExpression></input>
      <input id="Input_criteriosInternacao" label="Critérios de Internação Atendidos"><inputExpression id="InputExpression_criteriosInternacao" typeRef="boolean"><text>criteriosInternacao</text></inputExpression></input>
      <input id="Input_horasObservacao" label="Horas em Observação"><inputExpression id="InputExpression_horasObservacao" typeRef="number"><text>horasObservacao</text></inputExpression></input>
      <input id="Input_evolucaoClinica" label="Evolução Clínica Favorável"><inputExpression id="InputExpression_evolucaoClinica" typeRef="boolean"><text>evolucaoClinica</text></inputExpression></input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string"><outputValues id="UnaryTests_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues></output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_diag"><text>"Dor Torácica Atípica","Síncope","Vertigem"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_crit"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R001_horas"><text>&lt; 24</text></inputEntry>
        <inputEntry id="UnaryTests_R001_evol"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao"><text>"Observação <24h com boa evolução suficiente. Internação desnecessária (economia 40-50%)"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao"><text>"AHA/ACC: Dor torácica baixo risco com troponina negativa pode receber alta da observação em 6-12h"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_diag"><text>"Desidratação Leve/Moderada","Gastroenterite"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_crit"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R002_horas"><text>&lt; 12</text></inputEntry>
        <inputEntry id="UnaryTests_R002_evol"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao"><text>"Hidratação em observação <12h resolveu quadro. Internação não indicada"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao"><text>"SBP: Desidratação leve/moderada responde a hidratação venosa em 6-12h. Internação desnecessária"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_diag"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R003_crit"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R003_horas"><text>&gt; 24</text></inputEntry>
        <inputEntry id="UnaryTests_R003_evol"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao"><text>"Critérios de internação preenchidos com evolução desfavorável >24h"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao"><text>"Observação >24h sem melhora indica necessidade de internação para investigação/tratamento"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_diag"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R004_crit"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R004_horas"><text>&lt; 24</text></inputEntry>
        <inputEntry id="UnaryTests_R004_evol"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao"><text>"Observação resolveu quadro. Alta hospitalar indicada"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao"><text>"ANS: Observação é alternativa custo-efetiva à internação para quadros autolimitados"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R005">
        <inputEntry id="UnaryTests_R005_diag"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R005_crit"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R005_horas"><text>[24..48]</text></inputEntry>
        <inputEntry id="UnaryTests_R005_evol"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R005_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_observacao"><text>"Observação 24-48h: avaliar necessidade de internação vs alta"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_fundamentacao"><text>"Observação prolongada (>24h) deve ser reavaliada. Considerar internação ou alta conforme evolução"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_diag"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_crit"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_horas"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_evol"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao"><text>"Análise manual necessária"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao"><text>"Avaliação por médico auditor requerida"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_HCLV009">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
