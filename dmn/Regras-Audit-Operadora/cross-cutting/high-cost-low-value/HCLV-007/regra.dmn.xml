<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_HCLV007" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_HCLV007" name="HCLV-007: Otimização do Local de Atendimento">
    <decisionTable id="DecisionTable_HCLV007" hitPolicy="FIRST">
      <input id="Input_procedimento" label="Procedimento"><inputExpression id="InputExpression_procedimento" typeRef="string"><text>procedimento</text></inputExpression></input>
      <input id="Input_localSolicitado" label="Local Solicitado"><inputExpression id="InputExpression_localSolicitado" typeRef="string"><text>localSolicitado</text></inputExpression></input>
      <input id="Input_complexidadeProcedimento" label="Complexidade"><inputExpression id="InputExpression_complexidadeProcedimento" typeRef="string"><text>complexidadeProcedimento</text></inputExpression></input>
      <input id="Input_anestesia" label="Tipo de Anestesia"><inputExpression id="InputExpression_anestesia" typeRef="string"><text>anestesia</text></inputExpression></input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string"><outputValues id="UnaryTests_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues></output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_proc"><text>"Colonoscopia","Endoscopia","Artroscopia"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_local"><text>"Hospital"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_complex"><text>"Baixa","Média"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_anest"><text>"Sedação","Local"</text></inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao"><text>"Procedimento pode ser realizado em centro ambulatorial (redução 40-60% custo)"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao"><text>"AMB/CFM: Procedimentos baixa complexidade com sedação leve podem ser ambulatoriais. Hospital desnecessário"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_proc"><text>"Quimioterapia","Imunoterapia"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_local"><text>"Hospital"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_complex"><text>"Baixa"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_anest"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao"><text>"Quimio/imuno de baixa complexidade: pode ser ambulatorial/domiciliar (redução 50% custo)"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao"><text>"SBOC: Protocolos de baixo risco emético/sem neutropenia podem ser ambulatoriais ou home-based"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_proc"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R003_local"><text>"Hospital"</text></inputEntry>
        <inputEntry id="UnaryTests_R003_complex"><text>"Alta"</text></inputEntry>
        <inputEntry id="UnaryTests_R003_anest"><text>"Geral","Raquianestesia"</text></inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao"><text>"Alta complexidade com anestesia geral/raqui: ambiente hospitalar indicado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao"><text>"Procedimentos alta complexidade requerem estrutura hospitalar (recuperação, UTI de retaguarda)"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_proc"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R004_local"><text>"Ambulatório","Domiciliar"</text></inputEntry>
        <inputEntry id="UnaryTests_R004_complex"><text>"Baixa","Média"</text></inputEntry>
        <inputEntry id="UnaryTests_R004_anest"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao"><text>"Local apropriado para complexidade baixa/média"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao"><text>"Otimização do local de atendimento reduz custos sem comprometer segurança"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_proc"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_local"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_complex"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_anest"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao"><text>"Análise manual necessária"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao"><text>"Avaliação por gestor de utilização requerida"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_HCLV007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
