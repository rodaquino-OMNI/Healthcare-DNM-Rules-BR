<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_HCLV004" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_HCLV004" name="HCLV-004: Step Therapy (Terapia Escalonada)">
    <decisionTable id="DecisionTable_HCLV004" hitPolicy="FIRST">
      <input id="Input_medicamentoSolicitado" label="Medicamento Solicitado">
        <inputExpression id="InputExpression_medicamentoSolicitado" typeRef="string">
          <text>medicamentoSolicitado</text>
        </inputExpression>
      </input>
      <input id="Input_linhaTerapeutica" label="Linha Terapêutica">
        <inputExpression id="InputExpression_linhaTerapeutica" typeRef="string">
          <text>linhaTerapeutica</text>
        </inputExpression>
      </input>
      <input id="Input_tratamentosPrevios" label="Tratamentos de 1ª Linha Testados">
        <inputExpression id="InputExpression_tratamentosPrevios" typeRef="boolean">
          <text>tratamentosPreviosTestados</text>
        </inputExpression>
      </input>
      <input id="Input_falhaAnterior" label="Falha/Contraindicação 1ª Linha">
        <inputExpression id="InputExpression_falhaAnterior" typeRef="boolean">
          <text>falhaAnterior</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_medicamento"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R001_linha"><text>"2ª Linha","3ª Linha"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_previos"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R001_falha"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao"><text>"Medicamento de 2ª/3ª linha sem tentativa prévia com tratamentos de 1ª linha"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao"><text>"Protocolos clínicos: Tratamentos de 1ª linha devem ser tentados primeiro (menor custo, melhor perfil de segurança)"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_medicamento"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R002_linha"><text>"2ª Linha","3ª Linha"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_previos"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R002_falha"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao"><text>"Falha documentada com tratamentos de 1ª linha. Progressão para 2ª/3ª linha justificada"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao"><text>"Step therapy justificado: Ineficácia ou intolerância a 1ª linha permite progressão para alternativas"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_medicamento"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R003_linha"><text>"1ª Linha"</text></inputEntry>
        <inputEntry id="UnaryTests_R003_previos"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R003_falha"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao"><text>"Medicamento de 1ª linha: escolha apropriada conforme protocolos"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao"><text>"Tratamentos de 1ª linha são custo-efetivos e têm evidência robusta de eficácia/segurança"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_medicamento"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R004_linha"><text>"2ª Linha"</text></inputEntry>
        <inputEntry id="UnaryTests_R004_previos"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R004_falha"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao"><text>"Tratamentos de 1ª linha testados sem falha documentada. Necessária reavaliação antes de 2ª linha"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao"><text>"Se 1ª linha foi tolerada, otimização de dose/duração pode evitar progressão prematura para 2ª linha"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_medicamento"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_linha"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_previos"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_falha"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao"><text>"Análise manual necessária: linha terapêutica ou histórico de tratamentos não determinado"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao"><text>"Consultar protocolos clínicos e histórico do paciente"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_HCLV004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
