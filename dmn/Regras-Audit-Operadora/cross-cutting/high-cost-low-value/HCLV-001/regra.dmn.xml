<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_HCLV001" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_HCLV001" name="HCLV-001: Substituição por Genérico Obrigatória">
    <decisionTable id="DecisionTable_HCLV001" hitPolicy="FIRST">
      <input id="Input_medicamentoSolicitado" label="Medicamento Solicitado">
        <inputExpression id="InputExpression_medicamentoSolicitado" typeRef="string">
          <text>medicamentoSolicitado</text>
        </inputExpression>
      </input>
      <input id="Input_genericoDisponivel" label="Genérico Disponível">
        <inputExpression id="InputExpression_genericoDisponivel" typeRef="boolean">
          <text>genericoDisponivel</text>
        </inputExpression>
      </input>
      <input id="Input_contraindicacaoDocumentada" label="Contraindicação Documentada">
        <inputExpression id="InputExpression_contraindicacaoDocumentada" typeRef="boolean">
          <text>contraindicacaoDocumentada</text>
        </inputExpression>
      </input>
      <input id="Input_classeReferencia" label="Referência ou Similar">
        <inputExpression id="InputExpression_classeReferencia" typeRef="string">
          <text>classeReferencia</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- REPROVADO: Referência/Similar quando genérico disponível sem contraindicação -->
      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_medicamento">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R001_generico">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R001_contraindicacao">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R001_classe">
          <text>"Referência","Similar"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao">
          <text>"Genérico disponível. Substituição obrigatória por medicamento de referência/similar sem justificativa"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao">
          <text>"ANVISA Lei 9787/1999: Genéricos têm equivalência terapêutica comprovada. Redução de custo 30-70% sem perda de eficácia"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVADO: Genérico já solicitado -->
      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_medicamento">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R002_generico">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R002_contraindicacao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R002_classe">
          <text>"Genérico"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao">
          <text>"Genérico já prescrito. Conduta adequada"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao">
          <text>"Prescrição com genérico otimiza custos sem comprometer qualidade assistencial"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVADO: Contraindicação documentada para genérico -->
      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_medicamento">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R003_generico">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R003_contraindicacao">
          <text>true</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R003_classe">
          <text>"Referência","Similar"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao">
          <text>"Contraindicação documentada ao genérico (ex: alergia a excipiente). Referência/similar justificado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao">
          <text>"ANVISA: Excipientes podem diferir entre genéricos e referência. Contraindicação documentada justifica uso de referência"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- PENDENTE: Genérico não disponível no mercado -->
      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_medicamento">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R004_generico">
          <text>false</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R004_contraindicacao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R004_classe">
          <text>"Referência"</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao">
          <text>"Genérico não disponível no mercado. Medicamento de referência aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao">
          <text>"Ausência de genérico registrado na ANVISA justifica uso de referência (ex: medicamentos recentes, órfãos)"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_medicamento">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_Fallback_generico">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_Fallback_contraindicacao">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_Fallback_classe">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao">
          <text>"Análise manual necessária: classificação do medicamento ou disponibilidade de genérico não determinada"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao">
          <text>"Consultar lista ANVISA de medicamentos genéricos registrados"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_HCLV001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
