<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_HCLV010" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_HCLV010" name="HCLV-010: Choosing Wisely - Cuidados de Baixo Valor">
    <decisionTable id="DecisionTable_HCLV010" hitPolicy="FIRST">
      <input id="Input_procedimento" label="Procedimento/Exame"><inputExpression id="InputExpression_procedimento" typeRef="string"><text>procedimento</text></inputExpression></input>
      <input id="Input_indicacaoClinica" label="Indicação Clínica Robusta"><inputExpression id="InputExpression_indicacaoClinica" typeRef="boolean"><text>indicacaoClinica</text></inputExpression></input>
      <input id="Input_alterariaConduta" label="Resultado Alteraria Conduta"><inputExpression id="InputExpression_alterariaConduta" typeRef="boolean"><text>alterariaConduta</text></inputExpression></input>
      <input id="Input_pacienteAssintomatico" label="Paciente Assintomático"><inputExpression id="InputExpression_pacienteAssintomatico" typeRef="boolean"><text>pacienteAssintomatico</text></inputExpression></input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string"><outputValues id="UnaryTests_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues></output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_proc"><text>"TC Crânio Cefaleia Sem Sinais Neurológicos","RM Lombar Lombalgia Aguda <6 sem"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_indic"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R001_alter"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R001_assint"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao"><text>"Choosing Wisely: Exame não alterará conduta. Custo desnecessário + ansiedade + achados incidentais"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao"><text>"Choosing Wisely Brasil/SBCM: TC crânio para cefaleia sem red flags e RM lombar lombalgia <6 sem são baixo valor"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_proc"><text>"PSA Anual >75 anos","Colonoscopia >85 anos","Papanicolaou >65 anos"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_indic"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R002_alter"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R002_assint"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao"><text>"Rastreamento fora da faixa etária recomendada. Riscos > benefícios"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao"><text>"Choosing Wisely/USPSTF: Rastreamentos em idosos >75-85 anos geram sobrediagnóstico sem reduzir mortalidade"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_proc"><text>"EEG Síncope","EEG Cefaleia","Rx Tórax Pré-Op Baixo Risco"</text></inputEntry>
        <inputEntry id="UnaryTests_R003_indic"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R003_alter"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R003_assint"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao"><text>"Exame sem indicação clínica estabelecida. Não contribui para diagnóstico/conduta"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao"><text>"Choosing Wisely: EEG para síncope/cefaleia sem indicação neurológica e Rx pré-op baixo risco são baixo valor"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_proc"><text>"Vitamina D Assintomáticos","PCR Infecções Virais","D-dímero Baixa Probabilidade TEP"</text></inputEntry>
        <inputEntry id="UnaryTests_R004_indic"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R004_alter"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R004_assint"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao"><text>"Exame laboratorial sem indicação precisa. Alto custo com baixa utilidade clínica"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao"><text>"Choosing Wisely: Vitamina D de rotina, PCR viral, D-dímero screening são sobreutilizados sem impacto clínico"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R005">
        <inputEntry id="UnaryTests_R005_proc"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R005_indic"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R005_alter"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R005_assint"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R005_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_observacao"><text>"Indicação clínica robusta. Resultado alterará conduta terapêutica"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_fundamentacao"><text>"Exame com indicação clara e impacto na decisão clínica tem alto valor"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R006">
        <inputEntry id="UnaryTests_R006_proc"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R006_indic"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R006_alter"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R006_assint"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R006_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_R006_observacao"><text>"Resultado não alteraria conduta. Questionar necessidade do exame"</text></outputEntry>
        <outputEntry id="LiteralExpression_R006_fundamentacao"><text>"Princípio de alto valor: Só solicitar exames cujos resultados modificarão a conduta"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R006_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R006_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_proc"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_indic"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_alter"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_assint"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao"><text>"Análise manual necessária"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao"><text>"Consultar guidelines Choosing Wisely Brasil"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_HCLV010">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
