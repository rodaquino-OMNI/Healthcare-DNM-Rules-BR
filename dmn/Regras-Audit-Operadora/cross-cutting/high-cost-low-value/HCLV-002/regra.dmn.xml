<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_HCLV002" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_HCLV002" name="HCLV-002: Preferência por Biosimilar">
    <decisionTable id="DecisionTable_HCLV002" hitPolicy="FIRST">
      <input id="Input_medicamentoBiologico" label="Medicamento Biológico">
        <inputExpression id="InputExpression_medicamentoBiologico" typeRef="string">
          <text>medicamentoBiologico</text>
        </inputExpression>
      </input>
      <input id="Input_biosimilarDisponivel" label="Biosimilar Disponível">
        <inputExpression id="InputExpression_biosimilarDisponivel" typeRef="boolean">
          <text>biosimilarDisponivel</text>
        </inputExpression>
      </input>
      <input id="Input_pacienteNaive" label="Paciente Naive (novo tratamento)">
        <inputExpression id="InputExpression_pacienteNaive" typeRef="boolean">
          <text>pacienteNaive</text>
        </inputExpression>
      </input>
      <input id="Input_falhaAnteriorBiosimilar" label="Falha Anterior com Biosimilar">
        <inputExpression id="InputExpression_falhaAnteriorBiosimilar" typeRef="boolean">
          <text>falhaAnteriorBiosimilar</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_biologico"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R001_biosimilar"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R001_naive"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R001_falha"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao"><text>"Paciente naive: biosimilar deve ser primeira escolha (redução 20-40% custo)"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao"><text>"ANVISA RDC 55/2010: Biosimilares demonstram equivalência clínica. Preferência em pacientes naive otimiza recursos"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_biologico"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R002_biosimilar"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R002_naive"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R002_falha"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao"><text>"Falha terapêutica prévia com biosimilar. Biológico de referência justificado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao"><text>"SBR/SBRA: Falha terapêutica documentada com biosimilar justifica switch para biológico referência"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_biologico"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R003_biosimilar"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R003_naive"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R003_falha"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao"><text>"Biosimilar não disponível no mercado. Biológico de referência aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao"><text>"Ausência de biosimilar registrado na ANVISA justifica uso de biológico referência"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_biologico"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R004_biosimilar"><text>true</text></inputEntry>
        <inputEntry id="UnaryTests_R004_naive"><text>false</text></inputEntry>
        <inputEntry id="UnaryTests_R004_falha"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao"><text>"Paciente em tratamento estável com referência. Avaliação de switch para biosimilar recomendada"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao"><text>"EMA/SBR: Switch de referência para biosimilar é seguro, mas requer monitoramento. Decisão compartilhada recomendada"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_biologico"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_biosimilar"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_naive"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_falha"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao"><text>"Análise manual necessária: histórico de tratamento ou disponibilidade de biosimilar não determinada"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao"><text>"Consultar lista ANVISA de biossimilares registrados e histórico do paciente"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_HCLV002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
