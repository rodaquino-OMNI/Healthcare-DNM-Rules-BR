<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_OVER004" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_OVER004" name="OVER-004: Protocolo de Desmame Ventilatório">
    <decisionTable id="DecisionTable_OVER004" hitPolicy="FIRST">
      <input id="Input_diasVentilacao" label="Dias em Ventilação Mecânica">
        <inputExpression id="InputExpression_diasVentilacao" typeRef="number">
          <text>diasVentilacao</text>
        </inputExpression>
      </input>
      <input id="Input_pao2fio2" label="Relação PaO2/FiO2">
        <inputExpression id="InputExpression_pao2fio2" typeRef="number">
          <text>pao2fio2</text>
        </inputExpression>
      </input>
      <input id="Input_peepAtual" label="PEEP Atual (cmH2O)">
        <inputExpression id="InputExpression_peepAtual" typeRef="number">
          <text>peepAtual</text>
        </inputExpression>
      </input>
      <input id="Input_testeDesmameRealizado" label="Teste de Respiração Espontânea Realizado">
        <inputExpression id="InputExpression_testeDesmameRealizado" typeRef="boolean">
          <text>testeDesmameRealizado</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <!-- REPROVADO: Ventilação prolongada sem critérios de desmame -->
      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_dias">
          <text>&gt; 7</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R001_pao2">
          <text>&gt;= 300</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R001_peep">
          <text>&lt;= 5</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R001_teste">
          <text>false</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao">
          <text>"Ventilação >7 dias com critérios de desmame atingidos mas teste não realizado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao">
          <text>"Protocolo AMIB: PaO2/FiO2 >300, PEEP ≤5 cmH2O → realizar teste de respiração espontânea. Ventilação desnecessária aumenta risco de PAVM"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- REPROVADO: Ventilação prolongada sem justificativa -->
      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_dias">
          <text>&gt; 14</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R002_pao2">
          <text>&gt;= 250</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R002_peep">
          <text>&lt;= 8</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R002_teste">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao">
          <text>"Ventilação mecânica prolongada (>14 dias) com parâmetros que sugerem possibilidade de desmame"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao">
          <text>"Ventilação prolongada >14 dias aumenta mortalidade e custos. PEEP ≤8 e PaO2/FiO2 ≥250 sugerem capacidade de desmame"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVADO: Critérios para desmame não atingidos -->
      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_dias">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R003_pao2">
          <text>&lt; 150</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R003_peep">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R003_teste">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao">
          <text>"PaO2/FiO2 <150 indica SDRA moderada/grave. Ventilação mecânica justificada"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao">
          <text>"Definição Berlin SDRA: PaO2/FiO2 <200 = SDRA moderada, <100 = grave. Ventilação necessária"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVADO: PEEP alto indicando hipoxemia -->
      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_dias">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R004_pao2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R004_peep">
          <text>&gt; 10</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R004_teste">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao">
          <text>"PEEP >10 cmH2O indica necessidade de suporte ventilatório avançado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao">
          <text>"PEEP >10 cmH2O usado em SDRA moderada/grave. Desmame prematuro pode causar colapso alveolar e hipoxemia refratária"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- APROVADO: Ventilação curta ou teste realizado -->
      <rule id="DecisionRule_R005">
        <inputEntry id="UnaryTests_R005_dias">
          <text>&lt;= 7</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R005_pao2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R005_peep">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R005_teste">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R005_resultado">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R005_observacao">
          <text>"Ventilação ≤7 dias. Dentro do tempo esperado para recuperação"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R005_fundamentacao">
          <text>"Tempo mediano de ventilação mecânica: 4-7 dias para pneumonia/sepse, até resolução da causa base"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R005_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- PENDENTE: Teste de desmame realizado, aguardar resultado -->
      <rule id="DecisionRule_R006">
        <inputEntry id="UnaryTests_R006_dias">
          <text>&gt; 7</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R006_pao2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R006_peep">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_R006_teste">
          <text>true</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_R006_resultado">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R006_observacao">
          <text>"Teste de respiração espontânea em andamento. Aguardar resultado para decisão de extubação"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_R006_fundamentacao">
          <text>"Protocolo de desmame: teste de respiração espontânea (30-120min) com avaliação de parâmetros clínicos e gasométricos"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_R006_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R006_5"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_dias">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_Fallback_pao2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_Fallback_peep">
          <text>-</text>
        </inputEntry>
        <inputEntry id="UnaryTests_Fallback_teste">
          <text>-</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao">
          <text>"Análise manual necessária: dados gasométricos ou ventilatórios insuficientes"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao">
          <text>"Avaliação por intensivista requerida com análise completa de parâmetros ventilatórios e clínicos"</text>
        </outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_OVER004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
