<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_OVER010" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_OVER010" name="OVER-010: Adequação de Horas de Cuidados de Enfermagem">
    <decisionTable id="DecisionTable_OVER010" hitPolicy="FIRST">
      <input id="Input_horasCuidadoDia" label="Horas de Cuidado/Dia">
        <inputExpression id="InputExpression_horasCuidadoDia" typeRef="number">
          <text>horasCuidadoDia</text>
        </inputExpression>
      </input>
      <input id="Input_niveComplexidade" label="Nível de Complexidade (1-5)">
        <inputExpression id="InputExpression_nivelComplexidade" typeRef="number">
          <text>nivelComplexidade</text>
        </inputExpression>
      </input>
      <input id="Input_dependenciaAVD" label="Dependência para AVDs">
        <inputExpression id="InputExpression_dependenciaAVD" typeRef="string">
          <text>dependenciaAVD</text>
        </inputExpression>
      </input>
      <input id="Input_dispositivos" label="Dispositivos/Cateteres (nº)">
        <inputExpression id="InputExpression_dispositivos" typeRef="number">
          <text>dispositivos</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_horas"><text>&gt; 12</text></inputEntry>
        <inputEntry id="UnaryTests_R001_complexidade"><text>[1..2]</text></inputEntry>
        <inputEntry id="UnaryTests_R001_avd"><text>"Independente","Parcialmente Dependente"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_dispositivos"><text>&lt;= 1</text></inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao"><text>"Mais de 12h/dia de cuidados para paciente de baixa complexidade independente/parcialmente dependente"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao"><text>"COFEN Res. 543/2017: Pacientes baixa complexidade requerem 3-6h/dia de cuidados. >12h sugere uso excessivo"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_horas"><text>&gt; 18</text></inputEntry>
        <inputEntry id="UnaryTests_R002_complexidade"><text>[3..4]</text></inputEntry>
        <inputEntry id="UnaryTests_R002_avd"><text>"Parcialmente Dependente"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_dispositivos"><text>[2..3]</text></inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao"><text>"Mais de 18h/dia para complexidade média sem dependência total ou múltiplos dispositivos"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao"><text>"COFEN: Complexidade intermediária 6-12h/dia. 18-24h reservado para alta dependência (ventilação, NPT, etc)"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_horas"><text>&gt;= 18</text></inputEntry>
        <inputEntry id="UnaryTests_R003_complexidade"><text>5</text></inputEntry>
        <inputEntry id="UnaryTests_R003_avd"><text>"Totalmente Dependente"</text></inputEntry>
        <inputEntry id="UnaryTests_R003_dispositivos"><text>&gt;= 4</text></inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao"><text>"Cuidados 18-24h para paciente complexidade máxima, totalmente dependente com múltiplos dispositivos"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao"><text>"COFEN: Alta complexidade (ventilação, drogas vasoativas, NPT, múltiplos cateteres) requer 12-24h/dia"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_horas"><text>[12..18]</text></inputEntry>
        <inputEntry id="UnaryTests_R004_complexidade"><text>[4..5]</text></inputEntry>
        <inputEntry id="UnaryTests_R004_avd"><text>"Totalmente Dependente"</text></inputEntry>
        <inputEntry id="UnaryTests_R004_dispositivos"><text>&gt;= 3</text></inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao"><text>"Cuidados 12-18h para alta complexidade com dependência total e dispositivos"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao"><text>"Pacientes com traqueostomia, gastrostomia, cateter central, sonda vesical requerem cuidados intensivos"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R005">
        <inputEntry id="UnaryTests_R005_horas"><text>[6..12]</text></inputEntry>
        <inputEntry id="UnaryTests_R005_complexidade"><text>[2..4]</text></inputEntry>
        <inputEntry id="UnaryTests_R005_avd"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R005_dispositivos"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R005_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_observacao"><text>"Cuidados 6-12h apropriados para complexidade baixa-média"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_fundamentacao"><text>"Faixa padrão conforme COFEN para pacientes intermediários com necessidades moderadas"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R006">
        <inputEntry id="UnaryTests_R006_horas"><text>&lt; 6</text></inputEntry>
        <inputEntry id="UnaryTests_R006_complexidade"><text>[4..5]</text></inputEntry>
        <inputEntry id="UnaryTests_R006_avd"><text>"Totalmente Dependente"</text></inputEntry>
        <inputEntry id="UnaryTests_R006_dispositivos"><text>&gt;= 3</text></inputEntry>
        <outputEntry id="LiteralExpression_R006_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R006_observacao"><text>"Cuidados insuficientes (<6h/dia) para paciente alta complexidade totalmente dependente"</text></outputEntry>
        <outputEntry id="LiteralExpression_R006_fundamentacao"><text>"Risco de eventos adversos (queda, lesão por pressão, infecção) com cuidados inadequados"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R006_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R006_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_horas"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_complexidade"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_avd"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_dispositivos"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao"><text>"Análise manual necessária: dados de complexidade ou dependência insuficientes"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao"><text>"Avaliação por enfermeiro gestor de caso requerida"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_OVER010">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
