<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_OVER007" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_OVER007" name="OVER-007: Critérios para Oxigenoterapia">
    <decisionTable id="DecisionTable_OVER007" hitPolicy="FIRST">
      <input id="Input_spo2Atual" label="SpO2 Atual (%)">
        <inputExpression id="InputExpression_spo2Atual" typeRef="number">
          <text>spo2Atual</text>
        </inputExpression>
      </input>
      <input id="Input_pao2Gasometria" label="PaO2 Gasometria (mmHg)">
        <inputExpression id="InputExpression_pao2Gasometria" typeRef="number">
          <text>pao2Gasometria</text>
        </inputExpression>
      </input>
      <input id="Input_diasO2Suplementar" label="Dias em O2 Suplementar">
        <inputExpression id="InputExpression_diasO2Suplementar" typeRef="number">
          <text>diasO2Suplementar</text>
        </inputExpression>
      </input>
      <input id="Input_condicaoCronica" label="Condição Crônica (DPOC, Fibrose)">
        <inputExpression id="InputExpression_condicaoCronica" typeRef="boolean">
          <text>condicaoCronica</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_spo2"><text>&lt; 88</text></inputEntry>
        <inputEntry id="UnaryTests_R001_pao2"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R001_dias"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R001_cronica"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao"><text>"SpO2 <88%: hipoxemia grave. O2 suplementar indicado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao"><text>"BTS Guidelines: SpO2 <88% = hipoxemia grave. Meta SpO2 88-92% (DPOC) ou 94-98% (outras condições)"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_spo2"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R002_pao2"><text>&lt; 55</text></inputEntry>
        <inputEntry id="UnaryTests_R002_dias"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R002_cronica"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao"><text>"PaO2 <55 mmHg: critério absoluto para O2 domiciliar prolongado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao"><text>"GOLD/ATS: PaO2 <55 mmHg (ou <60 mmHg com cor pulmonale) indica O2 domiciliar >15h/dia"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_spo2"><text>&gt;= 95</text></inputEntry>
        <inputEntry id="UnaryTests_R003_pao2"><text>&gt; 70</text></inputEntry>
        <inputEntry id="UnaryTests_R003_dias"><text>&gt; 3</text></inputEntry>
        <inputEntry id="UnaryTests_R003_cronica"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao"><text>"SpO2 ≥95% com PaO2 normal >3 dias: O2 suplementar não justificado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao"><text>"Oxigênio em excesso causa toxicidade (atelectasias, vasoconstrição) sem benefício clínico"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_spo2"><text>[88..92]</text></inputEntry>
        <inputEntry id="UnaryTests_R004_pao2"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R004_dias"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R004_cronica"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao"><text>"DPOC/Fibrose: meta SpO2 88-92% (evitar hipercapnia por hiperoxia)"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao"><text>"GOLD 2023: Pacientes DPOC devem manter SpO2 88-92% para evitar retenção de CO2"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R005">
        <inputEntry id="UnaryTests_R005_spo2"><text>[92..95)</text></inputEntry>
        <inputEntry id="UnaryTests_R005_pao2"><text>[60..70]</text></inputEntry>
        <inputEntry id="UnaryTests_R005_dias"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R005_cronica"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R005_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_observacao"><text>"Hipoxemia leve-moderada. Avaliar necessidade de O2 conforme sintomas e atividade"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_fundamentacao"><text>"PaO2 60-70 mmHg é limítrofe. Considerar O2 se sintomas (dispneia, fadiga) ou dessaturação com esforço"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_spo2"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_pao2"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_dias"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_cronica"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao"><text>"Análise manual necessária: dados de oximetria/gasometria insuficientes"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao"><text>"Avaliação por pneumologista requerida"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_OVER007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
