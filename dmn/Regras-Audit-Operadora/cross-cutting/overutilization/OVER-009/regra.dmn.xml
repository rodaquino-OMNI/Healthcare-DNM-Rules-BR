<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_OVER009" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <decision id="Decision_OVER009" name="OVER-009: Frequência de Sessões de Reabilitação">
    <decisionTable id="DecisionTable_OVER009" hitPolicy="FIRST">
      <input id="Input_tipoReabilitacao" label="Tipo de Reabilitação">
        <inputExpression id="InputExpression_tipoReabilitacao" typeRef="string">
          <text>tipoReabilitacao</text>
        </inputExpression>
      </input>
      <input id="Input_sessoesSemanais" label="Sessões Semanais">
        <inputExpression id="InputExpression_sessoesSemanais" typeRef="number">
          <text>sessoesSemanais</text>
        </inputExpression>
      </input>
      <input id="Input_faseTratamento" label="Fase do Tratamento">
        <inputExpression id="InputExpression_faseTratamento" typeRef="string">
          <text>faseTratamento</text>
        </inputExpression>
      </input>
      <input id="Input_evolucaoFuncional" label="Evolução Funcional Documentada">
        <inputExpression id="InputExpression_evolucaoFuncional" typeRef="boolean">
          <text>evolucaoFuncional</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="DecisionRule_R001">
        <inputEntry id="UnaryTests_R001_tipo"><text>"Fonoaudiologia"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_sessoes"><text>&gt; 5</text></inputEntry>
        <inputEntry id="UnaryTests_R001_fase"><text>"Manutenção"</text></inputEntry>
        <inputEntry id="UnaryTests_R001_evolucao"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R001_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_observacao"><text>"Fonoaudiologia >5x/semana na fase de manutenção sem ganhos funcionais"</text></outputEntry>
        <outputEntry id="LiteralExpression_R001_fundamentacao"><text>"SBFa: Fase de manutenção 2-3x/semana. Frequência intensiva (5x) reservada para fase aguda/intensiva"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R001_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R002">
        <inputEntry id="UnaryTests_R002_tipo"><text>"Terapia Ocupacional"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_sessoes"><text>&gt; 5</text></inputEntry>
        <inputEntry id="UnaryTests_R002_fase"><text>"Crônica"</text></inputEntry>
        <inputEntry id="UnaryTests_R002_evolucao"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R002_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_observacao"><text>"Terapia ocupacional >5x/semana na fase crônica sem evidência de progresso"</text></outputEntry>
        <outputEntry id="LiteralExpression_R002_fundamentacao"><text>"COFFITO: Fase crônica 2-3x/semana para manutenção. Alta frequência requer justificativa de nova meta funcional"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R002_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R003">
        <inputEntry id="UnaryTests_R003_tipo"><text>"Fisioterapia"</text></inputEntry>
        <inputEntry id="UnaryTests_R003_sessoes"><text>&gt; 7</text></inputEntry>
        <inputEntry id="UnaryTests_R003_fase"><text>"Aguda","Pós-Cirúrgica"</text></inputEntry>
        <inputEntry id="UnaryTests_R003_evolucao"><text>false</text></inputEntry>
        <outputEntry id="LiteralExpression_R003_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_observacao"><text>"Fisioterapia >7x/semana (mais que diária) sem ganhos documentados"</text></outputEntry>
        <outputEntry id="LiteralExpression_R003_fundamentacao"><text>"Protocolo COFFITO: Máximo 5-7x/semana na fase intensiva. Ausência de progresso indica necessidade de reavaliação"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R003_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R004">
        <inputEntry id="UnaryTests_R004_tipo"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R004_sessoes"><text>&gt;= 5</text></inputEntry>
        <inputEntry id="UnaryTests_R004_fase"><text>"Aguda","Pós-AVC &lt;3 meses","Pós-Cirúrgica"</text></inputEntry>
        <inputEntry id="UnaryTests_R004_evolucao"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R004_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_observacao"><text>"Reabilitação intensiva na fase aguda com progresso funcional documentado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R004_fundamentacao"><text>"Fase aguda/pós-cirúrgica justifica alta frequência (5-7x/semana) quando há ganhos funcionais mensuráveis"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R004_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R005">
        <inputEntry id="UnaryTests_R005_tipo"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R005_sessoes"><text>[2..5]</text></inputEntry>
        <inputEntry id="UnaryTests_R005_fase"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R005_evolucao"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_R005_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_observacao"><text>"Frequência 2-5x/semana apropriada para a maioria das condições de reabilitação"</text></outputEntry>
        <outputEntry id="LiteralExpression_R005_fundamentacao"><text>"Frequência padrão conforme diretrizes COFFITO/SBFa para reabilitação ambulatorial"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R005_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_R006">
        <inputEntry id="UnaryTests_R006_tipo"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_R006_sessoes"><text>&gt; 5</text></inputEntry>
        <inputEntry id="UnaryTests_R006_fase"><text>"Manutenção","Crônica"</text></inputEntry>
        <inputEntry id="UnaryTests_R006_evolucao"><text>true</text></inputEntry>
        <outputEntry id="LiteralExpression_R006_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_R006_observacao"><text>"Alta frequência na fase crônica com ganhos. Requer justificativa de novos objetivos terapêuticos"</text></outputEntry>
        <outputEntry id="LiteralExpression_R006_fundamentacao"><text>"Ganhos funcionais em fase crônica podem justificar intensificação temporária. Necessária documentação de metas"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R006_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_R006_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_Fallback">
        <inputEntry id="UnaryTests_Fallback_tipo"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_sessoes"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_fase"><text>-</text></inputEntry>
        <inputEntry id="UnaryTests_Fallback_evolucao"><text>-</text></inputEntry>
        <outputEntry id="LiteralExpression_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_observacao"><text>"Análise manual necessária: dados de fase ou evolução insuficientes"</text></outputEntry>
        <outputEntry id="LiteralExpression_Fallback_fundamentacao"><text>"Avaliação por equipe de reabilitação requerida"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_DecisionRule_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_OVER009">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
