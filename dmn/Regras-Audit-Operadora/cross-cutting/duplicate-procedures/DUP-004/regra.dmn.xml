<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="regra-DUP-004" name="Limite Sessoes Reabilitacao" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="DUP-004" name="Limite Sessoes Reabilitacao">
    <decisionTable id="decisionTable_DUP004" hitPolicy="FIRST">
      <input id="input_1" label="Tipo Reabilitacao">
        <inputExpression id="inputExpression_1" typeRef="string">
          <text>tipoReabilitacao</text>
        </inputExpression>
        <inputValues id="inputValues_1">
          <text>"Fisioterapia","Fonoaudiologia","TerapiaOcupacional","Psicologia","Hidroterapia"</text>
        </inputValues>
      </input>
      <input id="input_2" label="Sessoes Realizadas 30 Dias">
        <inputExpression id="inputExpression_2" typeRef="integer">
          <text>sessoesRealizadas30Dias</text>
        </inputExpression>
      </input>
      <input id="input_3" label="Laudo Medico Renovacao">
        <inputExpression id="inputExpression_3" typeRef="boolean">
          <text>laudoMedicoRenovacao</text>
        </inputExpression>
      </input>
      <input id="input_4" label="Condicao Cronica">
        <inputExpression id="inputExpression_4" typeRef="boolean">
          <text>condicaoCronica</text>
        </inputExpression>
      </input>
      <output id="output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_1">
          <text>"Aprovado","Reprovado"</text>
        </outputValues>
      </output>
      <output id="output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />
      
      <output id="output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="outputValues_4">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      

      <output id="output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="outputValues_5">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="rule_001">
        <inputEntry id="input_001_1">
          <text>"Fisioterapia"</text>
        </inputEntry>
        <inputEntry id="input_001_2">
          <text>&gt; 24</text>
        </inputEntry>
        <inputEntry id="input_001_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="input_001_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_001_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_001_2">
          <text>"Excesso de sessoes: fisioterapia ultrapassou 24 sessoes/mes sem laudo de renovacao. Custo evitavel: R$ 600-1200. Limite ANS: 20-24 sessoes/mes para tratamentos agudos."</text>
        </outputEntry>
        <outputEntry id="output_001_3">
          <text>"ANS RN 465/2021 Art. 17; COFFITO Diretrizes Fisioterapia; Lei 8.856/1994"</text>
        </outputEntry>
        <outputEntry id="OE_rule_001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_rule_001_5"><text>"NEGAR"</text></outputEntry>

      </rule>
      <rule id="rule_002">
        <inputEntry id="input_002_1">
          <text>"Fonoaudiologia"</text>
        </inputEntry>
        <inputEntry id="input_002_2">
          <text>&gt; 12</text>
        </inputEntry>
        <inputEntry id="input_002_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_002_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_002_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_002_2">
          <text>"Limite excedido: fonoaudiologia ultrapassou 12 sessoes/mes. Custo evitavel: R$ 480-960. Necessaria reavaliacao fonoaudiologica para continuidade."</text>
        </outputEntry>
        <outputEntry id="output_002_3">
          <text>"ANS RN 465/2021 Art. 17; CFFa Resolucao 492/2015; Lei 6.965/1981"</text>
        </outputEntry>
        <outputEntry id="OE_rule_002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_rule_002_5"><text>"NEGAR"</text></outputEntry>

      </rule>
      <rule id="rule_003">
        <inputEntry id="input_003_1">
          <text>"Psicologia"</text>
        </inputEntry>
        <inputEntry id="input_003_2">
          <text>&gt; 8</text>
        </inputEntry>
        <inputEntry id="input_003_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_003_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_003_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_003_2">
          <text>"Frequencia excessiva: psicologia ultrapassou 8 sessoes/mes. Custo evitavel: R$ 640-1280. Limite padrao: 4-8 sessoes/mes exceto casos graves documentados."</text>
        </outputEntry>
        <outputEntry id="output_003_3">
          <text>"ANS RN 465/2021 Art. 17; CFP Resolucao 010/2005; Lei 4.119/1962"</text>
        </outputEntry>
        <outputEntry id="OE_rule_003_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_rule_003_5"><text>"NEGAR"</text></outputEntry>

      </rule>
      <rule id="rule_004">
        <inputEntry id="input_004_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_004_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_004_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_004_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="output_004_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_004_2">
          <text>"Condicao cronica documentada: tratamento continuado necessario. Reabilitacao para condicoes cronicas permite frequencia aumentada com acompanhamento medico."</text>
        </outputEntry>
        <outputEntry id="output_004_3">
          <text>"ANS RN 465/2021 Art. 17; Portaria MS 702/2018 - Doencas Cronicas"</text>
        </outputEntry>
        <outputEntry id="OE_rule_004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_004_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_005">
        <inputEntry id="input_005_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_005_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_005_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="input_005_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_005_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_005_2">
          <text>"Laudo medico de renovacao apresentado: reavaliacao clinica justifica continuidade do tratamento. Progresso terapeutico documentado."</text>
        </outputEntry>
        <outputEntry id="output_005_3">
          <text>"ANS RN 465/2021 Art. 17; CFM Diretrizes Reabilitacao"</text>
        </outputEntry>
        <outputEntry id="OE_rule_005_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_005_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_006">
        <inputEntry id="input_006_1">
          <text>"Fisioterapia"</text>
        </inputEntry>
        <inputEntry id="input_006_2">
          <text>&lt;= 24</text>
        </inputEntry>
        <inputEntry id="input_006_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_006_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_006_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_006_2">
          <text>"Dentro do limite ANS: fisioterapia ate 24 sessoes/mes permitidas para tratamento inicial ou fase aguda."</text>
        </outputEntry>
        <outputEntry id="output_006_3">
          <text>"ANS RN 465/2021 Art. 17; COFFITO Diretrizes Fisioterapia"</text>
        </outputEntry>
        <outputEntry id="OE_rule_006_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_006_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_007">
        <inputEntry id="input_007_1">
          <text>"Fonoaudiologia"</text>
        </inputEntry>
        <inputEntry id="input_007_2">
          <text>&lt;= 12</text>
        </inputEntry>
        <inputEntry id="input_007_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_007_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_007_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_007_2">
          <text>"Dentro do limite ANS: fonoaudiologia ate 12 sessoes/mes adequadas para reabilitacao fonoaudiologica."</text>
        </outputEntry>
        <outputEntry id="output_007_3">
          <text>"ANS RN 465/2021 Art. 17; CFFa Resolucao 492/2015"</text>
        </outputEntry>
        <outputEntry id="OE_rule_007_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_007_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_008">
        <inputEntry id="input_008_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_008_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_008_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_008_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_008_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_008_2">
          <text>"Sessao aprovada. Avaliar evolucao terapeutica caso frequencia aumente."</text>
        </outputEntry>
        <outputEntry id="output_008_3">
          <text>"ANS RN 465/2021 Art. 17; Diretrizes Reabilitacao Multiprofissional"</text>
        </outputEntry>
        <outputEntry id="OE_rule_008_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_008_5"><text>"APROVAR"</text></outputEntry>

      </rule>
    </decisionTable>
  </decision>
</definitions>
