<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="regra-DUP-005" name="Encaminhamentos Duplicados" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="DUP-005" name="Encaminhamentos Duplicados">
    <decisionTable id="decisionTable_DUP005" hitPolicy="FIRST">
      <input id="input_1" label="Tipo Encaminhamento">
        <inputExpression id="inputExpression_1" typeRef="string">
          <text>tipoEncaminhamento</text>
        </inputExpression>
        <inputValues id="inputValues_1">
          <text>"Interconsulta","SegundaOpiniao","RetornoEspecialista","Urgencia","Exame"</text>
        </inputValues>
      </input>
      <input id="input_2" label="Encaminhamentos Previos 30 Dias">
        <inputExpression id="inputExpression_2" typeRef="integer">
          <text>encaminhamentosPrevios30Dias</text>
        </inputExpression>
      </input>
      <input id="input_3" label="Mesmo Especialista">
        <inputExpression id="inputExpression_3" typeRef="boolean">
          <text>mesmoEspecialista</text>
        </inputExpression>
      </input>
      <input id="input_4" label="Justificativa Nova">
        <inputExpression id="inputExpression_4" typeRef="boolean">
          <text>justificativaNova</text>
        </inputExpression>
      </input>
      <output id="output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_1">
          <text>"Aprovado","Reprovado"</text>
        </outputValues>
      </output>
      <output id="output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />
      
      <output id="output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="outputValues_4">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      

      <output id="output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="outputValues_5">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="rule_001">
        <inputEntry id="input_001_1">
          <text>"Interconsulta","RetornoEspecialista"</text>
        </inputEntry>
        <inputEntry id="input_001_2">
          <text>&gt;= 2</text>
        </inputEntry>
        <inputEntry id="input_001_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="input_001_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="output_001_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_001_2">
          <text>"Encaminhamento duplicado: 2+ solicitacoes ao mesmo especialista em 30 dias sem justificativa clinica. Custo evitavel: R$ 300-600. Verificar adequacao do primeiro atendimento."</text>
        </outputEntry>
        <outputEntry id="output_001_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Resolucao 1.974/2011 - Encaminhamento Medico"</text>
        </outputEntry>
        <outputEntry id="OE_rule_001_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_rule_001_5"><text>"NEGAR"</text></outputEntry>

      </rule>
      <rule id="rule_002">
        <inputEntry id="input_002_1">
          <text>"SegundaOpiniao"</text>
        </inputEntry>
        <inputEntry id="input_002_2">
          <text>&gt;= 1</text>
        </inputEntry>
        <inputEntry id="input_002_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_002_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="output_002_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_002_2">
          <text>"Segunda opiniao ja obtida nos ultimos 90 dias. Custo evitavel: R$ 400-800. Duplicidade desnecessaria exceto se surgirem novos achados clinicos relevantes."</text>
        </outputEntry>
        <outputEntry id="output_002_3">
          <text>"ANS RN 465/2021 Art. 16; CDC Art. 51 - Prevencao Abuso; CFM Codigo Etica Medica"</text>
        </outputEntry>
        <outputEntry id="OE_rule_002_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_rule_002_5"><text>"NEGAR"</text></outputEntry>

      </rule>
      <rule id="rule_003">
        <inputEntry id="input_003_1">
          <text>"Urgencia"</text>
        </inputEntry>
        <inputEntry id="input_003_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_003_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_003_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_003_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_003_2">
          <text>"Encaminhamento de urgencia: prioridade clinica justifica solicitacao imediata independente de encaminhamentos previos."</text>
        </outputEntry>
        <outputEntry id="output_003_3">
          <text>"ANS RN 465/2021 Art. 3 - Urgencia/Emergencia; CFM Resolucao 2.158/2016"</text>
        </outputEntry>
        <outputEntry id="OE_rule_003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_003_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_004">
        <inputEntry id="input_004_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_004_2">
          <text>&gt;= 2</text>
        </inputEntry>
        <inputEntry id="input_004_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_004_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="output_004_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_004_2">
          <text>"Justificativa clinica nova apresentada: mudanca no quadro clinico, piora sintomatica ou surgimento de novas queixas justifica reencaminhamento."</text>
        </outputEntry>
        <outputEntry id="output_004_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Resolucao 1.974/2011"</text>
        </outputEntry>
        <outputEntry id="OE_rule_004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_004_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_005">
        <inputEntry id="input_005_1">
          <text>"Exame"</text>
        </inputEntry>
        <inputEntry id="input_005_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_005_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_005_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_005_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_005_2">
          <text>"Encaminhamento para exame complementar: solicitacao diagnostica nao caracteriza duplicidade de interconsulta."</text>
        </outputEntry>
        <outputEntry id="output_005_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Resolucao 1.638/2002 - Prontuario Medico"</text>
        </outputEntry>
        <outputEntry id="OE_rule_005_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_005_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_006">
        <inputEntry id="input_006_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_006_2">
          <text>0</text>
        </inputEntry>
        <inputEntry id="input_006_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_006_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_006_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_006_2">
          <text>"Primeiro encaminhamento ao especialista: solicitacao inicial adequada para avaliacao especializada."</text>
        </outputEntry>
        <outputEntry id="output_006_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Resolucao 1.974/2011"</text>
        </outputEntry>
        <outputEntry id="OE_rule_006_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_006_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_007">
        <inputEntry id="input_007_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_007_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_007_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_007_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_007_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_007_2">
          <text>"Encaminhamento aprovado. Avaliar necessidade caso haja multiplas solicitacoes ao mesmo especialista."</text>
        </outputEntry>
        <outputEntry id="output_007_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Resolucao 1.974/2011"</text>
        </outputEntry>
        <outputEntry id="OE_rule_007_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_007_5"><text>"APROVAR"</text></outputEntry>

      </rule>
    </decisionTable>
  </decision>
</definitions>
