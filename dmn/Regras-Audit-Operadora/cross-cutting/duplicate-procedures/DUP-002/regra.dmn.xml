<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="regra-DUP-002" name="Duplicidade de Exames Laboratoriais no Mesmo Dia" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="DUP-002" name="Duplicidade de Exames Laboratoriais no Mesmo Dia">
    <decisionTable id="decisionTable_DUP002" hitPolicy="FIRST">
      <input id="input_tipoExame" label="Tipo de Exame" camunda:inputVariable="tipoExame">
        <inputExpression id="inputExpression_tipoExame" typeRef="string">
          <text>tipoExame</text>
        </inputExpression>
        <inputValues id="inputValues_tipoExame">
          <text>"Hemograma","Bioquímica","Coagulação","Gasometria","Urinálise","Cultura","Hormonal","Sorologia"</text>
        </inputValues>
      </input>
      <input id="input_horasDesdeUltimo" label="Horas Desde Último Exame" camunda:inputVariable="horasDesdeUltimo">
        <inputExpression id="inputExpression_horasDesdeUltimo" typeRef="integer">
          <text>horasDesdeUltimo</text>
        </inputExpression>
      </input>
      <input id="input_justificativaMedica" label="Justificativa Médica Documentada" camunda:inputVariable="justificativaMedica">
        <inputExpression id="inputExpression_justificativaMedica" typeRef="boolean">
          <text>justificativaMedica</text>
        </inputExpression>
      </input>
      <input id="input_pacienteCritico" label="Paciente Crítico (UTI)" camunda:inputVariable="pacienteCritico">
        <inputExpression id="inputExpression_pacienteCritico" typeRef="boolean">
          <text>pacienteCritico</text>
        </inputExpression>
      </input>
      <output id="output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="output_fundamentacao" label="Fundamentação Legal" name="fundamentacao" typeRef="string" />
      
      <output id="output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="outputValues_4">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      

      <output id="output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="outputValues_5">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>


      <rule id="rule_001">
        <inputEntry id="input_001_1">
          <text>"Hemograma","Bioquímica"</text>
        </inputEntry>
        <inputEntry id="input_001_2">
          <text>&lt; 12</text>
        </inputEntry>
        <inputEntry id="input_001_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="input_001_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="output_001_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_001_2">
          <text>"Repetição de Hemograma/Bioquímica em menos de 12h sem justificativa em paciente estável. Economia estimada: R$ 85-150 por exame evitado. Coleta desnecessária."</text>
        </outputEntry>
        <outputEntry id="output_001_3">
          <text>"SBPC/ML 2023 - Boas Práticas Laboratoriais; ANS RN 465/2021 Art. 18; Choosing Wisely Brasil - Medicina Laboratorial"</text>
        </outputEntry>
        <outputEntry id="OE_rule_001_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_rule_001_5"><text>"NEGAR"</text></outputEntry>

      </rule>

      <rule id="rule_002">
        <inputEntry id="input_002_1">
          <text>"Hormonal","Sorologia"</text>
        </inputEntry>
        <inputEntry id="input_002_2">
          <text>&lt; 24</text>
        </inputEntry>
        <inputEntry id="input_002_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="input_002_4">
          <text></text>
        </inputEntry>
        <outputEntry id="output_002_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_002_2">
          <text>"Repetição de exame Hormonal/Sorológico em menos de 24h. Economia estimada: R$ 120-450 por exame evitado. Variação diurna não justifica repetição imediata."</text>
        </outputEntry>
        <outputEntry id="output_002_3">
          <text>"SBEM 2023 - Protocolos Dosagens Hormonais; SBPC/ML - Intervalos Mínimos; ANS RN 465/2021"</text>
        </outputEntry>
        <outputEntry id="OE_rule_002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_rule_002_5"><text>"NEGAR"</text></outputEntry>

      </rule>

      <rule id="rule_003">
        <inputEntry id="input_003_1">
          <text>"Urinálise"</text>
        </inputEntry>
        <inputEntry id="input_003_2">
          <text>&lt; 8</text>
        </inputEntry>
        <inputEntry id="input_003_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="input_003_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="output_003_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_003_2">
          <text>"Repetição de Urinálise em menos de 8h sem mudança clínica. Economia estimada: R$ 45-80 por exame evitado. Resultados prévios ainda válidos."</text>
        </outputEntry>
        <outputEntry id="output_003_3">
          <text>"SBU 2023 - Protocolos Urinálise; SBPC/ML - Boas Práticas; ANS RN 465/2021 Art. 18"</text>
        </outputEntry>
        <outputEntry id="OE_rule_003_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_rule_003_5"><text>"NEGAR"</text></outputEntry>

      </rule>

      <rule id="rule_004">
        <inputEntry id="input_004_1">
          <text>"Gasometria"</text>
        </inputEntry>
        <inputEntry id="input_004_2">
          <text></text>
        </inputEntry>
        <inputEntry id="input_004_3">
          <text></text>
        </inputEntry>
        <inputEntry id="input_004_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="output_004_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_004_2">
          <text>"Gasometria arterial em paciente crítico (UTI) aprovada sem restrição horária. Monitorização contínua do status ácido-base é protocolo padrão."</text>
        </outputEntry>
        <outputEntry id="output_004_3">
          <text>"AMIB 2023 - Protocolos UTI; SBPT - Monitorização Respiratória; ANS RN 465/2021 Art. 20 - Exceções Paciente Crítico"</text>
        </outputEntry>
        <outputEntry id="OE_rule_004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_004_5"><text>"APROVAR"</text></outputEntry>

      </rule>

      <rule id="rule_005">
        <inputEntry id="input_005_1">
          <text>"Cultura"</text>
        </inputEntry>
        <inputEntry id="input_005_2">
          <text></text>
        </inputEntry>
        <inputEntry id="input_005_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="input_005_4">
          <text></text>
        </inputEntry>
        <outputEntry id="output_005_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_005_2">
          <text>"Repetição de Cultura aprovada com justificativa médica documentada (coleta inadequada, contaminação, mudança antibioticoterapia). Necessário para controle infecção."</text>
        </outputEntry>
        <outputEntry id="output_005_3">
          <text>"SBPC/ML 2023 - Controle Qualidade Pré-Analítico; ANVISA RDC 302/2005; ANS RN 465/2021 Art. 19"</text>
        </outputEntry>
        <outputEntry id="OE_rule_005_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_005_5"><text>"APROVAR"</text></outputEntry>

      </rule>

      <rule id="rule_006">
        <inputEntry id="input_006_1">
          <text></text>
        </inputEntry>
        <inputEntry id="input_006_2">
          <text></text>
        </inputEntry>
        <inputEntry id="input_006_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="input_006_4">
          <text></text>
        </inputEntry>
        <outputEntry id="output_006_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_006_2">
          <text>"Justificativa médica documentada autoriza repetição do exame laboratorial. Autonomia profissional respeitada com fundamentação clínica."</text>
        </outputEntry>
        <outputEntry id="output_006_3">
          <text>"CFM Resolução 2.218/2018 - Autonomia Médica; SBPC/ML - Exceções Clínicas; ANS RN 465/2021 Art. 19"</text>
        </outputEntry>
        <outputEntry id="OE_rule_006_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_006_5"><text>"APROVAR"</text></outputEntry>

      </rule>

      <rule id="rule_007">
        <inputEntry id="input_007_1">
          <text>"Hemograma","Bioquímica","Coagulação"</text>
        </inputEntry>
        <inputEntry id="input_007_2">
          <text>&gt;= 12</text>
        </inputEntry>
        <inputEntry id="input_007_3">
          <text></text>
        </inputEntry>
        <inputEntry id="input_007_4">
          <text></text>
        </inputEntry>
        <outputEntry id="output_007_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_007_2">
          <text>"Intervalo mínimo de 12h cumprido para exames básicos. Repetição dentro do protocolo de acompanhamento clínico."</text>
        </outputEntry>
        <outputEntry id="output_007_3">
          <text>"SBPC/ML 2023 - Intervalos Mínimos; ANS RN 465/2021; Choosing Wisely Brasil"</text>
        </outputEntry>
        <outputEntry id="OE_rule_007_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_007_5"><text>"APROVAR"</text></outputEntry>

      </rule>

      <rule id="Rule_Default_Fallback">
        <inputEntry id="input_fallback_1">
          <text></text>
        </inputEntry>
        <inputEntry id="input_fallback_2">
          <text></text>
        </inputEntry>
        <inputEntry id="input_fallback_3">
          <text></text>
        </inputEntry>
        <inputEntry id="input_fallback_4">
          <text></text>
        </inputEntry>
        <outputEntry id="output_fallback_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="output_fallback_2">
          <text>"Caso não coberto pelas regras padrão. Análise manual necessária considerando tipo de exame, perfil do paciente e protocolo institucional."</text>
        </outputEntry>
        <outputEntry id="output_fallback_3">
          <text>"ANS RN 465/2021 Art. 22 - Revisão Humana; SBPC/ML - Casos Especiais; CFM Resolução 2.218/2018"</text>
        </outputEntry>
        <outputEntry id="OE_Rule_Default_Fallback_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Rule_Default_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>

      </rule>
    </decisionTable>
  </decision>
</definitions>
