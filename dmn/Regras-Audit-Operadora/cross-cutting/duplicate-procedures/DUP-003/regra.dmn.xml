<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="regra-DUP-003" name="Controle Frequencia Consultas" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="DUP-003" name="Controle Frequencia Consultas">
    <decisionTable id="decisionTable_DUP003" hitPolicy="FIRST">
      <input id="input_1" label="Especialidade">
        <inputExpression id="inputExpression_1" typeRef="string">
          <text>especialidade</text>
        </inputExpression>
        <inputValues id="inputValues_1">
          <text>"Clinica_Geral","Cardiologia","Ortopedia","Dermatologia","Psiquiatria","Nutricao","Fisioterapia","Outras"</text>
        </inputValues>
      </input>
      <input id="input_2" label="Consultas Ultimos 30 Dias">
        <inputExpression id="inputExpression_2" typeRef="integer">
          <text>consultasUltimos30Dias</text>
        </inputExpression>
      </input>
      <input id="input_3" label="Justificativa Continuidade">
        <inputExpression id="inputExpression_3" typeRef="boolean">
          <text>justificativaContinuidade</text>
        </inputExpression>
      </input>
      <input id="input_4" label="Tratamento Ativo">
        <inputExpression id="inputExpression_4" typeRef="boolean">
          <text>tratamentoAtivo</text>
        </inputExpression>
      </input>
      <output id="output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="outputValues_1">
          <text>"Aprovado","Reprovado"</text>
        </outputValues>
      </output>
      <output id="output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />
      
      <output id="output_4" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="outputValues_4">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      

      <output id="output_5" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="outputValues_5">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="rule_001">
        <inputEntry id="input_001_1">
          <text>"Cardiologia","Ortopedia","Dermatologia","Outras"</text>
        </inputEntry>
        <inputEntry id="input_001_2">
          <text>&gt;= 3</text>
        </inputEntry>
        <inputEntry id="input_001_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="input_001_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_001_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_001_2">
          <text>"Frequencia excessiva: 3+ consultas mesma especialidade em 30 dias sem justificativa. Custo evitavel: R$ 450-900. Solicitar relatorio medico detalhando necessidade de acompanhamento intensivo."</text>
        </outputEntry>
        <outputEntry id="output_001_3">
          <text>"ANS RN 465/2021 Art. 16 - Utilizacao Racional; CFM Diretrizes Consultas Especializadas 2022"</text>
        </outputEntry>
        <outputEntry id="OE_rule_001_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_rule_001_5"><text>"NEGAR"</text></outputEntry>

      </rule>
      <rule id="rule_002">
        <inputEntry id="input_002_1">
          <text>"Clinica_Geral","Cardiologia","Ortopedia","Dermatologia","Fisioterapia","Outras"</text>
        </inputEntry>
        <inputEntry id="input_002_2">
          <text>&gt;= 5</text>
        </inputEntry>
        <inputEntry id="input_002_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_002_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="output_002_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="output_002_2">
          <text>"Utilizacao abusiva: 5+ consultas em 30 dias sem tratamento ativo documentado. Custo evitavel: R$ 750-1500. Necessaria auditoria medica para avaliar adequacao do plano terapeutico."</text>
        </outputEntry>
        <outputEntry id="output_002_3">
          <text>"ANS RN 465/2021 Art. 16; CDC Art. 51 - Prevencao Abuso; CFM Resolucao 2.217/2018"</text>
        </outputEntry>
        <outputEntry id="OE_rule_002_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OE_rule_002_5"><text>"NEGAR"</text></outputEntry>

      </rule>
      <rule id="rule_003">
        <inputEntry id="input_003_1">
          <text>"Psiquiatria","Nutricao"</text>
        </inputEntry>
        <inputEntry id="input_003_2">
          <text>&lt;= 4</text>
        </inputEntry>
        <inputEntry id="input_003_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_003_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_003_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_003_2">
          <text>"Frequencia compativel com tratamento psiquiatrico/nutricional: ate 4 consultas/mes permitidas. Especialidades com acompanhamento intensivo reconhecido."</text>
        </outputEntry>
        <outputEntry id="output_003_3">
          <text>"ANS RN 465/2021 - Saude Mental/Nutricao; CFM Diretrizes Psiquiatria; CFN Acompanhamento Nutricional"</text>
        </outputEntry>
        <outputEntry id="OE_rule_003_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_003_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_004">
        <inputEntry id="input_004_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_004_2">
          <text>&gt;= 3</text>
        </inputEntry>
        <inputEntry id="input_004_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_004_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="output_004_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_004_2">
          <text>"Tratamento ativo documentado justifica frequencia aumentada. Monitoramento clinico necessario para condicao em tratamento."</text>
        </outputEntry>
        <outputEntry id="output_004_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Diretrizes Acompanhamento Terapeutico"</text>
        </outputEntry>
        <outputEntry id="OE_rule_004_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_004_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_005">
        <inputEntry id="input_005_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_005_2">
          <text>&gt;= 3</text>
        </inputEntry>
        <inputEntry id="input_005_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="input_005_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_005_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_005_2">
          <text>"Justificativa medica de continuidade apresentada. Acompanhamento especializado necessario conforme relatorio clinico."</text>
        </outputEntry>
        <outputEntry id="output_005_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Codigo Etica Medica Art. 14"</text>
        </outputEntry>
        <outputEntry id="OE_rule_005_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_005_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_006">
        <inputEntry id="input_006_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_006_2">
          <text>[1..2]</text>
        </inputEntry>
        <inputEntry id="input_006_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_006_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_006_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_006_2">
          <text>"Frequencia dentro dos padroes: 1-2 consultas/mes considerada adequada para acompanhamento ambulatorial."</text>
        </outputEntry>
        <outputEntry id="output_006_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Diretrizes Consultas Ambulatoriais"</text>
        </outputEntry>
        <outputEntry id="OE_rule_006_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_006_5"><text>"APROVAR"</text></outputEntry>

      </rule>
      <rule id="rule_007">
        <inputEntry id="input_007_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_007_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_007_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="input_007_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="output_007_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="output_007_2">
          <text>"Consulta aprovada. Necessaria avaliacao caso a caso para frequencias atipicas."</text>
        </outputEntry>
        <outputEntry id="output_007_3">
          <text>"ANS RN 465/2021 Art. 16; CFM Resolucao 2.217/2018"</text>
        </outputEntry>
        <outputEntry id="OE_rule_007_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_rule_007_5"><text>"APROVAR"</text></outputEntry>

      </rule>
    </decisionTable>
  </decision>
</definitions>
