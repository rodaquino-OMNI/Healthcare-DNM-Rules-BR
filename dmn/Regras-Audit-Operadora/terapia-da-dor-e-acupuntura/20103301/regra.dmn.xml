<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_20103301" name="SINDROMES MIOFASCIAIS Infiltracao de ponto gatilho" targetNamespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_20103301" name="SINDROMES MIOFASCIAIS Infiltracao de ponto gatilho">
    <decisionTable id="DecisionTable_20103301" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Escala de Dor VAS/NRS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>escalaDor</text>
        </inputExpression>
      </input>
      <input id="Input_3" label="Tratamentos Previos">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
      </input>
      <input id="Input_4" label="Laudo de Especialista">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoEspecialista</text>
        </inputExpression>
      </input>

      <output id="Output_1" name="resultado" label="Resultado da Analise" typeRef="string" />
      <output id="Output_2" name="observacao" label="Observacao" typeRef="string" />
      <output id="Output_3" name="fundamentacao" label="Fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_20103301_Contraindication_Infection">
        <description>Contraindicacao - Infeccao local</description>
        <inputEntry id="InputEntry_1_1">
          <text>contains(diagnosticoPrincipal, "L08")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Infeccao local presente - contraindicacao absoluta"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"SBED - Contraindicacoes para procedimentos invasivos em dor"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_20103301_Contraindication_Coagulopathy">
        <description>Contraindicacao - Coagulopatia</description>
        <inputEntry id="InputEntry_1_2">
          <text>contains(diagnosticoPrincipal, "D65") or contains(diagnosticoPrincipal, "D68")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Coagulopatia nao controlada - contraindicacao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"SBED - Seguranca em procedimentos invasivos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_20103301_Approved_Chronic">
        <description>Aprovado - Dor miofascial cronica documentada</description>
        <inputEntry id="InputEntry_1_3">
          <text>contains(diagnosticoPrincipal, "M79.1")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>number(escalaDor) &amp;gt;= 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_3">
          <text>contains(tratamentosPrevios, "fisioterapia") and contains(tratamentosPrevios, "analgesicos")</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"Criterios clinicos atendidos para infiltracao de pontos gatilho"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"ANS RN 465/2021 - Procedimento coberto. SBED - Indicacao adequada"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_20103301_Approved_Musculoskeletal">
        <description>Aprovado - Dor musculoesqueletica cronica</description>
        <inputEntry id="InputEntry_1_4">
          <text>contains(diagnosticoPrincipal, "M") and (contains(diagnosticoPrincipal, "M25") or contains(diagnosticoPrincipal, "M60") or contains(diagnosticoPrincipal, "M62"))</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>number(escalaDor) &amp;gt;= 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_4">
          <text>contains(tratamentosPrevios, "conservador")</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"Dor musculoesqueletica com indicacao de infiltracao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"SBMFC - Protocolo de Sindrome Dolorosa Miofascial"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_20103301_Pending_Insufficient">
        <description>Pendente - Documentacao insuficiente</description>
        <inputEntry id="InputEntry_1_5">
          <text>contains(diagnosticoPrincipal, "M79") or contains(diagnosticoPrincipal, "M60")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_5">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_5">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_5">
          <text>"Necessario laudo de especialista documentando pontos gatilho e localizacao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_5">
          <text>"ANS - Requisito de documentacao complementar"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_20103301">
        <description>Fallback - Criterios nao atendidos</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Criterios clinicos nao atendidos para infiltracao de pontos gatilho"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 - Criterios nao preenchidos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
    </decisionTable>
  </decision>

</definitions>