<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30713137" name="Puncao Articular Tornozelo" targetNamespace="http://camunda.org/schema/1.0/dmn">
  

  <decision id="Decision_30713137" name="Puncao Articular Tornozelo">
    <decisionTable id="DecisionTable_30713137" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Escala de Dor VAS/NRS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>escalaDor</text>
        </inputExpression>
      </input>
      <input id="Input_3" label="Tratamentos Previos">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
      </input>
      <input id="Input_4" label="Derrame Articular">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>derrameArticular</text>
        </inputExpression>
      </input>
      <input id="Input_5" label="Exame de Imagem">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>exameImagem</text>
        </inputExpression>
      </input>

      <output id="Output_1" name="resultado" label="Resultado da Analise" typeRef="string" />
      <output id="Output_2" name="observacao" label="Observacao" typeRef="string" />
      <output id="Output_3" name="fundamentacao" label="Fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_30713137_Contraindication_Infection">
        <description>Contraindicacao - Infeccao local</description>
        <inputEntry id="InputEntry_1_1">
          <text>contains(diagnosticoPrincipal, "L08") or contains(diagnosticoPrincipal, "A40") or contains(diagnosticoPrincipal, "A41")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_5_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Infeccao local ou sepse - contraindicacao absoluta"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"SBOT - Contraindicacoes para artrocentese"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_30713137_Contraindication_Coagulopathy">
        <description>Contraindicacao - Coagulopatia</description>
        <inputEntry id="InputEntry_1_2">
          <text>contains(diagnosticoPrincipal, "D65") or contains(diagnosticoPrincipal, "D68")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Coagulopatia nao corrigida - contraindicacao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"SBOT - Criterios de seguranca"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_30713137_Approved_Inflammatory">
        <description>Aprovado - Artrite inflamatoria aguda</description>
        <inputEntry id="InputEntry_1_3">
          <text>contains(diagnosticoPrincipal, "M13.1") or contains(diagnosticoPrincipal, "M06")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>number(escalaDor) &amp;gt;= 7</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_3">
          <text>contains(tratamentosPrevios, "analgesicos")</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_5_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"Artrite inflamatoria aguda com derrame - indicacao de artrocentese"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"ANS RN 465/2021 - Procedimento coberto. SBOT - Indicacao adequada"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_30713137_Approved_Diagnostic">
        <description>Aprovado - Necessidade diagnostica</description>
        <inputEntry id="InputEntry_1_4">
          <text>contains(diagnosticoPrincipal, "M25.47")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"Derrame articular com necessidade de analise do liquido sinovial"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"SBMF - Diretrizes para artrocentese diagnostica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_30713137_Pending_NoImaging">
        <description>Pendente - Ausencia de exame de imagem</description>
        <inputEntry id="InputEntry_1_5">
          <text>contains(diagnosticoPrincipal, "M")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_5">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_5_5">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_5">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_5">
          <text>"Necessario exame de imagem (ultrassonografia ou radiografia) para confirmacao de derrame"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_5">
          <text>"ANS - Requisito de documentacao complementar"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30713137">
        <description>Fallback - Criterios nao atendidos</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_5">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Criterios clinicos nao atendidos para puncao articular de tornozelo"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 - Criterios nao preenchidos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
    </decisionTable>
  </decision>

</definitions>