        <outputEntry id="OutputEntry_R2_observacao"><text>"ETT realizado ha menos de 90 dias em paciente clinicamente estavel - aguardar intervalo minimo"</text></outputEntry>
        <outputEntry id="OutputEntry_R2_fundamentacao"><text>"SBC/DIC 2024 - Intervalo minimo de 90 dias para repeticao de ETT em pacientes estaveis sem alteracao clinica"</text></outputEntry>
        <outputEntry id="OutputEntry_R2_alertasDesperdicio"><text>"DUP"</text></outputEntry>
        <outputEntry id="OutputEntry_R2_acaoRecomendada"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <!-- RULE 3: DUP - Duplicidade em menos de 30 dias sem justificativa aguda -->
      <rule id="Rule_Reprovado_DUP_30d">
