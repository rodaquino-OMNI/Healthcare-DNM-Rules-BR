        <outputEntry id="OutputEntry_R3_fundamentacao"><text>"SBC/DIC 2024, ASE 2023 - Repeticao precoce de ETT requer justificativa de evento agudo ou mudanca clinica significativa"</text></outputEntry>
        <outputEntry id="OutputEntry_R3_alertasDesperdicio"><text>"DUP"</text></outputEntry>
        <outputEntry id="OutputEntry_R3_acaoRecomendada"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <!-- RULE 4: FREQ - Frequencia excessiva (mais de 2/ano sem mudanca) -->
