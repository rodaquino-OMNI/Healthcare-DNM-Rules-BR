        <inputEntry id="InputEntry_R3_1"><text>"HAS","CheckupRotina","Outro"</text></inputEntry>
        <inputEntry id="InputEntry_R3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R3_3"><text>"Classe IIa","Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_R3_4"><text>&lt; 30</text></inputEntry>
        <inputEntry id="InputEntry_R3_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R3_resultado"><text>"Reprovado"</text></outputEntry>
