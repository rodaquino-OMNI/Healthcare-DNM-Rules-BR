        <outputEntry id="OutputEntry_Reprovado_PROT_observacao"><text>"Angina estavel requer terapia medicamentosa otimizada (TMO) antes de revascularizacao"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_PROT_fundamentacao"><text>"SBC/SBHCI PCI 2024, ACC/AHA 2023 - DAC estavel deve ter TMO por 4-12 semanas antes de considerar ICP"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_PROT_alerta"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_PROT_acao"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_DUP_Recente">
