<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_40101037"
             name="Ecocardiograma Transtoracico (ETT)"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    TUSS: 40101037
    Procedimento: Ecocardiograma Transtoracico (ETT)
    Versao: 1.0.0-LEAN-TIER2
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS (DUT/ANS + ACC AUC 2024):
    - Avaliacao inicial de funcao ventricular
    - Sopro cardiaco novo
    - Dispneia de origem cardiaca
    - Insuficiencia cardiaca inicial/seguimento
    - Valvulopatias
    - Pre-operatorio cardiaco

    CONTRAINDICACOES:
    Classe III, ausencia de indicacao clinica documentada

    REFERENCIAS TECNICAS:
    ACC/AHA AUC 2024, ESC Imaging Guidelines 2023, SBC/DIC 2023

    ALERTAS DE DESPERDICIO (LEAN TIER-2):
    - DUP: Duplicidade - ETT repetido em intervalo curto sem justificativa
    - FREQ: Frequencia excessiva - Mais de 2 exames/ano sem alteracao clinica
    - ESC: Escalonamento inadequado - ETT como screening sem indicacao
    - DOC: Documentacao insuficiente - Falta justificativa clinica
    - PROT: Protocolo nao seguido - Desvio de diretriz clinica
    - MULTI: Multiplos alertas combinados

    ===============================================================================
  -->

  <decision id="Decision_40101037" name="Regra Clinica - Ecocardiograma Transtoracico">
    <decisionTable id="DecisionTable_40101037" hitPolicy="FIRST">

      <!-- ========== INPUTS (6 - Padrao LEAN TIER-2 Cardiologia) ========== -->

      <input id="Input_indicacaoClinica" label="Indicacao Clinica" camunda:inputVariable="indicacaoClinica">
        <inputExpression id="InputExpression_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"AvaliacaoFuncaoVE","SoproCardiacoNovo","DispneiaCardiaca","InsuficienciaCardiaca","Valvulopatia","PreOperatorioCardiaco","ScreeningAssintomatico","FollowUpRotina"</text>
        </inputValues>
      </input>

      <input id="Input_historicoClinico" label="Historico Clinico" camunda:inputVariable="historicoClinico">
        <inputExpression id="InputExpression_historicoClinico" typeRef="string">
          <text>historicoClinico</text>
        </inputExpression>
        <inputValues id="InputValues_historicoClinico">
          <text>"Adequado","Incompleto","NaoDocumentado"</text>
        </inputValues>
      </input>

      <input id="Input_classeIndicacao" label="Classe de Indicacao (ACC/AHA)" camunda:inputVariable="classeIndicacao">
        <inputExpression id="InputExpression_classeIndicacao" typeRef="string">
          <text>classeIndicacao</text>
        </inputExpression>
        <inputValues id="InputValues_classeIndicacao">
          <text>"Classe I","Classe IIa","Classe IIb","Classe III"</text>
        </inputValues>
      </input>

      <input id="Input_urgencia" label="Nivel de Urgencia" camunda:inputVariable="urgencia">
        <inputExpression id="InputExpression_urgencia" typeRef="string">
          <text>urgencia</text>
        </inputExpression>
        <inputValues id="InputValues_urgencia">
          <text>"Emergencia","Urgencia","Eletivo","FollowUp"</text>
        </inputValues>
      </input>

      <input id="Input_ultimoProcedimentoDias" label="Dias desde ultimo ETT" camunda:inputVariable="ultimoProcedimentoDias">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="integer">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>

      <input id="Input_ecgPrevio" label="ECG Realizado" camunda:inputVariable="ecgPrevio">
        <inputExpression id="InputExpression_ecgPrevio" typeRef="boolean">
          <text>ecgPrevio</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 - Padrao LEAN TIER-2) ========== -->

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- ========== RULES - REPROVACOES (Waste Detection First) ========== -->

      <!-- RULE 1: Contraindicacao Classe III -->
      <rule id="Rule_Reprovado_ClasseIII">
        <description>Reprovado: Classe III - Uso inapropriado</description>
        <inputEntry id="InputEntry_R1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R1_3"><text>"Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_R1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R1_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R1_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R1_observacao"><text>"Uso inapropriado conforme ACC AUC 2024 - Classe III"</text></outputEntry>
        <outputEntry id="OutputEntry_R1_fundamentacao"><text>"ACC/AHA AUC 2024: Classe III indica uso inapropriado, sem beneficio clinico esperado"</text></outputEntry>
        <outputEntry id="OutputEntry_R1_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_R1_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- RULE 2: ESC - Screening em assintomaticos -->
      <rule id="Rule_Reprovado_ESC_ScreeningAssintomatico">
        <description>Reprovado: Screening em assintomaticos de baixo risco</description>
        <inputEntry id="InputEntry_R2_1"><text>"ScreeningAssintomatico"</text></inputEntry>
        <inputEntry id="InputEntry_R2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R2_4"><text>"Eletivo","FollowUp"</text></inputEntry>
        <inputEntry id="InputEntry_R2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R2_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R2_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R2_observacao"><text>"ETT nao indicado para screening em assintomaticos de baixo risco"</text></outputEntry>
        <outputEntry id="OutputEntry_R2_fundamentacao"><text>"ACC AUC 2024: Raramente apropriado para rastreamento em populacao assintomatica sem fatores de risco"</text></outputEntry>
        <outputEntry id="OutputEntry_R2_alertasDesperdicio"><text>"ESC"</text></outputEntry>
        <outputEntry id="OutputEntry_R2_acaoRecomendada"><text>"SUGERIR_ALTERNATIVA"</text></outputEntry>
      </rule>

      <!-- RULE 3: DUP - Duplicidade em menos de 90 dias para follow-up -->
      <rule id="Rule_Reprovado_DUP_FollowUp90d">
        <description>Reprovado: ETT duplicado em menos de 90 dias - follow-up rotina</description>
        <inputEntry id="InputEntry_R3_1"><text>"FollowUpRotina"</text></inputEntry>
        <inputEntry id="InputEntry_R3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R3_3"><text>"Classe IIb","Classe III"</text></inputEntry>
        <inputEntry id="InputEntry_R3_4"><text>"FollowUp"</text></inputEntry>
        <inputEntry id="InputEntry_R3_5"><text>&lt; 90</text></inputEntry>
        <inputEntry id="InputEntry_R3_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R3_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R3_observacao"><text>"ETT realizado ha menos de 90 dias sem mudanca clinica documentada"</text></outputEntry>
        <outputEntry id="OutputEntry_R3_fundamentacao"><text>"ACC AUC 2024: Repeticao de ETT em menos de 3 meses sem mudanca de status clinico nao e apropriada"</text></outputEntry>
        <outputEntry id="OutputEntry_R3_alertasDesperdicio"><text>"DUP"</text></outputEntry>
        <outputEntry id="OutputEntry_R3_acaoRecomendada"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <!-- RULE 4: FREQ - Frequencia excessiva (mais de 2/ano em eletivo) -->
      <rule id="Rule_Reprovado_FREQ_Excessiva">
        <description>Reprovado: Frequencia excessiva - mais de 2 ETT/ano em paciente estavel</description>
        <inputEntry id="InputEntry_R4_1"><text>"FollowUpRotina"</text></inputEntry>
        <inputEntry id="InputEntry_R4_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_R4_3"><text>"Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_R4_4"><text>"Eletivo","FollowUp"</text></inputEntry>
        <inputEntry id="InputEntry_R4_5"><text>&lt; 180</text></inputEntry>
        <inputEntry id="InputEntry_R4_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R4_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R4_observacao"><text>"ETT solicitado em menos de 6 meses sem alteracao clinica - frequencia excessiva"</text></outputEntry>
        <outputEntry id="OutputEntry_R4_fundamentacao"><text>"SBC/DIC 2023 - Seguimento ecocardiografico anual para a maioria das condicoes cronicas estaveis"</text></outputEntry>
        <outputEntry id="OutputEntry_R4_alertasDesperdicio"><text>"FREQ"</text></outputEntry>
        <outputEntry id="OutputEntry_R4_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- RULE 5: DOC - Documentacao ausente -->
      <rule id="Rule_Pendente_DOC_Ausente">
        <description>Pendente: Documentacao clinica ausente</description>
        <inputEntry id="InputEntry_R5_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R5_2"><text>"NaoDocumentado"</text></inputEntry>
        <inputEntry id="InputEntry_R5_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R5_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R5_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R5_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R5_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_R5_observacao"><text>"Historico clinico nao documentado - necessario anexar justificativa clinica"</text></outputEntry>
        <outputEntry id="OutputEntry_R5_fundamentacao"><text>"ANS RN 465/2021, CDC Art. 6 - Direito a informacao adequada sobre servicos de saude"</text></outputEntry>
        <outputEntry id="OutputEntry_R5_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_R5_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- RULE 6: DOC - Documentacao incompleta -->
      <rule id="Rule_Pendente_DOC_Incompleto">
        <description>Pendente: Documentacao clinica incompleta</description>
        <inputEntry id="InputEntry_R6_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R6_2"><text>"Incompleto"</text></inputEntry>
        <inputEntry id="InputEntry_R6_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R6_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R6_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R6_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R6_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_R6_observacao"><text>"Historico clinico incompleto - complementar documentacao clinica"</text></outputEntry>
        <outputEntry id="OutputEntry_R6_fundamentacao"><text>"ANS RN 465/2021 - Documentacao clinica adequada necessaria para autorizacao"</text></outputEntry>
        <outputEntry id="OutputEntry_R6_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_R6_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- RULE 7: ESC - Step therapy incompleto - ECG nao realizado -->
      <rule id="Rule_Pendente_ESC_SemECG">
        <description>Pendente: Step therapy incompleto - ECG nao realizado</description>
        <inputEntry id="InputEntry_R7_1"><text>"InsuficienciaCardiaca","SoproCardiacoNovo","AvaliacaoFuncaoVE"</text></inputEntry>
        <inputEntry id="InputEntry_R7_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_R7_3"><text>"Classe I","Classe IIa","Classe IIb"</text></inputEntry>
        <inputEntry id="InputEntry_R7_4"><text>"Eletivo"</text></inputEntry>
        <inputEntry id="InputEntry_R7_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R7_6"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_R7_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_R7_observacao"><text>"Step therapy incompleto - ECG deve preceder ETT em avaliacoes eletivas"</text></outputEntry>
        <outputEntry id="OutputEntry_R7_fundamentacao"><text>"ACC AUC 2024: ECG deve preceder ETT em avaliacoes eletivas como triagem inicial"</text></outputEntry>
        <outputEntry id="OutputEntry_R7_alertasDesperdicio"><text>"ESC"</text></outputEntry>
        <outputEntry id="OutputEntry_R7_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- ========== RULES - APROVACOES ========== -->

      <!-- RULE 8: Aprovado Emergencia cardiaca -->
      <rule id="Rule_Aprovado_Emergencia">
        <description>Aprovado: Emergencia cardiaca - avaliacao imediata</description>
        <inputEntry id="InputEntry_R8_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R8_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R8_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_R8_4"><text>"Emergencia"</text></inputEntry>
        <inputEntry id="InputEntry_R8_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R8_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R8_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R8_observacao"><text>"Emergencia cardiaca - ETT indicado para avaliacao imediata"</text></outputEntry>
        <outputEntry id="OutputEntry_R8_fundamentacao"><text>"ACC AUC 2024, ESC 2023: ETT e primeira linha para avaliacao de emergencias cardiacas"</text></outputEntry>
        <outputEntry id="OutputEntry_R8_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_R8_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- RULE 9: Aprovado Urgencia com indicacao Classe I -->
      <rule id="Rule_Aprovado_Urgencia_ClasseI">
        <description>Aprovado: Urgencia com indicacao Classe I</description>
        <inputEntry id="InputEntry_R9_1"><text>"DispneiaCardiaca","InsuficienciaCardiaca"</text></inputEntry>
        <inputEntry id="InputEntry_R9_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_R9_3"><text>"Classe I"</text></inputEntry>
        <inputEntry id="InputEntry_R9_4"><text>"Urgencia"</text></inputEntry>
        <inputEntry id="InputEntry_R9_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R9_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R9_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R9_observacao"><text>"Urgencia cardiaca com indicacao Classe I - ETT essencial"</text></outputEntry>
        <outputEntry id="OutputEntry_R9_fundamentacao"><text>"ACC AUC 2024: ETT apropriado para dispneia ou IC com suspeita de etiologia cardiaca"</text></outputEntry>
        <outputEntry id="OutputEntry_R9_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_R9_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- RULE 10: Aprovado IC inicial com ECG previo -->
      <rule id="Rule_Aprovado_IC_Inicial">
        <description>Aprovado: IC suspeita - avaliacao inicial com step therapy</description>
        <inputEntry id="InputEntry_R10_1"><text>"InsuficienciaCardiaca"</text></inputEntry>
        <inputEntry id="InputEntry_R10_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_R10_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_R10_4"><text>"Eletivo","Urgencia"</text></inputEntry>
        <inputEntry id="InputEntry_R10_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R10_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_R10_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R10_observacao"><text>"IC suspeita com ECG realizado - ETT indicado como padrao-ouro"</text></outputEntry>
        <outputEntry id="OutputEntry_R10_fundamentacao"><text>"ACC AUC 2024, SBC 2023: ETT e padrao-ouro para avaliacao inicial de IC"</text></outputEntry>
        <outputEntry id="OutputEntry_R10_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_R10_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- RULE 11: Aprovado Sopro cardiaco novo -->
      <rule id="Rule_Aprovado_Sopro">
        <description>Aprovado: Sopro cardiaco novo com ECG previo</description>
        <inputEntry id="InputEntry_R11_1"><text>"SoproCardiacoNovo"</text></inputEntry>
        <inputEntry id="InputEntry_R11_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_R11_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_R11_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R11_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R11_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_R11_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R11_observacao"><text>"Sopro cardiaco novo - ETT indicado para avaliacao estrutural"</text></outputEntry>
        <outputEntry id="OutputEntry_R11_fundamentacao"><text>"ACC AUC 2024: ETT apropriado para avaliacao de sopro cardiaco novo"</text></outputEntry>
        <outputEntry id="OutputEntry_R11_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_R11_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- RULE 12: Aprovado Valvulopatia seguimento -->
      <rule id="Rule_Aprovado_Valvulopatia">
        <description>Aprovado: Valvulopatia conhecida - seguimento anual</description>
        <inputEntry id="InputEntry_R12_1"><text>"Valvulopatia"</text></inputEntry>
        <inputEntry id="InputEntry_R12_2"><text>"Adequado"</text></inputEntry>
        <inputEntry id="InputEntry_R12_3"><text>"Classe I","Classe IIa"</text></inputEntry>
        <inputEntry id="InputEntry_R12_4"><text>"Eletivo"</text></inputEntry>
        <inputEntry id="InputEntry_R12_5"><text>&gt;= 180</text></inputEntry>
        <inputEntry id="InputEntry_R12_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_R12_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_R12_observacao"><text>"Valvulopatia conhecida - seguimento ecocardiografico indicado"</text></outputEntry>
        <outputEntry id="OutputEntry_R12_fundamentacao"><text>"ACC AUC 2024, ESC VHD 2021: Seguimento anual para valvulopatias moderadas a graves"</text></outputEntry>
        <outputEntry id="OutputEntry_R12_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_R12_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ========== FALLBACK RULE ========== -->

      <rule id="Rule_Fallback_40101037">
        <description>Regra padrao - analise clinica adicional necessaria</description>
        <inputEntry id="InputEntry_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_6"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_observacao"><text>"Caso requer analise clinica adicional"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_fundamentacao"><text>"ACC AUC 2024: Avaliacao individualizada necessaria conforme complexidade clinica"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
