<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30602203" name="Mamotomia" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30602203" name="Autorizacao para Mamotomia">
    <decisionTable id="DecisionTable_30602203" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Oncologico (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"C50.x", "D05.x", "D24", "R92", "AUSENTE"</text>
        </inputValues>
      </input>
      <input id="Input_2" label="Classificacao BIRADS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"BIRADS_0", "BIRADS_1", "BIRADS_2", "BIRADS_3", "BIRADS_4", "BIRADS_5", "BIRADS_6"</text>
        </inputValues>
      </input>
      <input id="Input_3" label="Mutacao BRCA">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"BRCA1_POSITIVO", "BRCA2_POSITIVO", "NEGATIVO", "NAO_TESTADO"</text>
        </inputValues>
      </input>
      <input id="Input_4" label="Estadiamento TNM">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"T0", "T1", "T2", "T3", "T4", "Tis", "NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      <input id="Input_5" label="Laudo Radiologico com Localizacao">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado", "Reprovado", "Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_BIRADS2">
        <description>BIRADS 2 sem indicacao de biopsia</description>
        <inputEntry id="InputEntry_1_1"><text>"D24", "AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>"BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_1"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"BIRADS 2 (benigno certeiro). Mamotomia nao indicada - seguimento por imagem suficiente."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"ACR BI-RADS 5th Ed: BIRADS 2 VPN &gt;99%, sem indicacao biopsia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Pendente_SemLaudo">
        <description>Documentacao de localizacao obrigatoria</description>
        <inputEntry id="InputEntry_1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Pendente - Anexar laudo radiologico documentando localizacao precisa da lesao (mamografia/USG/RM)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"SBM 2023: Laudo radiologico obrigatorio para guiar mamotomia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_BIRADS0">
        <description>BIRADS 0 requer avaliacao adicional</description>
        <inputEntry id="InputEntry_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>"BIRADS_0"</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Pendente - BIRADS 0 (avaliacao incompleta). Completar investigacao (USG/RM/tomossintese) antes de mamotomia."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"ACR: BIRADS 0 requer exames complementares para caracterizacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_BIRADS4">
        <description>Microcalcificacoes ou lesoes suspeitas BIRADS 4</description>
        <inputEntry id="InputEntry_1_4"><text>"R92", "D05.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>"BIRADS_4"</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>"NAO_APLICAVEL", "Tis"</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Aprovado - BIRADS 4 (suspeito). Mamotomia indicada para diagnostico histologico de lesao nao palpavel."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"SBM 2023: BIRADS 4 VPP 2-95%, mamotomia preferencial para microcalcificacoes"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_BIRADS3Seguimento">
        <description>BIRADS 3 com seguimento insatisfatorio</description>
        <inputEntry id="InputEntry_1_5"><text>"D24", "R92"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>"BIRADS_3"</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Aprovado - BIRADS 3 (provavelmente benigno). Mamotomia pode ser alternativa ao seguimento semestral se ansiedade paciente ou impossibilidade seguimento."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"ACR BI-RADS: BIRADS 3 VPP &lt;2%, biopsia opcional se seguimento inviavel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_BIRADS5">
        <description>BIRADS 5 lesao nao palpavel</description>
        <inputEntry id="InputEntry_1_6"><text>"C50.x", "D05.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>"BIRADS_5"</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>"Tis", "T1"</text></inputEntry>
        <inputEntry id="InputEntry_5_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Aprovado - BIRADS 5 (altamente suspeito) lesao nao palpavel. Mamotomia para diagnostico antes de planejamento cirurgico definitivo."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"SBM 2023: Mamotomia aceitavel BIRADS 5 nao palpavel (VPP &gt;95%)"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_30602203">
        <description>Dados incompletos</description>
        <inputEntry id="InputEntry_1_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_7"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Dados incompletos. Necessario: (1) Diagnostico (CID R92/D24/D05), (2) BIRADS, (3) Laudo radiologico com localizacao."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"SBM 2023: Documentacao completa obrigatoria para mamotomia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>