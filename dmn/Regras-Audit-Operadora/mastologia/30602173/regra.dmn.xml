<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30602173" name="Mamaplastia Redutora Bilateral" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30602173" name="Autorizacao para Mamaplastia Redutora Bilateral">
    <decisionTable id="DecisionTable_30602173" hitPolicy="FIRST">
      
      <input id="Input_1" label="Diagnostico Oncologico (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"C50.x", "D05.x", "N62", "N64.8", "AUSENTE"</text>
        </inputValues>
      </input>
      
      <input id="Input_2" label="Classificacao BIRADS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"BIRADS_0", "BIRADS_1", "BIRADS_2", "BIRADS_3", "BIRADS_4", "BIRADS_5", "BIRADS_6"</text>
        </inputValues>
      </input>
      
      <input id="Input_3" label="Mutacao BRCA">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"BRCA1_POSITIVO", "BRCA2_POSITIVO", "NEGATIVO", "NAO_TESTADO"</text>
        </inputValues>
      </input>
      
      <input id="Input_4" label="Estadiamento TNM">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"T0", "T1", "T2", "T3", "T4", "Tis", "NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      
      <input id="Input_5" label="Laudo Mastologico (Peso &gt;500g/mama)">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado", "Reprovado", "Pendente"</text>
        </outputValues>
      </output>
      
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_CancerBilateral">
        <description>Cancer bilateral ativo contraindicacao absoluta para reducao estetica</description>
        <inputEntry id="InputEntry_1_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>"BIRADS_5", "BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>"T2", "T3", "T4"</text></inputEntry>
        <inputEntry id="InputEntry_5_1"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Cancer de mama bilateral ativo. Mamaplastia redutora inapropriada - requer tratamento oncologico prioritario (mastectomia se indicada)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"NCCN 2023: Controle oncologico prioritario em cancer bilateral"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Contraindicacao_PesoInsuficiente">
        <description>Peso ressecado &lt;500g caracteriza procedimento estetico</description>
        <inputEntry id="InputEntry_1_2"><text>"N62", "N64.8", "AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>"BIRADS_1", "BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Reprovado - Peso estimado ressecado &lt;500g por mama. Caracteriza procedimento estetico (nao coberto). Necessario peso minimo 500g bilateral."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"SBM 2023: Criterio SBM/SBCP peso minimo 500g/mama para indicacao funcional"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_SemLaudo">
        <description>Documentacao de sintomatologia obrigatoria</description>
        <inputEntry id="InputEntry_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Pendente - Anexar laudo mastologico documentando: (1) Peso estimado por mama (&gt;500g), (2) Sintomas (dorsalgia/cervicalgia), (3) Falha tratamento conservador."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"ANS RN 465/2021: Documentacao clinica obrigatoria para autorizacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_BIRADS0ou4">
        <description>BIRADS 0 ou 4 requer definicao antes de cirurgia</description>
        <inputEntry id="InputEntry_1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>"BIRADS_0", "BIRADS_4"</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Pendente - BIRADS 0/4 (investigacao incompleta). Completar propedeutica oncologica (USG/RM/biopsia) antes de autorizar mamaplastia."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"SBM 2023: Mamaplastia requer exclusao de malignidade (BIRADS 1-3)"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Gigantomastia">
        <description>Hipertrofia mamaria bilateral com peso documentado</description>
        <inputEntry id="InputEntry_1_5"><text>"N62"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>"BIRADS_1", "BIRADS_2", "BIRADS_3"</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>"NEGATIVO", "NAO_TESTADO"</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Aprovado - Gigantomastia bilateral (N62) com peso estimado &gt;500g/mama. Mamaplastia redutora bilateral indicada (funcional, nao estetica)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"ANS RN 465/2021 + SBCP 2023: Indicacao funcional com peso minimo documentado"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Macromastia">
        <description>Macromastia com sintomas incapacitantes</description>
        <inputEntry id="InputEntry_1_6"><text>"N64.8", "N62"</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>"BIRADS_1", "BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Aprovado - Macromastia bilateral sintomatica com peso &gt;500g/mama. Indicacao funcional (dorsalgia/cervicalgia incapacitante)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"SBCP 2023: Mamaplastia redutora coberta quando sintomatologia documentada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Assimetria">
        <description>Simetrizacao apos mastectomia contralateral</description>
        <inputEntry id="InputEntry_1_7"><text>"C50.x", "D05.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>"BIRADS_1", "BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>"T0"</text></inputEntry>
        <inputEntry id="InputEntry_5_7"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Aprovado - Mamaplastia redutora contralateral para simetrizacao pos-mastectomia. Indicacao oncoplastica coberta."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"ANS RN 465/2021: Simetrizacao pos-mastectomia procedimento funcional coberto"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30602173">
        <description>Regra padrao para casos incompletos</description>
        <inputEntry id="InputEntry_1_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_8"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_8"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_8"><text>"Dados incompletos. Necessario: (1) Diagnostico (CID N62/N64.8), (2) BIRADS (excluir malignidade), (3) Laudo mastologico com peso estimado &gt;500g/mama bilateral, (4) Sintomatologia documentada."</text></outputEntry>
        <outputEntry id="OutputEntry_3_8"><text>"SBCP 2023: Documentacao completa obrigatoria para mamaplastia funcional"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>