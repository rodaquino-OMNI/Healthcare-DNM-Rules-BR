<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30602114" name="Ginecomastia Unilateral - Tratamento Cirurgico" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30602114" name="Autorizacao para Tratamento de Ginecomastia Unilateral">
    <decisionTable id="DecisionTable_30602114" hitPolicy="FIRST">
      
      <input id="Input_1" label="Diagnostico Oncologico (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"C50.x", "N62", "E29.1", "AUSENTE"</text>
        </inputValues>
      </input>
      
      <input id="Input_2" label="Classificacao BIRADS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"BIRADS_0", "BIRADS_1", "BIRADS_2", "BIRADS_3", "BIRADS_4", "BIRADS_5", "NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      
      <input id="Input_3" label="Mutacao BRCA">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"BRCA1_POSITIVO", "BRCA2_POSITIVO", "NEGATIVO", "NAO_TESTADO"</text>
        </inputValues>
      </input>
      
      <input id="Input_4" label="Estadiamento TNM">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"T0", "T1", "T2", "T3", "T4", "NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      
      <input id="Input_5" label="Laudo Mastologico com USG Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado", "Reprovado", "Pendente"</text>
        </outputValues>
      </output>
      
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_CancerMama">
        <description>Cancer de mama masculino requer mastectomia oncologica (30602149)</description>
        <inputEntry id="InputEntry_1_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>"BIRADS_4", "BIRADS_5"</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_1"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Suspeita de cancer de mama masculino (CID C50.x) com BIRADS 4-5. Indicar mastectomia radical (30602149), nao tratamento de ginecomastia."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"SBM Protocolo Ginecomastia 2023: Exclusao de malignidade e obrigatoria antes de cirurgia benigna"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Contraindicacao_Pseudoginecomastia">
        <description>Pseudoginecomastia (tecido adiposo) nao tem cobertura obrigatoria</description>
        <inputEntry id="InputEntry_1_2"><text>"AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Pseudoginecomastia (ausencia de disco glandular ao USG) - procedimento estetico sem cobertura obrigatoria."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"ANS RN 465/2021: Cobertura restrita a ginecomastia verdadeira (tecido glandular documentado)"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_SemUSG">
        <description>USG mamario obrigatorio para documentacao de tecido glandular</description>
        <inputEntry id="InputEntry_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Pendente - Anexar laudo de ultrassonografia mamaria documentando presenca e tamanho do disco glandular (&amp;gt;2cm para indicacao cirurgica)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"SBM 2023: USG e padrao-ouro para diferenciar ginecomastia verdadeira de pseudoginecomastia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_BIRADSSuspeito">
        <description>BIRADS 4-5 requer biopsia antes de cirurgia</description>
        <inputEntry id="InputEntry_1_4"><text>"N62"</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>"BIRADS_4", "BIRADS_5"</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Pendente - BIRADS 4-5 detectado. Solicitar biopsia (core biopsy) para exclusao de malignidade antes de autorizar cirurgia."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"SBM 2023: Biopsia obrigatoria em BIRADS 4-5 antes de qualquer cirurgia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_GinecomastiaVerdadeira">
        <description>Ginecomastia com tecido glandular documentado por USG</description>
        <inputEntry id="InputEntry_1_5"><text>"N62"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>"BIRADS_1", "BIRADS_2", "NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Aprovado - Ginecomastia unilateral verdadeira documentada por USG. Tecido glandular presente. Indicacao cirurgica procedente."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"ANS RN 465/2021 + SBM 2023: Cobertura obrigatoria para ginecomastia verdadeira refrataria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_GinecomastiaIdiopatica">
        <description>Ginecomastia sem causa especifica identificada</description>
        <inputEntry id="InputEntry_1_6"><text>"E29.1"</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>"BIRADS_2", "NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>"NEGATIVO", "NAO_TESTADO"</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Aprovado - Ginecomastia idiopatica (E29.1) com tecido glandular documentado. Procedimento autorizado."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"Endocrine Society Guideline: Tratamento cirurgico indicado em ginecomastia idiopatica persistente"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30602114">
        <description>Regra padrao para casos incompletos</description>
        <inputEntry id="InputEntry_1_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_7"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Dados incompletos. Necessario: (1) Diagnostico (CID N62 ou E29.1), (2) USG mamario documentando tecido glandular, (3) Exclusao de malignidade se BIRADS 4-5."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"SBM 2023: Documentacao completa obrigatoria para autorizacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>