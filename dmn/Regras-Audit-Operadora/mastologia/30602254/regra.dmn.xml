<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30602254" name="Mastectomia Subcutanea" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  
  <decision id="Decision_30602254" name="Mastectomia Subcutanea">
    <decisionTable id="DecisionTable_30602254" hitPolicy="FIRST">
      <input id="Input_diagnosticoOncologico" label="diagnosticoOncologico">
        <inputExpression id="InputExpression_diagnosticoOncologico" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoOncologico">
          <text>"N62","C50.x","D24","D48.6","AUSENTE"</text>
        </inputValues>
      </input>
      <input id="Input_classificacaoBIRADS" label="classificacaoBIRADS">
        <inputExpression id="InputExpression_classificacaoBIRADS" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_classificacaoBIRADS">
          <text>"BIRADS_0","BIRADS_1","BIRADS_2","BIRADS_3","BIRADS_4","BIRADS_5","BIRADS_6"</text>
        </inputValues>
      </input>
      <input id="Input_mutacaoBRCA" label="mutacaoBRCA">
        <inputExpression id="InputExpression_mutacaoBRCA" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_mutacaoBRCA">
          <text>"BRCA1_POSITIVO","BRCA2_POSITIVO","NEGATIVO","NAO_TESTADO"</text>
        </inputValues>
      </input>
      <input id="Input_estadiamentoTNM" label="estadiamentoTNM">
        <inputExpression id="InputExpression_estadiamentoTNM" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_estadiamentoTNM">
          <text>"T0","T1","T2","T3","T4","Tis","NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      <input id="Input_laudoMastologico" label="laudoMastologico">
        <inputExpression id="InputExpression_laudoMastologico" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      <output id="Output_decisao" label="decisao" name="decisao" typeRef="string">
        <outputValues id="OutputValues_decisao">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_justificativa" label="justificativa" name="justificativa" typeRef="string" />
      <output id="Output_referencia" label="referencia" name="referencia" typeRef="string" />
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_30602254_Contraindicacao_1">
        <description>Contraindicacao - Ausencia de laudo mastologico</description>
        <inputEntry id="IE_Contra1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Laudo de indicacao cirurgica por mastologista e obrigatorio"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"SBM 2023, ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
      <rule id="Rule_30602254_Contraindicacao_2">
        <description>Contraindicacao - Doenca metastatica T4</description>
        <inputEntry id="IE_Contra2_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>"T4"</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Invasao de parede toracica ou pele (T4) contraindica mastectomia subcutanea"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"NCCN Breast Cancer Guidelines v2.2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
      <rule id="Rule_30602254_Contraindicacao_3">
        <description>Contraindicacao - Ausencia de diagnostico confirmado</description>
        <inputEntry id="IE_Contra3_1"><text>"AUSENTE"</text></inputEntry>
        <inputEntry id="IE_Contra3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra3_2"><text>"Diagnostico oncologico confirmado e obrigatorio para mastectomia"</text></outputEntry>
        <outputEntry id="OE_Contra3_3"><text>"SBM 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
      <rule id="Rule_30602254_Aprovacao_1">
        <description>Aprovacao - Profilaxia BRCA positivo</description>
        <inputEntry id="IE_Apr1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr1_2"><text>"BIRADS_1","BIRADS_2","BIRADS_3"</text></inputEntry>
        <inputEntry id="IE_Apr1_3"><text>"BRCA1_POSITIVO","BRCA2_POSITIVO"</text></inputEntry>
        <inputEntry id="IE_Apr1_4"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="IE_Apr1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr1_2"><text>"Mastectomia profilatica indicada para portadores de mutacao BRCA1/BRCA2"</text></outputEntry>
        <outputEntry id="OE_Apr1_3"><text>"SBM 2023, NCCN Guidelines - Genetic/Familial High-Risk"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
      <rule id="Rule_30602254_Aprovacao_2">
        <description>Aprovacao - DCIS extenso</description>
        <inputEntry id="IE_Apr2_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="IE_Apr2_2"><text>"BIRADS_5","BIRADS_6"</text></inputEntry>
        <inputEntry id="IE_Apr2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr2_4"><text>"Tis"</text></inputEntry>
        <inputEntry id="IE_Apr2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr2_2"><text>"Carcinoma ductal in situ extenso indicado para mastectomia subcutanea"</text></outputEntry>
        <outputEntry id="OE_Apr2_3"><text>"SBM 2023, NCCN Breast Cancer Guidelines"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
      <rule id="Rule_30602254_Aprovacao_3">
        <description>Aprovacao - Cancer invasivo inicial T1-T2</description>
        <inputEntry id="IE_Apr3_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="IE_Apr3_2"><text>"BIRADS_5","BIRADS_6"</text></inputEntry>
        <inputEntry id="IE_Apr3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr3_4"><text>"T1","T2"</text></inputEntry>
        <inputEntry id="IE_Apr3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr3_2"><text>"Cancer invasivo inicial T1-T2 elegivel para mastectomia subcutanea"</text></outputEntry>
        <outputEntry id="OE_Apr3_3"><text>"SBM 2023, NCCN Breast Cancer Guidelines v2.2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
    </decisionTable>
  </decision>
