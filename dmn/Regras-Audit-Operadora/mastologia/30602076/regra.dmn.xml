<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30602076" name="ROLL - Exerese de Lesao por Marcacao Estereotaxica" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30602076" name="Autorizacao para ROLL/Marcacao Estereotaxica">
    <decisionTable id="DecisionTable_30602076" hitPolicy="FIRST">
      
      <input id="Input_1" label="Diagnostico Oncologico (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"C50.x", "D05.x", "D24", "R92", "AUSENTE"</text>
        </inputValues>
      </input>
      
      <input id="Input_2" label="Classificacao BIRADS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"BIRADS_0", "BIRADS_1", "BIRADS_2", "BIRADS_3", "BIRADS_4", "BIRADS_5", "BIRADS_6"</text>
        </inputValues>
      </input>
      
      <input id="Input_3" label="Mutacao BRCA">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"BRCA1_POSITIVO", "BRCA2_POSITIVO", "NEGATIVO", "NAO_TESTADO"</text>
        </inputValues>
      </input>
      
      <input id="Input_4" label="Estadiamento TNM">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"T0", "T1", "T2", "T3", "T4", "Tis", "NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      
      <input id="Input_5" label="Laudo Mastologico + Marcacao Documentada">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado", "Reprovado", "Pendente"</text>
        </outputValues>
      </output>
      
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_CancerExtenso">
        <description>Cancer extenso requer mastectomia, nao exerese conservadora</description>
        <inputEntry id="InputEntry_1_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>"BIRADS_5", "BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>"T3", "T4"</text></inputEntry>
        <inputEntry id="InputEntry_5_1"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Cancer de mama extenso (T3/T4). ROLL inapropriado - indicar mastectomia radical (30602149) ou simples (30602246)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"ASCO/SSO 2023: Tumores T3/T4 contraindicam cirurgia conservadora sem neoadjuvancia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Contraindicacao_BIRADS2">
        <description>BIRADS 2 (benigno) sem indicacao cirurgica</description>
        <inputEntry id="InputEntry_1_2"><text>"D24", "AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>"BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"BIRADS 2 (achado benigno certeiro). Sem indicacao cirurgica - manter seguimento por imagem."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"ACR BI-RADS 5th Ed: BIRADS 2 tem valor preditivo negativo &amp;gt;99%, nao requer biopsia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_SemMarcacao">
        <description>Marcacao intraoperatoria obrigatoria para lesoes nao palpaveis</description>
        <inputEntry id="InputEntry_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Pendente - Anexar laudo de marcacao pre-operatoria (ROLL, fio metalico ou semente radioativa) documentando localizacao da lesao nao palpavel."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"SBM 2023: Marcacao obrigatoria para lesoes nao palpaveis (requisito tecnico)"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_BIRADS0">
        <description>BIRADS 0 requer investigacao adicional antes de cirurgia</description>
        <inputEntry id="InputEntry_1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>"BIRADS_0"</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Pendente - BIRADS 0 (avaliacao incompleta). Solicitar investigacao adicional (USG, RM, tomossintese) antes de indicar procedimento."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"ACR BI-RADS: Categoria 0 requer exames complementares para caracterizacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_BIRADS4">
        <description>BIRADS 4 (provavelmente benigno mas suspeito) com marcacao adequada</description>
        <inputEntry id="InputEntry_1_5"><text>"R92", "D05.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>"BIRADS_4"</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>"T0", "Tis", "NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Aprovado - BIRADS 4 (suspeito) com marcacao pre-operatoria documentada. Exerese cirurgica com ROLL indicada para diagnostico/tratamento."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"ANS RN 465/2021 + SBM 2023: BIRADS 4 tem indicacao de biopsia cirurgica (VPP 2-95%)"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_BIRADS5">
        <description>BIRADS 5 com lesao localizada (T1/T2) candidata a conservacao</description>
        <inputEntry id="InputEntry_1_6"><text>"C50.x", "D05.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>"BIRADS_5"</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>"T1", "T2", "Tis"</text></inputEntry>
        <inputEntry id="InputEntry_5_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Aprovado - BIRADS 5 com tumor localizado (T1-T2). Candidata a cirurgia conservadora com ROLL. Avaliar margens cirurgicas intraoperatorias."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"ASCO/SSO 2023: Cirurgia conservadora com ROLL apropriada para T1-T2 com margens livres"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Microcalcificacoes">
        <description>Microcalcificacoes agrupadas suspeitas</description>
        <inputEntry id="InputEntry_1_7"><text>"R92"</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>"BIRADS_4"</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>"NEGATIVO", "NAO_TESTADO"</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_7"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Aprovado - Microcalcificacoes suspeitas (BIRADS 4) requerem biopsia cirurgica com ROLL para diagnostico definitivo."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"SBM 2023: Microcalcificacoes BIRADS 4 nao palpaveis requerem marcacao intraoperatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30602076">
        <description>Regra padrao para casos incompletos</description>
        <inputEntry id="InputEntry_1_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_8"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_8"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_8"><text>"Dados incompletos. Necessario: (1) Diagnostico (CID R92/D05/C50), (2) BIRADS da lesao, (3) Laudo de marcacao ROLL/fio metalico, (4) Estadiamento TNM se maligno."</text></outputEntry>
        <outputEntry id="OutputEntry_3_8"><text>"SBM 2023: Documentacao completa obrigatoria para autorizacao de ROLL"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>