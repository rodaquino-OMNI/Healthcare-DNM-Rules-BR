<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30602122" name="Correcao da Hipertrofia Mamaria Unilateral" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30602122" name="Autorizacao para Mamaplastia Redutora Unilateral">
    <decisionTable id="DecisionTable_30602122" hitPolicy="FIRST">
      
      <input id="Input_1" label="Diagnostico Oncologico (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"C50.x", "Z90.1", "N62", "Q83.1", "AUSENTE"</text>
        </inputValues>
      </input>
      
      <input id="Input_2" label="Classificacao BIRADS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"BIRADS_0", "BIRADS_1", "BIRADS_2", "BIRADS_3", "NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      
      <input id="Input_3" label="Mutacao BRCA">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"BRCA1_POSITIVO", "BRCA2_POSITIVO", "NEGATIVO", "NAO_TESTADO"</text>
        </inputValues>
      </input>
      
      <input id="Input_4" label="Estadiamento TNM">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"NAO_APLICAVEL", "Z90"</text>
        </inputValues>
      </input>
      
      <input id="Input_5" label="Laudo Mastologico com Documentacao Funcional">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado", "Reprovado", "Pendente"</text>
        </outputValues>
      </output>
      
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Bilateral">
        <description>Hipertrofia bilateral requer codigo especifico</description>
        <inputEntry id="InputEntry_1_1"><text>"N62"</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_1"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Hipertrofia bilateral detectada. Indicar mamaplastia redutora bilateral (30602173), nao unilateral."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"SBCP 2023: Hipertrofia bilateral requer procedimento bilateral para simetria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Contraindicacao_Estetico">
        <description>Assimetria puramente estetica nao tem cobertura obrigatoria</description>
        <inputEntry id="InputEntry_1_2"><text>"AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>"BIRADS_1", "BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Assimetria mamaria sem documentacao de impacto funcional (dor, limitacao). Procedimento considerado estetico - sem cobertura obrigatoria."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"ANS RN 465/2021: Correcao estetica sem impacto funcional nao tem cobertura obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_SemDocumentacao">
        <description>Documentacao de impacto funcional obrigatoria</description>
        <inputEntry id="InputEntry_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Pendente - Anexar laudo medico documentando: (1) assimetria mamaria significativa, (2) impacto funcional (dor cervical/dorsal), (3) limitacao em AVDs."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"SBCP 2023: Impacto funcional obrigatorio para caracterizar procedimento nao estetico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_BIRADS0">
        <description>Avaliacao mamografica incompleta</description>
        <inputEntry id="InputEntry_1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>"BIRADS_0"</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Pendente - BIRADS 0 (avaliacao incompleta). Completar investigacao mamografica antes de cirurgia."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"SBM 2023: Mamografia pre-operatoria obrigatoria para cirurgias mamarias"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_PosMastectomia">
        <description>Simetrizacao da mama contralateral pos-mastectomia oncologica</description>
        <inputEntry id="InputEntry_1_5"><text>"Z90.1"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>"BIRADS_1", "BIRADS_2", "NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>"Z90"</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Aprovado - Simetrizacao da mama contralateral pos-mastectomia oncologica. Procedimento reparador coberto."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"ANS RN 465/2021: Simetrizacao pos-mastectomia tem cobertura obrigatoria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_GigantomastiaCongenita">
        <description>Gigantomastia unilateral congenita ou adquirida com impacto funcional</description>
        <inputEntry id="InputEntry_1_6"><text>"N62", "Q83.1"</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>"BIRADS_1", "BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>"NEGATIVO", "NAO_TESTADO"</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Aprovado - Hipertrofia mamaria unilateral com impacto funcional documentado. Mamaplastia redutora unilateral indicada."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"SBM 2023: Hipertrofia funcional tem cobertura quando documentado impacto em AVDs"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_AssimetriaFuncional">
        <description>Assimetria mamaria significativa com impacto funcional</description>
        <inputEntry id="InputEntry_1_7"><text>"N62", "Q83.1"</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>"BIRADS_1", "BIRADS_2", "BIRADS_3"</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_7"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Aprovado - Assimetria mamaria significativa com impacto funcional documentado. Reducao unilateral para simetrizacao aprovada."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"SBCP 2023: Assimetria &amp;gt;2 copos com impacto funcional tem indicacao cirurgica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30602122">
        <description>Dados incompletos</description>
        <inputEntry id="InputEntry_1_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_8"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_8"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_8"><text>"Dados incompletos. Necessario: (1) Diagnostico (CID N62/Q83.1/Z90.1), (2) Mamografia BIRADS, (3) Laudo documentando impacto funcional (dor, limitacao AVDs)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_8"><text>"SBCP 2023: Documentacao completa obrigatoria para diferenciar procedimento funcional de estetico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>