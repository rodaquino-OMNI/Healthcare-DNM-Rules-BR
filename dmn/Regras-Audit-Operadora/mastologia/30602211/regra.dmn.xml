<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30602211" name="Mastectomia com Adenectomia Axilar" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30602211" name="Autorizacao para Mastectomia com Adenectomia">
    <decisionTable id="DecisionTable_30602211" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Oncologico (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"C50.x", "D05.x", "D24", "AUSENTE"</text>
        </inputValues>
      </input>
      <input id="Input_2" label="Classificacao BIRADS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"BIRADS_0", "BIRADS_1", "BIRADS_2", "BIRADS_3", "BIRADS_4", "BIRADS_5", "BIRADS_6"</text>
        </inputValues>
      </input>
      <input id="Input_3" label="Mutacao BRCA">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"BRCA1_POSITIVO", "BRCA2_POSITIVO", "NEGATIVO", "NAO_TESTADO"</text>
        </inputValues>
      </input>
      <input id="Input_4" label="Estadiamento TNM">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"T0", "T1", "T2", "T3", "T4", "Tis", "NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      <input id="Input_5" label="Laudo Mastologico + BLS Positiva">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado", "Reprovado", "Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Metastatico">
        <description>Doenca metastatica requer tratamento sistemico primeiro</description>
        <inputEntry id="InputEntry_1_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>"BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>"T4"</text></inputEntry>
        <inputEntry id="InputEntry_5_1"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Cancer metastatico (M1) ou localmente avancado T4. Mastectomia nao indicada antes de controle sistemico/neoadjuvancia."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"NCCN 2023: Doenca metastatica requer QT sistemica prioritaria"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Pendente_SemDiagnostico">
        <description>Diagnostico oncologico obrigatorio</description>
        <inputEntry id="InputEntry_1_2"><text>"AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Pendente - Mastectomia oncologica requer diagnostico histologico confirmado (C50.x ou D05.x)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"SBM 2023: Biopsia pre-operatoria obrigatoria em cancer de mama"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Pendente_SemLaudo">
        <description>Documentacao de BLS obrigatoria</description>
        <inputEntry id="InputEntry_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Pendente - Anexar laudo de BLS (biop linfonodo sentinela). Se BLS negativa, adenectomia axilar nao indicada."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"NCCN 2023: BLS obrigatoria antes de adenectomia axilar eletiva"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_T1T2_BLSPositiva">
        <description>Cancer inicial com BLS positiva</description>
        <inputEntry id="InputEntry_1_4"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>"BIRADS_5", "BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>"T1", "T2"</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Aprovado - Carcinoma invasivo T1-T2 com BLS positiva. Mastectomia + adenectomia axilar indicada."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"NCCN 2023: BLS+ indica adenectomia axilar nivel I-II"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_CDISExtenso">
        <description>CDIS extenso requer mastectomia</description>
        <inputEntry id="InputEntry_1_5"><text>"D05.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>"BIRADS_4", "BIRADS_5"</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>"Tis"</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Aprovado - CDIS extenso (&amp;gt;4cm) ou multicentrico. Mastectomia indicada (BLS pode ser omitida em CDIS puro)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"SBM 2023: CDIS extenso indicacao mastectomia, BLS opcional"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_T3LocalmenteAvancado">
        <description>Cancer localmente avancado pos-neoadjuvancia</description>
        <inputEntry id="InputEntry_1_6"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>"BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>"T3"</text></inputEntry>
        <inputEntry id="InputEntry_5_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Aprovado - T3 pos-neoadjuvancia com BLS positiva. Mastectomia + adenectomia indicada apos resposta ao tratamento sistemico."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"NCCN 2023: T3 tratavel com cirurgia apos neoadjuvancia se ressecavel"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_30602211">
        <description>Dados incompletos</description>
        <inputEntry id="InputEntry_1_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_7"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Dados incompletos. Necessario: (1) Diagnostico histologico (C50.x/D05.x), (2) Estadiamento TNM, (3) Resultado BLS, (4) BIRADS."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"SBM 2023: Documentacao completa obrigatoria para mastectomia oncologica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>