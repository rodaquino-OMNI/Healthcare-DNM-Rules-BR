<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_30602246" name="Mastectomia Simples" namespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_30602246" name="Regra Clínica - Mastectomia Simples">
    <decisionTable id="DecisionTable_30602246" hitPolicy="FIRST">

      

      <input id="Input_diagnosticoOncologico" label="Diagnóstico Oncológico" camunda:inputVariable="diagnosticoOncologico">
        <inputExpression id="InputExpression_diagnosticoOncologico" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoOncologico">
          <text>"C50.x","D05.x","Z80.3","N60.x","N62","AUSENTE"</text>
        </inputValues>
      </input>

      <input id="Input_classificacaoBIRADS" label="Classificação BI-RADS" camunda:inputVariable="classificacaoBIRADS">
        <inputExpression id="InputExpression_classificacaoBIRADS" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_classificacaoBIRADS">
          <text>"BIRADS_0","BIRADS_1","BIRADS_2","BIRADS_3","BIRADS_4","BIRADS_5","BIRADS_6"</text>
        </inputValues>
      </input>

      <input id="Input_mutacaoBRCA" label="Mutação BRCA" camunda:inputVariable="mutacaoBRCA">
        <inputExpression id="InputExpression_mutacaoBRCA" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_mutacaoBRCA">
          <text>"BRCA1_POSITIVO","BRCA2_POSITIVO","NEGATIVO","NAO_TESTADO"</text>
        </inputValues>
      </input>

      <input id="Input_estadiamentoTNM" label="Estadiamento TNM" camunda:inputVariable="estadiamentoTNM">
        <inputExpression id="InputExpression_estadiamentoTNM" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_estadiamentoTNM">
          <text>"T0","T1","T2","T3","T4","Tis","NAO_APLICAVEL"</text>
        </inputValues>
      </input>

      <input id="Input_laudoMastologico" label="Laudo Mastológico Anexado" camunda:inputVariable="laudoMastologico">
        <inputExpression id="InputExpression_laudoMastologico" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>

      

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observação Clínica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentação Técnica" name="fundamentacao" typeRef="string" />

      

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_30602246_1">
        <description>Contraindicação: Ausência de diagnóstico oncológico confirmado</description>
        <inputEntry id="InputEntry_Contraind_30602246_1_1"><text>"AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_1_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_1_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_1_observacao"><text>"Mastectomia simples requer diagnóstico oncológico confirmado por biópsia. Sem diagnóstico, não há indicação cirúrgica oncológica."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_1_fundamentacao"><text>"SBM Diretrizes 2023 - Diagnóstico histopatológico é mandatório antes de cirurgia oncológica. ANS RN 465/2021."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_30602246_2">
        <description>Contraindicação: BI-RADS 1-2 sem confirmação de malignidade</description>
        <inputEntry id="InputEntry_Contraind_30602246_2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_2_2"><text>"BIRADS_1","BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_2_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_2_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_2_observacao"><text>"BI-RADS 1-2 indicam achados normais ou benignos. Mastectomia não está indicada sem confirmação de malignidade."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_2_fundamentacao"><text>"SBM 2023 - BI-RADS 1-2 não justificam procedimento cirúrgico oncológico. NCCN Breast Cancer Guidelines v2.2024."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_30602246_3">
        <description>Contraindicação: Estadiamento T0 sem tumor primário identificado</description>
        <inputEntry id="InputEntry_Contraind_30602246_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_3_4"><text>"T0"</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_3_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_3_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_3_observacao"><text>"Estadiamento T0 indica ausência de tumor primário. Mastectomia não está indicada sem lesão primária identificada."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_3_fundamentacao"><text>"NCCN Breast Cancer Guidelines v2.2024 - T0 requer investigação adicional antes de procedimento cirúrgico. SBM 2023."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraind_30602246_4">
        <description>Contraindicação: Ausência de laudo mastológico obrigatório</description>
        <inputEntry id="InputEntry_Contraind_30602246_4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Contraind_30602246_4_5"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_4_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_4_observacao"><text>"Laudo mastológico com indicação cirúrgica é documentação obrigatória para autorização de mastectomia simples."</text></outputEntry>
        <outputEntry id="OutputEntry_Contraind_30602246_4_fundamentacao"><text>"ANS RN 465/2021 - Documentação clínica completa é pré-requisito para autorização. SBM Diretrizes 2023."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_30602246_1">
        <description>Aprovado: CDIS extenso (Tis) ou câncer invasivo T2-T3 com laudo mastológico</description>
        <inputEntry id="InputEntry_Aprovado_30602246_1_1"><text>"C50.x","D05.x"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_1_2"><text>"BIRADS_4","BIRADS_5","BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_1_4"><text>"Tis","T2","T3"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_1_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_1_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_1_observacao"><text>"Indicação adequada para mastectomia simples: CDIS extenso (Tis) ou câncer invasivo T2-T3 com confirmação diagnóstica e laudo mastológico."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_1_fundamentacao"><text>"SBM Diretrizes 2023 - Mastectomia simples indicada para CDIS extenso &amp;gt;4cm ou tumor T2-T3. NCCN Breast Cancer Guidelines v2.2024."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_30602246_2">
        <description>Aprovado: Câncer de mama T1 multifocal/multicêntrico com contraindicação à conservadora</description>
        <inputEntry id="InputEntry_Aprovado_30602246_2_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_2_2"><text>"BIRADS_5","BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_2_4"><text>"T1"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_2_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_2_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_2_observacao"><text>"Indicação adequada: tumor T1 com laudo mastológico documentando multifocalidade/multicentricidade ou contraindicação à cirurgia conservadora."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_2_fundamentacao"><text>"NCCN Breast Cancer Guidelines v2.2024 - Mastectomia indicada em casos multifocais/multicêntricos T1. SBM Diretrizes 2023."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_30602246_3">
        <description>Aprovado: Tumor T4 localmente avançado com indicação cirúrgica</description>
        <inputEntry id="InputEntry_Aprovado_30602246_3_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_3_2"><text>"BIRADS_5","BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_3_4"><text>"T4"</text></inputEntry>
        <inputEntry id="InputEntry_Aprovado_30602246_3_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_3_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_3_observacao"><text>"Indicação adequada: tumor localmente avançado T4 com indicação cirúrgica documentada em laudo mastológico após avaliação multidisciplinar."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_30602246_3_fundamentacao"><text>"SBM Diretrizes 2023 - Mastectomia pode ser indicada em T4 após quimioterapia neoadjuvante. NCCN Breast Cancer Guidelines v2.2024."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30602246">
        <description>Regra padrão - Análise adicional necessária</description>
        <inputEntry id="InputEntry_Fallback_30602246_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_30602246_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_30602246_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_30602246_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Fallback_30602246_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fallback_30602246_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_30602246_observacao"><text>"Caso requer análise clínica adicional por equipe multidisciplinar para avaliação de indicação cirúrgica e adequação do procedimento."</text></outputEntry>
        <outputEntry id="OutputEntry_Fallback_30602246_fundamentacao"><text>"ANS RN 465/2021 - Casos não enquadrados em critérios objetivos requerem avaliação individualizada."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>