<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30602092" name="Exerese de Nodulo de Mama" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30602092" name="Autorizacao para Exerese de Nodulo Mamario">
    <decisionTable id="DecisionTable_30602092" hitPolicy="FIRST">
      
      <input id="Input_1" label="Diagnostico Oncologico (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"C50.x", "D05.x", "D24", "N60.2", "N63", "AUSENTE"</text>
        </inputValues>
      </input>
      
      <input id="Input_2" label="Classificacao BIRADS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"BIRADS_0", "BIRADS_1", "BIRADS_2", "BIRADS_3", "BIRADS_4", "BIRADS_5", "BIRADS_6"</text>
        </inputValues>
      </input>
      
      <input id="Input_3" label="Mutacao BRCA">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"BRCA1_POSITIVO", "BRCA2_POSITIVO", "NEGATIVO", "NAO_TESTADO"</text>
        </inputValues>
      </input>
      
      <input id="Input_4" label="Estadiamento TNM">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"T0", "T1", "T2", "T3", "T4", "NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      
      <input id="Input_5" label="Laudo Mastologico com USG/Biopsia">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado", "Reprovado", "Pendente"</text>
        </outputValues>
      </output>
      
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_CancerExtenso">
        <description>Cancer T3/T4 requer mastectomia, nao nodulectomia</description>
        <inputEntry id="InputEntry_1_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>"BIRADS_5", "BIRADS_6"</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>"T3", "T4"</text></inputEntry>
        <inputEntry id="InputEntry_5_1"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Cancer de mama extenso (T3/T4). Exerese simples inadequada - indicar mastectomia radical (30602149) ou simples (30602246)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"SBM 2023: Tumores T3/T4 contraindicam cirurgia conservadora limitada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Contraindicacao_BIRADS2Assintomatico">
        <description>Nodulo benigno certeiro sem sintomas - seguimento clinico</description>
        <inputEntry id="InputEntry_1_2"><text>"D24", "N60.2"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>"BIRADS_2"</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"BIRADS 2 (fibroadenoma/cisto simples) assintomatico. Sem indicacao cirurgica - manter seguimento anual por imagem."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"ACR BI-RADS: Categoria 2 tem VPN &amp;gt;99%, seguimento anual suficiente se assintomatico"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_SemDocumentacao">
        <description>Laudo USG e/ou biopsia obrigatorios para nodulo palpavel</description>
        <inputEntry id="InputEntry_1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Pendente - Anexar laudo de USG mamario com BIRADS e, se BIRADS 4-5, resultado de biopsia percutanea (core biopsy ou PAAF)."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"SBM 2023: Caracterizacao por imagem obrigatoria antes de exerese cirurgica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Pendente_BIRADS0">
        <description>Avaliacao incompleta requer exames adicionais</description>
        <inputEntry id="InputEntry_1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>"BIRADS_0"</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Pendente - BIRADS 0 (avaliacao incompleta). Completar investigacao com USG adicional, tomossintese ou RM antes de indicar cirurgia."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"ACR BI-RADS: Categoria 0 requer exames complementares para caracterizacao"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_BIRADS3Sintomatico">
        <description>Fibroadenoma sintomatico ou em crescimento tem indicacao cirurgica</description>
        <inputEntry id="InputEntry_1_5"><text>"D24", "N60.2"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>"BIRADS_3"</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>"NEGATIVO", "NAO_TESTADO"</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Aprovado - BIRADS 3 (fibroadenoma) sintomatico ou em crescimento. Exerese cirurgica indicada para alivio de sintomas e/ou diagnostico."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"SBM 2023: BIRADS 3 sintomatico ou &amp;gt;3cm tem indicacao de exerese"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_BIRADS4Persistente">
        <description>BIRADS 4 com biopsia benigna mas nodulo persistente</description>
        <inputEntry id="InputEntry_1_6"><text>"D24", "N63"</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>"BIRADS_4"</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_5_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Aprovado - BIRADS 4 com biopsia benigna mas persistencia de nodulo suspeito. Exerese cirurgica para diagnostico definitivo indicada."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"SBM 2023: Discordancia radiologica-patologica em BIRADS 4 requer exerese cirurgica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_BIRADS45Localizada">
        <description>Cancer localizado T1-T2 candidato a cirurgia conservadora</description>
        <inputEntry id="InputEntry_1_7"><text>"C50.x", "D05.x"</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>"BIRADS_4", "BIRADS_5"</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>"T1", "T2"</text></inputEntry>
        <inputEntry id="InputEntry_5_7"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Aprovado - Cancer localizado (T1-T2) com BIRADS 4-5. Candidata a cirurgia conservadora (setorectomia/quadrantectomia). Avaliar margens intraoperatorias."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"ASCO/SSO 2023: T1-T2 com margens livres podem ser tratados conservadoramente"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30602092">
        <description>Dados incompletos</description>
        <inputEntry id="InputEntry_1_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_8"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_1_8"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_8"><text>"Dados incompletos. Necessario: (1) Diagnostico (CID D24/N60.2/C50), (2) BIRADS do nodulo, (3) Laudo USG + biopsia se BIRADS 4-5, (4) TNM se maligno."</text></outputEntry>
        <outputEntry id="OutputEntry_3_8"><text>"SBM 2023: Documentacao completa obrigatoria para autorizacao de nodulectomia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>