<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:ns2="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30602289" name="Reconstrucao Mamaria Tardia" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  
  <decision id="Decision_30602289" name="Reconstrucao Mamaria Tardia">
    <decisionTable id="DecisionTable_30602289" hitPolicy="FIRST">
      <input id="Input_diagnosticoOncologico" label="diagnosticoOncologico">
        <inputExpression id="InputExpression_diagnosticoOncologico" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoOncologico">
          <text>"N62","C50.x","D24","D48.6","AUSENTE"</text>
        </inputValues>
      </input>
      <input id="Input_classificacaoBIRADS" label="classificacaoBIRADS">
        <inputExpression id="InputExpression_classificacaoBIRADS" typeRef="string">
          <text>classificacaoBIRADS</text>
        </inputExpression>
        <inputValues id="InputValues_classificacaoBIRADS">
          <text>"BIRADS_0","BIRADS_1","BIRADS_2","BIRADS_3","BIRADS_4","BIRADS_5","BIRADS_6"</text>
        </inputValues>
      </input>
      <input id="Input_mutacaoBRCA" label="mutacaoBRCA">
        <inputExpression id="InputExpression_mutacaoBRCA" typeRef="string">
          <text>mutacaoBRCA</text>
        </inputExpression>
        <inputValues id="InputValues_mutacaoBRCA">
          <text>"BRCA1_POSITIVO","BRCA2_POSITIVO","NEGATIVO","NAO_TESTADO"</text>
        </inputValues>
      </input>
      <input id="Input_estadiamentoTNM" label="estadiamentoTNM">
        <inputExpression id="InputExpression_estadiamentoTNM" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_estadiamentoTNM">
          <text>"T0","T1","T2","T3","T4","Tis","NAO_APLICAVEL"</text>
        </inputValues>
      </input>
      <input id="Input_laudoMastologico" label="laudoMastologico">
        <inputExpression id="InputExpression_laudoMastologico" typeRef="boolean">
          <text>laudoMastologico</text>
        </inputExpression>
      </input>
      <output id="Output_decisao" label="decisao" name="decisao" typeRef="string">
        <outputValues id="OutputValues_decisao">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_justificativa" label="justificativa" name="justificativa" typeRef="string" />
      <output id="Output_referencia" label="referencia" name="referencia" typeRef="string" />
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_30602289_Contraindicacao_1">
        <description>Contraindicacao - Ausencia de laudo mastologico</description>
        <inputEntry id="IE_Contra1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Laudo de indicacao cirurgica e avaliacao tecidual obrigatorios"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"Lei 12.802/2013, SBCP 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
      <rule id="Rule_30602289_Contraindicacao_2">
        <description>Contraindicacao - Recorrencia tumoral suspeita</description>
        <inputEntry id="IE_Contra2_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>"BIRADS_4","BIRADS_5","BIRADS_6"</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Suspeita de recorrencia tumoral contraindica reconstrucao tardia"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"SBM 2023, NCCN Breast Cancer Guidelines"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
      <rule id="Rule_30602289_Contraindicacao_3">
        <description>Contraindicacao - Estadiamento incompleto</description>
        <inputEntry id="IE_Contra3_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="IE_Contra3_2"><text>"BIRADS_0"</text></inputEntry>
        <inputEntry id="IE_Contra3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra3_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Contra3_2"><text>"Estadiamento atualizado obrigatorio para confirmar ausencia de doenca"</text></outputEntry>
        <outputEntry id="OE_Contra3_3"><text>"SBM 2023, Lei 12.802/2013"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
      <rule id="Rule_30602289_Aprovacao_1">
        <description>Aprovacao - Pos-mastectomia doenca estavel</description>
        <inputEntry id="IE_Apr1_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="IE_Apr1_2"><text>"BIRADS_1","BIRADS_2"</text></inputEntry>
        <inputEntry id="IE_Apr1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr1_4"><text>"T0"</text></inputEntry>
        <inputEntry id="IE_Apr1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr1_2"><text>"Reconstrucao tardia indicada pos-mastectomia com doenca estavel"</text></outputEntry>
        <outputEntry id="OE_Apr1_3"><text>"Lei 12.802/2013, SBM 2023, SBCP 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
      <rule id="Rule_30602289_Aprovacao_2">
        <description>Aprovacao - Pos-mastectomia profilatica BRCA</description>
        <inputEntry id="IE_Apr2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr2_2"><text>"BIRADS_1","BIRADS_2"</text></inputEntry>
        <inputEntry id="IE_Apr2_3"><text>"BRCA1_POSITIVO","BRCA2_POSITIVO"</text></inputEntry>
        <inputEntry id="IE_Apr2_4"><text>"NAO_APLICAVEL"</text></inputEntry>
        <inputEntry id="IE_Apr2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr2_2"><text>"Reconstrucao tardia indicada pos-mastectomia profilatica BRCA+"</text></outputEntry>
        <outputEntry id="OE_Apr2_3"><text>"Lei 12.802/2013, SBM 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
      <rule id="Rule_30602289_Aprovacao_3">
        <description>Aprovacao - Pos-DCIS tratado com sucesso</description>
        <inputEntry id="IE_Apr3_1"><text>"C50.x"</text></inputEntry>
        <inputEntry id="IE_Apr3_2"><text>"BIRADS_1","BIRADS_2"</text></inputEntry>
        <inputEntry id="IE_Apr3_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Apr3_4"><text>"Tis"</text></inputEntry>
        <inputEntry id="IE_Apr3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Apr3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Apr3_2"><text>"Reconstrucao tardia pos-mastectomia por DCIS com tratamento completo"</text></outputEntry>
        <outputEntry id="OE_Apr3_3"><text>"Lei 12.802/2013, SBM 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_30602289">
      <dmndi:DMNShape id="DMNShape_30602289" dmnElementRef="Decision_30602289">
        <ns2:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>