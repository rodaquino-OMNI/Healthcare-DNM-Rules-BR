<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_30803110"
             name="TOR-LUN-001 Lobectomia para CPNPC"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    TUSS: 30803110
    Procedimento: Lobectomia para Cancer de Pulmao Nao Pequenas Celulas (CPNPC)
    Especialidade: Cirurgia Toracica
    Codigo Regra: TOR-LUN-001
    Versao: 3.0.0-LEAN-TIER2
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS:
    - CPNPC estadio I (T1a-T2a, N0, M0) - indicacao primaria
    - CPNPC estadio II (T1-T2, N1, M0 ou T3, N0, M0)
    - CPNPC estadio IIIA selecionado (T3, N1, M0 ou T1-T3, N2, M0 apos neoadjuvancia)
    - VEF1 previsto pos-operatorio maior que 40%
    - DLCO maior que 40% do previsto
    - Performance status ECOG 0-2

    CONTRAINDICACOES:
    - Estadio IV (metastases a distancia - M1)
    - Comprometimento N3 (linfonodos contralaterais ou supraclaviculares)
    - VEF1 ppo menor que 30% (contraindicacao absoluta)
    - DLCO menor que 30% do previsto
    - Performance status ECOG 3-4
    - Comorbidades graves descompensadas (ICC classe IV, cirrose Child C)
    - Invasao de estruturas irressecaveis (coluna, grandes vasos, coracao)

    REFERENCIAS TECNICAS:
    - IASLC Staging Manual 8th Edition 2024
    - SBCT 2023 (Sociedade Brasileira de Cirurgia Toracica)
    - NCCN Non-Small Cell Lung Cancer Guidelines 2024
    - ACCP Evidence-Based Clinical Practice Guidelines
    - DUT ANS 2024
    ===============================================================================
  -->

  <decision id="Decision_30803110" name="Regra Clinica - Lobectomia para CPNPC">
    <decisionTable id="DecisionTable_30803110" hitPolicy="FIRST">

      <!-- ========== INPUTS (5) - Padrao TIER-1 Cirurgia Toracica ========== -->

      <input id="Input_diagnosticoPulmonar" label="Diagnostico Pulmonar" camunda:inputVariable="diagnosticoPulmonar">
        <inputExpression id="InputExpression_diagnosticoPulmonar" typeRef="string">
          <text>diagnosticoPulmonar</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoPulmonar">
          <text>"CPNPC_CONFIRMADO","CPNPC_SUSPEITO","NODULO_INDETERMINADO","BENIGNO","NAO_INFORMADO"</text>
        </inputValues>
      </input>

      <input id="Input_estadiamentoTNM" label="Estadiamento TNM" camunda:inputVariable="estadiamentoTNM">
        <inputExpression id="InputExpression_estadiamentoTNM" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_estadiamentoTNM">
          <text>"IA1","IA2","IA3","IB","IIA","IIB","IIIA","IIIB","IIIC","IVA","IVB","NAO_ESTADIADO"</text>
        </inputValues>
      </input>

      <input id="Input_funcaoPulmonar" label="Funcao Pulmonar" camunda:inputVariable="funcaoPulmonar">
        <inputExpression id="InputExpression_funcaoPulmonar" typeRef="string">
          <text>funcaoPulmonar</text>
        </inputExpression>
        <inputValues id="InputValues_funcaoPulmonar">
          <text>"ADEQUADA_VEF1_MAIOR_60","LIMITROFE_VEF1_40_60","INSUFICIENTE_VEF1_MENOR_40","NAO_AVALIADA"</text>
        </inputValues>
      </input>

      <input id="Input_comorbidades" label="Comorbidades" camunda:inputVariable="comorbidades">
        <inputExpression id="InputExpression_comorbidades" typeRef="string">
          <text>comorbidades</text>
        </inputExpression>
        <inputValues id="InputValues_comorbidades">
          <text>"CONTROLADAS","DESCOMPENSADAS_GRAVES","ASA_I_II","ASA_III","ASA_IV_V","NAO_AVALIADAS"</text>
        </inputValues>
      </input>

      <input id="Input_documentacaoClinica" label="Documentacao Clinica" camunda:inputVariable="documentacaoClinica">
        <inputExpression id="InputExpression_documentacaoClinica" typeRef="string">
          <text>documentacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_documentacaoClinica">
          <text>"COMPLETA","PARCIAL_SEM_BIOPSIA","PARCIAL_SEM_ESTADIAMENTO","PARCIAL_SEM_ESPIROMETRIA","INCOMPLETA"</text>
        </inputValues>
      </input>

      <!-- ========== OUTPUTS (3) - Padrao TIER-1 ========== -->

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- ========== REGRAS - Ordem: Reprovado, Aprovado, Pendente, Fallback ========== -->

      <!-- REPROVACOES (Contraindicacoes Absolutas) -->

      <rule id="Rule_Reprovado_Metastase">
        <description>Reprovado: Estadio IV - Metastases a distancia (M1)</description>
        <inputEntry id="InputEntry_Rep_Meta_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Meta_2"><text>"IVA","IVB"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Meta_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Meta_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Meta_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_Meta_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Meta_observacao"><text>"CPNPC estadio IV com metastases a distancia. Lobectomia nao indicada - doenca sistemica requer tratamento oncologico."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Meta_fundamentacao"><text>"IASLC 8th Ed 2024, NCCN NSCLC 2024: Estadio IV (M1) contraindica resseccao curativa. Tratamento sistemico e primeira linha."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Metastase_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Metastase_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Metastase_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Metastase_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_EstadioIIIBC">
        <description>Reprovado: Estadio IIIB/IIIC - Doenca localmente avancada irressecavel</description>
        <inputEntry id="InputEntry_Rep_IIIBC_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_IIIBC_2"><text>"IIIB","IIIC"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_IIIBC_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_IIIBC_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_IIIBC_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_IIIBC_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_IIIBC_observacao"><text>"CPNPC estadio IIIB/IIIC. Doenca localmente avancada geralmente irressecavel - considerar quimiorradioterapia definitiva."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_IIIBC_fundamentacao"><text>"NCCN NSCLC 2024, SBCT 2023: Estadios IIIB-C (N3 ou T4 com invasao de estruturas vitais) sao contraindicacao para lobectomia primaria."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_EstadioIIIBC_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_EstadioIIIBC_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_EstadioIIIBC_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_EstadioIIIBC_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_FuncaoPulmonarInsuficiente">
        <description>Reprovado: Funcao pulmonar insuficiente para lobectomia</description>
        <inputEntry id="InputEntry_Rep_Func_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Func_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Func_3"><text>"INSUFICIENTE_VEF1_MENOR_40"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Func_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Func_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_Func_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Func_observacao"><text>"VEF1 menor que 40% do previsto. Risco proibitivo para lobectomia - considerar resseccao sublobar ou tratamento nao cirurgico."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Func_fundamentacao"><text>"ACCP Guidelines, SBCT 2023: VEF1 ppo menor 30-40% contraindica lobectomia. DLCO menor 40% aumenta mortalidade perioperatoria significativamente."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_FuncaoPulmonarInsuficiente_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_FuncaoPulmonarInsuficiente_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_FuncaoPulmonarInsuficiente_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_FuncaoPulmonarInsuficiente_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_ComorbidadesGraves">
        <description>Reprovado: Comorbidades graves descompensadas (ASA IV-V)</description>
        <inputEntry id="InputEntry_Rep_Comorb_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Comorb_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Comorb_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Comorb_4"><text>"ASA_IV_V","DESCOMPENSADAS_GRAVES"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Comorb_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_Comorb_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Comorb_observacao"><text>"Comorbidades graves descompensadas (ASA IV-V). Risco cirurgico proibitivo para lobectomia pulmonar."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Comorb_fundamentacao"><text>"SBCT 2023, ASA Classification: Pacientes ASA IV-V apresentam mortalidade perioperatoria inaceitavel para cirurgia eletiva oncologica."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_ComorbidadesGraves_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_ComorbidadesGraves_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_ComorbidadesGraves_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_ComorbidadesGraves_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_DiagnosticoNaoConfirmado">
        <description>Reprovado: Diagnostico de CPNPC nao confirmado</description>
        <inputEntry id="InputEntry_Rep_Diag_1"><text>"BENIGNO","NAO_INFORMADO"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Diag_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Diag_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Diag_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Diag_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_Diag_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Diag_observacao"><text>"Diagnostico de CPNPC nao confirmado ou lesao benigna. Lobectomia oncologica requer confirmacao histopatologica."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Diag_fundamentacao"><text>"NCCN NSCLC 2024, SBCT 2023: Confirmacao histologica ou citologica de malignidade e mandatoria antes de resseccao pulmonar oncologica."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_DiagnosticoNaoConfirmado_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_DiagnosticoNaoConfirmado_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_DiagnosticoNaoConfirmado_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_DiagnosticoNaoConfirmado_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->

      <rule id="Rule_Aprovado_EstadioI">
        <description>Aprovado: CPNPC Estadio I com funcao pulmonar adequada</description>
        <inputEntry id="InputEntry_Apr_EI_1"><text>"CPNPC_CONFIRMADO"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EI_2"><text>"IA1","IA2","IA3","IB"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EI_3"><text>"ADEQUADA_VEF1_MAIOR_60","LIMITROFE_VEF1_40_60"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EI_4"><text>"CONTROLADAS","ASA_I_II","ASA_III"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EI_5"><text>"COMPLETA"</text></inputEntry>
        <outputEntry id="OutputEntry_Apr_EI_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_EI_observacao"><text>"CPNPC estadio I confirmado com funcao pulmonar adequada. Lobectomia e tratamento padrao com intencao curativa."</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_EI_fundamentacao"><text>"IASLC 8th Ed, NCCN NSCLC 2024, SBCT 2023: Lobectomia com linfadenectomia mediastinal e tratamento padrao para CPNPC estadio I. Sobrevida 5 anos 70-90%."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioI_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioI_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioI_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioI_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_EstadioII">
        <description>Aprovado: CPNPC Estadio II com funcao pulmonar adequada</description>
        <inputEntry id="InputEntry_Apr_EII_1"><text>"CPNPC_CONFIRMADO"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EII_2"><text>"IIA","IIB"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EII_3"><text>"ADEQUADA_VEF1_MAIOR_60","LIMITROFE_VEF1_40_60"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EII_4"><text>"CONTROLADAS","ASA_I_II","ASA_III"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EII_5"><text>"COMPLETA"</text></inputEntry>
        <outputEntry id="OutputEntry_Apr_EII_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_EII_observacao"><text>"CPNPC estadio II confirmado. Lobectomia indicada seguida de quimioterapia adjuvante conforme protocolos."</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_EII_fundamentacao"><text>"NCCN NSCLC 2024, LACE Meta-analysis: Lobectomia em estadio II seguida de QT adjuvante melhora sobrevida. Indicacao classe I."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioII_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioII_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioII_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioII_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_EstadioIIIA">
        <description>Aprovado: CPNPC Estadio IIIA selecionado (pos-neoadjuvancia ou N2 limitado)</description>
        <inputEntry id="InputEntry_Apr_EIIIA_1"><text>"CPNPC_CONFIRMADO"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EIIIA_2"><text>"IIIA"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EIIIA_3"><text>"ADEQUADA_VEF1_MAIOR_60"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EIIIA_4"><text>"CONTROLADAS","ASA_I_II"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_EIIIA_5"><text>"COMPLETA"</text></inputEntry>
        <outputEntry id="OutputEntry_Apr_EIIIA_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_EIIIA_observacao"><text>"CPNPC estadio IIIA com criterios para ressecabilidade. Avaliar resposta a neoadjuvancia. Decisao em equipe multidisciplinar."</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_EIIIA_fundamentacao"><text>"NCCN NSCLC 2024, SBCT 2023: IIIA selecionado (T3N1 ou N2 single-station) pode ser ressecavel. Discussao em tumor board obrigatoria."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioIIIA_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioIIIA_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioIIIA_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_EstadioIIIA_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- PENDENCIAS -->

      <rule id="Rule_Pendente_DocumentacaoIncompleta">
        <description>Pendente: Documentacao clinica incompleta</description>
        <inputEntry id="InputEntry_Pend_Doc_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Doc_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Doc_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Doc_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Doc_5"><text>"PARCIAL_SEM_BIOPSIA","PARCIAL_SEM_ESTADIAMENTO","PARCIAL_SEM_ESPIROMETRIA","INCOMPLETA"</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Doc_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Doc_observacao"><text>"Documentacao clinica incompleta. Anexar: laudo histopatologico, estadiamento TNM (PET-CT/TC), espirometria com VEF1/CVF."</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Doc_fundamentacao"><text>"DUT ANS 2024, SBCT 2023: Autorizacao de lobectomia requer documentacao completa de estadiamento e avaliacao funcional."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_DocumentacaoIncompleta_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_DocumentacaoIncompleta_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_DocumentacaoIncompleta_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_DocumentacaoIncompleta_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Pendente_FuncaoNaoAvaliada">
        <description>Pendente: Funcao pulmonar nao avaliada</description>
        <inputEntry id="InputEntry_Pend_Func_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Func_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Func_3"><text>"NAO_AVALIADA"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Func_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Func_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Func_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Func_observacao"><text>"Espirometria nao realizada. Obrigatorio VEF1 e DLCO para avaliacao de risco cirurgico em resseccao pulmonar."</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Func_fundamentacao"><text>"ACCP Guidelines, SBCT 2023: Avaliacao funcional pulmonar pre-operatoria e mandatoria. VEF1 ppo e DLCO definem operabilidade."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_FuncaoNaoAvaliada_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_FuncaoNaoAvaliada_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_FuncaoNaoAvaliada_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_FuncaoNaoAvaliada_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Pendente_NaoEstadiado">
        <description>Pendente: Tumor nao estadiado</description>
        <inputEntry id="InputEntry_Pend_Est_1"><text>"CPNPC_CONFIRMADO","CPNPC_SUSPEITO"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Est_2"><text>"NAO_ESTADIADO"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Est_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Est_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Est_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Est_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Est_observacao"><text>"Estadiamento TNM nao realizado. Necessario PET-CT e/ou TC de torax com contraste para definir extensao da doenca."</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_Est_fundamentacao"><text>"IASLC 8th Ed, NCCN 2024: Estadiamento completo com PET-CT e/ou mediastinoscopia e obrigatorio antes de resseccao curativa."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_NaoEstadiado_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_NaoEstadiado_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_NaoEstadiado_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_NaoEstadiado_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Pendente_Suspeito">
        <description>Pendente: CPNPC suspeito sem confirmacao histologica</description>
        <inputEntry id="InputEntry_Pend_Susp_1"><text>"CPNPC_SUSPEITO","NODULO_INDETERMINADO"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Susp_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Susp_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Susp_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_Susp_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_Susp_resultado"><text>"Pendente"</text></outputEntry>
