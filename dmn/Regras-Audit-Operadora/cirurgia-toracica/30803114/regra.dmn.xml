<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:camunda="http://camunda.org/schema/1.0/dmn"
             id="Definitions_30803114"
             name="TOR-LUN-005 Metastasectomia Pulmonar"
             namespace="http://camunda.org/schema/1.0/dmn">

  <!--
    ===============================================================================
    TUSS: 30803114
    Procedimento: Metastasectomia Pulmonar para Doenca Oligometastatica
    Especialidade: Cirurgia Toracica
    Codigo Regra: TOR-LUN-005
    Versao: 3.0.0-LEAN-TIER2
    Data: 2026-02-07
    ===============================================================================

    INDICACOES CLINICAS (Criterios de Ressecabilidade - IRLM Criteria):
    - Tumor primario controlado ou controlavel
    - Ausencia de metastases extratoracicas ou ressecaveis
    - Metastases ressecaveis com margens adequadas
    - Funcao pulmonar pos-resseccao adequada (VEF1 ppo maior 40%)
    - Performance status ECOG 0-1
    - Intervalo livre de doenca favoravel (maior 12 meses para maioria)
    - Numero limitado de metastases (geralmente menor ou igual 3-5)

    HISTOLOGIAS COM MELHOR BENEFICIO:
    - Carcinoma colorretal (sobrevida 5 anos 40-60%)
    - Sarcomas de partes moles
    - Carcinoma renal
    - Tumores germinativos
    - Osteossarcoma

    CONTRAINDICACOES:
    - Tumor primario nao controlado
    - Metastases extratoracicas nao ressecaveis
    - Comprometimento linfonodal mediastinal (N1/N2)
    - Funcao pulmonar insuficiente para resseccao proposta
    - Intervalo livre de doenca muito curto (menor 6 meses)
    - Multiplas metastases bilaterais extensas

    REFERENCIAS TECNICAS:
    - IRLM (International Registry of Lung Metastases) Criteria
    - ESMO Guidelines Pulmonary Metastasectomy 2024
    - NCCN Site-Specific Guidelines (CRC, RCC, Sarcoma)
    - PulMiCC Trial Results 2020
    - SBCT 2023 - Metastasectomia
    ===============================================================================
  -->

  <decision id="Decision_30803114" name="Regra Clinica - Metastasectomia Pulmonar">
    <decisionTable id="DecisionTable_30803114" hitPolicy="FIRST">

      <!-- ========== INPUTS (5) - Padrao TIER-1 Cirurgia Toracica ========== -->

      <input id="Input_diagnosticoPulmonar" label="Histologia do Tumor Primario e Controle" camunda:inputVariable="diagnosticoPulmonar">
        <inputExpression id="InputExpression_diagnosticoPulmonar" typeRef="string">
          <text>diagnosticoPulmonar</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoPulmonar">
          <text>"COLORRETAL_CONTROLADO","SARCOMA_CONTROLADO","RENAL_CONTROLADO","GERMINATIVO_CONTROLADO","OSTEOSSARCOMA_CONTROLADO","PRIMARIO_NAO_CONTROLADO","HISTOLOGIA_DESFAVORAVEL"</text>
        </inputValues>
      </input>

      <input id="Input_estadiamentoTNM" label="Extensao da Doenca Metastatica" camunda:inputVariable="estadiamentoTNM">
        <inputExpression id="InputExpression_estadiamentoTNM" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="InputValues_estadiamentoTNM">
          <text>"OLIGOMETASTATICO_1_3","OLIGOMETASTATICO_4_5","MULTIPLAS_MAIOR_5","BILATERAL_RESSECAVEL","BILATERAL_IRRESSECAVEL","EXTRATORACICO_PRESENTE","N1_N2_MEDIASTINAL"</text>
        </inputValues>
      </input>

      <input id="Input_funcaoPulmonar" label="Funcao Pulmonar e Reserva" camunda:inputVariable="funcaoPulmonar">
        <inputExpression id="InputExpression_funcaoPulmonar" typeRef="string">
          <text>funcaoPulmonar</text>
        </inputExpression>
        <inputValues id="InputValues_funcaoPulmonar">
          <text>"ADEQUADA_MULTIPLAS_RESSECCOES","LIMITADA_RESSECCAO_UNICA","INSUFICIENTE","NAO_AVALIADA"</text>
        </inputValues>
      </input>

      <input id="Input_comorbidades" label="Intervalo Livre de Doenca e Performance" camunda:inputVariable="comorbidades">
        <inputExpression id="InputExpression_comorbidades" typeRef="string">
          <text>comorbidades</text>
        </inputExpression>
        <inputValues id="InputValues_comorbidades">
          <text>"ILD_MAIOR_24M_ECOG_0_1","ILD_12_24M_ECOG_0_1","ILD_6_12M","ILD_MENOR_6M","ECOG_2_OU_MAIOR","METASTASECTOMIA_PREVIA"</text>
        </inputValues>
      </input>

      <input id="Input_documentacaoClinica" label="Documentacao e Discussao Multidisciplinar" camunda:inputVariable="documentacaoClinica">
        <inputExpression id="InputExpression_documentacaoClinica" typeRef="string">
          <text>documentacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_documentacaoClinica">
          <text>"COMPLETA_TUMOR_BOARD","COMPLETA_SEM_TUMOR_BOARD","PARCIAL","INCOMPLETA"</text>
        </inputValues>
      </input>

      <!-- ========== OUTPUTS (3) - Padrao TIER-1 ========== -->

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- ========== REGRAS - Ordem: Reprovado, Aprovado, Pendente, Fallback ========== -->

      <!-- REPROVACOES -->

      <rule id="Rule_Reprovado_PrimarioNaoControlado">
        <description>Reprovado: Tumor primario nao controlado</description>
        <inputEntry id="InputEntry_Rep_Prim_1"><text>"PRIMARIO_NAO_CONTROLADO"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Prim_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Prim_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Prim_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Prim_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_Prim_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Prim_observacao"><text>"Metastasectomia contraindicada - tumor primario nao controlado. Controle do sitio primario e pre-requisito para resseccao de metastases."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Prim_fundamentacao"><text>"IRLM Criteria, ESMO 2024: Controle do tumor primario e criterio fundamental. Metastasectomia sem controle primario tem beneficio questionavel."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_PrimarioNaoControlado_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_PrimarioNaoControlado_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_PrimarioNaoControlado_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_PrimarioNaoControlado_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_Extratoracico">
        <description>Reprovado: Metastases extratoracicas presentes</description>
        <inputEntry id="InputEntry_Rep_Extra_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Extra_2"><text>"EXTRATORACICO_PRESENTE"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Extra_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Extra_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Extra_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_Extra_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Extra_observacao"><text>"Metastases extratoracicas presentes. Metastasectomia pulmonar nao indicada em doenca sistemica - tratamento oncologico sistemico."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Extra_fundamentacao"><text>"IRLM, ESMO: Ausencia de metastases extratoracicas e criterio essencial. Excecao: doenca oligo-recorrente em sitios multiplos ressecaveis."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Extratoracico_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Extratoracico_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Extratoracico_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Extratoracico_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_N1N2_Mediastinal">
        <description>Reprovado: Comprometimento linfonodal mediastinal</description>
        <inputEntry id="InputEntry_Rep_N12_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_N12_2"><text>"N1_N2_MEDIASTINAL"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_N12_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_N12_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_N12_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_N12_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_N12_observacao"><text>"Comprometimento linfonodal mediastinal confirmado (N1/N2). Metastasectomia contraindicada - indica doenca sistemica."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_N12_fundamentacao"><text>"IRLM, ESMO 2024: Linfonodos mediastinais positivos sao fator prognostico adverso e geralmente contraindicam resseccao curativa."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_N1N2_Mediastinal_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_N1N2_Mediastinal_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_N1N2_Mediastinal_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_N1N2_Mediastinal_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_FuncaoInsuficiente">
        <description>Reprovado: Funcao pulmonar insuficiente</description>
        <inputEntry id="InputEntry_Rep_Func_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Func_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Func_3"><text>"INSUFICIENTE"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Func_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Func_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_Func_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Func_observacao"><text>"Funcao pulmonar insuficiente para metastasectomia. VEF1 ppo deve permitir resseccao de todas as lesoes com margem funcional."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Func_fundamentacao"><text>"SBCT 2023: Metastasectomia pode requerer multiplas resseccoes. VEF1 ppo apos todas as resseccoes planejadas deve ser maior que 40%."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_FuncaoInsuficiente_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_FuncaoInsuficiente_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_FuncaoInsuficiente_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_FuncaoInsuficiente_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_ILD_Curto">
        <description>Reprovado: Intervalo livre de doenca muito curto</description>
        <inputEntry id="InputEntry_Rep_ILD_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_ILD_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_ILD_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_ILD_4"><text>"ILD_MENOR_6M"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_ILD_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_ILD_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_ILD_observacao"><text>"Intervalo livre de doenca menor que 6 meses. Biologia tumoral agressiva - metastasectomia com beneficio questionavel."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_ILD_fundamentacao"><text>"IRLM: ILD menor 12-36 meses e fator prognostico adverso. ILD muito curto (menor 6m) sugere doenca biologicamente agressiva."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_ILD_Curto_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_ILD_Curto_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_ILD_Curto_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_ILD_Curto_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Reprovado_Bilateral_Extenso">
        <description>Reprovado: Metastases bilaterais extensas irressecaveis</description>
        <inputEntry id="InputEntry_Rep_Bilat_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Bilat_2"><text>"BILATERAL_IRRESSECAVEL","MULTIPLAS_MAIOR_5"</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Bilat_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Bilat_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Rep_Bilat_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Rep_Bilat_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Bilat_observacao"><text>"Metastases pulmonares extensas bilaterais irressecaveis. Tratamento sistemico indicado."</text></outputEntry>
        <outputEntry id="OutputEntry_Rep_Bilat_fundamentacao"><text>"ESMO 2024: Numero elevado de metastases (maior 5-7) e fator prognostico adverso. Resseccao geralmente nao beneficia estes pacientes."</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Bilateral_Extenso_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Bilateral_Extenso_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Bilateral_Extenso_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Reprovado_Bilateral_Extenso_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- APROVACOES -->

      <rule id="Rule_Aprovado_Colorretal_Ideal">
        <description>Aprovado: Metastases colorretais - indicacao classica</description>
        <inputEntry id="InputEntry_Apr_CRC_1"><text>"COLORRETAL_CONTROLADO"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_CRC_2"><text>"OLIGOMETASTATICO_1_3"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_CRC_3"><text>"ADEQUADA_MULTIPLAS_RESSECCOES"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_CRC_4"><text>"ILD_MAIOR_24M_ECOG_0_1","ILD_12_24M_ECOG_0_1"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_CRC_5"><text>"COMPLETA_TUMOR_BOARD","COMPLETA_SEM_TUMOR_BOARD"</text></inputEntry>
        <outputEntry id="OutputEntry_Apr_CRC_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_CRC_observacao"><text>"Metastasectomia pulmonar aprovada para carcinoma colorretal oligometastatico. Sobrevida 5 anos de 40-60% em casos selecionados."</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_CRC_fundamentacao"><text>"IRLM, NCCN CRC 2024: Metastasectomia em CRC e bem estabelecida. Resseccao completa (R0) e principal fator prognostico."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Colorretal_Ideal_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Colorretal_Ideal_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Colorretal_Ideal_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Colorretal_Ideal_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Sarcoma_Renal">
        <description>Aprovado: Sarcoma ou carcinoma renal oligometastatico</description>
        <inputEntry id="InputEntry_Apr_Sarc_1"><text>"SARCOMA_CONTROLADO","RENAL_CONTROLADO"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Sarc_2"><text>"OLIGOMETASTATICO_1_3","OLIGOMETASTATICO_4_5"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Sarc_3"><text>"ADEQUADA_MULTIPLAS_RESSECCOES","LIMITADA_RESSECCAO_UNICA"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Sarc_4"><text>"ILD_MAIOR_24M_ECOG_0_1","ILD_12_24M_ECOG_0_1"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Sarc_5"><text>"COMPLETA_TUMOR_BOARD","COMPLETA_SEM_TUMOR_BOARD"</text></inputEntry>
        <outputEntry id="OutputEntry_Apr_Sarc_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_Sarc_observacao"><text>"Metastasectomia aprovada para sarcoma ou carcinoma renal. Histologias com bom beneficio de resseccao de metastases pulmonares."</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_Sarc_fundamentacao"><text>"ESMO Sarcoma, NCCN RCC: Sarcomas e RCC tem beneficio documentado de metastasectomia pulmonar em doenca oligometastatica."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Sarcoma_Renal_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Sarcoma_Renal_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Sarcoma_Renal_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Sarcoma_Renal_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Germinativo_Osteo">
        <description>Aprovado: Tumores germinativos ou osteossarcoma</description>
        <inputEntry id="InputEntry_Apr_Germ_1"><text>"GERMINATIVO_CONTROLADO","OSTEOSSARCOMA_CONTROLADO"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Germ_2"><text>"OLIGOMETASTATICO_1_3","OLIGOMETASTATICO_4_5","BILATERAL_RESSECAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Germ_3"><text>"ADEQUADA_MULTIPLAS_RESSECCOES"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Germ_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Germ_5"><text>"COMPLETA_TUMOR_BOARD","COMPLETA_SEM_TUMOR_BOARD"</text></inputEntry>
        <outputEntry id="OutputEntry_Apr_Germ_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_Germ_observacao"><text>"Metastasectomia aprovada para tumor germinativo ou osteossarcoma. Indicacao forte mesmo em doenca bilateral se ressecavel."</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_Germ_fundamentacao"><text>"NCCN Testicular, Bone: Metastasectomia e parte integral do tratamento. Em germinativos, essencial para estadiamento patologico pos-QT."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Germinativo_Osteo_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Germinativo_Osteo_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Germinativo_Osteo_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Germinativo_Osteo_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_Reoperacao">
        <description>Aprovado: Re-metastasectomia em paciente selecionado</description>
        <inputEntry id="InputEntry_Apr_Reop_1"><text>"COLORRETAL_CONTROLADO","SARCOMA_CONTROLADO"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Reop_2"><text>"OLIGOMETASTATICO_1_3"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Reop_3"><text>"ADEQUADA_MULTIPLAS_RESSECCOES","LIMITADA_RESSECCAO_UNICA"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Reop_4"><text>"METASTASECTOMIA_PREVIA"</text></inputEntry>
        <inputEntry id="InputEntry_Apr_Reop_5"><text>"COMPLETA_TUMOR_BOARD"</text></inputEntry>
        <outputEntry id="OutputEntry_Apr_Reop_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_Reop_observacao"><text>"Re-metastasectomia pulmonar aprovada. Pacientes com recorrencia tardia apos primeira metastasectomia podem se beneficiar de nova resseccao."</text></outputEntry>
        <outputEntry id="OutputEntry_Apr_Reop_fundamentacao"><text>"IRLM: Re-metastasectomia tem sobrevida comparavel a primeira resseccao em pacientes selecionados. ILD longo e criterio favoravel."</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Reoperacao_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Reoperacao_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Reoperacao_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprovado_Reoperacao_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- PENDENCIAS -->

      <rule id="Rule_Pendente_ILD_Intermediario">
        <description>Pendente: ILD intermediario - avaliar biologia tumoral</description>
        <inputEntry id="InputEntry_Pend_ILD_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_ILD_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_ILD_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_ILD_4"><text>"ILD_6_12M"</text></inputEntry>
        <inputEntry id="InputEntry_Pend_ILD_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Pend_ILD_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_ILD_observacao"><text>"ILD entre 6-12 meses - zona intermediaria. Considerar observacao com imagem ou QT neoadjuvante para avaliar biologia tumoral antes de cirurgia."</text></outputEntry>
        <outputEntry id="OutputEntry_Pend_ILD_fundamentacao"><text>"ESMO: Periodo de observacao de 3 meses pode ajudar a identificar pacientes com doenca rapidamente progressiva que nao se beneficiam de cirurgia."</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_ILD_Intermediario_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_ILD_Intermediario_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_ILD_Intermediario_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Pendente_ILD_Intermediario_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_Pendente_SemTumorBoard">
        <description>Pendente: Caso complexo sem discussao em tumor board</description>
        <inputEntry id="InputEntry_Pend_TB_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_Pend_TB_2"><text>"OLIGOMETASTATICO_4_5","BILATERAL_RESSECAVEL"</text></inputEntry>
