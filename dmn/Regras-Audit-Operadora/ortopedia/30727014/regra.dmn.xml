<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_30727014"
             name="ORT-CONS-001: Fisioterapia Antes Artroscopia"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    TUSS 30727014 - Fisioterapia Antes Artroscopia
    Versao: 3.0.0 (2026-02-07)
    Especialidade: Ortopedia

    Indicacoes:
    - Artroscopia joelho requer minimo 6 semanas fisioterapia documentada
    - Falha de melhora com tratamento conservador

    Excecoes:
    - Joelho bloqueado por fragmento meniscal
    - Lesao meniscal aguda com sintomas mecanicos

    Contraindicacoes:
    - Artroscopia sem tentativa fisioterapia (alto indice procedimentos desnecessarios)

    Waste Prevention:
    - R$8-15K economia por artroscopia evitada

    Referencias:
    - SBO Guidelines on Knee Arthroscopy
    - AAOS Clinical Practice Guidelines
  -->
  <decision id="Decision_30727014" name="ORT-CONS-001: Fisioterapia Antes Artroscopia">
    <decisionTable id="DecisionTable_30727014" hitPolicy="FIRST">
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado (CID-10)">
        <inputExpression id="InputExpression_diagnostico" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
      </input>
      <input id="Input_tratamentosPrevios" label="Tratamentos Previos">
        <inputExpression id="InputExpression_tratamentos" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
      </input>
      <input id="Input_classificacaoLesao" label="Classificacao Lesao">
        <inputExpression id="InputExpression_classificacao" typeRef="string">
          <text>classificacaoLesao</text>
        </inputExpression>
      </input>
      <input id="Input_riscoCirurgico" label="Risco Cirurgico (ASA)">
        <inputExpression id="InputExpression_risco" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
      </input>
      <input id="Input_laudoIndicacao" label="Laudo Indicacao Anexado">
        <inputExpression id="InputExpression_laudo" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_decisao">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao Tecnica" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_30727014_Contraindicacao_1">
        <description>Contraindicacao - Risco cirurgico proibitivo ASA V</description>
        <inputEntry id="IE_Contra1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>"ASA_V"</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contra1_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contra1_observacao"><text>"Paciente ASA V - risco cirurgico proibitivo para artroscopia eletiva"</text></outputEntry>
        <outputEntry id="OutputEntry_Contra1_fundamentacao"><text>"ASA Classification Guidelines"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Contraindicacao_1_alertasDesperdicio"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Contraindicacao_1_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_30727014_Contraindicacao_2">
        <description>Contraindicacao - Fisioterapia nao tentada</description>
        <inputEntry id="IE_Contra2_1"><text>"M17.x"</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>"Nao_Documentado","Primeira_Linha"</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>"Grau_I","Grau_II"</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Contra2_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Contra2_observacao"><text>"Artroscopia requer minimo 6 semanas fisioterapia documentada exceto em lesoes mecanicas agudas"</text></outputEntry>
        <outputEntry id="OutputEntry_Contra2_fundamentacao"><text>"SBO Guidelines, AAOS Knee Arthroscopy Criteria"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Contraindicacao_2_alertasDesperdicio"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Contraindicacao_2_acaoRecomendada"><text>"NEGAR"</text></outputEntry>
      </rule>
      <rule id="Rule_30727014_Aprovacao_1">
        <description>Aprovacao - Joelho bloqueado por fragmento meniscal</description>
        <inputEntry id="IE_Aprov1_1"><text>"M17.x"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>"Nao_Aplicavel"</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>"Grau_III","Grau_IV"</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>"ASA_I","ASA_II","ASA_III"</text></inputEntry>
        <inputEntry id="IE_Aprov1_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprov1_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprov1_observacao"><text>"Joelho bloqueado por fragmento meniscal: indicacao cirurgica direta sem necessidade fisioterapia previa"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprov1_fundamentacao"><text>"SBO Guidelines, AAOS Acute Meniscal Injury Criteria"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Aprovacao_1_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Aprovacao_1_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_30727014_Aprovacao_2">
        <description>Aprovacao - Lesao meniscal aguda com sintomas mecanicos</description>
        <inputEntry id="IE_Aprov2_1"><text>"M17.x"</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>"Nao_Aplicavel"</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>"Grau_III"</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>"ASA_I","ASA_II","ASA_III"</text></inputEntry>
        <inputEntry id="IE_Aprov2_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprov2_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprov2_observacao"><text>"Lesao meniscal aguda com sintomas mecanicos documentados: artroscopia indicada precocemente"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprov2_fundamentacao"><text>"SBO Guidelines, AAOS Meniscal Injury Criteria"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Aprovacao_2_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Aprovacao_2_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_30727014_Aprovacao_3">
        <description>Aprovacao - Fisioterapia ≥6 semanas falhou</description>
        <inputEntry id="IE_Aprov3_1"><text>"M17.x"</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>"Tratamento_Clinico_Falhou","Documentado"</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>"Grau_II","Grau_III"</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>"ASA_I","ASA_II","ASA_III"</text></inputEntry>
        <inputEntry id="IE_Aprov3_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_Aprov3_resultado"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprov3_observacao"><text>"Falha documentada de fisioterapia ≥6 semanas com sintomas persistentes: artroscopia indicada"</text></outputEntry>
        <outputEntry id="OutputEntry_Aprov3_fundamentacao"><text>"SBO Guidelines, AAOS Knee Arthroscopy Criteria"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Aprovacao_3_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Aprovacao_3_acaoRecomendada"><text>"APROVAR"</text></outputEntry>
      </rule>
      <rule id="Rule_30727014_Fallback">
        <description>Fallback - Casos nao cobertos requerem analise manual</description>
        <inputEntry id="IE_Fall_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fall_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fall_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fall_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fall_5"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_Fall_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_Fall_observacao"><text>"Caso requer analise manual: criterios de fisioterapia previa nao claramente atendidos"</text></outputEntry>
        <outputEntry id="OutputEntry_Fall_fundamentacao"><text>"SBO Guidelines"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Fallback_alertasDesperdicio"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_30727014_Fallback_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_30727014">
      <dmndi:DMNShape id="DMNShape_30727014" dmnElementRef="Decision_30727014">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
