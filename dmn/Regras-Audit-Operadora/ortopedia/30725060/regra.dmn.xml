<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_TRAUMA_003" name="ORT_TRAUMA_003" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_TRAUMA_003" name="Fratura patológica - avaliação">
    <decisionTable id="DecisionTable_ORT_TRAUMA_003" hitPolicy="FIRST">
      <input id="Input_lesaoLitica" label="Lesão Lítica &amp;gt;50% Cortical">
        <inputExpression id="InputExpression_lesaoLitica" typeRef="boolean">
          <text>lesaoLitica</text>
        </inputExpression>
      </input>
      <input id="Input_dor Prévia" label="Dor Prévia à Fratura">
        <inputExpression id="InputExpression_dor Prévia" typeRef="boolean">
          <text>dor Prévia</text>
        </inputExpression>
      </input>
      <input id="Input_biopsia" label="Biópsia Realizada">
        <inputExpression id="InputExpression_biopsia" typeRef="boolean">
          <text>biopsia</text>
        </inputExpression>
      </input>
      <input id="Input_oncologistaConsultado" label="Oncologista Consultado">
        <inputExpression id="InputExpression_oncologistaConsultado" typeRef="boolean">
          <text>oncologistaConsultado</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_lesaoLitica_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Prévia_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_biopsia_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_oncologistaConsultado_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Fratura patológica requer biópsia antes de fixação definitiva"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AO: diagnóstico histológico é mandatório antes de tratamento"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_2">
        <inputEntry id="InputEntry_lesaoLitica_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Prévia_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_biopsia_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_oncologistaConsultado_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Fratura patológica requer abordagem multidisciplinar com oncologia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"ASCO: tratamento de metástases ósseas deve envolver oncologista"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_lesaoLitica_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Prévia_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_biopsia_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_oncologistaConsultado_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Fratura patológica com workup oncológico completo - prosseguir com tratamento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AO: fixação profilática ou curativa baseada em diagnóstico e prognóstico"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_lesaoLitica_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Prévia_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_biopsia_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_oncologistaConsultado_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AO Trauma e ASCO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_TRAUMA_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
