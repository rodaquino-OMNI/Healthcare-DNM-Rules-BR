<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_SPI_007" name="ORT_SPI_007" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_SPI_007" name="Fusão cervical posterior">
    <decisionTable id="DecisionTable_ORT_SPI_007" hitPolicy="FIRST">
      <input id="Input_traumaAgudo" label="Trauma Agudo Cervical">
        <inputExpression id="InputExpression_traumaAgudo" typeRef="boolean">
          <text>traumaAgudo</text>
        </inputExpression>
      </input>
      <input id="Input_instabilidadeSevera" label="Instabilidade Severa">
        <inputExpression id="InputExpression_instabilidadeSevera" typeRef="boolean">
          <text>instabilidadeSevera</text>
        </inputExpression>
      </input>
      <input id="Input_cifoseTraumatica" label="Cifose Traumática">
        <inputExpression id="InputExpression_cifoseTraumatica" typeRef="boolean">
          <text>cifoseTraumatica</text>
        </inputExpression>
      </input>
      <input id="Input_facetectomiaPrevia" label="Facetectomia Prévia">
        <inputExpression id="InputExpression_facetectomiaPrevia" typeRef="boolean">
          <text>facetectomiaPrevia</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Aprovado_1">
        <inputEntry id="InputEntry_traumaAgudo_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeSevera_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_cifoseTraumatica_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_facetectomiaPrevia_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Trauma cervical com instabilidade - fusão posterior indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AO Spine: instabilidade traumática requer fusão posterior"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_traumaAgudo_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeSevera_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_cifoseTraumatica_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_facetectomiaPrevia_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Cifose cervical pós-traumática - fusão posterior indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AO Spine: cifose requer abordagem posterior para correção"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_traumaAgudo_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeSevera_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_cifoseTraumatica_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_facetectomiaPrevia_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Instabilidade pós-facetectomia - fusão posterior indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AO Spine: facetectomia ampla desestabiliza coluna"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_4">
        <inputEntry id="InputEntry_traumaAgudo_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeSevera_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_cifoseTraumatica_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_facetectomiaPrevia_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_4">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_4">
          <text>"Patologia degenerativa sem instabilidade - ACDF anterior preferível"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_4">
          <text>"NASS: via anterior é primeira escolha em degeneração sem instabilidade"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_traumaAgudo_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeSevera_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_cifoseTraumatica_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_facetectomiaPrevia_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AO Spine"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_SPI_007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
