<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_013" name="ORT_DEV_013" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_013" name="Âncoras manguito rotador - tipos">
    <decisionTable id="DecisionTable_ORT_DEV_013" hitPolicy="FIRST">
      <input id="Input_qualidadeOssea" label="Qualidade Óssea (boa/ruim)">
        <inputExpression id="InputExpression_qualidadeOssea" typeRef="string">
          <text>qualidadeOssea</text>
        </inputExpression>
      </input>
      <input id="Input_tamanhoRuptura" label="Tamanho Ruptura (cm)">
        <inputExpression id="InputExpression_tamanhoRuptura" typeRef="double">
          <text>tamanhoRuptura</text>
        </inputExpression>
      </input>
      <input id="Input_idade" label="Idade">
        <inputExpression id="InputExpression_idade" typeRef="integer">
          <text>idade</text>
        </inputExpression>
      </input>
      <input id="Input_revisao" label="Cirurgia de Revisão">
        <inputExpression id="InputExpression_revisao" typeRef="boolean">
          <text>revisao</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Pendente_1">
        <inputEntry id="InputEntry_qualidadeOssea_1">
          <text>"ruim"</text>
        </inputEntry>
        <inputEntry id="InputEntry_tamanhoRuptura_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_1">
          <text>&gt; 70</text>
        </inputEntry>
        <inputEntry id="InputEntry_revisao_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Osteoporose severa - avaliar viabilidade do reparo e tipo de âncora"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: osteoporose severa pode requerer âncoras de maior diâmetro ou técnica alternativa"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_qualidadeOssea_2">
          <text>"boa"</text>
        </inputEntry>
        <inputEntry id="InputEntry_tamanhoRuptura_2">
          <text>&lt; 2</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_2">
          <text>&lt; 65</text>
        </inputEntry>
        <inputEntry id="InputEntry_revisao_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Ruptura pequena com osso de qualidade - âncoras simples adequadas"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: rupturas pequenas requerem 1-2 âncoras standard"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_qualidadeOssea_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_tamanhoRuptura_3">
          <text>&gt; 3</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_revisao_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Ruptura extensa ou revisão - âncoras múltiplas e/ou suture bridge"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AAOS: rupturas grandes requerem técnicas de fileira dupla com múltiplas âncoras"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_qualidadeOssea_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_tamanhoRuptura_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_revisao_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_013">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
