<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_021" name="ORT_DEV_021" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_021" name="Placas bloqueadas vs convencionais">
    <decisionTable id="DecisionTable_ORT_DEV_021" hitPolicy="FIRST">
      <input id="Input_qualidadeOssea" label="Qualidade Óssea">
        <inputExpression id="InputExpression_qualidadeOssea" typeRef="string">
          <text>qualidadeOssea</text>
        </inputExpression>
      </input>
      <input id="Input_localizacao" label="Localização (metáfise/diáfise)">
        <inputExpression id="InputExpression_localizacao" typeRef="string">
          <text>localizacao</text>
        </inputExpression>
      </input>
      <input id="Input_cominutiva" label="Fratura Cominutiva">
        <inputExpression id="InputExpression_cominutiva" typeRef="boolean">
          <text>cominutiva</text>
        </inputExpression>
      </input>
      <input id="Input_periarticular" label="Fratura Periarticular">
        <inputExpression id="InputExpression_periarticular" typeRef="boolean">
          <text>periarticular</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Aprovado_1">
        <inputEntry id="InputEntry_qualidadeOssea_1">
          <text>"ruim"</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacao_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_periarticular_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Osteoporose - placas bloqueadas têm fixação superior"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AO: placas bloqueadas essenciais em osso osteoporótico"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_qualidadeOssea_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacao_2">
          <text>"metáfise"</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_periarticular_2">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Fratura periarticular cominutiva - placa bloqueada indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AO: fraturas periarticulares requerem fixação angular de placas bloqueadas"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_qualidadeOssea_3">
          <text>"boa"</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacao_3">
          <text>"diáfise"</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_periarticular_3">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Fratura diafisária simples em osso de qualidade - ambas as placas aceitáveis"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AO: em osso normal, placas convencionais e bloqueadas têm resultados equivalentes"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_qualidadeOssea_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacao_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_periarticular_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AO Trauma"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_021">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
