<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30715409" name="ORT-CONS-009 Infiltracao Articular Pre-Cirurgica" namespace="http://camunda.org/schema/1.0/dmn">
  <!-- TUSS 30715409 - ORT-CONS-009 | Versao 3.0.0 | AAOS 2023, SBO 2022 -->
  <decision id="Decision_30715409" name="Criterios Infiltracao Articular">
    <decisionTable id="DecisionTable_30715409" hitPolicy="FIRST">
      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string"><text>diagnosticoCid10</text></inputExpression>
        <inputValues id="InputValues_diagnostico"><text>"M17.0","M17.1","M16.0","M16.1","M19.0"</text></inputValues>
      </input>
      <input id="Input_tipoInfiltracao" label="Tipo de Infiltracao">
        <inputExpression id="InputExpr_tipoInfiltracao" typeRef="string"><text>tipoInfiltracao</text></inputExpression>
        <inputValues id="InputValues_tipoInfiltracao"><text>"Corticoide","Viscossuplementacao","PRP","Celulas_Tronco"</text></inputValues>
      </input>
      <input id="Input_infiltracoesAno" label="Infiltracoes no Ano">
        <inputExpression id="InputExpr_infiltracoesAno" typeRef="number"><text>infiltracoesNoAno</text></inputExpression>
      </input>
      <input id="Input_indicacaoCirurgica" label="Indicacao Cirurgica">
        <inputExpression id="InputExpr_indicacaoCirurgica" typeRef="boolean"><text>indicacaoCirurgicaConfirmada</text></inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_30715409_Reprovado_ExcedeMaximo">
        <description>Reprovado - Excede maximo</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>"Corticoide"</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Limite de 3 infiltracoes de corticoide por ano atingido"</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"AAOS 2023 - Maximum Injections"</text></outputEntry>
      </rule>
      <rule id="Rule_30715409_Reprovado_CelulasTronco">
        <description>Reprovado - Nao coberto</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>"Celulas_Tronco"</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Celulas-tronco nao consta no Rol ANS"</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"RN 465/2021 - Nao Listado"</text></outputEntry>
      </rule>
      <rule id="Rule_30715409_Aprovado_Corticoide">
        <description>Aprovado - Corticoide</description>
        <inputEntry id="IE_A1_1"><text>"M17.0","M17.1","M16.0","M16.1","M19.0"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>"Corticoide"</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>&lt; 3</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Infiltracao de corticoide aprovada. Dentro do limite anual."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"AAOS 2023 - Corticosteroid Injection"</text></outputEntry>
      </rule>
      <rule id="Rule_30715409_Aprovado_Viscossuplementacao">
        <description>Aprovado - Visco</description>
        <inputEntry id="IE_A2_1"><text>"M17.0","M17.1","M16.0","M16.1"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>"Viscossuplementacao"</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>false</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Viscossuplementacao aprovada para osteoartrose"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"AAOS 2023 - Hyaluronic Acid"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_30715409">
        <description>Fallback</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"Requer analise manual por auditor medico."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Auditoria Medica - RN 428/2017"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
