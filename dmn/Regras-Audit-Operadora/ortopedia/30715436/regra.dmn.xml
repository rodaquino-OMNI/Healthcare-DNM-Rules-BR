<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30715436" name="ORT-SPI-006 Cirurgia de Coluna" namespace="http://camunda.org/schema/1.0/dmn">
  <!-- TUSS 30715436 - ORT-SPI-006 | Versao 3.0.0 | NASS 2023, SBC Coluna 2022 -->
  <decision id="Decision_30715436" name="Criterios Cirurgia Coluna 006">
    <decisionTable id="DecisionTable_30715436" hitPolicy="FIRST">
      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string"><text>diagnosticoCid10</text></inputExpression>
        <inputValues id="InputValues_diagnostico"><text>"M51.1","M51.2","M48.0","M47.8","G55.1","M43.1","S32.0","M40.1"</text></inputValues>
      </input>
      <input id="Input_tipoCirurgia" label="Tipo de Cirurgia">
        <inputExpression id="InputExpr_tipoCirurgia" typeRef="string"><text>tipoCirurgiaColuna</text></inputExpression>
        <inputValues id="InputValues_tipoCirurgia"><text>"Discectomia","Laminectomia","Artrodese","Descompressao","Substituicao_Disco","Vertebroplastia","Cifoplastia"</text></inputValues>
      </input>
      <input id="Input_deficitNeurologico" label="Status Neurologico">
        <inputExpression id="InputExpr_deficitNeurologico" typeRef="string"><text>statusNeurologico</text></inputExpression>
        <inputValues id="InputValues_deficitNeurologico"><text>"Sem_Deficit","Deficit_Sensitivo","Deficit_Motor","Deficit_Progressivo","Cauda_Equina"</text></inputValues>
      </input>
      <input id="Input_tratamentoConservador" label="Meses Tratamento Conservador">
        <inputExpression id="InputExpr_tratamentoConservador" typeRef="number"><text>mesesTratamentoConservador</text></inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      
      <rule id="Rule_30715436_Reprovado_SemTratamentoConservador">
        <description>Reprovado - Sem tratamento conservador previo</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>"Discectomia","Laminectomia","Artrodese"</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>"Sem_Deficit","Deficit_Sensitivo"</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>&lt; 2</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Cirurgia eletiva de coluna requer minimo 6-12 semanas de tratamento conservador previo documentado"</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"NASS 2023 - Conservative Trial Required"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715436_Aprovado_CaudaEquina">
        <description>Aprovado - Emergencia cauda equina</description>
        <inputEntry id="IE_A1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"Cauda_Equina"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"EMERGENCIA - Sindrome da cauda equina. Cirurgia urgente em menos de 48h."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"NASS 2023 - Cauda Equina Emergency"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715436_Aprovado_DeficitProgressivo">
        <description>Aprovado - Deficit motor progressivo</description>
        <inputEntry id="IE_A2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Deficit_Progressivo","Deficit_Motor"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Deficit neurologico progressivo ou motor - indicacao cirurgica sem necessidade de tratamento conservador prolongado"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"NASS 2023 - Progressive Deficit"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715436_Aprovado_FalhaTratamento">
        <description>Aprovado - Falha tratamento conservador</description>
        <inputEntry id="IE_A3_1"><text>"M51.1","M51.2","M48.0"</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>"Discectomia","Laminectomia","Descompressao"</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"Sem_Deficit","Deficit_Sensitivo"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>&gt;= 3</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Cirurgia aprovada apos 3+ meses de tratamento conservador sem melhora significativa"</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"NASS 2023 - Surgery After Conservative Failure"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715436_Pendente_Artrodese">
        <description>Pendente - Artrodese requer junta medica</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>"Artrodese"</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"Sem_Deficit","Deficit_Sensitivo"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Artrodese de coluna requer junta medica para avaliacao de indicacao e numero de niveis"</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"SBC Coluna 2022 - Fusion Evaluation"</text></outputEntry>
      </rule>
      
      <rule id="Rule_Fallback_30715436">
        <description>Fallback</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"Cirurgia de coluna requer analise por neurocirurgiao ou ortopedista de coluna. Verificar indicacao, tratamento previo, deficit neurologico."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Junta Medica Coluna - RN 428/2017"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
