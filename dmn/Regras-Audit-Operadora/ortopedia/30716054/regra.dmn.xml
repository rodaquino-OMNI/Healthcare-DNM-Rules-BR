<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_016" name="ORT_DEV_016" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_016" name="Scaffold meniscal">
    <decisionTable id="DecisionTable_ORT_DEV_016" hitPolicy="FIRST">
      <input id="Input_meniscectomiaPrevia" label="Meniscectomia Subtotal Prévia">
        <inputExpression id="InputExpression_meniscectomiaPrevia" typeRef="boolean">
          <text>meniscectomiaPrevia</text>
        </inputExpression>
      </input>
      <input id="Input_idade" label="Idade">
        <inputExpression id="InputExpression_idade" typeRef="integer">
          <text>idade</text>
        </inputExpression>
      </input>
      <input id="Input_estabilidadeJoelho" label="Joelho Estável">
        <inputExpression id="InputExpression_estabilidadeJoelho" typeRef="boolean">
          <text>estabilidadeJoelho</text>
        </inputExpression>
      </input>
      <input id="Input_alinhamentoNormal" label="Alinhamento Normal">
        <inputExpression id="InputExpression_alinhamentoNormal" typeRef="boolean">
          <text>alinhamentoNormal</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_meniscectomiaPrevia_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_estabilidadeJoelho_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_alinhamentoNormal_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Scaffold meniscal indicado apenas após meniscectomia subtotal prévia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"ESSKA: implante de scaffold é para deficiência meniscal pós-meniscectomia"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_2">
        <inputEntry id="InputEntry_meniscectomiaPrevia_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_estabilidadeJoelho_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_alinhamentoNormal_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Instabilidade ou malalinhamento devem ser corrigidos antes de scaffold"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"ESSKA: scaffold requer joelho estável e alinhado para sobrevivência do implante"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_meniscectomiaPrevia_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_3">
          <text>&lt; 50</text>
        </inputEntry>
        <inputEntry id="InputEntry_estabilidadeJoelho_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_alinhamentoNormal_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Deficiência meniscal sintomática em joelho estável - scaffold indicado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"ESSKA: scaffold meniscal em joelho estável e alinhado pode prevenir artrose"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_meniscectomiaPrevia_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_estabilidadeJoelho_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_alinhamentoNormal_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes ESSKA"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_016">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
