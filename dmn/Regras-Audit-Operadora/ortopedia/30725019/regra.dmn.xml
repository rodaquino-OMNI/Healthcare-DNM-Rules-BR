<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_019" name="ORT_DEV_019" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_019" name="Fixador externo - indicações">
    <decisionTable id="DecisionTable_ORT_DEV_019" hitPolicy="FIRST">
      <input id="Input_fraturaExposta" label="Fratura Exposta Gustilo">
        <inputExpression id="InputExpression_fraturaExposta" typeRef="integer">
          <text>fraturaExposta</text>
        </inputExpression>
      </input>
      <input id="Input_lesaoPartesMoles" label="Lesão Severa Partes Moles">
        <inputExpression id="InputExpression_lesaoPartesMoles" typeRef="boolean">
          <text>lesaoPartesMoles</text>
        </inputExpression>
      </input>
      <input id="Input_instabilidadeHemodinamica" label="Instabilidade Hemodinâmica">
        <inputExpression id="InputExpression_instabilidadeHemodinamica" typeRef="boolean">
          <text>instabilidadeHemodinamica</text>
        </inputExpression>
      </input>
      <input id="Input_infeccao" label="Infecção Local">
        <inputExpression id="InputExpression_infeccao" typeRef="boolean">
          <text>infeccao</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Aprovado_1">
        <inputEntry id="InputEntry_fraturaExposta_1">
          <text>&gt;= 3</text>
        </inputEntry>
        <inputEntry id="InputEntry_lesaoPartesMoles_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeHemodinamica_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Fratura exposta Gustilo III - fixador externo como tratamento definitivo ou temporário"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AO: fraturas expostas severas requerem fixação externa para controle partes moles"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_fraturaExposta_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_lesaoPartesMoles_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeHemodinamica_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Politrauma com instabilidade - fixação externa temporária (damage control)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AO: fixadores externos permitem estabilização rápida em politraumatizados"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_fraturaExposta_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_lesaoPartesMoles_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeHemodinamica_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Infecção óssea ativa - fixador externo evita implante interno"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AO: infecção é indicação para fixação externa"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_4">
        <inputEntry id="InputEntry_fraturaExposta_4">
          <text>0</text>
        </inputEntry>
        <inputEntry id="InputEntry_lesaoPartesMoles_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeHemodinamica_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_4">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_4">
          <text>"Fratura fechada simples - fixação interna preferível a fixador externo"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_4">
          <text>"AO: fraturas fechadas sem complicação têm melhor resultado com fixação interna"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_fraturaExposta_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_lesaoPartesMoles_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidadeHemodinamica_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AO Trauma"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_019">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
