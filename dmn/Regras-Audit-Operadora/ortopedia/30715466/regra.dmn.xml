<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_SPI_024" name="ORT_SPI_024" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_SPI_024" name="Revisão coluna vertebral">
    <decisionTable id="DecisionTable_ORT_SPI_024" hitPolicy="FIRST">
      <input id="Input_pseudoartrose" label="Pseudoartrose Confirmada">
        <inputExpression id="InputExpression_pseudoartrose" typeRef="boolean">
          <text>pseudoartrose</text>
        </inputExpression>
      </input>
      <input id="Input_infeccao" label="Infecção Implante">
        <inputExpression id="InputExpression_infeccao" typeRef="boolean">
          <text>infeccao</text>
        </inputExpression>
      </input>
      <input id="Input_sindromeJuncional" label="Síndrome Juncional">
        <inputExpression id="InputExpression_sindromeJuncional" typeRef="boolean">
          <text>sindromeJuncional</text>
        </inputExpression>
      </input>
      <input id="Input_falhaImplante" label="Falha Mecânica Implante">
        <inputExpression id="InputExpression_falhaImplante" typeRef="boolean">
          <text>falhaImplante</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Aprovado_1">
        <inputEntry id="InputEntry_pseudoartrose_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_sindromeJuncional_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_falhaImplante_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Infecção de implante espinal - revisão com limpeza e troca indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"NASS: infecção requer desbridamento e pode necessitar troca de implante"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_pseudoartrose_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_sindromeJuncional_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_falhaImplante_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Pseudoartrose sintomática - revisão com nova fusão indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"NASS: pseudoartrose sintomática requer revisão"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_pseudoartrose_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_sindromeJuncional_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_falhaImplante_3">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Síndrome juncional sintomática - extensão da fusão indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"NASS: doença juncional adjacente sintomática pode requerer extensão"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_4">
        <inputEntry id="InputEntry_pseudoartrose_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_sindromeJuncional_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_falhaImplante_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_4">
          <text>"Falha mecânica de implante - revisão e troca indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_4">
          <text>"NASS: quebra de parafuso ou barra requer revisão"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_pseudoartrose_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_sindromeJuncional_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_falhaImplante_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes NASS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_SPI_024">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
