<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30715410" name="ORT-CONS-010 Terapia por Ondas de Choque" namespace="http://camunda.org/schema/1.0/dmn">
  <!-- TUSS 30715410 - ORT-CONS-010 | Versao 3.0.0 | AAOS 2023, SBO 2022 -->
  <decision id="Decision_30715410" name="Criterios TOC">
    <decisionTable id="DecisionTable_30715410" hitPolicy="FIRST">
      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string"><text>diagnosticoCid10</text></inputExpression>
        <inputValues id="InputValues_diagnostico"><text>"M72.2","M77.1","M75.3","M76.6"</text></inputValues>
      </input>
      <input id="Input_mesesTratamento" label="Meses Tratamento Convencional">
        <inputExpression id="InputExpr_mesesTratamento" typeRef="number"><text>mesesTratamentoConvencional</text></inputExpression>
      </input>
      <input id="Input_falhaConservador" label="Falha Documentada">
        <inputExpression id="InputExpr_falhaConservador" typeRef="boolean"><text>falhaTratamentoDocumentada</text></inputExpression>
      </input>
      <input id="Input_sessoesToc" label="Sessoes TOC Previas">
        <inputExpression id="InputExpr_sessoesToc" typeRef="number"><text>sessoesTocPrevias</text></inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_30715410_Reprovado_SemFalha">
        <description>Reprovado - Sem falha documentada</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"TOC indicada apenas apos falha documentada de tratamento conservador"</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"AAOS 2023 - ESWT After Failure"</text></outputEntry>
      </rule>
      <rule id="Rule_30715410_Reprovado_ExcedeSessoes">
        <description>Reprovado - Excede sessoes</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>&gt; 5</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Maximo 5 sessoes TOC atingido"</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"SBO 2022 - Maximum TOC"</text></outputEntry>
      </rule>
      <rule id="Rule_30715410_Aprovado_Fasciite">
        <description>Aprovado - Fasciite plantar</description>
        <inputEntry id="IE_A1_1"><text>"M72.2"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>true</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>&lt;= 5</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"TOC aprovada para fasciite plantar refrataria"</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"AAOS 2023 - ESWT Plantar Fasciitis"</text></outputEntry>
      </rule>
      <rule id="Rule_30715410_Aprovado_Epicondilite">
        <description>Aprovado - Epicondilite</description>
        <inputEntry id="IE_A2_1"><text>"M77.1"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>&lt;= 5</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"TOC aprovada para epicondilite refrataria"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"AAOS 2023 - ESWT Epicondylitis"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_30715410">
        <description>Fallback</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"Requer analise manual por auditor medico."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Auditoria Medica - RN 428/2017"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
