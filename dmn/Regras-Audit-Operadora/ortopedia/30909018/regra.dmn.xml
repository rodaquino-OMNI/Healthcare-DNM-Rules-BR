<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_CONS_010" name="ORT_CONS_010" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_CONS_010" name="De Quervain conservador">
    <decisionTable id="DecisionTable_ORT_CONS_010" hitPolicy="FIRST">
      <input id="Input_testeFinkelstein" label="Teste Finkelstein Positivo">
        <inputExpression id="InputExpression_testeFinkelstein" typeRef="boolean">
          <text>testeFinkelstein</text>
        </inputExpression>
      </input>
      <input id="Input_imobilizacaoFeita" label="Imobilização com Tala">
        <inputExpression id="InputExpression_imobilizacaoFeita" typeRef="boolean">
          <text>imobilizacaoFeita</text>
        </inputExpression>
      </input>
      <input id="Input_infiltracaoFeita" label="Infiltração Realizada">
        <inputExpression id="InputExpression_infiltracaoFeita" typeRef="boolean">
          <text>infiltracaoFeita</text>
        </inputExpression>
      </input>
      <input id="Input_duracaoSintomas" label="Duração (meses)">
        <inputExpression id="InputExpression_duracaoSintomas" typeRef="integer">
          <text>duracaoSintomas</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_testeFinkelstein_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_imobilizacaoFeita_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_infiltracaoFeita_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_duracaoSintomas_1">
          <text>&lt; 3</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Tenossinovite de De Quervain requer imobilização e AINE antes de infiltração"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: tratamento conservador inicial inclui imobilização por 4-6 semanas"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_testeFinkelstein_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_imobilizacaoFeita_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_infiltracaoFeita_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_duracaoSintomas_2">
          <text>&gt;= 6</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Falha tratamento conservador completo - indicação cirúrgica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: cirurgia indicada após falha de imobilização e infiltração"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_3">
        <inputEntry id="InputEntry_testeFinkelstein_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_imobilizacaoFeita_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_infiltracaoFeita_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_duracaoSintomas_3">
          <text>[3..6)</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Realizou imobilização mas não tentou infiltração - completar tratamento conservador"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"Infiltração deve ser tentada antes de indicação cirúrgica"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_testeFinkelstein_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_imobilizacaoFeita_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infiltracaoFeita_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_duracaoSintomas_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_CONS_010">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
