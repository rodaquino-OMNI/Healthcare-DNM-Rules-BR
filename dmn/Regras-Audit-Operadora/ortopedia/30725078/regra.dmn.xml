<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_TRAUMA_004" name="ORT_TRAUMA_004" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_TRAUMA_004" name="Pseudoartrose - timing cirurgia">
    <decisionTable id="DecisionTable_ORT_TRAUMA_004" hitPolicy="FIRST">
      <input id="Input_mesesAposFratura" label="Meses Desde Fratura">
        <inputExpression id="InputExpression_mesesAposFratura" typeRef="integer">
          <text>mesesAposFratura</text>
        </inputExpression>
      </input>
      <input id="Input_progressaoConsolidacao" label="Progressão Radiográfica">
        <inputExpression id="InputExpression_progressaoConsolidacao" typeRef="boolean">
          <text>progressaoConsolidacao</text>
        </inputExpression>
      </input>
      <input id="Input_sintomatica" label="Dor/Limitação Funcional">
        <inputExpression id="InputExpression_sintomatica" typeRef="boolean">
          <text>sintomatica</text>
        </inputExpression>
      </input>
      <input id="Input_estimulacaoOsseaTentada" label="Estimulação Óssea Tentada">
        <inputExpression id="InputExpression_estimulacaoOsseaTentada" typeRef="boolean">
          <text>estimulacaoOsseaTentada</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_mesesAposFratura_1">
          <text>&lt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_progressaoConsolidacao_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_sintomatica_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_estimulacaoOsseaTentada_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Consolidação em andamento - aguardar pelo menos 6 meses"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AO: pseudoartrose é definida após 6-9 meses sem progressão"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_2">
        <inputEntry id="InputEntry_mesesAposFratura_2">
          <text>[6..9]</text>
        </inputEntry>
        <inputEntry id="InputEntry_progressaoConsolidacao_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_sintomatica_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_estimulacaoOsseaTentada_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Considerar estimulação óssea não-invasiva antes de revisão cirúrgica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AO: ultrassom/eletromagnético pode evitar cirurgia em casos selecionados"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_mesesAposFratura_3">
          <text>&gt; 9</text>
        </inputEntry>
        <inputEntry id="InputEntry_progressaoConsolidacao_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_sintomatica_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_estimulacaoOsseaTentada_3">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Pseudoartrose estabelecida sintomática - revisão cirúrgica indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AO: pseudoartrose &amp;gt;9 meses sem progressão requer intervenção"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_mesesAposFratura_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_progressaoConsolidacao_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_sintomatica_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_estimulacaoOsseaTentada_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AO Trauma"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_TRAUMA_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
