<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30715459" name="ORT-TRA-004 Trauma Ortopedico" namespace="http://camunda.org/schema/1.0/dmn">
  <!-- TUSS 30715459 - ORT-TRA-004 | Versao 3.0.0 | OTA 2023, AO Foundation -->
  <decision id="Decision_30715459" name="Criterios Trauma Ortopedico 004">
    <decisionTable id="DecisionTable_30715459" hitPolicy="FIRST">
      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string"><text>diagnosticoCid10</text></inputExpression>
        <inputValues id="InputValues_diagnostico"><text>"S72.0","S72.1","S72.3","S82.0","S82.1","S42.2","S52.0","S52.5","S32.0"</text></inputValues>
      </input>
      <input id="Input_tipoFratura" label="Tipo de Fratura">
        <inputExpression id="InputExpr_tipoFratura" typeRef="string"><text>tipoFratura</text></inputExpression>
        <inputValues id="InputValues_tipoFratura"><text>"Fechada_Simples","Fechada_Cominutiva","Exposta_GI","Exposta_GII","Exposta_GIII","Patologica"</text></inputValues>
      </input>
      <input id="Input_classificacaoAO" label="Classificacao AO">
        <inputExpression id="InputExpr_classificacaoAO" typeRef="string"><text>classificacaoAO</text></inputExpression>
        <inputValues id="InputValues_classificacaoAO"><text>"Tipo_A","Tipo_B","Tipo_C"</text></inputValues>
      </input>
      <input id="Input_tempoTrauma" label="Horas desde Trauma">
        <inputExpression id="InputExpr_tempoTrauma" typeRef="number"><text>horasDesdeTrauma</text></inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      
      <rule id="Rule_30715459_Reprovado_FraturaPatologicaSemInvestigacao">
        <description>Reprovado - Fratura patologica sem investigacao oncologica</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>"Patologica"</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Fratura patologica requer investigacao oncologica e parecer oncologista antes de fixacao"</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"OTA 2023 - Pathologic Fracture Protocol"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715459_Aprovado_ExpostaUrgente">
        <description>Aprovado - Fratura exposta urgente</description>
        <inputEntry id="IE_A1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>"Exposta_GI","Exposta_GII","Exposta_GIII"</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>&lt; 24</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"URGENCIA - Fratura exposta. Desbridamento e fixacao em menos de 24h obrigatorio."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"OTA 2023 - Open Fracture Emergency"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715459_Aprovado_ColoFemur">
        <description>Aprovado - Fratura colo femur</description>
        <inputEntry id="IE_A2_1"><text>"S72.0","S72.1"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>"Fechada_Simples","Fechada_Cominutiva"</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>&lt; 48</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Fratura de colo femoral - indicacao cirurgica em menos de 48h para reduzir mortalidade"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"OTA 2023 - Hip Fracture Time-Critical"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715459_Aprovado_FraturaDeslocada">
        <description>Aprovado - Fratura deslocada tipo C</description>
        <inputEntry id="IE_A3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_A3_2"><text>"Fechada_Simples","Fechada_Cominutiva"</text></inputEntry>
        <inputEntry id="IE_A3_3"><text>"Tipo_C"</text></inputEntry>
        <inputEntry id="IE_A3_4"><text>-</text></inputEntry>
        <outputEntry id="OE_A3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A3_2"><text>"Fratura tipo C AO com indicacao cirurgica - instabilidade articular"</text></outputEntry>
        <outputEntry id="OE_A3_3"><text>"AO Foundation - Type C Fracture"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715459_Pendente_TratamentoConservador">
        <description>Pendente - Avaliar tratamento conservador</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>"Fechada_Simples"</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>"Tipo_A","Tipo_B"</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Fratura fechada tipo A/B - avaliar possibilidade de tratamento conservador antes de indicar cirurgia"</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"OTA 2023 - Conservative vs Surgical"</text></outputEntry>
      </rule>
      
      <rule id="Rule_Fallback_30715459">
        <description>Fallback</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"Fratura requer analise manual. Verificar: diagnostico, tipo fratura, classificacao AO, tempo de trauma."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Auditoria Trauma - RN 428/2017"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
