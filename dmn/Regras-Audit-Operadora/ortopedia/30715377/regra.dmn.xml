<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_SPI_015" name="ORT_SPI_015" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_SPI_015" name="Vertebroplastia/Cifoplastia">
    <decisionTable id="DecisionTable_ORT_SPI_015" hitPolicy="FIRST">
      <input id="Input_fraturaAguda" label="Fratura Aguda (&amp;lt;6 semanas)">
        <inputExpression id="InputExpression_fraturaAguda" typeRef="boolean">
          <text>fraturaAguda</text>
        </inputExpression>
      </input>
      <input id="Input_dorIntensa" label="Dor Intensa Incapacitante">
        <inputExpression id="InputExpression_dorIntensa" typeRef="boolean">
          <text>dorIntensa</text>
        </inputExpression>
      </input>
      <input id="Input_rmEdema" label="Edema Medular RM">
        <inputExpression id="InputExpression_rmEdema" typeRef="boolean">
          <text>rmEdema</text>
        </inputExpression>
      </input>
      <input id="Input_comprometimentoCanal" label="Comprometimento Canal Espinal">
        <inputExpression id="InputExpression_comprometimentoCanal" typeRef="boolean">
          <text>comprometimentoCanal</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_fraturaAguda_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_dorIntensa_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_rmEdema_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_comprometimentoCanal_1">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Comprometimento do canal espinal contraindica vertebroplastia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"NASS: fragmentos no canal são contraindicação absoluta"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_fraturaAguda_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_dorIntensa_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_rmEdema_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_comprometimentoCanal_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Fratura osteoporótica aguda sintomática - vertebroplastia indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"NASS: fraturas agudas com edema na RM respondem bem à cimentação"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_3">
        <inputEntry id="InputEntry_fraturaAguda_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_dorIntensa_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_rmEdema_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_comprometimentoCanal_3">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Fratura consolidada sem edema - vertebroplastia não indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"NASS: ausência de edema indica consolidação - cimento não alivia dor"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_fraturaAguda_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_dorIntensa_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_rmEdema_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_comprometimentoCanal_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes NASS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_SPI_015">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
