<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30715452" name="ORT-ART-007 Artroplastia Articular" namespace="http://camunda.org/schema/1.0/dmn">
  <!-- TUSS 30715452 - ORT-ART-007 | Versao 3.0.0 | AAOS 2023, SBCQ 2022 -->
  <decision id="Decision_30715452" name="Criterios Artroplastia 007">
    <decisionTable id="DecisionTable_30715452" hitPolicy="FIRST">
      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string"><text>diagnosticoCid10</text></inputExpression>
        <inputValues id="InputValues_diagnostico"><text>"M16.0","M16.1","M17.0","M17.1","M19.0","M87.0","S72.0","M75.1"</text></inputValues>
      </input>
      <input id="Input_tipoArtroplastia" label="Tipo Artroplastia">
        <inputExpression id="InputExpr_tipoArtroplastia" typeRef="string"><text>tipoArtroplastia</text></inputExpression>
        <inputValues id="InputValues_tipoArtroplastia"><text>"ATQ_Primaria","ATQ_Revisao","ATJ_Primaria","ATJ_Revisao","Ombro_Total","Ombro_Reversa","Unicompartimental"</text></inputValues>
      </input>
      <input id="Input_grauArtrose" label="Grau Artrose">
        <inputExpression id="InputExpr_grauArtrose" typeRef="string"><text>grauArtrose</text></inputExpression>
        <inputValues id="InputValues_grauArtrose"><text>"Grau_I","Grau_II","Grau_III","Grau_IV"</text></inputValues>
      </input>
      <input id="Input_tratamentoConservador" label="Meses Tratamento Conservador">
        <inputExpression id="InputExpr_tratamentoConservador" typeRef="number"><text>mesesTratamentoConservador</text></inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      
      <rule id="Rule_30715452_Reprovado_GrauI">
        <description>Reprovado - Artrose grau I nao indica artroplastia</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>"ATQ_Primaria","ATJ_Primaria"</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>"Grau_I"</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Artrose grau I nao indica artroplastia. Manter tratamento conservador."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"AAOS 2023 - Mild OA Conservative"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715452_Reprovado_SemConservador">
        <description>Reprovado - Sem tratamento conservador documentado</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>"ATQ_Primaria","ATJ_Primaria"</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>"Grau_II","Grau_III"</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>&lt; 3</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Artroplastia primaria requer minimo 3 meses de tratamento conservador documentado para graus II-III."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"SBCQ 2022 - Conservative Before TJR"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715452_Aprovado_GrauIV">
        <description>Aprovado - Artrose grau IV</description>
        <inputEntry id="IE_A1_1"><text>"M16.0","M16.1","M17.0","M17.1"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>"ATQ_Primaria","ATJ_Primaria"</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>"Grau_IV"</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Artroplastia aprovada - Artrose grau IV com indicacao cirurgica direta"</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"AAOS 2023 - Severe OA Surgical Indication"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715452_Aprovado_FalhaTratamento">
        <description>Aprovado - Falha tratamento conservador</description>
        <inputEntry id="IE_A2_1"><text>"M16.0","M16.1","M17.0","M17.1"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>"ATQ_Primaria","ATJ_Primaria"</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>"Grau_III"</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>&gt;= 3</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Artroplastia aprovada - Grau III com falha de tratamento conservador por 3+ meses"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"SBCQ 2022 - TJR After Conservative Failure"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715452_Pendente_Revisao">
        <description>Pendente - Revisao requer junta medica</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>"ATQ_Revisao","ATJ_Revisao"</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Artroplastia de revisao requer junta medica para avaliacao de causa de falha e planejamento cirurgico"</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"SBCQ 2022 - Revision TJR Evaluation"</text></outputEntry>
      </rule>
      
      <rule id="Rule_Fallback_30715452">
        <description>Fallback</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"Artroplastia requer analise manual. Verificar: diagnostico, grau artrose, tratamento previo, tipo de protese."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Junta Medica Ortopedia - RN 428/2017"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
