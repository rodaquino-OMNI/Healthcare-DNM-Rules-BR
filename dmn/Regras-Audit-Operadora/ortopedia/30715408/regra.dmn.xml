<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30715408" name="ORT-CONS-008 Tratamento Conservador Punho e Mao" namespace="http://camunda.org/schema/1.0/dmn">
  <!-- TUSS 30715408 - ORT-CONS-008 | Versao 3.0.0 | ASSH 2023, SBO 2022 -->
  <decision id="Decision_30715408" name="Criterios Tratamento Conservador Punho e Mao">
    <decisionTable id="DecisionTable_30715408" hitPolicy="FIRST">
      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string"><text>diagnosticoCid10</text></inputExpression>
        <inputValues id="InputValues_diagnostico"><text>"G56.0","M65.4","M65.3","M19.0","M18.0"</text></inputValues>
      </input>
      <input id="Input_gravidade" label="Gravidade da Lesao">
        <inputExpression id="InputExpr_gravidade" typeRef="string"><text>gravidadeLesao</text></inputExpression>
        <inputValues id="InputValues_gravidade"><text>"Leve","Moderada","Grave","Atrofia_Tenar"</text></inputValues>
      </input>
      <input id="Input_semanasTratamento" label="Semanas de Tratamento">
        <inputExpression id="InputExpr_semanasTratamento" typeRef="number"><text>semanasTratamentoConservador</text></inputExpression>
      </input>
      <input id="Input_enmgRealizada" label="ENMG Realizada">
        <inputExpression id="InputExpr_enmgRealizada" typeRef="boolean"><text>enmgRealizada</text></inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_30715408_Reprovado_AtrofiaTenar">
        <description>Reprovado - Atrofia tenar requer cirurgia</description>
        <inputEntry id="IE_R1_1"><text>"G56.0"</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>"Atrofia_Tenar"</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"STC com atrofia tenar - indicacao cirurgica urgente"</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"ASSH 2023 - CTS Surgical Emergency"</text></outputEntry>
      </rule>
      <rule id="Rule_30715408_Aprovado_CTSLeve">
        <description>Aprovado - STC leve</description>
        <inputEntry id="IE_A1_1"><text>"G56.0"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>"Leve"</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>&gt;= 8</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Tratamento conservador aprovado para STC leve"</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"ASSH 2023 - Mild CTS Protocol"</text></outputEntry>
      </rule>
      <rule id="Rule_30715408_Aprovado_Quervain">
        <description>Aprovado - Quervain</description>
        <inputEntry id="IE_A2_1"><text>"M65.4"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>"Leve","Moderada"</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>&gt;= 4</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Tratamento aprovado para tenossinovite de Quervain"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"ASSH 2023 - De Quervain Protocol"</text></outputEntry>
      </rule>
      <rule id="Rule_30715408_Pendente">
        <description>Pendente - Tratamento insuficiente</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>"Leve","Moderada"</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>&lt; 4</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Tratamento insuficiente. Continuar protocolo."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"ASSH 2023 - Conservative Trial"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_30715408">
        <description>Fallback</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"Requer analise manual por auditor medico."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Auditoria Medica - RN 428/2017"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
