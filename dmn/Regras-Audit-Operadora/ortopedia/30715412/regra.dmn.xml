<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/" id="Definitions_30715412" name="ORT-OPME-002 Controle OPME Ortopedico" namespace="http://camunda.org/schema/1.0/dmn">
  <!-- TUSS 30715412 - ORT-OPME-002 | Versao 3.0.0 | ANS OPME 2024, ANVISA -->
  <decision id="Decision_30715412" name="Criterios OPME Ortopedico 002">
    <decisionTable id="DecisionTable_30715412" hitPolicy="FIRST">
      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string"><text>diagnosticoCid10</text></inputExpression>
        <inputValues id="InputValues_diagnostico"><text>"M16.0","M16.1","M17.0","M17.1","M75.1","S72.0","S82.0","M84.1"</text></inputValues>
      </input>
      <input id="Input_tipoOpme" label="Tipo OPME">
        <inputExpression id="InputExpr_tipoOpme" typeRef="string"><text>tipoOpme</text></inputExpression>
        <inputValues id="InputValues_tipoOpme"><text>"Protese_Total","Protese_Parcial","Placa_Parafuso","Haste_Intramedular","Espacador","Ancora","Parafuso_Bioabsorvivel"</text></inputValues>
      </input>
      <input id="Input_registroAnvisa" label="Registro ANVISA Valido">
        <inputExpression id="InputExpr_registroAnvisa" typeRef="boolean"><text>registroAnvisaValido</text></inputExpression>
      </input>
      <input id="Input_justificativaMedica" label="Justificativa Medica Detalhada">
        <inputExpression id="InputExpr_justificativaMedica" typeRef="boolean"><text>justificativaMedicaDetalhada</text></inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado"><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      
      <rule id="Rule_30715412_Reprovado_SemAnvisa">
        <description>Reprovado - Sem registro ANVISA</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"OPME sem registro ANVISA valido. Material nao autorizado para uso no Brasil."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"ANVISA RDC 185/2001 - Registro Obrigatorio"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715412_Reprovado_SemJustificativa">
        <description>Reprovado - Sem justificativa medica</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>false</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Justificativa medica detalhada obrigatoria para OPME de alto custo."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"ANS OPME 2024 - Justificativa Obrigatoria"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715412_Aprovado_ProteseTotal">
        <description>Aprovado - Protese total com documentacao</description>
        <inputEntry id="IE_A1_1"><text>"M16.0","M16.1","M17.0","M17.1"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>"Protese_Total"</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>true</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"OPME aprovado - Protese total com registro ANVISA e justificativa adequada"</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"ANS OPME 2024 - Criterios Atendidos"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715412_Aprovado_FixacaoFratura">
        <description>Aprovado - Fixacao de fratura</description>
        <inputEntry id="IE_A2_1"><text>"S72.0","S82.0","M84.1"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>"Placa_Parafuso","Haste_Intramedular"</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>true</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"OPME aprovado para fixacao de fratura - material adequado"</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"OTA 2023 - Fracture Fixation Guidelines"</text></outputEntry>
      </rule>
      
      <rule id="Rule_30715412_Pendente_Ancora">
        <description>Pendente - Ancora requer avaliacao</description>
        <inputEntry id="IE_P1_1"><text>"M75.1"</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>"Ancora"</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>true</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Ancora artroscopica - verificar quantidade e especificacao tecnica"</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"Auditoria OPME - Quantidade"</text></outputEntry>
      </rule>
      
      <rule id="Rule_Fallback_30715412">
        <description>Fallback</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"OPME requer analise manual. Verificar: registro ANVISA, justificativa, compatibilidade diagnostico-material."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Auditoria OPME - RN 428/2017"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
