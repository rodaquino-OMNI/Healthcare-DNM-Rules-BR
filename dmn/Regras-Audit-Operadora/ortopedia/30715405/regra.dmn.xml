<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             id="Definitions_30715405"
             name="ORT-CONS-005 Tratamento Conservador Coluna Cervical"
             namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 30715405 - ORT-CONS-005 Tratamento Conservador Coluna Cervical
    Versao: 3.0.0 (2026-02-07)
    Especialidade: Ortopedia - Tratamento Conservador

    ============================================================
    INDICACOES CLINICAS (NASS 2023, SBC Coluna 2022):
    ============================================================
    - Cervicobraquialgia sem mielopatia
    - Hernia discal cervical com radiculopatia leve
    - Espondilose cervical sem compressao medular
    - Minimo 6-8 semanas de tratamento conservador

    ============================================================
    CONTRAINDICACOES:
    ============================================================
    - Mielopatia cervical com sinais de compressao medular
    - Deficit motor progressivo
    - Instabilidade cervical
    - Fratura ou luxacao cervical

    ============================================================
    REFERENCIAS NORMATIVAS:
    ============================================================
    - NASS Clinical Guidelines Cervical Radiculopathy 2023
    - SBC Coluna Diretrizes Cervical 2022
    - DUT ANS 2024
  -->

  <decision id="Decision_30715405" name="Criterios Tratamento Conservador Coluna Cervical">
    <decisionTable id="DecisionTable_30715405" hitPolicy="FIRST">

      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string">
          <text>diagnosticoCid10</text>
        </inputExpression>
        <inputValues id="InputValues_diagnostico">
          <text>"M50.1","M50.2","M54.2","M47.2","G54.2","M47.8"</text>
        </inputValues>
      </input>

      <input id="Input_mielopatia" label="Status Mielopatia">
        <inputExpression id="InputExpr_mielopatia" typeRef="string">
          <text>statusMielopatia</text>
        </inputExpression>
        <inputValues id="InputValues_mielopatia">
          <text>"Ausente","Leve","Moderada","Grave"</text>
        </inputValues>
      </input>

      <input id="Input_semanasTratamento" label="Semanas de Tratamento">
        <inputExpression id="InputExpr_semanasTratamento" typeRef="number">
          <text>semanasTratamentoConservador</text>
        </inputExpression>
      </input>

      <input id="Input_neckDisabilityIndex" label="Neck Disability Index Documentado">
        <inputExpression id="InputExpr_neckDisabilityIndex" typeRef="boolean">
          <text>ndiDocumentado</text>
        </inputExpression>
      </input>

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string"/>

      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- REGRAS DE REPROVACAO -->
      <rule id="Rule_30715405_Reprovado_MielopatiaGrave">
        <description>Reprovado - Mielopatia grave requer cirurgia</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>"Grave"</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Mielopatia cervical grave com sinais de compressao medular. Tratamento conservador contraindicado. Indicacao cirurgica prioritaria."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"NASS 2023 - Cervical Myelopathy Surgical Indication"</text></outputEntry>
      </rule>

      <rule id="Rule_30715405_Reprovado_MielopatiaModerada">
        <description>Reprovado - Mielopatia moderada requer avaliacao cirurgica</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>"Moderada"</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"Mielopatia moderada documentada. Tratamento conservador isolado pode resultar em progressao. Indicar avaliacao cirurgica."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"NASS 2023 - Moderate Myelopathy Management"</text></outputEntry>
      </rule>

      <!-- REGRAS DE APROVACAO -->
      <rule id="Rule_30715405_Aprovado_RadiculopatiaSemMielopatia">
        <description>Aprovado - Radiculopatia sem mielopatia</description>
        <inputEntry id="IE_A1_1"><text>"M50.1","M50.2","G54.2"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>"Ausente"</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Tratamento conservador aprovado para radiculopatia cervical sem mielopatia. 6+ semanas de fisioterapia e medicacao cumpridas."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"NASS 2023 Section 4.1 - Conservative Treatment Cervical Radiculopathy"</text></outputEntry>
      </rule>

      <rule id="Rule_30715405_Aprovado_Cervicobraquialgia">
        <description>Aprovado - Cervicobraquialgia com tratamento adequado</description>
        <inputEntry id="IE_A2_1"><text>"M54.2","M47.8"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>"Ausente","Leve"</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>&gt;= 8</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Tratamento conservador aprovado para cervicobraquialgia. 8+ semanas de programa de reabilitacao completado."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"SBC Coluna 2022 - Cervicobrachialgia Protocol"</text></outputEntry>
      </rule>

      <!-- REGRA DE PENDENCIA -->
      <rule id="Rule_30715405_Pendente_TratamentoInsuficiente">
        <description>Pendente - Tratamento conservador insuficiente</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>"Ausente","Leve"</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>&lt; 6</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Periodo de tratamento conservador insuficiente. Minimo 6 semanas para radiculopatia, 8 semanas para cervicobraquialgia. Continuar reabilitacao."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"NASS 2023 - Conservative Trial Duration"</text></outputEntry>
      </rule>

      <!-- FALLBACK -->
      <rule id="Rule_Fallback_30715405">
        <description>Fallback - Requer analise manual</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"Caso requer analise manual. Verificar: diagnostico CID-10, status de mielopatia, semanas de tratamento, Neck Disability Index."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Auditoria Medica Especializada - RN 428/2017"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
