<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_010" name="ORT_DEV_010" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_010" name="Prótese ombro sem haste (stemless)">
    <decisionTable id="DecisionTable_ORT_DEV_010" hitPolicy="FIRST">
      <input id="Input_qualidadeOssea" label="Qualidade Óssea Umeral">
        <inputExpression id="InputExpression_qualidadeOssea" typeRef="string">
          <text>qualidadeOssea</text>
        </inputExpression>
      </input>
      <input id="Input_idade" label="Idade">
        <inputExpression id="InputExpression_idade" typeRef="integer">
          <text>idade</text>
        </inputExpression>
      </input>
      <input id="Input_anatomiaPreservada" label="Anatomia Proximal Preservada">
        <inputExpression id="InputExpression_anatomiaPreservada" typeRef="boolean">
          <text>anatomiaPreservada</text>
        </inputExpression>
      </input>
      <input id="Input_revisao" label="Cirurgia de Revisão">
        <inputExpression id="InputExpression_revisao" typeRef="boolean">
          <text>revisao</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_qualidadeOssea_1">
          <text>"ruim"</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_anatomiaPreservada_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_revisao_1">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Osteoporose ou revisão - prótese com haste convencional indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: próteses sem haste requerem osso de boa qualidade e anatomia preservada"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_qualidadeOssea_2">
          <text>"boa"</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_2">
          <text>&lt; 65</text>
        </inputEntry>
        <inputEntry id="InputEntry_anatomiaPreservada_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_revisao_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Candidato ideal stemless: jovem, osso de qualidade, anatomia preservada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: próteses sem haste preservam capital ósseo em pacientes jovens"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_3">
        <inputEntry id="InputEntry_qualidadeOssea_3">
          <text>"regular"</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_3">
          <text>&gt; 75</text>
        </inputEntry>
        <inputEntry id="InputEntry_anatomiaPreservada_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_revisao_3">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Idade avançada - avaliar risco-benefício de preservação óssea"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"Em idosos, haste convencional pode oferecer maior segurança"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_qualidadeOssea_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_anatomiaPreservada_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_revisao_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_010">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
