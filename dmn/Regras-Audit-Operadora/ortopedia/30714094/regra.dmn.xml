<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_011" name="ORT_DEV_011" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_011" name="Prótese reversa ombro">
    <decisionTable id="DecisionTable_ORT_DEV_011" hitPolicy="FIRST">
      <input id="Input_rupturaManguito" label="Ruptura Irreparável Manguito">
        <inputExpression id="InputExpression_rupturaManguito" typeRef="boolean">
          <text>rupturaManguito</text>
        </inputExpression>
      </input>
      <input id="Input_artropatiaRotura" label="Artropatia por Ruptura">
        <inputExpression id="InputExpression_artropatiaRotura" typeRef="boolean">
          <text>artropatiaRotura</text>
        </inputExpression>
      </input>
      <input id="Input_idade" label="Idade">
        <inputExpression id="InputExpression_idade" typeRef="integer">
          <text>idade</text>
        </inputExpression>
      </input>
      <input id="Input_deltoideFuncional" label="Deltóide Funcional">
        <inputExpression id="InputExpression_deltoideFuncional" typeRef="boolean">
          <text>deltoideFuncional</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_rupturaManguito_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_artropatiaRotura_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_deltoideFuncional_1">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Disfunção deltóide é contraindicação para prótese reversa"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: prótese reversa depende de deltóide funcional para movimento"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_2">
        <inputEntry id="InputEntry_rupturaManguito_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_artropatiaRotura_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_2">
          <text>&lt; 65</text>
        </inputEntry>
        <inputEntry id="InputEntry_deltoideFuncional_2">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Ruptura irreparável em jovem - considerar alternativas antes de reversa"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: prótese reversa em jovens &amp;lt;65 anos tem resultados inferiores a longo prazo"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_rupturaManguito_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_artropatiaRotura_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_3">
          <text>&gt;= 70</text>
        </inputEntry>
        <inputEntry id="InputEntry_deltoideFuncional_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Artropatia por ruptura em idoso - prótese reversa indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AAOS: artropatia por ruptura em idosos é indicação clássica para prótese reversa"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_rupturaManguito_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_artropatiaRotura_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_deltoideFuncional_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_011">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
