<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_CONS_014" name="ORT_CONS_014" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_CONS_014" name="Ombro congelado - fisioterapia">
    <decisionTable id="DecisionTable_ORT_CONS_014" hitPolicy="FIRST">
      <input id="Input_faseDoenca" label="Fase (congelante/congelado/descongelante)">
        <inputExpression id="InputExpression_faseDoenca" typeRef="string">
          <text>faseDoenca</text>
        </inputExpression>
      </input>
      <input id="Input_duracaoSintomas" label="Duração (meses)">
        <inputExpression id="InputExpression_duracaoSintomas" typeRef="integer">
          <text>duracaoSintomas</text>
        </inputExpression>
      </input>
      <input id="Input_fisioterapiaMeses" label="Fisioterapia (meses)">
        <inputExpression id="InputExpression_fisioterapiaMeses" typeRef="integer">
          <text>fisioterapiaMeses</text>
        </inputExpression>
      </input>
      <input id="Input_diabetes" label="Diabético">
        <inputExpression id="InputExpression_diabetes" typeRef="boolean">
          <text>diabetes</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_faseDoenca_1">
          <text>"congelante"</text>
        </inputEntry>
        <inputEntry id="InputEntry_duracaoSintomas_1">
          <text>&lt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_1">
          <text>&lt; 3</text>
        </inputEntry>
        <inputEntry id="InputEntry_diabetes_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Capsulite adesiva fase inicial requer fisioterapia por pelo menos 6 meses"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: tratamento conservador é primeira linha, com resolução em 12-24 meses na maioria dos casos"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_faseDoenca_2">
          <text>"congelado"</text>
        </inputEntry>
        <inputEntry id="InputEntry_duracaoSintomas_2">
          <text>&gt;= 18</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_2">
          <text>&gt;= 12</text>
        </inputEntry>
        <inputEntry id="InputEntry_diabetes_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Falha tratamento conservador após 18 meses - liberação artroscópica indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: após 12-18 meses de fisioterapia sem melhora, cirurgia pode ser considerada"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_3">
        <inputEntry id="InputEntry_faseDoenca_3">
          <text>"congelado"</text>
        </inputEntry>
        <inputEntry id="InputEntry_duracaoSintomas_3">
          <text>[12..18)</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_3">
          <text>&gt;= 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_diabetes_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Diabéticos têm evolução mais lenta - continuar conservador por mais 6 meses"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"Capsulite adesiva em diabéticos tem evolução mais prolongada"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_faseDoenca_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_duracaoSintomas_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_diabetes_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_CONS_014">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
