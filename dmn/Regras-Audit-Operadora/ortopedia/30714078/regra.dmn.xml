<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_009" name="ORT_DEV_009" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_009" name="Artroplastia de recapeamento quadril">
    <decisionTable id="DecisionTable_ORT_DEV_009" hitPolicy="FIRST">
      <input id="Input_idade" label="Idade">
        <inputExpression id="InputExpression_idade" typeRef="integer">
          <text>idade</text>
        </inputExpression>
      </input>
      <input id="Input_sexo" label="Sexo (M/F)">
        <inputExpression id="InputExpression_sexo" typeRef="string">
          <text>sexo</text>
        </inputExpression>
      </input>
      <input id="Input_qualidadeOssea" label="Qualidade Óssea">
        <inputExpression id="InputExpression_qualidadeOssea" typeRef="string">
          <text>qualidadeOssea</text>
        </inputExpression>
      </input>
      <input id="Input_tamanhoFemoral" label="Tamanho Cabeça Femoral (mm)">
        <inputExpression id="InputExpression_tamanhoFemoral" typeRef="integer">
          <text>tamanhoFemoral</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_idade_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_sexo_1">
          <text>"F"</text>
        </inputEntry>
        <inputEntry id="InputEntry_qualidadeOssea_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_tamanhoFemoral_1">
          <text>&lt; 48</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Sexo feminino ou cabeça femoral &amp;lt;48mm - recapeamento contraindicado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: mulheres e cabeças pequenas têm maior risco de falha do recapeamento"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_2">
        <inputEntry id="InputEntry_idade_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_sexo_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_qualidadeOssea_2">
          <text>"ruim"</text>
        </inputEntry>
        <inputEntry id="InputEntry_tamanhoFemoral_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Osteoporose é contraindicação para recapeamento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: recapeamento requer osso de excelente qualidade"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_idade_3">
          <text>&lt; 60</text>
        </inputEntry>
        <inputEntry id="InputEntry_sexo_3">
          <text>"M"</text>
        </inputEntry>
        <inputEntry id="InputEntry_qualidadeOssea_3">
          <text>"boa"</text>
        </inputEntry>
        <inputEntry id="InputEntry_tamanhoFemoral_3">
          <text>&gt;= 50</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Homem jovem, osso de qualidade, cabeça grande - candidato para recapeamento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AAOS: homens jovens com osso de qualidade são candidatos ideais para recapeamento"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_idade_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_sexo_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_qualidadeOssea_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_tamanhoFemoral_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_009">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
