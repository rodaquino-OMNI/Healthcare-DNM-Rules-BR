<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_CONS_012" name="ORT_CONS_012" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_CONS_012" name="Tendão Aquiles conservador">
    <decisionTable id="DecisionTable_ORT_CONS_012" hitPolicy="FIRST">
      <input id="Input_rupturaCompleta" label="Ruptura Completa">
        <inputExpression id="InputExpression_rupturaCompleta" typeRef="boolean">
          <text>rupturaCompleta</text>
        </inputExpression>
      </input>
      <input id="Input_cronificacao" label="Tendinopatia Crônica">
        <inputExpression id="InputExpression_cronificacao" typeRef="boolean">
          <text>cronificacao</text>
        </inputExpression>
      </input>
      <input id="Input_fisioterapiaMeses" label="Fisioterapia (meses)">
        <inputExpression id="InputExpression_fisioterapiaMeses" typeRef="integer">
          <text>fisioterapiaMeses</text>
        </inputExpression>
      </input>
      <input id="Input_idadeAtleta" label="Atleta &lt; 40 anos">
        <inputExpression id="InputExpression_idadeAtleta" typeRef="boolean">
          <text>idadeAtleta</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Aprovado_1">
        <inputEntry id="InputEntry_rupturaCompleta_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_cronificacao_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idadeAtleta_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Ruptura completa do tendão de Aquiles - indicação cirúrgica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: ruptura completa tem indicação cirúrgica, especialmente em atletas"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_2">
        <inputEntry id="InputEntry_rupturaCompleta_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_cronificacao_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_2">
          <text>&lt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_idadeAtleta_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Tendinopatia de Aquiles requer protocolo conservador de 6 meses"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: tendinopatia crônica deve ser tratada com fisioterapia por no mínimo 6 meses"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_rupturaCompleta_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_cronificacao_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_3">
          <text>&gt;= 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_idadeAtleta_3">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Falha tratamento conservador - desbridamento tendinoso indicado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AAOS: após 6 meses de falha conservadora, cirurgia pode ser considerada"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_rupturaCompleta_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_cronificacao_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idadeAtleta_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_CONS_012">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
