<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_018" name="ORT_DEV_018" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_018" name="Substitutos ósseos - materiais">
    <decisionTable id="DecisionTable_ORT_DEV_018" hitPolicy="FIRST">
      <input id="Input_tamanhoDefeito" label="Tamanho Defeito (cm³)">
        <inputExpression id="InputExpression_tamanhoDefeito" typeRef="integer">
          <text>tamanhoDefeito</text>
        </inputExpression>
      </input>
      <input id="Input_localizacaoCarga" label="Área de Carga">
        <inputExpression id="InputExpression_localizacaoCarga" typeRef="boolean">
          <text>localizacaoCarga</text>
        </inputExpression>
      </input>
      <input id="Input_infeccao" label="Infecção Prévia">
        <inputExpression id="InputExpression_infeccao" typeRef="boolean">
          <text>infeccao</text>
        </inputExpression>
      </input>
      <input id="Input_vascularizacao" label="Vascularização Adequada">
        <inputExpression id="InputExpression_vascularizacao" typeRef="boolean">
          <text>vascularizacao</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_tamanhoDefeito_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacaoCarga_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_vascularizacao_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Infecção ativa é contraindicação para substitutos ósseos sintéticos"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AO: infecção requer controle antes de enxerto ósseo"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_tamanhoDefeito_2">
          <text>&lt; 10</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacaoCarga_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_vascularizacao_2">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Defeito pequeno fora de carga - substituto ósseo sintético adequado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AO: defeitos &amp;lt;10cm³ respondem bem a substitutos sintéticos"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_tamanhoDefeito_3">
          <text>&gt; 50</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacaoCarga_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_vascularizacao_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Defeito grande em carga - autoenxerto ou aloenxerto estrutural indicado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AO: defeitos grandes em carga requerem enxerto com propriedades estruturais"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_4">
        <inputEntry id="InputEntry_tamanhoDefeito_4">
          <text>&gt; 20</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacaoCarga_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_vascularizacao_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_4">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_4">
          <text>"Vascularização inadequada - considerar retalho vascularizado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_4">
          <text>"AO: grandes defeitos avasculares podem requerer enxerto vascularizado"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_tamanhoDefeito_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_localizacaoCarga_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infeccao_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_vascularizacao_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AO Foundation"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_018">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
