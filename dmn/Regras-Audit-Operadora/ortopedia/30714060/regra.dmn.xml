<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_CONS_015" name="ORT_CONS_015" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_CONS_015" name="Impacto femoroacetabular conservador">
    <decisionTable id="DecisionTable_ORT_CONS_015" hitPolicy="FIRST">
      <input id="Input_tipoImpacto" label="Tipo (CAM/PINCER/misto)">
        <inputExpression id="InputExpression_tipoImpacto" typeRef="string">
          <text>tipoImpacto</text>
        </inputExpression>
      </input>
      <input id="Input_grauArtrose" label="Grau Artrose Tönnis (0-3)">
        <inputExpression id="InputExpression_grauArtrose" typeRef="integer">
          <text>grauArtrose</text>
        </inputExpression>
      </input>
      <input id="Input_fisioterapiaMeses" label="Fisioterapia (meses)">
        <inputExpression id="InputExpression_fisioterapiaMeses" typeRef="integer">
          <text>fisioterapiaMeses</text>
        </inputExpression>
      </input>
      <input id="Input_idade" label="Idade">
        <inputExpression id="InputExpression_idade" typeRef="integer">
          <text>idade</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_tipoImpacto_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_grauArtrose_1">
          <text>&gt;= 2</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Artrose grau 2+ é contraindicação para artroscopia de quadril"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: artrose Tönnis ≥2 tem resultado ruim com artroscopia - considerar artroplastia"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_tipoImpacto_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_grauArtrose_2">
          <text>&lt;= 1</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_2">
          <text>&gt;= 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_2">
          <text>&lt; 50</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"IFA sem artrose após falha conservadora - artroscopia indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: IFA sintomático sem artrose e após falha conservadora tem indicação cirúrgica"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_3">
        <inputEntry id="InputEntry_tipoImpacto_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_grauArtrose_3">
          <text>0</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_3">
          <text>&lt; 3</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_3">
          <text>&lt; 40</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"IFA requer protocolo conservador de 3-6 meses antes de cirurgia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AAOS: tratamento conservador deve ser tentado antes de artroscopia"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_tipoImpacto_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_grauArtrose_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaMeses_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_CONS_015">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
