<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_CONS_004" name="ORT-CONS-004" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_CONS_004" name="Fisioterapia pré-artroscopia ombro">
    <decisionTable id="DecisionTable_ORT_CONS_004" hitPolicy="FIRST">
      <input id="Input_duracaoSintomas" label="Duração Sintomas (meses)">
        <inputExpression id="InputExpression_duracaoSintomas" typeRef="integer">
          <text>duracaoSintomas</text>
        </inputExpression>
      </input>
      <input id="Input_fisioterapiaRealizada" label="Fisioterapia Realizada">
        <inputExpression id="InputExpression_fisioterapiaRealizada" typeRef="boolean">
          <text>fisioterapiaRealizada</text>
        </inputExpression>
      </input>
      <input id="Input_rupturaCompleta" label="Ruptura Completa">
        <inputExpression id="InputExpression_rupturaCompleta" typeRef="boolean">
          <text>rupturaCompleta</text>
        </inputExpression>
      </input>
      <input id="Input_trauma" label="História de Trauma">
        <inputExpression id="InputExpression_trauma" typeRef="boolean">
          <text>trauma</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- Reprovação: Sem fisioterapia prévia e sem critérios de urgência -->
      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_duracaoSintomas_1">
          <text>&lt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaRealizada_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_rupturaCompleta_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_trauma_1">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Necessário protocolo fisioterápico conservador de 6 meses antes de artroscopia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS Clinical Practice Guideline: tratamento conservador deve preceder artroscopia em lesões parciais do manguito rotador sem trauma agudo"</text>
        </outputEntry>
      </rule>

      <!-- Aprovação: Ruptura completa -->
      <rule id="DecisionRule_Aprovado_1">
        <inputEntry id="InputEntry_duracaoSintomas_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaRealizada_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_rupturaCompleta_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_trauma_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Aprovado: ruptura completa do manguito rotador - indicação cirúrgica direta"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"Rotator Cuff Tears: Evidence-based Clinical Practice Guideline - rupturas completas requerem reparo cirúrgico"</text>
        </outputEntry>
      </rule>

      <!-- Aprovação: Trauma agudo -->
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_duracaoSintomas_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaRealizada_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_rupturaCompleta_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_trauma_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Aprovado: lesão traumática aguda do ombro - indicação cirúrgica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AAOS: lesões traumáticas agudas do ombro têm indicação cirúrgica direta"</text>
        </outputEntry>
      </rule>

      <!-- Aprovação: Fisioterapia realizada sem sucesso -->
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_duracaoSintomas_4">
          <text>&gt;= 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaRealizada_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_rupturaCompleta_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_trauma_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_4">
          <text>"Aprovado: falha conservadora após 6 meses de fisioterapia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_4">
          <text>"AAOS: artroscopia indicada após falha do tratamento conservador de no mínimo 6 meses"</text>
        </outputEntry>
      </rule>

      <!-- Pendente: Casos intermediários -->
      <rule id="DecisionRule_Pendente_1">
        <inputEntry id="InputEntry_duracaoSintomas_5">
          <text>[3..6)</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaRealizada_5">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_rupturaCompleta_5">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_trauma_5">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_5">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_5">
          <text>"Análise adicional: fisioterapia iniciada mas não completou 6 meses"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_5">
          <text>"Necessária avaliação de evolução clínica e resposta parcial ao tratamento conservador"</text>
        </outputEntry>
      </rule>

      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_duracaoSintomas_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapiaRealizada_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_rupturaCompleta_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_trauma_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS e SBO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_CONS_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
