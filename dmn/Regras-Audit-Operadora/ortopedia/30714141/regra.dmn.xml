<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_022" name="ORT_DEV_022" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_022" name="Artroscopia ombro - implantes apropriados">
    <decisionTable id="DecisionTable_ORT_DEV_022" hitPolicy="FIRST">
      <input id="Input_procedimento" label="Tipo Procedimento (diagnóstico/reparo/capsuloplastia)">
        <inputExpression id="InputExpression_procedimento" typeRef="string">
          <text>procedimento</text>
        </inputExpression>
      </input>
      <input id="Input_numeroAncoras" label="Número de Âncoras">
        <inputExpression id="InputExpression_numeroAncoras" typeRef="integer">
          <text>numeroAncoras</text>
        </inputExpression>
      </input>
      <input id="Input_instrumentaisBioressorviveis" label="Uso Bioabsorvíveis">
        <inputExpression id="InputExpression_instrumentaisBioressorviveis" typeRef="boolean">
          <text>instrumentaisBioressorviveis</text>
        </inputExpression>
      </input>
      <input id="Input_idade" label="Idade">
        <inputExpression id="InputExpression_idade" typeRef="integer">
          <text>idade</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_procedimento_1">
          <text>"diagnóstico"</text>
        </inputEntry>
        <inputEntry id="InputEntry_numeroAncoras_1">
          <text>0</text>
        </inputEntry>
        <inputEntry id="InputEntry_instrumentaisBioressorviveis_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Artroscopia diagnóstica não requer âncoras ou implantes"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: procedimentos diagnósticos não utilizam implantes"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_2">
        <inputEntry id="InputEntry_procedimento_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_numeroAncoras_2">
          <text>&gt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_instrumentaisBioressorviveis_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Número excessivo de âncoras - justificar extensão da lesão"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: reparos standard usam 2-4 âncoras - &amp;gt;6 requer justificativa"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_procedimento_3">
          <text>"reparo"</text>
        </inputEntry>
        <inputEntry id="InputEntry_numeroAncoras_3">
          <text>[2..4]</text>
        </inputEntry>
        <inputEntry id="InputEntry_instrumentaisBioressorviveis_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_3">
          <text>&lt; 50</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Reparo do manguito rotador - 2-4 âncoras apropriadas"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AAOS: reparos do manguito usam tipicamente 2-4 âncoras"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_procedimento_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_numeroAncoras_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instrumentaisBioressorviveis_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_022">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
