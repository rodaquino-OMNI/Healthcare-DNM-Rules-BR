<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_020" name="ORT_DEV_020" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_020" name="Haste intramedular - seleção">
    <decisionTable id="DecisionTable_ORT_DEV_020" hitPolicy="FIRST">
      <input id="Input_localizacaoFratura" label="Localização (diáfise/metáfise)">
        <inputExpression id="InputExpression_localizacaoFratura" typeRef="string">
          <text>localizacaoFratura</text>
        </inputExpression>
      </input>
      <input id="Input_cominutiva" label="Fratura Cominutiva">
        <inputExpression id="InputExpression_cominutiva" typeRef="boolean">
          <text>cominutiva</text>
        </inputExpression>
      </input>
      <input id="Input_patologica" label="Fratura Patológica">
        <inputExpression id="InputExpression_patologica" typeRef="boolean">
          <text>patologica</text>
        </inputExpression>
      </input>
      <input id="Input_canalMedular" label="Canal Medular (normal/estreito)">
        <inputExpression id="InputExpression_canalMedular" typeRef="string">
          <text>canalMedular</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_localizacaoFratura_1">
          <text>"metáfise"</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_patologica_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_canalMedular_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Fratura metafisária - placa bloqueada preferível a haste intramedular"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AO: fraturas metafisárias têm fixação inadequada com hastes intramedulares"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_localizacaoFratura_2">
          <text>"diáfise"</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_patologica_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_canalMedular_2">
          <text>"normal"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Fratura diafisária cominutiva - haste intramedular bloqueada indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AO: hastes intramedulares são padrão-ouro para fraturas diafisárias cominutivas"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_3">
        <inputEntry id="InputEntry_localizacaoFratura_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_patologica_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_canalMedular_3">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Fratura patológica - avaliar necessidade de curetagem e enxerto"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AO: fraturas patológicas podem requerer procedimento adicional à fixação"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_4">
        <inputEntry id="InputEntry_localizacaoFratura_4">
          <text>"diáfise"</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_patologica_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_canalMedular_4">
          <text>"estreito"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_4">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_4">
          <text>"Canal medular estreito - avaliar viabilidade fresagem ou placa"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_4">
          <text>"AO: canais estreitos podem dificultar haste intramedular"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_localizacaoFratura_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_cominutiva_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_patologica_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_canalMedular_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AO Trauma"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_020">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
