<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_SPI_005" name="ORT_SPI_005" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_SPI_005" name="Artrodese cervical - critérios">
    <decisionTable id="DecisionTable_ORT_SPI_005" hitPolicy="FIRST">
      <input id="Input_mielopatia" label="Mielopatia Presente">
        <inputExpression id="InputExpression_mielopatia" typeRef="boolean">
          <text>mielopatia</text>
        </inputExpression>
      </input>
      <input id="Input_dor Refratária" label="Dor Refratária &amp;gt;6 meses">
        <inputExpression id="InputExpression_dor Refratária" typeRef="boolean">
          <text>dor Refratária</text>
        </inputExpression>
      </input>
      <input id="Input_instabilidade" label="Instabilidade Radiográfica">
        <inputExpression id="InputExpression_instabilidade" typeRef="boolean">
          <text>instabilidade</text>
        </inputExpression>
      </input>
      <input id="Input_conservadorFalhou" label="Falha Conservador">
        <inputExpression id="InputExpression_conservadorFalhou" typeRef="boolean">
          <text>conservadorFalhou</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Aprovado_1">
        <inputEntry id="InputEntry_mielopatia_1">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Refratária_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidade_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_conservadorFalhou_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Mielopatia cervical - indicação cirúrgica para evitar progressão"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"NASS: mielopatia cervical tem indicação cirúrgica estabelecida"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_2">
        <inputEntry id="InputEntry_mielopatia_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Refratária_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidade_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_conservadorFalhou_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Dor cervical sem mielopatia requer tratamento conservador inicial"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"NASS: dor cervical sem déficit neurológico deve ter tratamento conservador de 6 meses"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_mielopatia_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Refratária_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidade_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_conservadorFalhou_3">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Instabilidade cervical radiográfica - artrodese indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"NASS: instabilidade &amp;gt;3.5mm ou &amp;gt;11° requer fusão"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_4">
        <inputEntry id="InputEntry_mielopatia_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Refratária_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidade_4">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_conservadorFalhou_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_4">
          <text>"Radiculopatia cervical refratária após 6 meses - cirurgia indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_4">
          <text>"NASS: após falha conservadora de 6 meses, cirurgia tem indicação"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_mielopatia_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_dor Refratária_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_instabilidade_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_conservadorFalhou_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes North American Spine Society (NASS)"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_SPI_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
