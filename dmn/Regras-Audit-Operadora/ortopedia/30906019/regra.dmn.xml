<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_CONS_013" name="ORT_CONS_013" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_CONS_013" name="Epicondilite lateral - infiltrações">
    <decisionTable id="DecisionTable_ORT_CONS_013" hitPolicy="FIRST">
      <input id="Input_duracaoSintomas" label="Duração (meses)">
        <inputExpression id="InputExpression_duracaoSintomas" typeRef="integer">
          <text>duracaoSintomas</text>
        </inputExpression>
      </input>
      <input id="Input_infiltracoesRealizadas" label="Número Infiltrações">
        <inputExpression id="InputExpression_infiltracoesRealizadas" typeRef="integer">
          <text>infiltracoesRealizadas</text>
        </inputExpression>
      </input>
      <input id="Input_fisioterapia" label="Fisioterapia Realizada">
        <inputExpression id="InputExpression_fisioterapia" typeRef="boolean">
          <text>fisioterapia</text>
        </inputExpression>
      </input>
      <input id="Input_limitacaoFuncional" label="Limitação Funcional Severa">
        <inputExpression id="InputExpression_limitacaoFuncional" typeRef="boolean">
          <text>limitacaoFuncional</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_duracaoSintomas_1">
          <text>&lt; 3</text>
        </inputEntry>
        <inputEntry id="InputEntry_infiltracoesRealizadas_1">
          <text>0</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapia_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_limitacaoFuncional_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Epicondilite lateral requer fisioterapia e até 2 infiltrações antes de cirurgia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: tratamento conservador (fisio + infiltração) deve ser tentado por 6-12 meses"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_2">
        <inputEntry id="InputEntry_duracaoSintomas_2">
          <text>&gt;= 12</text>
        </inputEntry>
        <inputEntry id="InputEntry_infiltracoesRealizadas_2">
          <text>&gt;= 2</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapia_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_limitacaoFuncional_2">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Falha conservadora após 12 meses - liberação epicondiliana indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: cirurgia indicada após 12 meses de tratamento conservador sem sucesso"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Pendente_3">
        <inputEntry id="InputEntry_duracaoSintomas_3">
          <text>[6..12)</text>
        </inputEntry>
        <inputEntry id="InputEntry_infiltracoesRealizadas_3">
          <text>1</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapia_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_limitacaoFuncional_3">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Considerar segunda infiltração antes de indicação cirúrgica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"Até 2 infiltrações podem ser tentadas antes de cirurgia"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_duracaoSintomas_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_infiltracoesRealizadas_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fisioterapia_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_limitacaoFuncional_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_CONS_013">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
