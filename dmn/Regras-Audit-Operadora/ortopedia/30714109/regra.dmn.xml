<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_DEV_012" name="ORT_DEV_012" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_DEV_012" name="Prótese anatômica ombro - critérios">
    <decisionTable id="DecisionTable_ORT_DEV_012" hitPolicy="FIRST">
      <input id="Input_manguito Intacto" label="Manguito Rotador Íntegro">
        <inputExpression id="InputExpression_manguito Intacto" typeRef="boolean">
          <text>manguito Intacto</text>
        </inputExpression>
      </input>
      <input id="Input_glenoideErosao" label="Erosão Glenóide Posterior">
        <inputExpression id="InputExpression_glenoideErosao" typeRef="boolean">
          <text>glenoideErosao</text>
        </inputExpression>
      </input>
      <input id="Input_idade" label="Idade">
        <inputExpression id="InputExpression_idade" typeRef="integer">
          <text>idade</text>
        </inputExpression>
      </input>
      <input id="Input_qualidadeOssea" label="Qualidade Óssea">
        <inputExpression id="InputExpression_qualidadeOssea" typeRef="string">
          <text>qualidadeOssea</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_manguito Intacto_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_glenoideErosao_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_qualidadeOssea_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Ruptura do manguito rotador - prótese reversa indicada ao invés de anatômica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AAOS: manguito íntegro é pré-requisito para prótese anatômica"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_2">
        <inputEntry id="InputEntry_manguito Intacto_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_glenoideErosao_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_qualidadeOssea_2">
          <text>"ruim"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Erosão glenóide posterior severa - dificulta fixação componente glenoidal"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AAOS: erosão posterior &amp;gt;15mm dificulta prótese anatômica"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_manguito Intacto_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_glenoideErosao_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_3">
          <text>&lt; 75</text>
        </inputEntry>
        <inputEntry id="InputEntry_qualidadeOssea_3">
          <text>"boa"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Manguito íntegro, osso de qualidade - prótese anatômica indicada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AAOS: artrose glenoumeral com manguito íntegro é indicação para prótese anatômica"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_manguito Intacto_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_glenoideErosao_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_idade_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_qualidadeOssea_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AAOS"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_DEV_012">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
