<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             id="Definitions_30715407"
             name="ORT-CONS-007 Tratamento Conservador Cotovelo"
             namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 30715407 - ORT-CONS-007 Tratamento Conservador Cotovelo
    Versao: 3.0.0 (2026-02-07)
    Especialidade: Ortopedia - Tratamento Conservador
    Referencias: AAOS 2023, SBO 2022
  -->

  <decision id="Decision_30715407" name="Criterios Tratamento Conservador Cotovelo">
    <decisionTable id="DecisionTable_30715407" hitPolicy="FIRST">

      <input id="Input_diagnostico" label="Diagnostico CID-10">
        <inputExpression id="InputExpr_diagnostico" typeRef="string">
          <text>diagnosticoCid10</text>
        </inputExpression>
        <inputValues id="InputValues_diagnostico">
          <text>"M77.0","M77.1","M70.2","M70.3","S53.4"</text>
        </inputValues>
      </input>

      <input id="Input_tipoLesao" label="Tipo de Lesao">
        <inputExpression id="InputExpr_tipoLesao" typeRef="string">
          <text>tipoLesao</text>
        </inputExpression>
        <inputValues id="InputValues_tipoLesao">
          <text>"Epicondilite_Lateral","Epicondilite_Medial","Bursite","Ruptura_Ligamentar"</text>
        </inputValues>
      </input>

      <input id="Input_semanasTratamento" label="Semanas de Tratamento">
        <inputExpression id="InputExpr_semanasTratamento" typeRef="number">
          <text>semanasTratamentoConservador</text>
        </inputExpression>
      </input>

      <input id="Input_dashScore" label="DASH Score Documentado">
        <inputExpression id="InputExpr_dashScore" typeRef="boolean">
          <text>dashScoreDocumentado</text>
        </inputExpression>
      </input>

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string"/>

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="Rule_30715407_Reprovado_Instabilidade">
        <description>Reprovado - Instabilidade ou ruptura ligamentar</description>
        <inputEntry id="IE_R1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_2"><text>"Ruptura_Ligamentar"</text></inputEntry>
        <inputEntry id="IE_R1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_R1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R1_2"><text>"Ruptura ligamentar do cotovelo. Indicacao de avaliacao cirurgica."</text></outputEntry>
        <outputEntry id="OE_R1_3"><text>"AAOS 2023 - Elbow Instability"</text></outputEntry>
      </rule>

      <rule id="Rule_30715407_Reprovado_SemScore">
        <description>Reprovado - DASH Score obrigatorio</description>
        <inputEntry id="IE_R2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_R2_4"><text>false</text></inputEntry>
        <outputEntry id="OE_R2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_R2_2"><text>"DASH Score nao documentado. Escore funcional obrigatorio."</text></outputEntry>
        <outputEntry id="OE_R2_3"><text>"SBO 2022 - Documentacao Funcional"</text></outputEntry>
      </rule>

      <rule id="Rule_30715407_Aprovado_EpicondiliteLateral">
        <description>Aprovado - Epicondilite lateral</description>
        <inputEntry id="IE_A1_1"><text>"M77.1"</text></inputEntry>
        <inputEntry id="IE_A1_2"><text>"Epicondilite_Lateral"</text></inputEntry>
        <inputEntry id="IE_A1_3"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_A1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_A1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A1_2"><text>"Tratamento conservador aprovado. 6+ semanas de fisioterapia."</text></outputEntry>
        <outputEntry id="OE_A1_3"><text>"AAOS 2023 - Lateral Epicondylitis"</text></outputEntry>
      </rule>

      <rule id="Rule_30715407_Aprovado_EpicondiliteMedial">
        <description>Aprovado - Epicondilite medial</description>
        <inputEntry id="IE_A2_1"><text>"M77.0"</text></inputEntry>
        <inputEntry id="IE_A2_2"><text>"Epicondilite_Medial"</text></inputEntry>
        <inputEntry id="IE_A2_3"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_A2_4"><text>true</text></inputEntry>
        <outputEntry id="OE_A2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_A2_2"><text>"Tratamento conservador aprovado para epicondilite medial."</text></outputEntry>
        <outputEntry id="OE_A2_3"><text>"AAOS 2023 - Medial Epicondylitis"</text></outputEntry>
      </rule>

      <rule id="Rule_30715407_Pendente_TratamentoInsuficiente">
        <description>Pendente - Tratamento insuficiente</description>
        <inputEntry id="IE_P1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_P1_2"><text>"Epicondilite_Lateral","Epicondilite_Medial","Bursite"</text></inputEntry>
        <inputEntry id="IE_P1_3"><text>&lt; 6</text></inputEntry>
        <inputEntry id="IE_P1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_P1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_P1_2"><text>"Periodo insuficiente. Minimo 6 semanas."</text></outputEntry>
        <outputEntry id="OE_P1_3"><text>"AAOS 2023 - Conservative Trial"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_30715407">
        <description>Fallback - Requer analise manual</description>
        <inputEntry id="IE_F_1"><text>-</text></inputEntry>
        <inputEntry id="IE_F_2"><text>-</text></inputEntry>
        <inputEntry id="IE_F_3"><text>-</text></inputEntry>
        <inputEntry id="IE_F_4"><text>-</text></inputEntry>
        <outputEntry id="OE_F_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_F_2"><text>"Requer analise manual por auditor medico."</text></outputEntry>
        <outputEntry id="OE_F_3"><text>"Auditoria Medica - RN 428/2017"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
