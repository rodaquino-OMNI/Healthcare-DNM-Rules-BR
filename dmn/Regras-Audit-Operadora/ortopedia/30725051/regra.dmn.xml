<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_ORT_TRAUMA_002" name="ORT_TRAUMA_002" namespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_ORT_TRAUMA_002" name="Fratura exposta - protocolo">
    <decisionTable id="DecisionTable_ORT_TRAUMA_002" hitPolicy="FIRST">
      <input id="Input_classificacaoGustilo" label="Classificação Gustilo (I/II/III)">
        <inputExpression id="InputExpression_classificacaoGustilo" typeRef="string">
          <text>classificacaoGustilo</text>
        </inputExpression>
      </input>
      <input id="Input_horasAposTrauma" label="Horas Desde Trauma">
        <inputExpression id="InputExpression_horasAposTrauma" typeRef="integer">
          <text>horasAposTrauma</text>
        </inputExpression>
      </input>
      <input id="Input_antibiotico" label="Antibiótico Iniciado">
        <inputExpression id="InputExpression_antibiotico" typeRef="boolean">
          <text>antibiotico</text>
        </inputExpression>
      </input>
      <input id="Input_desbridamento" label="Desbridamento Realizado">
        <inputExpression id="InputExpression_desbridamento" typeRef="boolean">
          <text>desbridamento</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues>
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues>
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="DecisionRule_Reprovado_1">
        <inputEntry id="InputEntry_classificacaoGustilo_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_horasAposTrauma_1">
          <text>&lt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_antibiotico_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_desbridamento_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1">
          <text>"Fratura exposta requer antibiótico imediato (dentro de 3h)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1">
          <text>"AO: antibiótico deve ser iniciado na emergência"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Reprovado_2">
        <inputEntry id="InputEntry_classificacaoGustilo_2">
          <text>"III"</text>
        </inputEntry>
        <inputEntry id="InputEntry_horasAposTrauma_2">
          <text>&gt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_antibiotico_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_desbridamento_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_2">
          <text>"Gustilo III requer desbridamento urgente - não postergar"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_2">
          <text>"AO: fraturas III devem ser desbridadas em &amp;lt;6-8h"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_Aprovado_3">
        <inputEntry id="InputEntry_classificacaoGustilo_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_horasAposTrauma_3">
          <text>&lt; 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_antibiotico_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_desbridamento_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_3">
          <text>"Protocolo adequado: antibiótico + desbridamento precoce"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_3">
          <text>"AO: protocolo precoce reduz infecção de 25% para &amp;lt;5%"</text>
        </outputEntry>
      </rule>
      <!-- Regra de fallback -->
      <rule id="DecisionRule_Fallback">
        <inputEntry id="InputEntry_classificacaoGustilo_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_horasAposTrauma_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_antibiotico_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_desbridamento_fallback">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Caso não enquadrado nas regras padrão - requer análise individualizada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliação caso a caso conforme diretrizes AO Trauma"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ORT_TRAUMA_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
