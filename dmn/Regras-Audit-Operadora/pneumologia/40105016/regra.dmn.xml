<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40105016" name="Determinacao das pressoes respiratorias maximas" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40105016" name="Indicacao para Determinacao das pressoes respiratorias maximas">
    <decisionTable id="DecisionTable_40105016" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="CVF (% previsto)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>cvfPercent</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dispneia Presente">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>dispneiaPresente</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Tosse Ineficaz">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>tosseIneficaz</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40105016_1">
        <description>Pneumotórax não drenado é contraindicação</description>
        <inputEntry id="InputEntry_C1_1">
          <text>"J93.0", "J93.1", "J93.8", "J93.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Contraindicação: pneumotórax não drenado. Alto risco de expansão durante manobras de pressão"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"ATS/ERS Guidelines: Manobras de pressão respiratória máxima são contraindicadas em pneumotórax não tratado"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40105016_1">
        <description>Doença neuromuscular com dispneia e restrição</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"G70.0", "G70.1", "G70.2", "G71.0", "G71.1", "G71.2", "G12.2", "G61.0"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text>&amp;lt; 80</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Doença neuromuscular com comprometimento ventilatório. Avaliação de força muscular essencial"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"SBPT 2022: PImáx/PEmáx são essenciais para monitoramento de doenças neuromusculares. CVF &amp;lt;80% + dispneia indicam comprometimento"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105016_2">
        <description>Miastenia gravis - monitoramento</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"G70.0", "G70.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Miastenia gravis. Monitoramento periódico de força muscular respiratória indicado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"ATS/ERS: Medida de pressões respiratórias é indicada em todos os pacientes com miastenia gravis para monitoramento de resposta terapêutica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105016_3">
        <description>Distrofia muscular com tosse ineficaz</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"G71.0", "G71.1", "G71.2", "G71.3"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Distrofia muscular com tosse ineficaz. PEmáx avalia capacidade de proteção de vias aéreas"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"SBPT 2022: PEmáx &amp;lt;60 cmH2O indica risco de aspiração e necessidade de tosse assistida"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105016_4">
        <description>Dispneia inexplicada com CVF limítrofe</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"R06.0", "R06.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text>[70..85]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Dispneia com CVF levemente reduzida. Investigação de fraqueza muscular como causa"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"ATS/ERS: Fraqueza muscular pode causar dispneia com CVF normal ou minimamente reduzida. PImáx &amp;lt;-80 cmH2O confirma fraqueza"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105016_5">
        <description>Esclerose lateral amiotrófica</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"G12.2", "G12.20", "G12.21"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"ELA. Monitoramento trimestral de força muscular respiratória para indicação de VNI"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"SBPT 2022: PImáx &amp;lt;-60 cmH2O ou CVF &amp;lt;50% são critérios para iniciar VNI em ELA"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105016_6">
        <description>Síndrome de Guillain-Barré</description>
        <inputEntry id="InputEntry_A6_1">
          <text>"G61.0"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A6_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_2">
          <text>"Guillain-Barré. Monitoramento de força muscular respiratória para prever necessidade de IOT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_3">
          <text>"ATS/ERS: PImáx &amp;lt;-30 cmH2O ou CVF &amp;lt;20 mL/kg indicam alto risco de falência respiratória em Guillain-Barré"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105016_7">
        <description>Restrição pulmonar de causa indeterminada</description>
        <inputEntry id="InputEntry_A7_1">
          <text>"J98.4"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A7_2">
          <text>&amp;lt; 70</text>
        </inputEntry>
        <inputEntry id="InputEntry_A7_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A7_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A7_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A7_2">
          <text>"Restrição pulmonar inexplicada. Diferenciação entre causa parenquimatosa e neuromuscular"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A7_3">
          <text>"SBPT 2022: PImáx e PEmáx normais em restrição parenquimatosa. Reduzidas em fraqueza muscular. Essencial para diagnóstico diferencial"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40105016">
        <description>Regra padrão - Critérios não atendidos</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Documentação insuficiente. Enviar: espirometria completa, quadro clínico detalhado, força muscular periférica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>