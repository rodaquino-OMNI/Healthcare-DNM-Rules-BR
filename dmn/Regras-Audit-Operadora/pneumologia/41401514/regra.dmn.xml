<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_41401514" name="Oximetria não invasiva" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_41401514" name="Decisão de Cobertura para Oximetria Não Invasiva">
    <decisionTable id="DecisionTable_41401514" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Duração Monitoramento (horas)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>espirometria</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 Basal (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Prescrição Médica Anexada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoPneumologico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Aprovado_41401514_1">
        <description>DPOC com dessaturação para titulação de O2</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J44.0", "J44.1", "J44.8", "J44.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>&amp;lt; 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"DPOC com SpO2 &amp;lt;90% requer oximetria para titulação de oxigenoterapia domiciliar"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021: Oximetria é necessária para prescrição de O2 domiciliar. GOLD 2024: Titular fluxo para SpO2 88-92%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_41401514_2">
        <description>Suspeita de apneia do sono - oximetria noturna</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"G47.3", "G47.30", "G47.33"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>&amp;gt;= 6</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Suspeita de apneia do sono requer oximetria noturna (6+ horas) para triagem"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"ANS RN 465/2021: Oximetria noturna é método de triagem para apneia do sono. SBPT: Dessaturação &amp;gt;3% do basal sugere SAOS"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_41401514_3">
        <description>Fibrose pulmonar em acompanhamento</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"J84.0", "J84.1", "J84.8", "J84.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text>&amp;lt; 94</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Doença pulmonar intersticial com dessaturação requer oximetria para monitoramento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"SBPT 2024: Oximetria ambulatorial detecta dessaturação ao exercício em DPI. Indicação para O2 domiciliar se SpO2 &amp;lt;88%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_41401514_4">
        <description>Hipertensão pulmonar para avaliação de dessaturação</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"I27.0", "I27.2", "I27.8", "I27.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>&amp;lt; 92</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Hipertensão pulmonar com dessaturação requer oximetria para titulação de O2"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"ANS RN 465/2021: Oximetria necessária em HAP para otimização de terapia. Meta: SpO2 &amp;gt;90%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_41401514_5">
        <description>Pós-alta por COVID-19 com sequela respiratória</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"U09.9", "J99.8", "B34.2"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>&amp;lt; 94</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Sequela de COVID-19 com dessaturação requer oximetria domiciliar para monitoramento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"SBPT 2024: Oximetria domiciliar é recomendada em pós-COVID com SpO2 &amp;lt;94% para detecção de dessaturação silenciosa"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_41401514_6">
        <description>Insuficiência cardíaca com dispneia</description>
        <inputEntry id="InputEntry_A6_1">
          <text>"I50.0", "I50.1", "I50.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_3">
          <text>&amp;lt; 92</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A6_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_2">
          <text>"Insuficiência cardíaca com dessaturação requer oximetria para monitoramento e titulação de O2"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_3">
          <text>"ANS RN 465/2021: Oximetria ambulatorial detecta dessaturação em IC descompensada. ATS: Meta SpO2 &amp;gt;90%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_41401514_7">
        <description>Pneumonia pós-alta hospitalar</description>
        <inputEntry id="InputEntry_A7_1">
          <text>"J18.0", "J18.1", "J18.8", "J18.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A7_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A7_3">
          <text>[88..93]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A7_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A7_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A7_2">
          <text>"Pneumonia em recuperação com dessaturação borderline requer oximetria domiciliar"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A7_3">
          <text>"SBPT 2024: Oximetria domiciliar monitora recuperação pós-pneumonia. Alta segura com SpO2 estável &amp;gt;92%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_41401514">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar: prescrição médica detalhada, SpO2 basal, indicação para oximetria"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>