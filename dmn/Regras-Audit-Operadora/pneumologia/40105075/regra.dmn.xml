<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40105075" name="Prova de função pulmonar completa (espirometria)" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40105075" name="Decisão de Cobertura para Espirometria Completa">
    <decisionTable id="DecisionTable_40105075" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="FEV1/FVC (%)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>espirometria</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo Pneumológico Anexado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoPneumologico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40105075_1">
        <description>Pneumotórax agudo é contraindicação absoluta</description>
        <inputEntry id="InputEntry_C1_1">
          <text>"J93.0", "J93.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Pneumotórax agudo é contraindicação absoluta para espirometria por risco de agravamento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"ATS/ERS 2023: Pneumotórax não tratado é contraindicação absoluta para testes de função pulmonar"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Contraind_40105075_2">
        <description>Hemoptise ativa contraindica manobras forçadas</description>
        <inputEntry id="InputEntry_C2_1">
          <text>"R04.2"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C2_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C2_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C2_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C2_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C2_2">
          <text>"Hemoptise ativa contraindica espirometria por risco de sangramento aumentado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C2_3">
          <text>"SBPT 2024: Hemoptise ativa é contraindicação relativa. Aguardar estabilização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40105075_1">
        <description>Suspeita de asma ou DPOC com laudo</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J45.0", "J45.1", "J45.8", "J45.9", "J44.0", "J44.1", "J44.8", "J44.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Espirometria indicada para diagnóstico e classificação de asma/DPOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021: Espirometria é exame essencial para diagnóstico de asma e DPOC. SBPT 2024: Teste obrigatório para confirmação diagnóstica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105075_2">
        <description>Dispneia crônica não esclarecida com laudo</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"R06.0", "R06.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>&amp;lt; 95</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Dispneia com dessaturação justifica investigação funcional pulmonar"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"SBPT 2024: Dispneia crônica com SpO2 &amp;lt;95% requer espirometria para exclusão de doença pulmonar obstrutiva"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105075_3">
        <description>Avaliação pré-operatória para cirurgias de risco</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"Z01.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Espirometria pré-operatória para estratificação de risco cirúrgico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"ANS RN 465/2021: Espirometria pré-operatória está coberta para cirurgias torácicas e abdominais altas em pacientes com risco pulmonar"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105075_4">
        <description>Tosse crônica persistente com laudo</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"R05"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Tosse crônica justifica espirometria para exclusão de asma variante"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"SBPT 2024: Tosse crônica (&amp;gt;8 semanas) requer espirometria no algoritmo diagnóstico para exclusão de asma e DPOC"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105075_5">
        <description>Doença pulmonar intersticial suspeita ou confirmada</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"J84.0", "J84.1", "J84.8", "J84.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>&amp;lt; 94</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Doença pulmonar intersticial requer espirometria para avaliação funcional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"ANS RN 465/2021: Espirometria é essencial para monitoramento de doenças intersticiais. SBPT: Teste padrão-ouro para seguimento"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105075_6">
        <description>Fibrose pulmonar em acompanhamento</description>
        <inputEntry id="InputEntry_A6_1">
          <text>"J84.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A6_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_2">
          <text>"Fibrose pulmonar requer espirometria periódica para monitoramento de progressão"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_3">
          <text>"SBPT 2024: Espirometria a cada 3-6 meses para monitoramento de declínio funcional em fibrose pulmonar"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40105075">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar: laudo pneumológico detalhado, indicação clínica específica, e dados de saturação"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>