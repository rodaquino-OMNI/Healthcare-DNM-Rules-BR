<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40105148" name="Espirometria simples" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40105148" name="Decisão de Cobertura para Espirometria Simples">
    <decisionTable id="DecisionTable_40105148" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="FEV1/FVC (%)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>espirometria</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo Médico Anexado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoPneumologico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40105148_1">
        <description>Pneumotórax recente contraindica espirometria</description>
        <inputEntry id="InputEntry_C1_1">
          <text>"J93.0", "J93.1", "J93.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Pneumotórax é contraindicação para espirometria até resolução completa"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"ATS/ERS 2023: Aguardar pelo menos 2 semanas após resolução de pneumotórax"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40105148_1">
        <description>Asma em acompanhamento</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J45.0", "J45.1", "J45.8", "J45.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Espirometria indicada para monitoramento de asma"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021: Espirometria coberta para acompanhamento de asma. SBPT: Recomenda-se espirometria a cada 3-6 meses"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105148_2">
        <description>DPOC em seguimento</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"J44.0", "J44.1", "J44.8", "J44.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>&amp;lt; 70</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"DPOC confirmada com obstrução ao fluxo aéreo (FEV1/FVC &amp;lt;70%)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"GOLD 2024: FEV1/FVC &amp;lt;70% confirma obstrução. Espirometria periódica é essencial para monitoramento"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105148_3">
        <description>Avaliação ocupacional</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"Z10.0", "Z02.0", "Z02.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Espirometria para avaliação ocupacional ou admissional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"NR-7 (Portaria 3214/78): Espirometria obrigatória para expostos a agentes pneumotóxicos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105148_4">
        <description>Sintomas respiratórios não esclarecidos</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"R06.0", "R06.8", "R05", "R09.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>&amp;lt; 95</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Sintomas respiratórios com dessaturação justificam investigação funcional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"SBPT 2024: Espirometria é primeira linha para investigação de sintomas respiratórios persistentes"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105148_5">
        <description>Bronquite crônica em acompanhamento</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"J42", "J41.0", "J41.1", "J41.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Bronquite crônica requer avaliação funcional periódica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"ANS RN 465/2021: Espirometria coberta para acompanhamento de bronquite crônica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40105148">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar: laudo médico com indicação clínica detalhada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>