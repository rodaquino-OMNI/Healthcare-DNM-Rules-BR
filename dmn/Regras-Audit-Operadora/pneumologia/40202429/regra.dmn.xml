<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40202429" name="Laringoscopia/traqueoscopia rígida" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40202429" name="Decisão de Cobertura para Laringoscopia/Traqueoscopia Rígida">
    <decisionTable id="DecisionTable_40202429" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Grau de Estenose (%)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>espirometria</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo ORL/Pneumo Anexado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoPneumologico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40202429_1">
        <description>Insuficiência respiratória grave refratária</description>
        <inputEntry id="InputEntry_C1_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text>&amp;lt; 80</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Insuficiência respiratória grave (SpO2 &amp;lt;80%) requer estabilização antes de laringoscopia rígida"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"ELS 2023: SpO2 &amp;lt;80% contraindica procedimento eletivo. Traqueostomia de urgência pode ser necessária"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40202429_1">
        <description>Estridor laríngeo com obstrução significativa</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J38.0", "J38.5", "J38.6", "R06.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text>&amp;gt; 50</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>[80..92]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Estridor com obstrução &amp;gt;50% requer laringoscopia rígida para avaliação e possível intervenção"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021: Laringoscopia rígida é padrão-ouro para avaliação e tratamento de estenose laríngea. SBPT: Permite biópsia e dilatação simultâneas"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202429_2">
        <description>Estenose traqueal pós-intubação</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"J39.8", "J95.5"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>[40..80]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>&amp;gt;= 85</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Estenose traqueal 40-80% requer avaliação e tratamento por broncoscopia rígida"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"SBPT 2024: Broncoscopia rígida permite dilatação, ressecção a laser e colocação de stent em estenoses traqueais"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202429_3">
        <description>Corpo estranho em vias aéreas superiores</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"T17.2", "T17.3", "T17.4"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text>&amp;gt;= 85</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Corpo estranho em laringe/traqueia requer remoção por broncoscopia rígida"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"ANS RN 465/2021: Broncoscopia rígida é método de escolha para remoção de corpos estranhos maiores ou impactados"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202429_4">
        <description>Hemoptise maciça de origem traqueal</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"R04.2", "I97.5"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>&amp;gt;= 85</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Hemoptise maciça requer broncoscopia rígida para controle de sangramento e desobstrução"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"SBPT 2024: Broncoscopia rígida permite aspiração eficaz, tamponamento com balão e aplicação de laser/eletrocautério"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202429_5">
        <description>Tumor de laringe ou traqueia para biópsia</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"C32.0", "C32.1", "C32.2", "C33", "D14.1", "D14.2"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>&amp;gt;= 88</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Tumor de laringe/traqueia requer biópsia por laringoscopia rígida"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"ANS RN 465/2021: Laringoscopia rígida permite biópsias maiores e mais representativas de tumores laringotraqueais"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40202429">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar: TC de pescoço/tórax, laudo ORL/pneumológico, gasometria"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>