<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40201031" name="Broncoscopia com biópsia transbrônquica" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40201031" name="Decisão de Cobertura para Broncoscopia com Biópsia Transbrônquica">
    <decisionTable id="DecisionTable_40201031" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="TC de Tórax com Lesão (mm)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>espirometria</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo Pneumológico Anexado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoPneumologico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40201031_1">
        <description>Hipoxemia grave é contraindicação</description>
        <inputEntry id="InputEntry_C1_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text>&amp;lt; 88</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Hipoxemia grave (SpO2 &amp;lt;88%) contraindica broncoscopia com biópsia por alto risco de complicações"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"BTS 2023: SpO2 &amp;lt;88% mesmo com O2 suplementar é contraindicação relativa. Necessária estabilização prévia"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Contraind_40201031_2">
        <description>Pneumotórax não tratado</description>
        <inputEntry id="InputEntry_C2_1">
          <text>"J93.0", "J93.1", "J93.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C2_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C2_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C2_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C2_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C2_2">
          <text>"Pneumotórax é contraindicação absoluta para biópsia transbrônquica por risco de agravamento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C2_3">
          <text>"SBPT 2024: Biópsia transbrônquica aumenta risco de pneumotórax. Aguardar resolução completa"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40201031_1">
        <description>Doença pulmonar intersticial suspeita</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J84.0", "J84.1", "J84.8", "J84.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>&amp;gt;= 88</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Doença pulmonar intersticial justifica biópsia transbrônquica para diagnóstico histológico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021: Biópsia transbrônquica coberta para diagnóstico de DPI. SBPT: Rendimento de 80-90% quando guiada por TC"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40201031_2">
        <description>Nódulo pulmonar periférico</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"R91.1", "D38.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>[20..40]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>&amp;gt;= 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Nódulo periférico 20-40mm justifica biópsia transbrônquica guiada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"SBPT 2024: Nódulos periféricos 20-40mm têm rendimento diagnóstico de 70% com navegação brônquica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40201031_3">
        <description>Suspeita de rejeição em transplante pulmonar</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"T86.8", "Z94.2"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text>&amp;gt;= 88</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Suspeita de rejeição em transplantado pulmonar requer biópsia para confirmação histológica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"ANS RN 465/2021: Biópsia transbrônquica é padrão-ouro para diagnóstico de rejeição aguda em transplante pulmonar"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40201031_4">
        <description>Infiltrado pulmonar não esclarecido</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"J18.8", "J18.9", "R91.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>&amp;gt;= 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Infiltrado pulmonar persistente sem resposta a antibióticos justifica biópsia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"SBPT 2024: Infiltrado não esclarecido após 2 semanas de tratamento empírico requer investigação histológica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40201031_5">
        <description>Sarcoidose suspeita</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"D86.0", "D86.1", "D86.2", "D86.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>&amp;gt;= 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Sarcoidose suspeita requer biópsia transbrônquica para confirmação com granulomas"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"ANS RN 465/2021: Biópsia transbrônquica tem rendimento de 60-90% no diagnóstico de sarcoidose"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40201031">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar: TC de tórax, laudo pneumológico, coagulograma, gasometria"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>