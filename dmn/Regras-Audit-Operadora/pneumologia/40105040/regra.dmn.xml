<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40105040" name="Medida da difusao de monoxido de carbono (DLCO)" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40105040" name="Decisao Medida da difusao de monoxido de carbono">
    <decisionTable id="DecisionTable_40105040" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="VEF1/CVF">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>vef1CvfRatio</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="TC Tórax com Alterações">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>tcComAlteracoes</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Dessaturação ao Esforço">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>dessaturacaoEsforco</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40105040_1">
        <description>IAM recente é contraindicação</description>
        <inputEntry id="InputEntry_C1_1">
          <text>"I21.0", "I21.1", "I21.2", "I21.3", "I21.4", "I21.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Contraindicação: IAM recente. Aguardar 4 semanas para estabilização"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"ATS/ERS Guidelines: IAM recente é contraindicação relativa. Risco de descompensação cardíaca durante manobra"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40105040_1">
        <description>Doença pulmonar intersticial difusa</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J84.1", "J84.10", "J84.11", "J84.17", "J84.9", "M05.1", "M32.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Doença pulmonar intersticial. DLCO essencial para diagnóstico e monitoramento de gravidade"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ATS/ERS 2005: DLCO é o teste mais sensível para DPI. Redução da DLCO precede alterações espirométricas"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105040_2">
        <description>Enfisema - avaliação de extensão</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"J43.0", "J43.1", "J43.2", "J43.8", "J43.9", "J44.0", "J44.1", "J44.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>&amp;lt; 70</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"DPOC/Enfisema. DLCO avalia extensão de destruição alveolar e prediz dessaturação ao exercício"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"SBPT 2022: DLCO &amp;lt;40% previsto indica enfisema grave. Útil para indicação de oxigenoterapia e avaliação pré-operatória"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105040_3">
        <description>Diferenciação asma vs enfisema</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"J45.0", "J45.1", "J45.8", "J45.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text>&amp;lt; 70</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Asma com obstrução irreversível. DLCO diferencia remodelamento asmático de enfisema associado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"ATS/ERS: DLCO normal/aumentada em asma pura. DLCO reduzida indica componente enfisematoso (overlap syndrome)"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105040_4">
        <description>Hipertensão pulmonar</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"I27.0", "I27.2", "I27.8", "I27.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Hipertensão pulmonar. DLCO avalia gravidade funcional e prognóstico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"SBPT 2022: DLCO &amp;lt;40% em HP indica grave comprometimento capilar pulmonar e pior prognóstico"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105040_5">
        <description>Sarcoidose pulmonar</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"D86.0", "D86.1", "D86.2", "D86.3", "D86.8", "D86.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Sarcoidose com alterações tomográficas. DLCO monitora progressão e resposta ao tratamento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"ATS/ERS: DLCO é o teste mais sensível para detectar comprometimento parenquimatoso em sarcoidose estágios II-IV"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105040_6">
        <description>Tromboembolismo pulmonar crônico</description>
        <inputEntry id="InputEntry_A6_1">
          <text>"I27.82", "I27.9", "I26.0", "I26.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A6_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_2">
          <text>"TEP crônico. DLCO avalia extensão de comprometimento capilar residual"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_3">
          <text>"SBPT 2022: DLCO reduzida indica dano capilar permanente pós-TEP. Auxilia na indicação de tromboendarterectomia"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40105040_7">
        <description>Avaliação pré-quimioterapia com bleomicina</description>
        <inputEntry id="InputEntry_A7_1">
          <text>"C81.0", "C81.1", "C81.2", "C81.3", "C81.4", "C81.7", "C81.9", "Z51.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A7_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A7_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A7_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A7_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A7_2">
          <text>"Avaliação basal pré-bleomicina. DLCO identifica pacientes de risco para toxicidade pulmonar"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A7_3">
          <text>"ATS/ERS: DLCO &amp;lt;60% previsto é contraindicação relativa para bleomicina. Monitoramento essencial durante tratamento"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40105040">
        <description>Regra padrão - Critérios não atendidos</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Documentação insuficiente. Enviar: espirometria completa, TC tórax, teste de caminhada 6 minutos se disponível"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>