<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40302016" name="Gasometria arterial" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40302016" name="Decisão de Cobertura para Gasometria Arterial">
    <decisionTable id="DecisionTable_40302016" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Frequência Respiratória (irpm)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>espirometria</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo Médico Anexado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoPneumologico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Aprovado_40302016_1">
        <description>Insuficiência respiratória aguda</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J96.0", "J96.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text>&amp;gt; 24</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>&amp;lt; 92</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Insuficiência respiratória aguda com taquipneia e dessaturação requer gasometria urgente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021: Gasometria é essencial para diagnóstico e manejo de insuficiência respiratória. SBPT: Determina gravidade e guia oxigenoterapia"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40302016_2">
        <description>Exacerbação grave de DPOC ou asma</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"J44.0", "J44.1", "J45.0", "J45.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>&amp;lt; 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Exacerbação de DPOC/asma com dessaturação requer gasometria para avaliação de retenção de CO2"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"GOLD 2024: Gasometria obrigatória em exacerbações graves para detectar hipercapnia. GINA: Gasometria se SpO2 &amp;lt;92%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40302016_3">
        <description>Monitoramento de ventilação mecânica</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"Z99.1", "J96.1", "J96.2"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Paciente em ventilação mecânica requer gasometria periódica para ajuste de parâmetros"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"AARC 2023: Gasometria arterial é padrão-ouro para monitoramento de VMI. Recomenda-se a cada 4-6h em pacientes críticos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40302016_4">
        <description>Distúrbios ácido-base</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"E87.2", "E87.3", "E87.4"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Distúrbio ácido-base requer gasometria para diagnóstico e classificação"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"ANS RN 465/2021: Gasometria arterial é exame essencial para diagnóstico e tratamento de distúrbios ácido-base"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40302016_5">
        <description>Embolia pulmonar suspeita</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"I26.0", "I26.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text>&amp;gt; 20</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>&amp;lt; 94</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Suspeita de embolia pulmonar com taquipneia e dessaturação requer gasometria"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"SBPT 2024: Gasometria arterial faz parte da avaliação inicial de TEP. Gradiente A-a alveolar-arterial auxilia diagnóstico"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40302016_6">
        <description>Avaliação pré-operatória em pneumopata</description>
        <inputEntry id="InputEntry_A6_1">
          <text>"Z01.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A6_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_2">
          <text>"Avaliação pré-operatória em paciente pneumopata requer gasometria basal"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_3">
          <text>"ANS RN 465/2021: Gasometria pré-operatória é indicada em pneumopatas para estratificação de risco cirúrgico"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40302016">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar: laudo médico com indicação clínica, sinais vitais, SpO2"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>