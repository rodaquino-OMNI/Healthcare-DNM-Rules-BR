<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40202437" name="Laringoscopia/traqueoscopia flexível" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40202437" name="Decisão de Cobertura para Laringoscopia/Traqueoscopia Flexível">
    <decisionTable id="DecisionTable_40202437" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tempo de Sintoma (semanas)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>espirometria</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo ORL Anexado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoPneumologico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40202437_1">
        <description>Hipoxemia grave</description>
        <inputEntry id="InputEntry_C1_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text>&amp;lt; 85</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Hipoxemia grave (SpO2 &amp;lt;85%) contraindica laringoscopia eletiva"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"AAO-HNS 2023: SpO2 &amp;lt;85% requer estabilização antes de procedimentos endoscópicos eletivos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40202437_1">
        <description>Rouquidão persistente &amp;gt;3 semanas</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"R49.0", "J38.0"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text>&amp;gt; 3</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>&amp;gt;= 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Rouquidão persistente &amp;gt;3 semanas requer laringoscopia para exclusão de neoplasia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021: Rouquidão &amp;gt;3 semanas é sinal de alerta para câncer de laringe. AAO-HNS: Laringoscopia obrigatória"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202437_2">
        <description>Disfagia persistente com perda de peso</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"R13.1", "R13.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>&amp;gt; 4</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>&amp;gt;= 88</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Disfagia persistente &amp;gt;4 semanas requer avaliação laríngea e faríngea"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"SBPT 2024: Disfagia crônica pode indicar neoplasia de faringe/laringe ou disfunção neurológica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202437_3">
        <description>Suspeita de paralisia de prega vocal</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"J38.0", "J38.2", "R49.0"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text>&amp;gt;= 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Suspeita de paralisia de prega vocal requer laringoscopia para confirmação e investigação etiológica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"ANS RN 465/2021: Laringoscopia é padrão-ouro para diagnóstico de paralisia vocal. Permite videoestroboscopia"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202437_4">
        <description>Avaliação pré-operatória para via aérea difícil</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"Z01.8", "Z01.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>&amp;gt;= 92</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Avaliação pré-operatória de via aérea difícil por laringoscopia flexível"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"SBPT 2024: Laringoscopia pré-operatória previne complicações em pacientes com fatores de risco para via aérea difícil"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202437_5">
        <description>Suspeita de lesão laríngea em tabagista</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"D14.1", "C32.0", "C32.1", "C32.2", "C32.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>&amp;gt;= 88</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Suspeita de lesão laríngea requer laringoscopia com possível biópsia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"ANS RN 465/2021: Laringoscopia flexível permite visualização e biópsia de lesões suspeitas. Sensibilidade &amp;gt;90%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40202437">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar: laudo ORL detalhado, tempo de sintomas, fatores de risco"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>