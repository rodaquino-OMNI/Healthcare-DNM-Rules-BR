<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40202054" name="Broncoscopia com biópsia transbrônquica guiada por fluoroscopia" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40202054" name="Decisão de Cobertura para Broncoscopia com Biópsia Guiada por Fluoroscopia">
    <decisionTable id="DecisionTable_40202054" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tamanho da Lesão (mm)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>espirometria</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="TC de Tórax Anexada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoPneumologico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40202054_1">
        <description>Hipoxemia grave contraindica procedimento</description>
        <inputEntry id="InputEntry_C1_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text>&amp;lt; 85</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Hipoxemia grave (SpO2 &amp;lt;85%) contraindica biópsia guiada por alto risco"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"CHEST 2023: SpO2 &amp;lt;85% é contraindicação para procedimentos broncoscópicos invasivos. Estabilização prévia necessária"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40202054_1">
        <description>Nódulo pulmonar periférico maior que 20mm</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"R91.1", "D38.1", "C34.1", "C34.3"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text>&amp;gt; 20</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>&amp;gt;= 88</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Nódulo periférico &amp;gt;20mm justifica biópsia guiada por fluoroscopia para aumento de rendimento diagnóstico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021: Navegação brônquica aumenta acurácia em lesões periféricas. SBPT 2024: Rendimento 65-85% vs 35-50% sem navegação"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202054_2">
        <description>Massa mediastinal com extensão brônquica</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"C38.1", "C38.2", "C38.3", "D15.2"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>&amp;gt;= 30</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>&amp;gt;= 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Massa mediastinal com extensão brônquica requer biópsia guiada por fluoroscopia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"SBPT 2024: Fluoroscopia permite biópsia de lesões mediastinais com extensão brônquica, evitando mediastinoscopia"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202054_3">
        <description>Linfadenopatia hilar/mediastinal suspeita de sarcoidose</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"D86.0", "D86.1", "D86.2", "R59.0"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text>&amp;gt;= 15</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text>&amp;gt;= 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Linfadenopatia hilar/mediastinal requer biópsia guiada para confirmação de sarcoidose"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"ANS RN 465/2021: EBUS/fluoroscopia têm rendimento de 80-90% no diagnóstico de sarcoidose estágio I-II"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40202054_4">
        <description>Lesão pulmonar de localização complexa</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"C34.8", "C34.9", "R91.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text>[15..50]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>&amp;gt;= 88</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Lesão de localização complexa 15-50mm requer navegação por fluoroscopia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"CHEST 2023: Fluoroscopia aumenta taxa de sucesso em lesões de difícil acesso ou múltiplos lobos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40202054">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar: TC de tórax com medidas, laudo pneumológico, coagulograma"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>