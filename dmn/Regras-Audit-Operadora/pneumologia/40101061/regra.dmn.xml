<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40101061" name="Ergoespirometria ou teste cardiopulmonar de exercício completo" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40101061" name="Análise de Indicação para Ergoespirometria">
    <decisionTable id="DecisionTable_40101061" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="VEF1 (% previsto)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>vef1Percent</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dispneia aos Esforços">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>dispneiaEsforcos</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo Cardiológico Normal">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoCardioNormal</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_40101061_1">
        <description>IAM recente é contraindicação absoluta</description>
        <inputEntry id="InputEntry_C1_1">
          <text>"I21.0", "I21.1", "I21.2", "I21.3", "I21.4", "I21.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Contraindicação absoluta: IAM recente. Aguardar estabilização clínica por no mínimo 5 dias"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"ATS/ACCP Guidelines 2003: IAM recente é contraindicação absoluta para teste cardiopulmonar"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      <rule id="Rule_Contraind_40101061_2">
        <description>Insuficiência cardíaca descompensada</description>
        <inputEntry id="InputEntry_C2_1">
          <text>"I50.0", "I50.1", "I50.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C2_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C2_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C2_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_C2_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C2_2">
          <text>"Insuficiência cardíaca descompensada. Necessário compensação antes do teste"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C2_3">
          <text>"European Respiratory Journal 2007: IC descompensada é contraindicação relativa. Exame deve ser postergado"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_40101061_1">
        <description>Investigação de broncoespasmo induzido por exercício</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J45.0", "J45.1", "J45.8", "J45.9", "R06.0"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text>&amp;gt;= 70</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Indicação para investigação de broncoespasmo induzido por exercício (BIE)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ATS/ACCP 2003: Ergoespirometria é padrão-ouro para diagnóstico de BIE em pacientes com espirometria basal normal"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40101061_2">
        <description>Dispneia de etiologia indeterminada</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"R06.0", "R06.8"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>&amp;gt;= 80</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Dispneia aos esforços com provas cardíaca e pulmonar normais em repouso. Indicação para diferenciação de etiologia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"European Respiratory Journal 2007: Teste cardiopulmonar é indicado quando dispneia permanece inexplicada após avaliações básicas"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40101061_3">
        <description>Avaliação pré-operatória para cirurgia torácica</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"C34.0", "C34.1", "C34.2", "C34.3", "C34.8", "C34.9", "Z51.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text>[50..79]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Avaliação funcional pré-operatória para ressecção pulmonar. VEF1 em zona limítrofe"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"SBPT - Diretrizes para avaliação pré-operatória: VO2máx &amp;lt;10mL/kg/min prediz alto risco. Teste indicado para VEF1 50-79%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40101061_4">
        <description>DPOC com dispneia desproporcional</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"J44.0", "J44.1", "J44.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text>[40..70]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"DPOC com dispneia desproporcional ao grau de obstrução. Investigação de limitação cardíaca associada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"ATS/ACCP 2003: Ergoespirometria identifica limitação ventilatória vs cardiovascular em DPOC. Auxilia planejamento reabilitação"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40101061_5">
        <description>Doença pulmonar intersticial</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"J84.1", "J84.9", "M05.1"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text>&amp;lt; 80</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Doença pulmonar intersticial com limitação funcional. Avaliação de capacidade de exercício e dessaturação"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"SBPT - Diretrizes DPI: Teste cardiopulmonar avalia gravidade funcional e detecta dessaturação ao esforço"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_40101061_6">
        <description>Hipertensão pulmonar suspeita ou confirmada</description>
        <inputEntry id="InputEntry_A6_1">
          <text>"I27.0", "I27.2", "I27.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A6_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_2">
          <text>"Hipertensão pulmonar. Avaliação de capacidade funcional e resposta ao esforço"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_3">
          <text>"European Respiratory Journal 2007: Teste cardiopulmonar é essencial para avaliar gravidade e prognóstico em HP"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_40101061">
        <description>Regra padrão - Critérios não atendidos</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Favor enviar justificativa clínica detalhada: espirometria completa, ECG, ecocardiograma e descrição da sintomatologia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>