<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_20104235" name="Terapia inalatória - por nebulização" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_20104235" name="Decisão de Cobertura para Terapia Inalatória por Nebulização">
    <decisionTable id="DecisionTable_20104235" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnóstico Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Peak Flow (% previsto)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>peakFlowPercent</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="SpO2 (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>satO2</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Frequência Respiratória (irpm)">
        <inputExpression id="InputExpression_4" typeRef="integer">
          <text>frequenciaRespiratoria</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" typeRef="string" />
      <output id="Output_2" label="Observação" typeRef="string" />
      <output id="Output_3" label="Fundamentação" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraind_20104235_1">
        <description>Pneumotórax não tratado é contraindicação absoluta</description>
        <inputEntry id="InputEntry_C1_1">
          <text>"J93.0", "J93.1", "J93.8", "J93.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Negado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Pneumotórax é contraindicação para nebulização por risco de complicação"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"SBPT - Consenso Brasileiro de DPOC. Contraindicação: pneumotórax não drenado"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_20104235_1">
        <description>Exacerbação grave de asma</description>
        <inputEntry id="InputEntry_A1_1">
          <text>"J45.0", "J45.1", "J45.8", "J45.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text>&amp;lt; 50</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>&amp;lt; 92</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>&amp;gt; 25</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Exacerbação grave de asma com broncoespasmo documentado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"GINA 2024: Peak flow &amp;lt;50% indica exacerbação grave. SpO2 &amp;lt;92% indica hipoxemia. Nebulização com broncodilatador é primeira linha"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_20104235_2">
        <description>Exacerbação moderada de asma</description>
        <inputEntry id="InputEntry_A2_1">
          <text>"J45.0", "J45.1", "J45.8", "J45.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>[50..80]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>[90..94]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>&amp;gt; 20</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Exacerbação moderada de asma com indicação de nebulização"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"GINA 2024: Peak flow 50-80% indica exacerbação moderada. Nebulização com beta-2 agonista e anticolinérgico recomendada"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_20104235_3">
        <description>Exacerbação grave de DPOC</description>
        <inputEntry id="InputEntry_A3_1">
          <text>"J44.0", "J44.1", "J44.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A3_3">
          <text>&amp;lt; 90</text>
        </inputEntry>
        <inputEntry id="InputEntry_A3_4">
          <text>&amp;gt; 24</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_2">
          <text>"Exacerbação grave de DPOC com hipoxemia e taquipneia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A3_3">
          <text>"GOLD 2024: SpO2 &amp;lt;90% + FR &amp;gt;24 indicam exacerbação grave. Nebulização com broncodilatadores é tratamento de primeira linha"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_20104235_4">
        <description>Exacerbação moderada de DPOC</description>
        <inputEntry id="InputEntry_A4_1">
          <text>"J44.0", "J44.1", "J44.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A4_3">
          <text>[88..93]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A4_4">
          <text>[20..24]</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_2">
          <text>"Exacerbação moderada de DPOC com indicação de broncodilatação"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A4_3">
          <text>"GOLD 2024: Critérios para exacerbação moderada. Nebulização previne progressão para insuficiência respiratória"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_20104235_5">
        <description>Bronquite aguda com broncoespasmo</description>
        <inputEntry id="InputEntry_A5_1">
          <text>"J20.0", "J20.8", "J20.9", "J40"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A5_3">
          <text>&amp;lt; 94</text>
        </inputEntry>
        <inputEntry id="InputEntry_A5_4">
          <text>&amp;gt; 20</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A5_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_2">
          <text>"Bronquite aguda com componente obstrutivo e dessaturação"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A5_3">
          <text>"SBPT - Diretrizes de bronquite aguda: SpO2 &amp;lt;94% indica comprometimento respiratório. Nebulização alivia broncoespasmo"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_20104235_6">
        <description>Bronquiolite em adulto</description>
        <inputEntry id="InputEntry_A6_1">
          <text>"J21.0", "J21.8", "J21.9"</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_A6_3">
          <text>&amp;lt; 92</text>
        </inputEntry>
        <inputEntry id="InputEntry_A6_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_A6_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_2">
          <text>"Bronquiolite com hipoxemia documentada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A6_3">
          <text>"ANS RN 465/2021: Terapia inalatória indicada para bronquiolite com SpO2 &amp;lt;92%"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Fallback_20104235">
        <description>Regra padrão - Critérios não atendidos ou dados insuficientes</description>
        <inputEntry id="InputEntry_Default_1">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_2">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_3">
          <text />
        </inputEntry>
        <inputEntry id="InputEntry_Default_4">
          <text />
        </inputEntry>
        <outputEntry id="OutputEntry_Default_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_2">
          <text>"Avaliação médica adicional necessária. Favor enviar documentação clínica complementar: Peak flow, gasometria ou espirometria"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_Default_3">
          <text>"ANS RN 465/2021 Art. 4º - Documentação clínica insuficiente para autorização"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>