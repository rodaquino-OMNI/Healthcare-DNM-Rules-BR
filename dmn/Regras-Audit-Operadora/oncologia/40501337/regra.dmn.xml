        <outputEntry id="OutputEntry_fundamentacao_rel">
          <text>"MURANO: PFS 85% vs 36% aos 2 anos; OS superior. Duracao fixa de 24 meses"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_rel">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_rel">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
