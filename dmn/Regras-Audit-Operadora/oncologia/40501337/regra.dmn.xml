          <text>"venetoclax_rituximab","ven_rit"</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_rel">
          <text>>= 2</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_rel">
          <text>-</text>
        </inputEntry>
