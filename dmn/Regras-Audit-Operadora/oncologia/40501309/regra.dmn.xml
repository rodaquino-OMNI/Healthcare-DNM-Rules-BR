<?xml version="1.0" encoding="UTF-8"?>
<!-- Versao: 3.1.0-LEAN-TIER2 (2026-02-08) -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_ONQ_BIO_015" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_ONQ_BIO_015" name="ONQ-BIO-015: FLT3 for AML [LEAN TIER-2]">
    <decisionTable id="DecisionTable_ONQ_BIO_015" hitPolicy="FIRST">
      <input id="Input_diagnosis" label="Diagnosis">
        <inputExpression id="InputExpression_diagnosis" typeRef="string">
          <text>diagnosis</text>
        </inputExpression>
      </input>
      <input id="Input_age" label="Age">
        <inputExpression id="InputExpression_age" typeRef="number">
          <text>age</text>
        </inputExpression>
      </input>
      <input id="Input_mutation_type" label="Mutation Type">
        <inputExpression id="InputExpression_mutation_type" typeRef="string">
          <text>mutationType</text>
        </inputExpression>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="number">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_reject_non_aml">
        <inputEntry id="InputEntry_diagnosis_reject">
          <text>not("AML")</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_mutation_type_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_non_aml_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_reject">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_reject">
          <text>"Teste FLT3 indicado apenas para LMA conforme diretrizes ELN 2022"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_reject">
          <text>"Mutacoes FLT3 ocorrem em 25-30% de LMA; raras/nao-actionaveis em outras leucemias conforme ELN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_reject_alertasDesperdicio">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OE_reject_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_40501309_DUP_reteste">
        <description>DUP - Reteste molecular em menos de 90 dias sem justificativa</description>
        <inputEntry id="IE_DUP_diagnosis">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_age">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_mutation">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_ultimo">
          <text>&lt; 90</text>
        </inputEntry>
        <outputEntry id="OE_DUP_resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_observacao">
          <text>"Teste molecular repetido em menos de 90 dias"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_fundamentacao">
          <text>"NCCN 2024: Reteste molecular so indicado se progressao ou mudanca terapeutica"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_alertasDesperdicio">
          <text>"DUP"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_aml_all">
        <inputEntry id="InputEntry_diagnosis_aml">
          <text>"AML"</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_aml">
          <text>&gt;= 18</text>
        </inputEntry>
        <inputEntry id="InputEntry_mutation_type_aml">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_aml_all_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_aml">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_aml">
          <text>"Teste FLT3-ITD e FLT3-TKD aprovado para LMA recem-diagnosticada (biomarcador prognostico + terapeutico)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_aml">
          <text>"Razao aleica alta de FLT3-ITD prediz prognostico ruim; orienta adicao de midostaurin conforme estudo RATIFY (beneficio OS 18m) 2024"</text>
        </outputEntry>
        <outputEntry id="OE_aml_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_aml_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_flt3itd_frontline">
        <inputEntry id="InputEntry_diagnosis_frontline">
          <text>"AML"</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_frontline">
          <text>[18..60]</text>
        </inputEntry>
        <inputEntry id="InputEntry_mutation_type_frontline">
          <text>"FLT3-ITD"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_flt3itd_frontline_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_frontline">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_frontline">
          <text>"FLT3-ITD detectado. Midostaurin + quimio aprovado para pacientes elegivel conforme protocolo RATIFY"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_frontline">
          <text>"Midostaurin adicionado a inducao/consolidacao 7+3 melhora SO de 74.7 vs 25.6 meses conforme estudo RATIFY 2024"</text>
        </outputEntry>
        <outputEntry id="OE_frontline_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_frontline_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_relapsed">
        <inputEntry id="InputEntry_diagnosis_relapsed">
          <text>"AML"</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_relapsed">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_mutation_type_relapsed">
          <text>"FLT3-ITD","FLT3-TKD"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_relapsed_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_relapsed">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_relapsed">
          <text>"LMA mutante para FLT3 recidivada/refrataria. Gilteritinib monoterapia aprovada pela FDA"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_relapsed">
          <text>"Gilteritinib mostra taxa de RC de 21.1% vs 10.6% com quimioterapia; SO media 9.3 vs 5.6 meses conforme estudo ADMIRAL 2024"</text>
        </outputEntry>
        <outputEntry id="OE_relapsed_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_relapsed_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_pending">
        <inputEntry id="InputEntry_diagnosis_pending">
          <text>"AML"</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_mutation_type_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_pending_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_pending">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_pending">
          <text>"Teste FLT3 deve ser parte da avaliacao diagnostica de LMA. Solicitar analise de FLT3-ITD/TKD"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_pending">
          <text>"Retorno rapido (&lt;48h) recomendado conforme ELN 2022 para orientar selecao de terapia de primeira linha conforme NCCN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_pending_alertasDesperdicio">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OE_pending_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_fallback">
        <inputEntry id="InputEntry_diagnosis_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_mutation_type_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_fallback_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Consulta de hematologia-oncologia requerida para indicacao de teste FLT3"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Teste FLT3 integral a analise molecular de LMA conforme diretrizes ELN/NCCN/SBHH 2024"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ONQ_BIO_015">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
