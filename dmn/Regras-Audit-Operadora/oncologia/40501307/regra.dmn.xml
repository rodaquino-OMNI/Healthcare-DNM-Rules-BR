<?xml version="1.0" encoding="UTF-8"?>
<!-- Versao: 3.1.0-LEAN-TIER2 (2026-02-08) -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_ONQ_BIO_013" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_ONQ_BIO_013" name="ONQ-BIO-013: FGFR Testing for Bladder Cancer [LEAN TIER-2]">
    <decisionTable id="DecisionTable_ONQ_BIO_013" hitPolicy="FIRST">
      <input id="Input_tumor_type" label="Tumor Type">
        <inputExpression id="InputExpression_tumor_type" typeRef="string">
          <text>tumorType</text>
        </inputExpression>
      </input>
      <input id="Input_stage" label="Stage">
        <inputExpression id="InputExpression_stage" typeRef="string">
          <text>stage</text>
        </inputExpression>
      </input>
      <input id="Input_prior_treatment" label="Prior Treatment">
        <inputExpression id="InputExpression_prior_treatment" typeRef="string">
          <text>priorTreatment</text>
        </inputExpression>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="number">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_reject_non_bladder">
        <inputEntry id="InputEntry_tumor_type_reject">
          <text>not("urothelial_bladder","cholangiocarcinoma")</text>
        </inputEntry>
        <inputEntry id="InputEntry_stage_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_prior_treatment_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_non_bladder_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_reject">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_reject">
          <text>"Teste FGFR nao aprovado pela FDA para este tipo de tumor"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_reject">
          <text>"Erdafitinib aprovado apenas para cancer urotelial alterado para FGFR2/3 ou colangiocarcinoma com fusao FGFR2 conforme FDA 2024"</text>
        </outputEntry>
        <outputEntry id="OE_reject_alertasDesperdicio">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OE_reject_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_40501307_DUP_reteste">
        <description>DUP - Reteste molecular em menos de 90 dias sem justificativa</description>
        <inputEntry id="IE_DUP_tumor">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_stage">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_treatment">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_ultimo">
          <text>&lt; 90</text>
        </inputEntry>
        <outputEntry id="OE_DUP_resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_observacao">
          <text>"Teste molecular repetido em menos de 90 dias"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_fundamentacao">
          <text>"NCCN 2024: Reteste molecular so indicado se progressao ou mudanca terapeutica"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_alertasDesperdicio">
          <text>"DUP"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_bladder">
        <inputEntry id="InputEntry_tumor_type_bladder">
          <text>"urothelial_bladder"</text>
        </inputEntry>
        <inputEntry id="InputEntry_stage_bladder">
          <text>"locally_advanced","metastatic"</text>
        </inputEntry>
        <inputEntry id="InputEntry_prior_treatment_bladder">
          <text>"platinum_failed"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_bladder_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_bladder">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_bladder">
          <text>"Teste de mutacao/fusao FGFR2/3 aprovado para cancer urotelial refratario a platina (taxa de alteracao 15-20%)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_bladder">
          <text>"Erdafitinib mostra ORR de 40% em cancer urotelial alterado para FGFR conforme estudo BLC2001 2024"</text>
        </outputEntry>
        <outputEntry id="OE_bladder_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_bladder_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_cholangiocarcinoma">
        <inputEntry id="InputEntry_tumor_type_cholangio">
          <text>"cholangiocarcinoma"</text>
        </inputEntry>
        <inputEntry id="InputEntry_stage_cholangio">
          <text>"advanced","metastatic"</text>
        </inputEntry>
        <inputEntry id="InputEntry_prior_treatment_cholangio">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_cholangiocarcinoma_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_cholangio">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_cholangio">
          <text>"Teste de fusao/rearranjo FGFR2 aprovado para colangiocarcinoma (taxa de fusao 10-16%)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_cholangio">
          <text>"Pemigatinib aprovado pela FDA para colangiocarcinoma FGFR2+; ORR de 35.5% conforme estudo FIGHT-202 2024"</text>
        </outputEntry>
        <outputEntry id="OE_cholangio_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_cholangio_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_pending_early_line">
        <inputEntry id="InputEntry_tumor_type_pending">
          <text>"urothelial_bladder"</text>
        </inputEntry>
        <inputEntry id="InputEntry_stage_pending">
          <text>"locally_advanced","metastatic"</text>
        </inputEntry>
        <inputEntry id="InputEntry_prior_treatment_pending">
          <text>not("platinum_failed")</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_pending_early_line_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_pending">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_pending">
          <text>"Teste FGFR indicado apos falha com platina conforme rotulo FDA. Considerar teste precoce para planejamento terapeutico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_pending">
          <text>"NGS upfront pode identificar alteracoes FGFR para terapia futura conforme teste discricionario NCCN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_pending_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_pending_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_fallback">
        <inputEntry id="InputEntry_tumor_type_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_stage_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_prior_treatment_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_fallback_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Tipo de tumor, estadio e historico de tratamento previo requeridos"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Revisao por comite molecular de tumores para apropriacao do teste FGFR conforme NCCN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ONQ_BIO_013">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
