<?xml version="1.0" encoding="UTF-8"?>
<!--
  TUSS: 40314103 - Pesquisa de Mutação RAS (KRAS/NRAS) por PCR/NGS

  INDICAÇÕES CLÍNICAS:
  - Adenocarcinoma colorretal metastático (CCRm)
  - Antes de iniciar terapia anti-EGFR (cetuximabe, panitumumabe)
  - Estadio IV ou recidiva metastática irressecável
  - Qualquer linha de tratamento onde anti-EGFR seja considerado

  CONTRAINDICAÇÕES:
  - Tumores colorretais não adenocarcinoma (raro)
  - Doença localizada ressecável (estadio I-III operável)
  - Performance status ECOG 4 (cuidados paliativos exclusivos)
  - Pacientes sem perspectiva de uso de anti-EGFR por comorbidades graves

  FUNDAMENTAÇÃO TÉCNICA:
  - Mutações RAS (KRAS exons 2,3,4 + NRAS exons 2,3,4) ocorrem em ~55% CCR
  - RAS mutado = resistência primária a anti-EGFR (sem benefício clínico)
  - RAS selvagem = candidato a cetuximabe/panitumumabe
  - Teste obrigatório antes de qualquer linha com anti-EGFR

  REFERÊNCIAS:
  - SBOC Diretrizes Câncer Colorretal 2024
  - NCCN Colon Cancer Guidelines v1.2024
  - ESMO Molecular Testing in Colorectal Cancer 2024
  - FDA Label Cetuximab/Panitumumab 2024
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_RAS_PCR_NGS" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.20.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.4.0">
  <decision id="Decision_RAS_40314103" name="Autorização RAS PCR/NGS [LEAN TIER-2]">
    <decisionTable id="DecisionTable_RAS_40314103" hitPolicy="FIRST">
      <input id="Input_diagnostico_RAS" label="Diagnóstico Oncológico">
        <inputExpression id="InputExpression_diagnostico_RAS" typeRef="string">
          <text>diagnosticoOncologico</text>
        </inputExpression>
        <inputValues id="UnaryTests_diagnostico_RAS">
          <text>"C18.0","C18.1","C18.2","C18.3","C18.4","C18.5","C18.6","C18.7","C18.8","C18.9","C19","C20"</text>
        </inputValues>
      </input>
      <input id="Input_estadiamento_RAS" label="Estadiamento TNM">
        <inputExpression id="InputExpression_estadiamento_RAS" typeRef="string">
          <text>estadiamentoTNM</text>
        </inputExpression>
        <inputValues id="UnaryTests_estadiamento_RAS">
          <text>"T0","T1","T2","T3","T4","Tis","TX"</text>
        </inputValues>
      </input>
      <input id="Input_ecog_RAS" label="Performance Status ECOG">
        <inputExpression id="InputExpression_ecog_RAS" typeRef="string">
          <text>performanceStatusECOG</text>
        </inputExpression>
        <inputValues id="UnaryTests_ecog_RAS">
          <text>"0","1","2","3","4"</text>
        </inputValues>
      </input>
      <input id="Input_linhas_RAS" label="Linhas Tratamento Prévias">
        <inputExpression id="InputExpression_linhas_RAS" typeRef="string">
          <text>linhasTratamentoPrevias</text>
        </inputExpression>
        <inputValues id="UnaryTests_linhas_RAS">
          <text>"0","1","2","3+"</text>
        </inputValues>
      </input>
      <input id="Input_protocolo_RAS" label="Protocolo Terapêutico Definido">
        <inputExpression id="InputExpression_protocolo_RAS" typeRef="boolean">
          <text>protocoloTerapeutico</text>
        </inputExpression>
      </input>
      <!-- Input: Dias desde ultimo procedimento (DUP detection) -->
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="number">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>

      <output id="Output_resultado_RAS" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_observacao_RAS" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao_RAS" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio_RAS" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio_RAS">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada_RAS" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada_RAS">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      

      <!-- WASTE DETECTION: DUP -->
      <rule id="Rule_40314103_DUP">
        <description>DUP - Reteste RAS em menos de 90 dias. Resultado anterior ainda valido</description>
        <inputEntry id="IE_DUP_40314103_1"><text>-</text></inputEntry>
        <inputEntry id="IE_DUP_40314103_2"><text>-</text></inputEntry>
        <inputEntry id="IE_DUP_40314103_3"><text>-</text></inputEntry>
        <inputEntry id="IE_DUP_40314103_4"><text>-</text></inputEntry>
        <inputEntry id="IE_DUP_40314103_5"><text>-</text></inputEntry>
        <inputEntry id="IE_DUP_40314103_ultimo"><text>&lt; 90</text></inputEntry>
        <outputEntry id="OE_DUP_40314103_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_DUP_40314103_observacao"><text>"Reteste RAS em menos de 90 dias. Resultado anterior ainda valido"</text></outputEntry>
        <outputEntry id="OE_DUP_40314103_fundamentacao"><text>"NCCN 2024: Reteste RAS se progressao ou conversao pos-anti-EGFR"</text></outputEntry>
        <outputEntry id="OE_DUP_40314103_alertasDesperdicio"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_DUP_40314103_acaoRecomendada"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>
<rule id="Rule_Contraind_40314103_1">
