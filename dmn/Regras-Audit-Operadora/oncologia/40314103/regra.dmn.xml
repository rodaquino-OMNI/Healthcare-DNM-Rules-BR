        <outputEntry id="OE_contraind_1_alertas">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OE_contraind_1_acao">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_Contraind_40314103_2">
        <description>Diagnóstico fora de câncer colorretal</description>
