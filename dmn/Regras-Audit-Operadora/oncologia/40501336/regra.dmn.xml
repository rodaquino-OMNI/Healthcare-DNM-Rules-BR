<?xml version="1.0" encoding="UTF-8"?>
<!--
  ONQ-DRUG-011: Ibrutinib for CLL/MCL
  TUSS: 40501336
  Versao: 3.1.0-LEAN-TIER2 (2026-02-08)
  Especialidade: Oncologia

  ============================================================================
  INDICACOES CLINICAS:
  ============================================================================
  - LLC: 1L ou recidivada (RESONATE, RESONATE-2)
  - Linfoma do manto: recidivado pos-1 terapia
  - del(17p)/TP53: preferencia sobre quimioimunoterapia
  - Fibrilacao atrial: contraindicacao relativa (acalabrutinibe preferido)

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - RESONATE (CLL relapsed)
  - RESONATE-2 (CLL 1L elderly)
  - Wang et al MCL (Lancet)
  - NCCN CLL/SLL 2024, NCCN MCL 2024
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_ONQ_DRUG_011" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_ONQ_DRUG_011" name="ONQ-DRUG-011: Ibrutinib for CLL/MCL [LEAN TIER-2]">
    <decisionTable id="DecisionTable_ONQ_DRUG_011" hitPolicy="FIRST">
      <input id="Input_disease" label="Disease">
        <inputExpression id="InputExpression_disease" typeRef="string">
          <text>disease</text>
        </inputExpression>
      </input>
      <input id="Input_del17p_tp53" label="del(17p)/TP53 Mutation">
        <inputExpression id="InputExpression_del17p_tp53" typeRef="boolean">
          <text>del17pOrTp53</text>
        </inputExpression>
      </input>
      <input id="Input_atrial_fib" label="History of Atrial Fibrillation">
        <inputExpression id="InputExpression_atrial_fib" typeRef="boolean">
          <text>atrialFibrillation</text>
        </inputExpression>
      </input>
      <input id="Input_line" label="Treatment Line">
        <inputExpression id="InputExpression_line" typeRef="number">
          <text>treatmentLine</text>
        </inputExpression>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="number">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_reject_non_approved">
        <description>Reject non-approved diseases</description>
        <inputEntry id="InputEntry_disease_non">
          <text>not("CLL","SLL","MCL","mantle_cell_lymphoma")</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_non">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_non">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_non">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_non_approved_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_non">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_non">
          <text>"Ibrutinibe: apenas LLC/LLP ou linfoma do manto"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_non">
          <text>"Indicacoes aprovadas: RESONATE (LLC) e Wang (MCL). Outras neoplasias sem evidencia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_non">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_non">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_pending_afib">
        <description>Pending if atrial fibrillation - prefer acalabrutinib</description>
        <inputEntry id="InputEntry_disease_afib">
          <text>"CLL","SLL","MCL","mantle_cell_lymphoma"</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_afib">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_afib">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_afib">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_pending_afib_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_afib">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_afib">
          <text>"FA previa: ibrutinibe aumenta risco de arritmia. Considerar acalabrutinibe ou zanubrutinibe"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_afib">
          <text>"Meta-analise: ibrutinibe 6-16% FA vs 3-4% com BTKi 2a geracao; cardiologia antes de iniciar"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_afib">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_afib">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <!-- WASTE DETECTION: FREQ - Dispensacao excessiva medicamento oral -->
      <rule id="Rule_40501336_FREQ_dispensacao">
        <description>FREQ - Verificar frequencia dispensacao ibrutinibe oral</description>
        <inputEntry id="InputEntry_disease_FREQ">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_FREQ">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_FREQ">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_FREQ">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Rule_40501336_FREQ_dispensacao_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_FREQ">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_FREQ">
          <text>"Verificar dispensacao: ibrutinibe oral uso continuo, ciclo 28 dias. Dispensacao antecipada requer justificativa"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_FREQ">
          <text>"ANS RN 465/2021, SBOC 2023: Dispensacao oral oncologico ciclo 28 dias, max 3 ciclos por autorizacao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_FREQ">
          <text>"FREQ"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_FREQ">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <!-- WASTE DETECTION: ESC - Escalonamento inadequado -->
      <rule id="Rule_40501336_ESC_step">
        <description>ESC - Documentar falha quimioimunoterapia antes de ibrutinibe</description>
        <inputEntry id="InputEntry_disease_ESC">
          <text>"CLL","SLL","MCL","mantle_cell_lymphoma"</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_ESC">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_ESC">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_ESC">
          <text>1</text>
        </inputEntry>
        <inputEntry id="IE_Rule_40501336_ESC_step_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_ESC">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_ESC">
          <text>"LLC/LCM 1L sem del17p: documentar tentativa/contraindicacao quimioimunoterapia antes de ibrutinibe"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_ESC">
          <text>"NCCN 2024: Step therapy LLC/LCM - avaliar quimio first se sem del17p/TP53 ou comorbidades"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_ESC">
          <text>"ESC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_ESC">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_cll_del17p">
        <description>Approve CLL with del(17p)/TP53</description>
        <inputEntry id="InputEntry_disease_del17p">
          <text>"CLL","SLL"</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_del17p">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_del17p">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_del17p">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_cll_del17p_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_del17p">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_del17p">
          <text>"Ibrutinibe aprovado: LLC del(17p)/TP53 - preferencia sobre quimioimunoterapia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_del17p">
          <text>"RESONATE-17: ORR 83% em del17p; quimio ineficaz neste subgrupo. Tratar ate progressao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_del17p">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_del17p">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_cll_1l">
        <description>Approve CLL first-line</description>
        <inputEntry id="InputEntry_disease_1l">
          <text>"CLL","SLL"</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_1l">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_1l">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_1l">
          <text>1</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_cll_1l_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_1l">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1l">
          <text>"Ibrutinibe aprovado: LLC 1L. Considerar combinacao com obinutuzumabe ou venetoclax"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1l">
          <text>"RESONATE-2: PFS 89% vs 52% (clorambucil) aos 18m. Idem FCR em jovens mas menos toxico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_1l">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_1l">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_cll_relapsed">
        <description>Approve CLL relapsed</description>
        <inputEntry id="InputEntry_disease_rel">
          <text>"CLL","SLL"</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_rel">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_rel">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_rel">
          <text>>= 2</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_cll_relapsed_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_rel">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_rel">
          <text>"Ibrutinibe aprovado: LLC recidivada/refrataria"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_rel">
          <text>"RESONATE: PFS NR vs 8.1m (ofatumumabe); OS beneficio confirmado em longo prazo"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_rel">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_rel">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_mcl">
        <description>Approve MCL relapsed</description>
        <inputEntry id="InputEntry_disease_mcl">
          <text>"MCL","mantle_cell_lymphoma"</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_mcl">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_mcl">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_mcl">
          <text>>= 2</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_mcl_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_mcl">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_mcl">
          <text>"Ibrutinibe aprovado: linfoma do manto recidivado pos-1 terapia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_mcl">
          <text>"Wang et al: ORR 68%, CR 21%. Mediana PFS 13.9m em MCL recidivado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_mcl">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_mcl">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_reject_mcl_1l">
        <description>Reject MCL first-line monotherapy</description>
        <inputEntry id="InputEntry_disease_mcl1l">
          <text>"MCL","mantle_cell_lymphoma"</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_mcl1l">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_mcl1l">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_mcl1l">
          <text>1</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_mcl_1l_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_mcl1l">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_mcl1l">
          <text>"MCL 1L: ibrutinibe em combinacao com rituximabe; monoterapia reservada para recidiva"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_mcl1l">
          <text>"SHINE: ibrutinibe+BR superior a BR em 1L; monoterapia aprovada apenas para recidiva"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_mcl1l">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_mcl1l">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_fallback">
        <description>Fallback rule</description>
        <inputEntry id="InputEntry_disease_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_del17p_tp53_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_atrial_fib_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_fallback_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Dados insuficientes para avaliacao de ibrutinibe"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Necessario: doenca, status del17p/TP53, historia de FA, linha de tratamento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_fallback">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_fallback">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
