<?xml version="1.0" encoding="UTF-8"?>
<!--
  ONQ-DRUG-009: Niraparib Eligibility Criteria
  TUSS: 40501334
  Versao: 3.1.0-LEAN-TIER2 (2026-02-08)
  Especialidade: Oncologia

  ============================================================================
  INDICACOES CLINICAS:
  ============================================================================
  - Ovario/trompa/peritonio: manutencao 1L (PRIMA) ou recorrente platina-sensivel
  - HRD+ ou BRCA+: beneficio maior, mas aprovado para todas (PRIMA)
  - Ajuste de dose por peso/plaquetas: 200mg se <77kg ou plaq<150k

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - PRIMA/ENGOT-OV26 (1L maintenance)
  - NOVA (recurrent platinum-sensitive)
  - NCCN Ovarian Cancer, ESMO Guidelines
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_ONQ_DRUG_009" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_ONQ_DRUG_009" name="ONQ-DRUG-009: Niraparib Eligibility Criteria [LEAN TIER-2]">
    <decisionTable id="DecisionTable_ONQ_DRUG_009" hitPolicy="FIRST">
      <input id="Input_tumor_type" label="Tumor Type">
        <inputExpression id="InputExpression_tumor_type" typeRef="string">
          <text>tumorType</text>
        </inputExpression>
      </input>
      <input id="Input_hrd_status" label="HRD Status">
        <inputExpression id="InputExpression_hrd_status" typeRef="string">
          <text>hrdStatus</text>
        </inputExpression>
      </input>
      <input id="Input_platinum_response" label="Platinum Response">
        <inputExpression id="InputExpression_platinum_response" typeRef="string">
          <text>platinumResponse</text>
        </inputExpression>
      </input>
      <input id="Input_line" label="Treatment Line">
        <inputExpression id="InputExpression_line" typeRef="number">
          <text>treatmentLine</text>
        </inputExpression>
      </input>
      <!-- Input: Dias desde ultimo procedimento (DUP detection) -->
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="number">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentacao" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_reject_non_ovarian">
        <description>Reject non-ovarian tumors</description>
        <inputEntry id="InputEntry_tumor_type_non">
          <text>not("ovarian","fallopian_tube","peritoneal")</text>
        </inputEntry>
        <inputEntry id="InputEntry_hrd_status_non">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_platinum_response_non">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_non">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_non_ovarian_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_non">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_non">
          <text>"Niraparibe: indicacao restrita a cancer de ovario/trompa/peritonio"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_non">
          <text>"PRIMA/NOVA: estudos apenas em neoplasias ovarianas; outras indicacoes sem evidencia"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_non">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_non">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_reject_platinum_refractory">
        <description>Reject platinum-refractory</description>
        <inputEntry id="InputEntry_tumor_type_ref">
          <text>"ovarian","fallopian_tube","peritoneal"</text>
        </inputEntry>
        <inputEntry id="InputEntry_hrd_status_ref">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_platinum_response_ref">
          <text>"refractory","resistant"</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_ref">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_platinum_refractory_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_ref">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_ref">
          <text>"Refrataria/resistente a platina: niraparibe manutencao nao indicado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_ref">
          <text>"NOVA: inclui apenas platina-sensiveis; refratarios devem trocar classe terapeutica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_ref">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_ref">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>

      <!-- WASTE DETECTION: ESC -->
      <rule id="Rule_40501334_ESC">
        <description>ESC - Niraparib sem teste BRCA/HRD documentado</description>
        <inputEntry id="IE_ESC_40501334_1"><text>-</text></inputEntry>
        <inputEntry id="IE_ESC_40501334_2"><text>-</text></inputEntry>
        <inputEntry id="IE_ESC_40501334_3"><text>-</text></inputEntry>
        <inputEntry id="IE_ESC_40501334_4"><text>-</text></inputEntry>
        <inputEntry id="IE_ESC_40501334_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OE_ESC_40501334_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_ESC_40501334_observacao"><text>"Niraparib sem teste BRCA/HRD documentado"</text></outputEntry>
        <outputEntry id="OE_ESC_40501334_fundamentacao"><text>"NCCN Ovarian 2024: Niraparib manutencao requer BRCA ou HRD status. BRCA+ ou HRD+ preferencial"</text></outputEntry>
        <outputEntry id="OE_ESC_40501334_alertasDesperdicio"><text>"ESC"</text></outputEntry>
        <outputEntry id="OE_ESC_40501334_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- WASTE DETECTION: FREQ -->
      <rule id="Rule_40501334_FREQ">
        <description>FREQ - Verificar frequencia dispensacao medicamento oral oncologico. Ciclo 28 dias padr</description>
        <inputEntry id="IE_FREQ_40501334_1"><text>-</text></inputEntry>
        <inputEntry id="IE_FREQ_40501334_2"><text>-</text></inputEntry>
        <inputEntry id="IE_FREQ_40501334_3"><text>-</text></inputEntry>
        <inputEntry id="IE_FREQ_40501334_4"><text>-</text></inputEntry>
        <inputEntry id="IE_FREQ_40501334_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OE_FREQ_40501334_resultado"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_FREQ_40501334_observacao"><text>"Verificar frequencia dispensacao medicamento oral oncologico. Ciclo 28 dias padrao"</text></outputEntry>
        <outputEntry id="OE_FREQ_40501334_fundamentacao"><text>"ANS RN 465/2021, SBOC 2023: Dispensacao oral oncologico ciclo 28 dias, max 3 ciclos por autorizacao"</text></outputEntry>
        <outputEntry id="OE_FREQ_40501334_alertasDesperdicio"><text>"FREQ"</text></outputEntry>
        <outputEntry id="OE_FREQ_40501334_acaoRecomendada"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_approve_1L_maintenance">
        <description>Approve 1L maintenance all-comers</description>
        <inputEntry id="InputEntry_tumor_type_1l">
          <text>"ovarian","fallopian_tube","peritoneal"</text>
        </inputEntry>
        <inputEntry id="InputEntry_hrd_status_1l">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_platinum_response_1l">
          <text>"CR","PR"</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_1l">
          <text>1</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_1L_maintenance_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_1l">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_1l">
          <text>"Niraparibe 1L aprovado: manutencao pos-resposta a platina (PRIMA all-comers)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_1l">
          <text>"PRIMA: PFS 13.8 vs 8.2m (all); HRD+: 21.9 vs 10.4m. Ajustar dose se peso&lt;77kg ou plaq&lt;150k"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_1l">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_1l">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_recurrent_hrd">
        <description>Approve recurrent with HRD+</description>
        <inputEntry id="InputEntry_tumor_type_hrd">
          <text>"ovarian","fallopian_tube","peritoneal"</text>
        </inputEntry>
        <inputEntry id="InputEntry_hrd_status_hrd">
          <text>"HRD_positive","BRCA_positive","gBRCA","sBRCA"</text>
        </inputEntry>
        <inputEntry id="InputEntry_platinum_response_hrd">
          <text>"CR","PR","platinum_sensitive"</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_hrd">
          <text>>= 2</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_recurrent_hrd_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_hrd">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_hrd">
          <text>"Niraparibe recorrente aprovado: HRD+ com resposta a platina"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_hrd">
          <text>"NOVA gBRCA: PFS 21.0 vs 5.5m; HRD+non-gBRCA: 12.9 vs 3.8m"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_hrd">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_hrd">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_pending_recurrent_hrp">
        <description>Pending for recurrent HRD-negative</description>
        <inputEntry id="InputEntry_tumor_type_hrp">
          <text>"ovarian","fallopian_tube","peritoneal"</text>
        </inputEntry>
        <inputEntry id="InputEntry_hrd_status_hrp">
          <text>"HRD_negative","HRP"</text>
        </inputEntry>
        <inputEntry id="InputEntry_platinum_response_hrp">
          <text>"CR","PR","platinum_sensitive"</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_hrp">
          <text>>= 2</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_pending_recurrent_hrp_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_hrp">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_hrp">
          <text>"HRD negativo recorrente: beneficio modesto. Considerar olaparibe BRCA+ ou bevacizumabe"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_hrp">
          <text>"NOVA HRP: PFS 6.9 vs 3.8m - beneficio menor. Avaliar custo-efetividade"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_hrp">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_hrp">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_fallback">
        <description>Fallback rule</description>
        <inputEntry id="InputEntry_tumor_type_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_hrd_status_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_platinum_response_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_line_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_fallback_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Dados insuficientes para avaliacao de niraparibe"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Necessario: tipo tumoral, status HRD, resposta a platina, linha de tratamento"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_fallback">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_fallback">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ONQ_DRUG_009">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
