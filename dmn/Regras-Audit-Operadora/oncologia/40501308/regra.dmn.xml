<?xml version="1.0" encoding="UTF-8"?>
<!-- Versao: 3.1.0-LEAN-TIER2 (2026-02-08) -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_ONQ_BIO_014" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_ONQ_BIO_014" name="ONQ-BIO-014: IDH1/2 for AML [LEAN TIER-2]">
    <decisionTable id="DecisionTable_ONQ_BIO_014" hitPolicy="FIRST">
      <input id="Input_diagnosis" label="Diagnosis">
        <inputExpression id="InputExpression_diagnosis" typeRef="string">
          <text>diagnosis</text>
        </inputExpression>
      </input>
      <input id="Input_age" label="Age">
        <inputExpression id="InputExpression_age" typeRef="number">
          <text>age</text>
        </inputExpression>
      </input>
      <input id="Input_fitness" label="Fitness for Intensive Therapy">
        <inputExpression id="InputExpression_fitness" typeRef="string">
          <text>fitnessForIntensiveTherapy</text>
        </inputExpression>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="number">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_reject_non_aml">
        <inputEntry id="InputEntry_diagnosis_reject">
          <text>not("AML","MDS")</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fitness_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_non_aml_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_reject">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_reject">
          <text>"Teste IDH1/2 indicado apenas para LMA/SMD conforme diretrizes ELN/NCCN"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_reject">
          <text>"Mutacoes IDH1/2 ocorrem em 15-20% de LMA; raras em outras malignidades hematologicas conforme ELN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_reject_alertasDesperdicio">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OE_reject_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_40501308_DUP_reteste">
        <description>DUP - Reteste molecular em menos de 90 dias sem justificativa</description>
        <inputEntry id="IE_DUP_diagnosis">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_age">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_fitness">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_ultimo">
          <text>&lt; 90</text>
        </inputEntry>
        <outputEntry id="OE_DUP_resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_observacao">
          <text>"Teste molecular repetido em menos de 90 dias"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_fundamentacao">
          <text>"NCCN 2024: Reteste molecular so indicado se progressao ou mudanca terapeutica"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_alertasDesperdicio">
          <text>"DUP"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_aml">
        <inputEntry id="InputEntry_diagnosis_aml">
          <text>"AML"</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_aml">
          <text>&gt;= 18</text>
        </inputEntry>
        <inputEntry id="InputEntry_fitness_aml">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_aml_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_aml">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_aml">
          <text>"Teste de mutacao IDH1/2 aprovado para LMA recem-diagnosticada (taxa de mutacao 15-20%)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_aml">
          <text>"Ivosidenib (IDH1) e enasidenib (IDH2) aprovados pela FDA para LMA recidivada/refrataria conforme AG221-C-001/AG120-C-001 2024"</text>
        </outputEntry>
        <outputEntry id="OE_aml_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_aml_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_elderly">
        <inputEntry id="InputEntry_diagnosis_elderly">
          <text>"AML"</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_elderly">
          <text>&gt;= 60</text>
        </inputEntry>
        <inputEntry id="InputEntry_fitness_elderly">
          <text>"unfit_for_intensive_chemo"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_elderly_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_elderly">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_elderly">
          <text>"Teste IDH1/2 prioritario para pacientes idosos/nao-elegivel com LMA (opcao de terapia alvo)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_elderly">
          <text>"Ivosidenib+azacitidina mostra beneficio de sobrevida vs azacitidina isolada em LMA IDH1+ >=75 anos conforme estudo AGILE 2024"</text>
        </outputEntry>
        <outputEntry id="OE_elderly_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_elderly_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_relapsed">
        <inputEntry id="InputEntry_diagnosis_relapsed">
          <text>"AML"</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_relapsed">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fitness_relapsed">
          <text>"relapsed_refractory"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_relapsed_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_relapsed">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_relapsed">
          <text>"Teste IDH1/2 aprovado para LMA recidivada/refrataria (indicacao de monoterapia)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_relapsed">
          <text>"Ivosidenib: taxa de RC+CRh de 30.4%; Enasidenib: taxa de RC de 19.3% em LMA R/R conforme estudos pivotais 2024"</text>
        </outputEntry>
        <outputEntry id="OE_relapsed_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_relapsed_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_pending">
        <inputEntry id="InputEntry_diagnosis_pending">
          <text>"AML","MDS"</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fitness_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_pending_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_pending">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_pending">
          <text>"Idade e status de elegibilidade requeridos. Teste IDH deve ser parte da avaliacao diagnostica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_pending">
          <text>"Painel NGS captura IDH1/2 junto com outros marcadores prognosticos de LMA conforme recomendacoes ELN 2022"</text>
        </outputEntry>
        <outputEntry id="OE_pending_alertasDesperdicio">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OE_pending_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_fallback">
        <inputEntry id="InputEntry_diagnosis_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_age_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_fitness_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_fallback_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Consulta hematologica requerida para indicacao de teste IDH"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Teste IDH parte de analise molecular abrangente de LMA conforme diretrizes ELN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ONQ_BIO_014">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
