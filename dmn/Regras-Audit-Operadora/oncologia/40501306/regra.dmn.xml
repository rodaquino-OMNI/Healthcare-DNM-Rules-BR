<?xml version="1.0" encoding="UTF-8"?>
<!-- Versao: 3.1.0-LEAN-TIER2 (2026-02-08) -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_ONQ_BIO_012" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_ONQ_BIO_012" name="ONQ-BIO-012: MET Amplification [LEAN TIER-2]">
    <decisionTable id="DecisionTable_ONQ_BIO_012" hitPolicy="FIRST">
      <input id="Input_tumor_type" label="Tumor Type">
        <inputExpression id="InputExpression_tumor_type" typeRef="string">
          <text>tumorType</text>
        </inputExpression>
      </input>
      <input id="Input_clinical_context" label="Clinical Context">
        <inputExpression id="InputExpression_clinical_context" typeRef="string">
          <text>clinicalContext</text>
        </inputExpression>
      </input>
      <input id="Input_met_exon14" label="MET Exon 14 Status">
        <inputExpression id="InputExpression_met_exon14" typeRef="string">
          <text>metExon14Status</text>
        </inputExpression>
      </input>
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="number">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_reject_low_relevance">
        <inputEntry id="InputEntry_tumor_type_reject">
          <text>not("NSCLC","gastric","HCC")</text>
        </inputEntry>
        <inputEntry id="InputEntry_clinical_context_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_met_exon14_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_low_relevance_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_reject">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_reject">
          <text>"Teste de amplificacao de MET nao e clinicamente actionavel para este tipo de tumor"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_reject">
          <text>"Inibidores de MET aprovados apenas para NSCLC com skip de exon 14 ou amplificacao de alto nivel conforme FDA 2024"</text>
        </outputEntry>
        <outputEntry id="OE_reject_alertasDesperdicio">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OE_reject_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_40501306_DUP_reteste">
        <description>DUP - Reteste molecular em menos de 90 dias sem justificativa</description>
        <inputEntry id="IE_DUP_tumor">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_context">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_exon14">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DUP_ultimo">
          <text>&lt; 90</text>
        </inputEntry>
        <outputEntry id="OE_DUP_resultado">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_observacao">
          <text>"Teste molecular repetido em menos de 90 dias"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_fundamentacao">
          <text>"NCCN 2024: Reteste molecular so indicado se progressao ou mudanca terapeutica"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_alertasDesperdicio">
          <text>"DUP"</text>
        </outputEntry>
        <outputEntry id="OE_DUP_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_nsclc_exon14">
        <inputEntry id="InputEntry_tumor_type_exon14">
          <text>"NSCLC"</text>
        </inputEntry>
        <inputEntry id="InputEntry_clinical_context_exon14">
          <text>"advanced","metastatic"</text>
        </inputEntry>
        <inputEntry id="InputEntry_met_exon14_exon14">
          <text>"detected"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_nsclc_exon14_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_exon14">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_exon14">
          <text>"Skip de exon 14 de MET detectado. Capmatinib ou tepotinib aprovados conforme estudos GEOMETRY/VISION"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_exon14">
          <text>"Skip de exon 14 de MET em 3-4% de NSCLC; ORR de 68% com capmatinib conforme GEOMETRY mono-1 2024"</text>
        </outputEntry>
        <outputEntry id="OE_exon14_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_exon14_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_nsclc_amplification">
        <inputEntry id="InputEntry_tumor_type_amp">
          <text>"NSCLC"</text>
        </inputEntry>
        <inputEntry id="InputEntry_clinical_context_amp">
          <text>"EGFR_resistance"</text>
        </inputEntry>
        <inputEntry id="InputEntry_met_exon14_amp">
          <text>"not_detected"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_nsclc_amplification_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_amp">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_amp">
          <text>"Teste de amplificacao de MET aprovado para resistencia adquirida a TKI de EGFR (mecanismo de 5-20%)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_amp">
          <text>"Amplificacao de MET medeia resistencia ao osimertinib; terapia combinada em investigacao conforme NCCN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_amp_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_amp_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_gastric">
        <inputEntry id="InputEntry_tumor_type_gastric">
          <text>"gastric"</text>
        </inputEntry>
        <inputEntry id="InputEntry_clinical_context_gastric">
          <text>"HER2_negative_advanced"</text>
        </inputEntry>
        <inputEntry id="InputEntry_met_exon14_gastric">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_gastric_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_gastric">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_gastric">
          <text>"Teste de amplificacao de MET aprovado para cancer gastrico (taxa de amplificacao 5-10%)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_gastric">
          <text>"Inibidores experimentais de MET em estudos para cancer gastrico amplificado para MET conforme NCCN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_gastric_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_gastric_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_pending">
        <inputEntry id="InputEntry_tumor_type_pending">
          <text>"NSCLC"</text>
        </inputEntry>
        <inputEntry id="InputEntry_clinical_context_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_met_exon14_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_pending_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_pending">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_pending">
          <text>"Contexto clinico requerido. Teste de MET indicado para deteccao de exon 14 ou resistencia a EGFR"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_pending">
          <text>"Painel NGS captura tanto skip de exon 14 de MET quanto amplificacao conforme diretrizes NCCN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_pending_alertasDesperdicio">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OE_pending_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_fallback">
        <inputEntry id="InputEntry_tumor_type_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_clinical_context_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_met_exon14_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_fallback_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Revisao por comite molecular de tumores para indicacao de teste de MET"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Utilidade do teste de MET depende do tipo de tumor e cenario clinico conforme NCCN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ONQ_BIO_012">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
