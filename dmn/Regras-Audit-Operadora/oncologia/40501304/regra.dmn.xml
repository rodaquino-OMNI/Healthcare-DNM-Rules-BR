<?xml version="1.0" encoding="UTF-8"?>
<!-- Versao: 3.1.0-LEAN-TIER2 (2026-02-08) -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_ONQ_BIO_010" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_ONQ_BIO_010" name="ONQ-BIO-010: NTRK Fusion Testing [LEAN TIER-2]">
    <decisionTable id="DecisionTable_ONQ_BIO_010" hitPolicy="FIRST">
      <input id="Input_tumor_type" label="Tumor Type">
        <inputExpression id="InputExpression_tumor_type" typeRef="string">
          <text>tumorType</text>
        </inputExpression>
      </input>
      <input id="Input_prevalence_category" label="NTRK Prevalence">
        <inputExpression id="InputExpression_prevalence_category" typeRef="string">
          <text>ntrkPrevalence</text>
        </inputExpression>
      </input>
      <input id="Input_test_method" label="Test Method">
        <inputExpression id="InputExpression_test_method" typeRef="string">
          <text>testMethod</text>
        </inputExpression>
      </input>
      <!-- Input: Dias desde ultimo procedimento (DUP detection) -->
      <input id="Input_ultimoProcedimentoDias" label="Dias Desde Ultimo Procedimento">
        <inputExpression id="InputExpression_ultimoProcedimentoDias" typeRef="number">
          <text>ultimoProcedimentoDias</text>
        </inputExpression>
      </input>

      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_reject_low_prevalence">
        <inputEntry id="InputEntry_tumor_type_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_prevalence_category_reject">
          <text>not("high","intermediate")</text>
        </inputEntry>
        <inputEntry id="InputEntry_test_method_reject">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_reject_low_prevalence_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_reject">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_reject">
          <text>"Teste NTRK nao e custo-efetivo para tumores de baixa prevalencia (&lt;1%) segundo diretrizes ASCO"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_reject">
          <text>"Raro na maioria dos tumores solidos (&lt;0.5%); painel NGS preferido sobre teste isolado conforme ASCO 2024"</text>
        </outputEntry>
        <outputEntry id="OE_reject_alertasDesperdicio">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OE_reject_acaoRecomendada">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>

      <!-- WASTE DETECTION: DUP -->
      <rule id="Rule_40501304_DUP">
        <description>DUP - Reteste NTRK em menos de 90 dias</description>
        <inputEntry id="IE_DUP_40501304_1"><text>-</text></inputEntry>
        <inputEntry id="IE_DUP_40501304_2"><text>-</text></inputEntry>
        <inputEntry id="IE_DUP_40501304_3"><text>-</text></inputEntry>
        <inputEntry id="IE_DUP_40501304_ultimo"><text>&lt; 90</text></inputEntry>
        <outputEntry id="OE_DUP_40501304_resultado"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_DUP_40501304_observacao"><text>"Reteste NTRK em menos de 90 dias"</text></outputEntry>
        <outputEntry id="OE_DUP_40501304_fundamentacao"><text>"NCCN 2024: NTRK fusao resultado permanente, reteste se novo primario"</text></outputEntry>
        <outputEntry id="OE_DUP_40501304_alertasDesperdicio"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_DUP_40501304_acaoRecomendada"><text>"AGUARDAR_PRAZO"</text></outputEntry>
      </rule>

      <rule id="DecisionRule_approve_high_prevalence">
        <inputEntry id="InputEntry_tumor_type_high">
          <text>"salivary_gland","thyroid_papillary","infantile_fibrosarcoma","secretory_breast"</text>
        </inputEntry>
        <inputEntry id="InputEntry_prevalence_category_high">
          <text>"high"</text>
        </inputEntry>
        <inputEntry id="InputEntry_test_method_high">
          <text>"FISH","IHC","RT-PCR","NGS"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_high_prevalence_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_high">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_high">
          <text>"Teste de fusao NTRK aprovado para tumor de alta prevalencia (taxa de fusao 5-90%)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_high">
          <text>"Larotrectinib mostra ORR de 75% em tumores NTRK+ conforme estudos LOXO-TRK-14001/15002/15003 2024"</text>
        </outputEntry>
        <outputEntry id="OE_high_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_high_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_intermediate">
        <inputEntry id="InputEntry_tumor_type_intermediate">
          <text>"lung_adenocarcinoma","colorectal","melanoma","GIST"</text>
        </inputEntry>
        <inputEntry id="InputEntry_prevalence_category_intermediate">
          <text>"intermediate"</text>
        </inputEntry>
        <inputEntry id="InputEntry_test_method_intermediate">
          <text>"NGS_panel"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_intermediate_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_intermediate">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_intermediate">
          <text>"Teste de fusao NTRK via painel NGS aprovado (tumores de prevalencia 1-5%)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_intermediate">
          <text>"NGS permite teste simultaneo de outras alteracoes actionaveis (EGFR, ALK, ROS1, etc.) conforme diretrizes CAP 2024"</text>
        </outputEntry>
        <outputEntry id="OE_intermediate_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_intermediate_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_approve_refractory">
        <inputEntry id="InputEntry_tumor_type_refractory">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_prevalence_category_refractory">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_test_method_refractory">
          <text>"NGS_panel"</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_approve_refractory_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_refractory">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_refractory">
          <text>"Teste de fusao NTRK aprovado para tumor solido metastatico sem opcoes de tratamento padrao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_refractory">
          <text>"Aprovacao FDA independente de tecido para larotrectinib/entrectinib justifica teste em casos refratarios 2024"</text>
        </outputEntry>
        <outputEntry id="OE_refractory_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_refractory_acaoRecomendada">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_pending">
        <inputEntry id="InputEntry_tumor_type_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_prevalence_category_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_test_method_pending">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_pending_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_pending">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_pending">
          <text>"Tipo de tumor e categoria de prevalencia requeridos. Preferir painel NGS sobre teste NTRK isolado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_pending">
          <text>"Analise de risco-beneficio requer dados de prevalencia conforme diretrizes de oncologia de precisao ESMO/ASCO 2024"</text>
        </outputEntry>
        <outputEntry id="OE_pending_alertasDesperdicio">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OE_pending_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_fallback">
        <inputEntry id="InputEntry_tumor_type_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_prevalence_category_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_test_method_fallback">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_DecisionRule_fallback_ultimo"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_resultado_fallback">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_fallback">
          <text>"Revisao por comite molecular de tumores requerida para indicacao de teste NTRK"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_fallback">
          <text>"Avaliacao caso a caso necessaria dada indicacao pan-tumoral mas baixa prevalencia conforme NCCN 2024"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_alertasDesperdicio">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OE_fallback_acaoRecomendada">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_ONQ_BIO_010">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
