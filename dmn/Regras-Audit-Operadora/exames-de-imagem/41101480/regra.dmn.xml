<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             id="Definitions_41101480"
             name="Ressonancia Magnetica de Mama Bilateral"
             namespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 41101480 - Ressonancia Magnetica de Mama Bilateral
    Versao: 2.0.0 (2026-02-08 LEAN TIER-2)

    Indicacoes (DUT):
    - Mutacao BRCA1 ou BRCA2
    - Mutacao TP53 (Sindrome de Li-Fraumeni)
    - Historia familiar de alto risco para cancer de mama
    - Rastreamento de pacientes de alto risco genetico
    - Avaliacao de resposta a quimioterapia neoadjuvante
    - Discordancia clinico-radiologica

    Criterios de Alto Risco:
    - Risco vital >= 20% (modelos Gail, BRCAPRO, Tyrer-Cuzick)
    - Parente de 1o grau com mutacao BRCA
    - Irradiacao toracica entre 10-30 anos

    Contraindicacoes:
    - Ausencia de criterios de alto risco documentados
    - Uso para rastreamento de rotina em populacao geral

    Referencias:
    - DUT ANS - RM de Mama
    - NCCN Guidelines - Breast Cancer Screening
    - ACR Appropriateness Criteria
  -->
  <decision id="Decision_41101480" name="Auditoria RM Mama Bilateral">
    <decisionTable id="DecisionTable_41101480" hitPolicy="FIRST">
      <input id="Input_RiscoGenetico" label="Criterio de Risco Genetico">
        <inputExpression id="InputExpression_RiscoGenetico" typeRef="string">
          <text>criterioRiscoGenetico</text>
        </inputExpression>
        <inputValues id="InputValues_RiscoGenetico">
          <text>"BRCA1", "BRCA2", "TP53", "Parente_1o_Grau_BRCA", "Nenhum"</text>
        </inputValues>
      </input>
      <input id="Input_HistoriaFamiliar" label="Historia Familiar de Alto Risco">
        <inputExpression id="InputExpression_HistoriaFamiliar" typeRef="boolean">
          <text>historiaFamiliarAltoRisco</text>
        </inputExpression>
      </input>
      <input id="Input_RiscoVital" label="Risco Vital (%) - Modelos de Risco">
        <inputExpression id="InputExpression_RiscoVital" typeRef="double">
          <text>riscoVitalPercentual</text>
        </inputExpression>
      </input>
      <input id="Input_IrradiacaoToracica" label="Irradiacao Toracica entre 10-30 anos">
        <inputExpression id="InputExpression_IrradiacaoToracica" typeRef="boolean">
          <text>irradiacaoToracicaPrevia</text>
        </inputExpression>
      </input>
      <input id="Input_IndicacaoClinica" label="Indicacao Clinica Especial">
        <inputExpression id="InputExpression_IndicacaoClinica" typeRef="string">
          <text>indicacaoClinicaEspecial</text>
        </inputExpression>
        <inputValues id="InputValues_IndicacaoClinica">
          <text>"Resposta_Quimioterapia_Neoadjuvante", "Discordancia_Clinico_Radiologica", "Estadiamento_Pre_Operatorio", "Nenhuma"</text>
        </inputValues>
      </input>
      <input id="Input_DocumentacaoAnexada" label="Documentacao de Risco Anexada">
        <inputExpression id="InputExpression_DocumentacaoAnexada" typeRef="boolean">
          <text>documentacaoRiscoAnexada</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <rule id="Rule_SemDocumentacao">
        <description>Documentacao de risco obrigatoria</description>
        <inputEntry id="InputEntry_1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_1"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_1_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_1"><text>"Documentacao obrigatoria ausente: laudo de teste genetico, calculo de risco vital ou relatorio medico com criterios de alto risco."</text></outputEntry>
        <outputEntry id="OutputEntry_3_1"><text>"DUT ANS: documentacao de criterios de alto risco e obrigatoria para RM mama."</text></outputEntry>
        <outputEntry id="OutputEntry_4_1"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_5_1"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <rule id="Rule_BRCA1">
        <description>Mutacao BRCA1 confirmada</description>
        <inputEntry id="InputEntry_1_2"><text>"BRCA1"</text></inputEntry>
        <inputEntry id="InputEntry_2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_2"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_2"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_2"><text>"Aprovado - mutacao BRCA1 confirmada. RM mama bilateral indicada para rastreamento de alto risco."</text></outputEntry>
        <outputEntry id="OutputEntry_3_2"><text>"DUT ANS: RM mama indicada para portadoras de mutacao BRCA1 conforme protocolo de rastreamento."</text></outputEntry>
        <outputEntry id="OutputEntry_4_2"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_2"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_BRCA2">
        <description>Mutacao BRCA2 confirmada</description>
        <inputEntry id="InputEntry_1_3"><text>"BRCA2"</text></inputEntry>
        <inputEntry id="InputEntry_2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_3"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_3"><text>"Aprovado - mutacao BRCA2 confirmada. RM mama bilateral indicada para rastreamento de alto risco."</text></outputEntry>
        <outputEntry id="OutputEntry_3_3"><text>"DUT ANS: RM mama indicada para portadoras de mutacao BRCA2 conforme protocolo de rastreamento."</text></outputEntry>
        <outputEntry id="OutputEntry_4_3"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_3"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_TP53">
        <description>Mutacao TP53 (Sindrome de Li-Fraumeni)</description>
        <inputEntry id="InputEntry_1_4"><text>"TP53"</text></inputEntry>
        <inputEntry id="InputEntry_2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_4"><text>"Aprovado - mutacao TP53 (Sindrome de Li-Fraumeni) confirmada. RM mama bilateral indicada."</text></outputEntry>
        <outputEntry id="OutputEntry_3_4"><text>"DUT ANS: RM mama indicada para portadoras de mutacao TP53 conforme protocolo."</text></outputEntry>
        <outputEntry id="OutputEntry_4_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_4"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Parente1oGrau">
        <description>Parente de 1o grau com mutacao BRCA</description>
        <inputEntry id="InputEntry_1_5"><text>"Parente_1o_Grau_BRCA"</text></inputEntry>
        <inputEntry id="InputEntry_2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_5"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_5"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_5"><text>"Aprovado - parente de 1o grau com mutacao BRCA conhecida. RM mama bilateral indicada para rastreamento."</text></outputEntry>
        <outputEntry id="OutputEntry_3_5"><text>"DUT ANS: RM mama indicada para familiares de 1o grau de portadoras BRCA."</text></outputEntry>
        <outputEntry id="OutputEntry_4_5"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_HistoriaFamiliar">
        <description>Historia familiar de alto risco documentada</description>
        <inputEntry id="InputEntry_1_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_6"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_3_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_6"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_6"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_6"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_6"><text>"Aprovado - historia familiar de alto risco para cancer de mama documentada."</text></outputEntry>
        <outputEntry id="OutputEntry_3_6"><text>"DUT ANS: RM mama indicada para pacientes com historia familiar de alto risco conforme criterios DUT."</text></outputEntry>
        <outputEntry id="OutputEntry_4_6"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_6"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_RiscoVital20">
        <description>Risco vital >= 20% por modelos de risco</description>
        <inputEntry id="InputEntry_1_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_7"><text>>= 20</text></inputEntry>
        <inputEntry id="InputEntry_4_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_7"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_7"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_7"><text>"Aprovado - risco vital >= 20% calculado por modelo validado (Gail, BRCAPRO, Tyrer-Cuzick). RM mama indicada para rastreamento de alto risco."</text></outputEntry>
        <outputEntry id="OutputEntry_3_7"><text>"DUT ANS: RM mama indicada para risco vital >= 20% por modelos validados."</text></outputEntry>
        <outputEntry id="OutputEntry_4_7"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_7"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_IrradiacaoToracica">
        <description>Irradiacao toracica entre 10-30 anos de idade</description>
        <inputEntry id="InputEntry_1_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_8"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_5_8"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_8"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_8"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_8"><text>"Aprovado - historia de irradiacao toracica entre 10-30 anos de idade. RM mama indicada para rastreamento de alto risco."</text></outputEntry>
        <outputEntry id="OutputEntry_3_8"><text>"DUT ANS: RM mama indicada para pacientes com historia de irradiacao toracica na faixa etaria de risco."</text></outputEntry>
        <outputEntry id="OutputEntry_4_8"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_8"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_QuimioNeoadjuvante">
        <description>Avaliacao de resposta a quimioterapia neoadjuvante</description>
        <inputEntry id="InputEntry_1_9"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_9"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_9"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_9"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_9"><text>"Resposta_Quimioterapia_Neoadjuvante"</text></inputEntry>
        <inputEntry id="InputEntry_6_9"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_9"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_9"><text>"Aprovado - RM mama para avaliacao de resposta a quimioterapia neoadjuvante em paciente com cancer de mama."</text></outputEntry>
        <outputEntry id="OutputEntry_3_9"><text>"DUT ANS: RM mama indicada para avaliar resposta terapeutica em quimioterapia neoadjuvante."</text></outputEntry>
        <outputEntry id="OutputEntry_4_9"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_9"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Discordancia">
        <description>Discordancia clinico-radiologica</description>
        <inputEntry id="InputEntry_1_10"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_10"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_10"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_10"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_10"><text>"Discordancia_Clinico_Radiologica"</text></inputEntry>
        <inputEntry id="InputEntry_6_10"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_10"><text>"Aprovado - RM mama para esclarecimento de discordancia clinico-radiologica."</text></outputEntry>
        <outputEntry id="OutputEntry_3_10"><text>"DUT ANS: RM mama indicada para elucidacao de discordancia entre achados clinicos e radiologicos."</text></outputEntry>
        <outputEntry id="OutputEntry_4_10"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_10"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Estadiamento">
        <description>Estadiamento pre-operatorio de cancer de mama</description>
        <inputEntry id="InputEntry_1_11"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_11"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_11"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_11"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_11"><text>"Estadiamento_Pre_Operatorio"</text></inputEntry>
        <inputEntry id="InputEntry_6_11"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_11"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_11"><text>"Aprovado - RM mama para estadiamento pre-operatorio de cancer de mama diagnosticado."</text></outputEntry>
        <outputEntry id="OutputEntry_3_11"><text>"DUT ANS: RM mama indicada para estadiamento locorregional em cancer de mama diagnosticado."</text></outputEntry>
        <outputEntry id="OutputEntry_4_11"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_5_11"><text>"APROVAR"</text></outputEntry>
      </rule>

      <rule id="Rule_SemCriterios">
        <description>Ausencia de criterios de alto risco</description>
        <inputEntry id="InputEntry_1_12"><text>"Nenhum"</text></inputEntry>
        <inputEntry id="InputEntry_2_12"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_3_12"><text>&lt; 20</text></inputEntry>
        <inputEntry id="InputEntry_4_12"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_5_12"><text>"Nenhuma"</text></inputEntry>
        <inputEntry id="InputEntry_6_12"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_12"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OutputEntry_2_12"><text>"RM mama NAO indicada para rastreamento de rotina em populacao sem criterios de alto risco. Manter mamografia como exame padrao."</text></outputEntry>
        <outputEntry id="OutputEntry_3_12"><text>"DUT ANS: RM mama restrita a casos de alto risco. Uso em populacao geral nao tem evidencia de custo-efetividade."</text></outputEntry>
        <outputEntry id="OutputEntry_4_12"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_5_12"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback">
        <description>Dados incompletos</description>
        <inputEntry id="InputEntry_1_13"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_2_13"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_3_13"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_4_13"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_5_13"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_6_13"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_1_13"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OutputEntry_2_13"><text>"Dados incompletos. Necessario documentar: (1) Teste genetico, (2) Historia familiar, (3) Calculo de risco vital, (4) Indicacao clinica especifica."</text></outputEntry>
        <outputEntry id="OutputEntry_3_13"><text>"DUT ANS: documentacao completa de criterios de alto risco e obrigatoria para aprovacao de RM mama."</text></outputEntry>
        <outputEntry id="OutputEntry_4_13"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_5_13"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
