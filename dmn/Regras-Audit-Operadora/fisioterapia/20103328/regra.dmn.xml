<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dc="http://www.omg.org/spec/DD/20180521/DC/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" id="Definitions_20103328" name="Estabilizacao segmentar da coluna vertebral" targetNamespace="http://camunda.org/schema/1.0/dmn">

  

  <decision id="Decision_20103328" name="Indicação para Estabilizacao segmentar da coluna vertebral">
    <decisionTable id="DecisionTable_20103328" hitPolicy="FIRST">

      

      <input id="Input_1" label="Diagnóstico Funcional">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoFuncional</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Objetivo Terapêutico">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>objetivoTerapeutico</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Frequência Solicitada (sessões/semana)">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>frequenciaSolicitada</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Avaliação Fisiatrica Anexada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>avaliacaoFisiatrica</text>
        </inputExpression>
      </input>

      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Aprovado", "Reprovado", "Pendente"</text>
        </outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />

      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_1">
        <description>Ausência de diagnóstico funcional</description>
        <inputEntry id="InputEntry_C1_1">
          <text>"Ausente", "Nao Documentado"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_C1_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_C1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_2">
          <text>"Diagnóstico funcional ausente ou não documentado."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C1_3">
          <text>"ANS RN 465/2021 - Diagnóstico funcional obrigatório"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraindicacao_2">
        <description>Frequência solicitada excede protocolo</description>
        <inputEntry id="InputEntry_C2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_C2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_C2_3">
          <text>&amp;gt; 5</text>
        </inputEntry>
        <inputEntry id="InputEntry_C2_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_C2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C2_2">
          <text>"Frequência solicitada excede protocolo máximo (5 sessões/semana)."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C2_3">
          <text>"TUSS 2024 - Limite máximo de 5 sessões/semana"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraindicacao_3">
        <description>Objetivo terapêutico não especificado</description>
        <inputEntry id="InputEntry_C3_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_C3_2">
          <text>"Nao Especificado", "Ausente"</text>
        </inputEntry>
        <inputEntry id="InputEntry_C3_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_C3_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_C3_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C3_2">
          <text>"Objetivo terapêutico ausente. Defina metas mensuráveis."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_C3_3">
          <text>"COFFITO - Objetivo terapêutico obrigatório"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>


      

      <rule id="Rule_Aprovado_1">
        <description>Documentação completa com critérios atendidos</description>
        <inputEntry id="InputEntry_A1_1">
          <text>not("Ausente"), not("Nao Documentado")</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_2">
          <text>not("Nao Especificado"), not("Ausente")</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_3">
          <text>[1..5]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A1_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_2">
          <text>"Autorizado. Critérios clínicos e documentação adequados."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A1_3">
          <text>"ANS RN 465/2021 - Procedimento fisioterapêutico coberto"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_2">
        <description>Critérios clínicos básicos atendidos</description>
        <inputEntry id="InputEntry_A2_1">
          <text>not("Ausente")</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_2">
          <text>not("Ausente")</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_3">
          <text>[1..5]</text>
        </inputEntry>
        <inputEntry id="InputEntry_A2_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_A2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_2">
          <text>"Autorizado. Critérios básicos de indicação atendidos."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_A2_3">
          <text>"ANS RN 465/2021 - Cobertura para procedimento fisioterapêutico"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>


      
      <rule id="Rule_Fallback_20103328">
        <description>Regra padrão - Criterios automaticos nao satisfeitos</description>
        <inputEntry id="InputEntry_F_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_F_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_F_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_F_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_F_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_F_2">
          <text>"Aguardando avaliação manual por auditor médico. Verificar critérios clínicos e documentação."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_F_3">
          <text>"Análise manual necessária - critérios automáticos não satisfeitos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_20103328">
      <dmndi:DMNShape id="DMNShape_Decision_20103328" dmnElementRef="Decision_20103328">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>