<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30501350" name="Rinosseptoplastia_Funcional" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30501350" name="Indicacao para Rinosseptoplastia Funcional">
    <decisionTable id="DecisionTable_30501350" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Sintomas Obstrutivos Cronicos (meses)">
        <inputExpression id="InputExpression_2" typeRef="integer"><text>sintomasObstrutivos</text></inputExpression>
      </input>
      <input id="Input_3" label="Rinoscopia/TC Documentada">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>rinoscopiaDocumentada</text></inputExpression>
      </input>
      <input id="Input_4" label="Tratamento Conservador (meses)">
        <inputExpression id="InputExpression_4" typeRef="integer"><text>tratamentoConservador</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Tratamento_Insuficiente">
        <inputEntry id="IE_1"><text>-</text></inputEntry>
        <inputEntry id="IE_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>&amp;lt; 3</text></inputEntry>
        <inputEntry id="IE_5"><text>-</text></inputEntry>
        <outputEntry id="OE_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Contraindicacao: Tratamento conservador insuficiente. Requer minimo 3 meses de corticoides topicos/antihistaminicos"</text></outputEntry>
        <outputEntry id="OE_3"><text>"AAO-HNS 2023: Septoplastia indicada apenas apos falha terapeutica conservadora de &amp;gt;=3 meses. ABORL-CCF 2023 secao 7.2: Corticoides nasais + antihistaminicos primeira linha"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Desvio_Septal_J34">
        <inputEntry id="IE_6"><text>"J34.2", "J34.3"</text></inputEntry>
        <inputEntry id="IE_7"><text>&amp;gt;= 6</text></inputEntry>
        <inputEntry id="IE_8"><text>true</text></inputEntry>
        <inputEntry id="IE_9"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Desvio septal com obstrucao nasal cronica &amp;gt;=6 meses, refrataria a tratamento conservador. Rinoscopia documentada"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID J34.2/J34.3 com sintomas &amp;gt;=6 meses. AAO-HNS 2023 Criterio Principal: Obstrucao nasal documentada + falha terapeutica &amp;gt;=3 meses. ANS RN 465/2021 aprovacao automatica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Hipertrofia_Cornetos_J34">
        <inputEntry id="IE_11"><text>"J34.0", "J34.8"</text></inputEntry>
        <inputEntry id="IE_12"><text>&amp;gt;= 6</text></inputEntry>
        <inputEntry id="IE_13"><text>true</text></inputEntry>
        <inputEntry id="IE_14"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Hipertrofia de cornetos bilateral com obstrucao nasal cronica refrataria a corticoides topicos &amp;gt;=3 meses"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID J34.0/J34.8 (Hipertrofia cornetos). SBR 2024: Turbinectomia + septoplastia indicada em hipertrofia bilateral refrataria. ABORL-CCF 2023 secao 8.1"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Sinusite_Recorrente_J32">
        <inputEntry id="IE_16"><text>"J32.0", "J32.4", "J32.9"</text></inputEntry>
        <inputEntry id="IE_17"><text>&amp;gt;= 6</text></inputEntry>
        <inputEntry id="IE_18"><text>true</text></inputEntry>
        <inputEntry id="IE_19"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Sinusite cronica recorrente secundaria a obstrucao nasal por desvio septal. Rinosseptoplastia funcional indicada"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID J32.x (Sinusite cronica). AAO-HNS 2023: Desvio septal que impede drenagem sinusal: correcao cirurgica indicada. TC seios da face demonstrando obstrucao. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Apneia_Sleep_G47">
        <inputEntry id="IE_21"><text>"G47.3"</text></inputEntry>
        <inputEntry id="IE_22"><text>&amp;gt;= 6</text></inputEntry>
        <inputEntry id="IE_23"><text>true</text></inputEntry>
        <inputEntry id="IE_24"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Apneia do sono com obstrucao nasal anatom ica documentada. Rinosseptoplastia coadjuvante ao CPAP"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID G47.3 (Apneia) com desvio septal obstructivo. SBR 2024: Correcao nasal melhora adesao ao CPAP em 60% dos casos. ABORL-CCF 2023 secao 9.3"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30501350">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>