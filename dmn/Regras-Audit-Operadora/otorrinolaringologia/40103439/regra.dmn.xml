<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:camunda="http://camunda.org/schema/1.0/dmn" id="Definitions_40103439" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.25.0">
  <decision id="Decision_40103439" name="Impedanciometria - Timpanometria - 40103439">
    <decisionTable id="DecisionTable_40103439" hitPolicy="FIRST">
      
      <input id="Input_diagnostico" label="Diagnostico Confirmado" camunda:inputVariable="diagnosticoConfirmado">
        <inputExpression id="InputExpression_diagnostico" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="UnaryTests_diagnostico">
          <text>"H90.x","H91.x","H93.x","G47.3","J34.x","J35.x","R42","NAO_CONFIRMADO"</text>
        </inputValues>
      </input>
      <input id="Input_indicacao" label="Indicacao Clinica" camunda:inputVariable="indicacaoClinica">
        <inputExpression id="InputExpression_indicacao" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="UnaryTests_indicacao">
          <text>"Diagnostica","Terapeutica","Acompanhamento","Triagem_Neonatal","Pre_Cirurgica"</text>
        </inputValues>
      </input>
      <input id="Input_tratamentos" label="Tratamentos Previos" camunda:inputVariable="tratamentosPrevios">
        <inputExpression id="InputExpression_tratamentos" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="UnaryTests_tratamentos">
          <text>"Documentado","Nao_Documentado","Nao_Aplicavel","Tratamento_Clinico_Falhou","Primeira_Linha"</text>
        </inputValues>
      </input>
      <input id="Input_risco" label="Risco Cirurgico" camunda:inputVariable="riscoCirurgico">
        <inputExpression id="InputExpression_risco" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="UnaryTests_risco">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV","ASA_V"</text>
        </inputValues>
      </input>
      <input id="Input_laudo" label="Laudo Indicacao" camunda:inputVariable="laudoIndicacao">
        <inputExpression id="InputExpression_laudo" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>
      <output id="Output_decisao" label="Decisao" name="decisao" typeRef="string">
        <outputValues id="UnaryTests_decisao">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_justificativa" label="Justificativa" name="justificativa" typeRef="string" />
      <output id="Output_referencia" label="Referencia" name="referencia" typeRef="string" />
      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_40103439_Contra_1">
        <description>Contraindicacao - Ausencia de laudo de indicacao</description>
        <inputEntry id="IE_Contra1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Obrigatorio laudo medico otorrinolaringologista com hipotese diagnostica para indicacao de impedanciometria"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"ABORL-CCF Diretrizes 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
      <rule id="Rule_40103439_Contra_2">
        <description>Contraindicacao - Indicacao nao apropriada</description>
        <inputEntry id="IE_Contra2_1"><text>"NAO_CONFIRMADO"</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>"Triagem_Neonatal","Acompanhamento"</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Impedanciometria nao indicada para triagem neonatal. Utilizar otoemissoes acusticas ou PEATE para screening auditivo neonatal"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"ASHA Guidelines for Tympanometry"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
      
      <rule id="Rule_40103439_Aprov_1">
        <description>Aprovado - Indicacao diagnostica</description>
        <inputEntry id="IE_Aprov1_1"><text>"H90.x","H91.x","H93.x"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>"Diagnostica"</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text></inputEntry>
        <inputEntry id="IE_Aprov1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Impedanciometria aprovada para avaliacao de funcao orelha media, patologia tubaria e caracterizacao de perda auditiva condutiva"</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"ABORL-CCF Diretrizes 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
      <rule id="Rule_40103439_Aprov_2">
        <description>Aprovado - Indicacao pre-cirurgica</description>
        <inputEntry id="IE_Aprov2_1"><text>"H90.x","H91.x","H93.x"</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>"Pre_Cirurgica"</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>"Documentado","Tratamento_Clinico_Falhou"</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>"ASA_I","ASA_II","ASA_III"</text></inputEntry>
        <inputEntry id="IE_Aprov2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Impedanciometria aprovada para avaliacao pre-cirurgica de timpanoplastia, ossiculoplastia ou insercao de tubo de ventilacao"</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"AAO-HNS Clinical Practice Guideline: Otitis Media with Effusion"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
      <rule id="Rule_40103439_Aprov_3">
        <description>Aprovado - Indicacao terapeutica</description>
        <inputEntry id="IE_Aprov3_1"><text>"H90.x","H91.x"</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>"Terapeutica"</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>"Primeira_Linha","Nao_Aplicavel"</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>"ASA_I","ASA_II","ASA_III"</text></inputEntry>
        <inputEntry id="IE_Aprov3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Impedanciometria aprovada para monitoramento terapeutico de otite media com efusao ou disfuncao tubaria"</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"ABORL-CCF Diretrizes 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>
      
      <rule id="Rule_40103439_Fallback">
        <description>Fallback - Negacao por falta de criterios</description>
        <inputEntry id="IE_Fall_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fall_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fall_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fall_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fall_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fall_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Fall_2"><text>"Solicitacao nao atende criterios clinicos estabelecidos. Impedanciometria indicada para avaliacao de funcao orelha media e patologia tubaria"</text></outputEntry>
        <outputEntry id="OE_Fall_3"><text>"ABORL-CCF Diretrizes 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>