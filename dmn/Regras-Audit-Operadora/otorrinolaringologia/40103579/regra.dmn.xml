        <outputEntry id="OE_Fallback_3">
          <text>"ABORL-CCF 2023 - Documentacao audiologica completa incluindo BERA obrigatoria"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
