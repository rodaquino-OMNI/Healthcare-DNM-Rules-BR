<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103641" name="Exame Funcional Trompa Eustaquio" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_40103641" name="Criterios de Autorizacao - Exame Funcional Trompa Eustaquio">
    <decisionTable id="DecisionTable_40103641" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpr_diagnosticoConfirmado" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoConfirmado">
          <text>"OMS_Cronica","Retracoes_Timpanicas","Pre_Timpanoplastia","Disfuncao_Tubaria","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpr_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Pre_Cirurgico_Timpanoplastia","Avaliacao_Disfuncao_Tubaria","Monitoramento_OMS","Pre_Implante_Coclear","Pesquisa"</text>
        </inputValues>
      </input>

      
      <input id="Input_tratamentosPrevios" label="Tratamentos Previos">
        <inputExpression id="InputExpr_tratamentosPrevios" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentosPrevios">
          <text>"Timpanometria","Tratamento_Clinico","Cirurgia_Otologica_Previa","Tubo_Ventilacao","Nenhum"</text>
        </inputValues>
      </input>

      
      <input id="Input_riscoCirurgico" label="Risco Cirurgico ASA">
        <inputExpression id="InputExpr_riscoCirurgico" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="InputValues_riscoCirurgico">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoIndicacao" label="Laudo de Indicacao Assinado">
        <inputExpression id="InputExpr_laudoIndicacao" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_40103641_Contraindicacao_1">
        <description>Reprovado - Timpanometria nao realizada - obrigatoria antes de exame funcional trompa</description>
        <inputEntry id="IE_Contra1_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_3">
          <text>not("Timpanometria")</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_2">
          <text>"Timpanometria nao realizada - obrigatoria antes de exame funcional trompa"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_3">
          <text>"ABORL-CCF 2023, AAO-HNS - Timpanometria deve preceder exame funcional tubario"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_40103641_Contraindicacao_2">
        <description>Reprovado - Indicacao cirurgica ou clinica nao documentada - exame funcional requer contexto especifico</description>
        <inputEntry id="IE_Contra2_1">
          <text>not("Pre_Timpanoplastia","Pre_Implante_Coclear")</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_2">
          <text>not("Pre_Cirurgico_Timpanoplastia","Pre_Implante_Coclear")</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_3">
          <text>"Nenhum"</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_2">
          <text>"Indicacao cirurgica ou clinica nao documentada - exame funcional requer contexto especifico"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_3">
          <text>"IFOS Consensus - Exame funcional trompa indicado principalmente em contexto pre-cirurgico"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_40103641_Aprovacao_1">
        <description>Aprovado - Exame funcional trompa pre-timpanoplastia - avaliar prognostico cirurgico</description>
        <inputEntry id="IE_Aprov1_1">
          <text>"Pre_Timpanoplastia","Retracoes_Timpanicas"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_2">
          <text>"Pre_Cirurgico_Timpanoplastia"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_3">
          <text>"Timpanometria","Tratamento_Clinico"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_2">
          <text>"Exame funcional trompa aprovado pre-timpanoplastia - avaliar prognostico cirurgico"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_3">
          <text>"ABORL-CCF 2023, AAO-HNS - Funcao tubaria prediz sucesso timpanoplastia"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103641_Aprovacao_2">
        <description>Aprovado - Exame funcional trompa para disfuncao tubaria cronica refrataria</description>
        <inputEntry id="IE_Aprov2_1">
          <text>"Disfuncao_Tubaria","OMS_Cronica"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_2">
          <text>"Avaliacao_Disfuncao_Tubaria","Monitoramento_OMS"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_3">
          <text>"Timpanometria","Tratamento_Clinico"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_2">
          <text>"Exame funcional trompa aprovado para disfuncao tubaria cronica refrataria"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_3">
          <text>"IFOS Consensus, ABORL-CCF - Exame funcional diferencia tipos disfuncao tubaria"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103641_Aprovacao_3">
        <description>Aprovado - Exame funcional trompa pre-implante coclear - avaliar risco complicacoes</description>
        <inputEntry id="IE_Aprov3_1">
          <text>"Pre_Implante_Coclear"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_2">
          <text>"Pre_Implante_Coclear"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_3">
          <text>"Timpanometria"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_2">
          <text>"Exame funcional trompa aprovado pre-implante coclear - avaliar risco complicacoes"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_3">
          <text>"AAO-HNS, ABORL-CCF - Funcao tubaria importante na avaliacao pre-implante coclear"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_40103641">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado</description>
        <inputEntry id="IE_Fallback_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Fallback_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_2">
          <text>"Dados incompletos para analise - documentacao complementar necessaria"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_3">
          <text>"ABORL-CCF 2023 - Documentacao otologica completa incluindo timpanometria obrigatoria"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>