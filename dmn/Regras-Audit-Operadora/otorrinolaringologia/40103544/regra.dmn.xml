<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103544" name="Polissonograma CPAP BiPAP" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_40103544" name="Criterios de Autorizacao - Polissonograma CPAP BiPAP">
    <decisionTable id="DecisionTable_40103544" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpr_diagnosticoConfirmado" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoConfirmado">
          <text>"SAOS_Confirmada","SAOS_Grave","Sindrome_Overlap","Hipoventilacao_Obesidade","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpr_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Titulacao_CPAP","Titulacao_BiPAP","Split_Night","Retitulacao_Apos_Cirurgia","Ajuste_Pressao"</text>
        </inputValues>
      </input>

      
      <input id="Input_tratamentosPrevios" label="Tratamentos Previos">
        <inputExpression id="InputExpr_tratamentosPrevios" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentosPrevios">
          <text>"PSG_Diagnostica","CPAP_Empirico","Cirurgia_SAOS","Perda_Peso","Nenhum"</text>
        </inputValues>
      </input>

      
      <input id="Input_riscoCirurgico" label="Risco Cirurgico ASA">
        <inputExpression id="InputExpr_riscoCirurgico" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="InputValues_riscoCirurgico">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoIndicacao" label="Laudo de Indicacao Assinado">
        <inputExpression id="InputExpr_laudoIndicacao" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_40103544_Contraindicacao_1">
        <description>Reprovado - PSG diagnostica nao realizada - obrigatoria antes de titulacao CPAP/BiPAP</description>
        <inputEntry id="IE_Contra1_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_2">
          <text>not("Split_Night")</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_3">
          <text>not("PSG_Diagnostica")</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_2">
          <text>"PSG diagnostica nao realizada - obrigatoria antes de titulacao CPAP/BiPAP"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_3">
          <text>"AASM 2023, SBPT - PSG diagnostica obrigatoria antes de titulacao exceto split-night IAH grave"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_40103544_Contraindicacao_2">
        <description>Reprovado - Retitulacao sem mudanca clinica significativa documentada</description>
        <inputEntry id="IE_Contra2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_2">
          <text>"Retitulacao_Apos_Cirurgia","Ajuste_Pressao"</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_3">
          <text>"CPAP_Empirico"</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_2">
          <text>"Retitulacao &amp;lt;6 meses sem mudanca clinica significativa - ganho/perda peso &amp;gt;10% ou cirurgia"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_3">
          <text>"AASM 2023 - Retitulacao apenas com intervalo minimo 6 meses e mudanca clinica documentada"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_40103544_Aprovacao_1">
        <description>Aprovado - Titulacao CPAP pos-diagnostico SAOS confirmada</description>
        <inputEntry id="IE_Aprov1_1">
          <text>"SAOS_Confirmada","SAOS_Grave"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_2">
          <text>"Titulacao_CPAP"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_3">
          <text>"PSG_Diagnostica"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_2">
          <text>"Titulacao CPAP aprovada pos-diagnostico SAOS confirmada - IAH documentado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_3">
          <text>"AASM 2023, ABORL-CCF - Titulacao CPAP obrigatoria para tratamento SAOS confirmada"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103544_Aprovacao_2">
        <description>Aprovado - Titulacao BiPAP para sindrome overlap ou hipoventilacao</description>
        <inputEntry id="IE_Aprov2_1">
          <text>"Sindrome_Overlap","Hipoventilacao_Obesidade"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_2">
          <text>"Titulacao_BiPAP"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_3">
          <text>"PSG_Diagnostica"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_2">
          <text>"Titulacao BiPAP aprovada para sindrome overlap ou hipoventilacao obesidade"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_3">
          <text>"SBPT Consenso, AASM 2023 - BiPAP indicado em overlap DPOC-SAOS ou hipoventilacao"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103544_Aprovacao_3">
        <description>Aprovado - Retitulacao apos cirurgia via aerea ou mudanca peso significativa</description>
        <inputEntry id="IE_Aprov3_1">
          <text>"SAOS_Confirmada"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_2">
          <text>"Retitulacao_Apos_Cirurgia"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_3">
          <text>"CPAP_Empirico","Cirurgia_SAOS","Perda_Peso"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_2">
          <text>"Retitulacao aprovada apos cirurgia via aerea ou mudanca peso &amp;gt;10%"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_3">
          <text>"ABORL-CCF, AASM 2023 - Retitulacao obrigatoria apos cirurgia SAOS ou mudanca ponderal"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_40103544">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado</description>
        <inputEntry id="IE_Fallback_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Fallback_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_2">
          <text>"Dados incompletos para analise - documentacao complementar necessaria"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_3">
          <text>"AASM 2023 - Documentacao PSG diagnostica obrigatoria para autorizacao titulacao"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>