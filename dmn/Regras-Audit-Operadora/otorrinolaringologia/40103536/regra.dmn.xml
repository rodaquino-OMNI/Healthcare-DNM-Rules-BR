<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103536" name="Polissonograma EEG Noite Inteira" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_40103536" name="Criterios de Autorizacao - Polissonograma EEG Noite Inteira">
    <decisionTable id="DecisionTable_40103536" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpr_diagnosticoConfirmado" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoConfirmado">
          <text>"Epilepsia_Noturna","Parassonias_Complexas","Narcolepsia","Disturbio_Movimento_Sono","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpr_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Diferencial_Epilepsia_vs_Parassonia","Epilepsia_Refrataria_Noturna","NREM_Parassonia_Atipica","Narcolepsia_com_EEG","Pesquisa"</text>
        </inputValues>
      </input>

      
      <input id="Input_tratamentosPrevios" label="Tratamentos Previos">
        <inputExpression id="InputExpr_tratamentosPrevios" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentosPrevios">
          <text>"PSG_Convencional","Avaliacao_Neurologica","EEG_Vigilia","Tratamento_Anticonvulsivante","Nenhum"</text>
        </inputValues>
      </input>

      
      <input id="Input_riscoCirurgico" label="Risco Cirurgico ASA">
        <inputExpression id="InputExpr_riscoCirurgico" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="InputValues_riscoCirurgico">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoIndicacao" label="Laudo de Indicacao Assinado">
        <inputExpression id="InputExpr_laudoIndicacao" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_40103536_Contraindicacao_1">
        <description>Reprovado - PSG convencional nao realizada previamente - obrigatoria antes de PSG com EEG</description>
        <inputEntry id="IE_Contra1_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_3">
          <text>not("PSG_Convencional")</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_2">
          <text>"PSG convencional nao realizada previamente - obrigatoria antes de PSG com EEG"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_3">
          <text>"AASM 2023 - PSG convencional deve preceder PSG-EEG para excluir disturbios primarios sono"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_40103536_Contraindicacao_2">
        <description>Reprovado - Avaliacao neurologica nao realizada - obrigatoria para indicacao PSG-EEG</description>
        <inputEntry id="IE_Contra2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_3">
          <text>not("Avaliacao_Neurologica")</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_2">
          <text>"Avaliacao neurologica nao realizada - obrigatoria para indicacao PSG-EEG"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_3">
          <text>"ABN, Liga Epilepsia - Avaliacao neurologica especializada obrigatoria antes de PSG-EEG"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_40103536_Aprovacao_1">
        <description>Aprovado - PSG-EEG para diferencial epilepsia noturna vs parassonia complexa</description>
        <inputEntry id="IE_Aprov1_1">
          <text>"Epilepsia_Noturna","Parassonias_Complexas"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_2">
          <text>"Diferencial_Epilepsia_vs_Parassonia"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_3">
          <text>"PSG_Convencional","Avaliacao_Neurologica","EEG_Vigilia"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_2">
          <text>"PSG-EEG aprovada para diferencial epilepsia noturna vs parassonia complexa"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_3">
          <text>"AASM 2023, ABN - PSG-EEG essencial para diagnostico diferencial eventos noturnos estereotipados"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103536_Aprovacao_2">
        <description>Aprovado - PSG-EEG para caracterizacao epilepsia noturna refrataria</description>
        <inputEntry id="IE_Aprov2_1">
          <text>"Epilepsia_Noturna"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_2">
          <text>"Epilepsia_Refrataria_Noturna"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_3">
          <text>"PSG_Convencional","Avaliacao_Neurologica","Tratamento_Anticonvulsivante"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_2">
          <text>"PSG-EEG aprovada para caracterizacao epilepsia noturna refrataria"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_3">
          <text>"Liga Epilepsia, ABN - PSG-EEG fundamental em epilepsia noturna refrataria para ajuste terapeutico"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103536_Aprovacao_3">
        <description>Aprovado - PSG-EEG para parassonias NREM atipicas ou violentas</description>
        <inputEntry id="IE_Aprov3_1">
          <text>"Parassonias_Complexas"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_2">
          <text>"NREM_Parassonia_Atipica"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_3">
          <text>"PSG_Convencional","Avaliacao_Neurologica"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_2">
          <text>"PSG-EEG aprovada para parassonias NREM atipicas ou violentas"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_3">
          <text>"AASM 2023 - PSG-EEG com video obrigatoria para parassonias atipicas com risco lesao"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_40103536">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado</description>
        <inputEntry id="IE_Fallback_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Fallback_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_2">
          <text>"Dados incompletos para analise - documentacao complementar necessaria"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_3">
          <text>"AASM 2023 - Documentacao neurologica completa obrigatoria para autorizacao PSG-EEG"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>