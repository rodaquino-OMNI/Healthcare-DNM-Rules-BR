<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_20104065" name="Remocao_Cerumen_Unilateral" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_20104065" name="Indicacao para Remocao de Cerume Unilateral">
    <decisionTable id="DecisionTable_20104065" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Otoscopia Confirmada">
        <inputExpression id="InputExpression_2" typeRef="boolean"><text>otoscopiaConfirmada</text></inputExpression>
      </input>
      <input id="Input_3" label="Sintomas Presentes">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>sintomasPresentes</text></inputExpression>
      </input>
      <input id="Input_4" label="Perfuracao Timpanica">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>perfuracaoTimpanica</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Perfuracao">
        <inputEntry id="IE_1"><text>-</text></inputEntry>
        <inputEntry id="IE_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>true</text></inputEntry>
        <inputEntry id="IE_5"><text>-</text></inputEntry>
        <outputEntry id="OE_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Contraindicacao: Perfuracao timpanica ativa documentada"</text></outputEntry>
        <outputEntry id="OE_3"><text>"ANS RN 465/2021 - Perfuracao timpanica requer tratamento previo antes de lavagem ceruminosa. Diretrizes ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Impactacao_H61">
        <inputEntry id="IE_6"><text>"H61.2"</text></inputEntry>
        <inputEntry id="IE_7"><text>true</text></inputEntry>
        <inputEntry id="IE_8"><text>true</text></inputEntry>
        <inputEntry id="IE_9"><text>false</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Impactacao de cerume confirmada por otoscopia com sintomatologia associada"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID H61.2 (Impactacao de cerume) com otoscopia confirmatoria. Protocolo ABORL-CCF 2023 item 3.2 - Lavagem ou aspiracao indicada"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Perda_Auditiva_H90">
        <inputEntry id="IE_11"><text>"H90.2"</text></inputEntry>
        <inputEntry id="IE_12"><text>true</text></inputEntry>
        <inputEntry id="IE_13"><text>true</text></inputEntry>
        <inputEntry id="IE_14"><text>false</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Perda auditiva condutiva secundaria a cerume impactado"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID H90.2 (Perda auditiva condutiva) com cerume obstrutivo documentado. Diretriz SBP 2024 secao 4.1"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Outros_CID_Otorrino">
        <inputEntry id="IE_16"><text>starts with(diagnosticoPrincipal, "H6") or starts with(diagnosticoPrincipal, "H9")</text></inputEntry>
        <inputEntry id="IE_17"><text>true</text></inputEntry>
        <inputEntry id="IE_18"><text>true</text></inputEntry>
        <inputEntry id="IE_19"><text>false</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"CID otorrinolaringologico compativel com necessidade de remocao de cerume"</text></outputEntry>
        <outputEntry id="OE_12"><text>"Diagnostico otologico (H60-H95) com laudo especializado. ANS RN 465/2021 Art. 18"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_20104065">
        <inputEntry id="IE_21"><text>-</text></inputEntry>
        <inputEntry id="IE_22"><text>-</text></inputEntry>
        <inputEntry id="IE_23"><text>-</text></inputEntry>
        <inputEntry id="IE_24"><text>-</text></inputEntry>
        <inputEntry id="IE_25"><text>-</text></inputEntry>
        <outputEntry id="OE_13"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_15"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>