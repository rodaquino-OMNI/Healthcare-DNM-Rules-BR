<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103455" name="Otoemissoes Acusticas Produto de Distorcao (DPOAE)" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_40103455" name="Criterios de Autorizacao - DPOAE">
    <decisionTable id="DecisionTable_40103455" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpr_diagnosticoConfirmado" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoConfirmado">
          <text>"Perda_Auditiva","Zumbido","Monitoramento_Ototoxicidade","Triagem_Neonatal","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpr_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Diagnostico_Topodiagnostico","Monitoramento_Ototoxicidade","Triagem_Auditiva","Avaliacao_Coclear","Pesquisa"</text>
        </inputValues>
      </input>

      
      <input id="Input_tratamentosPrevios" label="Tratamentos/Exames Previos">
        <inputExpression id="InputExpr_tratamentosPrevios" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentosPrevios">
          <text>"Nenhum","Audiometria_Recente","Tratamento_Ototoxicos","Cirurgia_Otologica","Protese_Auditiva"</text>
        </inputValues>
      </input>

      
      <input id="Input_riscoCirurgico" label="Classificacao ASA">
        <inputExpression id="InputExpr_riscoCirurgico" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="InputValues_riscoCirurgico">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoIndicacao" label="Laudo Indicacao Presente">
        <inputExpression id="InputExpr_laudoIndicacao" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_40103455_Contraindicacao_1">
        <description>Contraindicacao - Laudo de indicacao clinica nao apresentado</description>
        <inputEntry id="IE_Contra1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>false</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Laudo de indicacao clinica nao apresentado - obrigatorio para exames audiologicos especializados assinado por otorrinolaringologista ou pediatra"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"ABORL-CCF 2023 - Documentacao clinica obrigatoria para exames audiologicos especializados incluindo indicacao clara, otoscopia e exames previos"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_40103455_Contraindicacao_2">
        <description>Contraindicacao - Audiometria nao realizada previamente exceto triagem neonatal</description>
        <inputEntry id="IE_Contra2_1"><text>not("Triagem_Neonatal")</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>not("Triagem_Auditiva")</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>"Nenhum"</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Audiometria tonal liminar nao realizada previamente - obrigatoria exceto triagem neonatal. Respeitar hierarquia diagnostica em avaliacao auditiva"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"ABORL-CCF 2023 - Hierarquia diagnostica: otoscopia, audiometria tonal liminar, depois exames complementares (DPOAE, BERA). Excecao: triagem neonatal e monitoramento ototoxicidade."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_40103455_Aprovacao_1">
        <description>Aprovacao - Monitoramento ototoxicidade durante farmacos ototoxicos</description>
        <inputEntry id="IE_Aprov1_1"><text>"Monitoramento_Ototoxicidade"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>"Monitoramento_Ototoxicidade"</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>"Tratamento_Ototoxicos"</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Monitoramento ototoxicidade aprovado durante tratamento com farmacos ototoxicos - quimioterapia (cisplatina), aminoglicosideos, diureticos alca. Essencial prevencao perda auditiva permanente."</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"JCIH 2019, AAA Guidelines 2022 - Monitoramento obrigatorio DPOAE basal + seriado em quimioterapia ototoxica, aminoglicosideos IV, diureticos alca. Detecta perda precoce reversivel."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103455_Aprovacao_2">
        <description>Aprovacao - Topodiagnostico coclear investigacao perda auditiva</description>
        <inputEntry id="IE_Aprov2_1"><text>"Perda_Auditiva","Zumbido"</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>"Diagnostico_Topodiagnostico","Avaliacao_Coclear"</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>"Audiometria_Recente"</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Topodiagnostico coclear aprovado para investigacao etiologica de perda auditiva - diferencia lesoes cocleares de retrococleares (neurinoma, esclerose multipla)"</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"ABORL-CCF 2023 - DPOAE essencial na diferenciacao entre lesoes cocleares (presente) e retrococleares (ausente) quando audiometria mostra perda neurossensorial"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103455_Aprovacao_3">
        <description>Aprovacao - Triagem auditiva neonatal universal - Lei 12.303/2010</description>
        <inputEntry id="IE_Aprov3_1"><text>"Triagem_Neonatal"</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>"Triagem_Auditiva"</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>"Nenhum"</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Triagem auditiva neonatal aprovada - Lei Federal 12.303/2010 torna Teste Orelhinha obrigatorio para todos recem-nascidos em maternidades e hospitais"</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"Lei 12.303/2010, JCIH 2019 - Triagem auditiva neonatal universal obrigatoria (TEOAE ou DPOAE) preferencialmente antes alta hospitalar, maximo 1 mes vida"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_40103455">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado em otorrinolaringologia</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Dados incompletos para decisao automatizada - documentacao complementar necessaria. Documentar indicacao clinica clara, exames previos (audiometria), otoscopia"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"ABORL-CCF 2023 - Documentacao clinica completa obrigatoria para autorizacao: laudo indicacao assinado, otoscopia, audiometria recente (exceto neonatal), protocolo quimioterapia se monitoramento. Auditor otorrinolaringologista acionado para analise individualizada."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>