<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103528" name="Polissonografia de Noite Inteira (PSG)" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_40103528" name="Criterios de Autorizacao - Polissonografia PSG">
    <decisionTable id="DecisionTable_40103528" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpr_diagnosticoConfirmado" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoConfirmado">
          <text>"SAOS_Suspeita","Insonia_Cronica","Narcolepsia","Parassonias","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpr_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Diagnostico_SAOS","Avaliacao_Insonia","Narcolepsia_Hipersonia","Parassonias_Violentas","Pre_Cirurgico_SAOS"</text>
        </inputValues>
      </input>

      
      <input id="Input_tratamentosPrevios" label="Tratamentos/Avaliacoes Previas">
        <inputExpression id="InputExpr_tratamentosPrevios" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentosPrevios">
          <text>"Nenhum","Questionario_Sono","Avaliacao_Clinica","CPAP_Trial","Cirurgia_Via_Aerea"</text>
        </inputValues>
      </input>

      
      <input id="Input_riscoCirurgico" label="Classificacao ASA">
        <inputExpression id="InputExpr_riscoCirurgico" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="InputValues_riscoCirurgico">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoIndicacao" label="Laudo Indicacao Presente">
        <inputExpression id="InputExpr_laudoIndicacao" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_40103528_Contraindicacao_1">
        <description>Contraindicacao - Avaliacao clinica previa nao realizada com questionarios validados</description>
        <inputEntry id="IE_Contra1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>"Nenhum"</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Avaliacao clinica previa nao realizada - obrigatoria antes da PSG com questionarios validados (Escala Epworth, STOP-BANG, Pittsburgh)"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"AASM 2023, ABS - Avaliacao clinica com questionarios validados obrigatoria antes da PSG para triagem apropriada e interpretacao resultados"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_40103528_Contraindicacao_2">
        <description>Contraindicacao - PSG repetida &lt;6 meses sem mudanca clinica significativa</description>
        <inputEntry id="IE_Contra2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>"CPAP_Trial","Cirurgia_Via_Aerea"</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"PSG repetida &lt;6 meses sem mudanca clinica significativa documentada - aguardar intervalo minimo ou documentar mudanca peso &gt;10%, sintomas novos, intolerancia CPAP"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"AASM 2023 - PSG de controle apenas com intervalo minimo 6 meses e mudanca clinica significativa (peso &gt;10%, sintomas novos, pos-cirurgia via aerea &gt;3 meses)"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_40103528_Aprovacao_1">
        <description>Aprovacao - PSG diagnostica SAOS com sintomas significativos Epworth &gt;10 ou STOP-BANG &gt;=3</description>
        <inputEntry id="IE_Aprov1_1"><text>"SAOS_Suspeita"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>"Diagnostico_SAOS"</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>"Questionario_Sono","Avaliacao_Clinica"</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"PSG diagnostica SAOS aprovada - Escala Epworth &gt;10 ou 3+ criterios STOP-BANG indicam probabilidade intermediaria/alta apneia sono"</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"ABORL-CCF Consenso SAOS 2022, AASM 2023 - PSG padrao-ouro para diagnostico SAOS. Epworth &gt;10 ou STOP-BANG &gt;=3 indicam alta probabilidade pre-teste."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103528_Aprovacao_2">
        <description>Aprovacao - PSG para investigacao narcolepsia ou insonia cronica refrataria</description>
        <inputEntry id="IE_Aprov2_1"><text>"Narcolepsia","Insonia_Cronica"</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>"Narcolepsia_Hipersonia","Avaliacao_Insonia"</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>"Avaliacao_Clinica"</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"PSG aprovada para investigacao narcolepsia (seguida TLMS) ou insonia cronica refrataria a tratamento comportamental (TCC-I, higiene sono)"</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"AASM 2023, ABS - PSG necessaria para diagnostico diferencial de disturbios sono: narcolepsia (seguida TLMS), insonia com suspeita SAOS comorbida, movimentos periodicos membros"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103528_Aprovacao_3">
        <description>Aprovacao - PSG pre-operatoria cirurgia bariatrica ou via aerea em pacientes risco SAOS</description>
        <inputEntry id="IE_Aprov3_1"><text>"SAOS_Suspeita"</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>"Pre_Cirurgico_SAOS"</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>"Avaliacao_Clinica"</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>"ASA_II","ASA_III","ASA_IV"</text></inputEntry>
        <inputEntry id="IE_Aprov3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"PSG pre-operatoria aprovada para cirurgia bariatrica ou via aerea superior em pacientes de risco SAOS (obesos, STOP-BANG &gt;=3, comorbidades)"</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"ABORL-CCF, SBCBM - PSG obrigatoria antes de cirurgia bariatrica ou via aerea (uvulopalatofaringoplastia, amigdalectomia adultos) em pacientes risco SAOS para ajuste anestesico e profilaxia complicacoes"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103528_Aprovacao_4">
        <description>Aprovacao - PSG para caracterizacao parassonias violentas ou atipicas com video</description>
        <inputEntry id="IE_Aprov4_1"><text>"Parassonias"</text></inputEntry>
        <inputEntry id="IE_Aprov4_2"><text>"Parassonias_Violentas"</text></inputEntry>
        <inputEntry id="IE_Aprov4_3"><text>"Avaliacao_Clinica"</text></inputEntry>
        <inputEntry id="IE_Aprov4_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov4_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov4_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov4_2"><text>"PSG com video aprovada para caracterizacao de parassonias violentas ou atipicas - diagnostico diferencial epilepsia frontal noturna"</text></outputEntry>
        <outputEntry id="OE_Aprov4_3"><text>"AASM 2023 - PSG com video obrigatoria para parassonias violentas ou atipicas (REM sem atonia, terror noturno adultos, sonambulismo violento) para diagnostico diferencial epilepsia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_40103528">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado em medicina do sono/otorrinolaringologia</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Dados incompletos para decisao automatizada - documentacao complementar necessaria. Documentar questionarios validados (Epworth, STOP-BANG), comorbidades, tentativas tratamento conservador"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"AASM 2023 - Documentacao clinica completa obrigatoria para autorizacao PSG: questionarios validados (Epworth, STOP-BANG, Pittsburgh), IMC, circunferencia cervical, comorbidades (HAS, DM, IAM, AVC, FA), tentativas tratamento conservador em insonia. Auditor medico especialista medicina sono acionado para analise individualizada."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>