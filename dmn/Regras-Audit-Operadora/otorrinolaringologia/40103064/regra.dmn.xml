<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103064" name="Audiometria_Tronco_Cerebral_PEATE_BERA" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40103064" name="Indicacao para Audiometria de Tronco Cerebral (PEATE/BERA)">
    <decisionTable id="DecisionTable_40103064" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Idade do Paciente (anos)">
        <inputExpression id="InputExpression_2" typeRef="integer"><text>idadePaciente</text></inputExpression>
      </input>
      <input id="Input_3" label="Risco Neonatal (UTI/Prematuro/Baixo Peso)">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>riscoNeonatal</text></inputExpression>
      </input>
      <input id="Input_4" label="Suspeita Neuropatia/Lesao Retrococlear">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>suspeitaRetro coclear</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Aprovado_Triagem_Neonatal_Z13">
        <inputEntry id="IE_1"><text>"Z13.5", "P07.1", "P07.3"</text></inputEntry>
        <inputEntry id="IE_2"><text>&amp;lt;= 1</text></inputEntry>
        <inputEntry id="IE_3"><text>true</text></inputEntry>
        <inputEntry id="IE_4"><text>-</text></inputEntry>
        <inputEntry id="IE_5"><text>true</text></inputEntry>
        <outputEntry id="OE_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Recém-nascido de alto risco (UTI neonatal/prematuro/baixo peso). PEATE indicado para triagem auditiva obrigatoria"</text></outputEntry>
        <outputEntry id="OE_3"><text>"CID Z13.5/P07.x (Triagem/Prematuro). JCIH 2019: PEATE obrigatorio em neonatos alto risco antes alta hospitalar. Portaria MS 2073/2004: Programa Nacional Triagem Neonatal. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Crianca_Nao_Cooperativa_H90">
        <inputEntry id="IE_6"><text>"H90.3", "H90.5", "H91.9"</text></inputEntry>
        <inputEntry id="IE_7"><text>&amp;lt;= 4</text></inputEntry>
        <inputEntry id="IE_8"><text>-</text></inputEntry>
        <inputEntry id="IE_9"><text>-</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Crianca &amp;lt;=4 anos com suspeita de perda auditiva. PEATE indicado para avaliacao objetiva (nao cooperativa para audiometria comportamental)"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID H90.x/H91.x (Surdez). AAO-HNS 2021: PEATE metodo objetivo gold standard em criancas &amp;lt;4 anos nao cooperativas. ANS RN 465/2021 aprovacao automatica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Neuropatia_Auditiva_H93">
        <inputEntry id="IE_11"><text>"H93.2", "H93.3"</text></inputEntry>
        <inputEntry id="IE_12"><text>-</text></inputEntry>
        <inputEntry id="IE_13"><text>-</text></inputEntry>
        <inputEntry id="IE_14"><text>true</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Suspeita de neuropatia auditiva ou lesao retrococlear (nervo auditivo/tronco cerebral). PEATE diagnostico essencial"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID H93.2/H93.3 (Disturbios nervo auditivo). ABORL-CCF 2023: PEATE diferencia surdez coclear vs. retrococlear. Latencias ondas I-V anormais: neuropatia confirmada. AAO-HNS 2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Neurinoma_Acustico_D33">
        <inputEntry id="IE_16"><text>"D33.3", "C72.4"</text></inputEntry>
        <inputEntry id="IE_17"><text>-</text></inputEntry>
        <inputEntry id="IE_18"><text>-</text></inputEntry>
        <inputEntry id="IE_19"><text>true</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Suspeita ou diagnostico de neurinoma do acustico (schwannoma vestibular). PEATE pre-operatorio obrigatorio"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID D33.3/C72.4 (Tumor nervo VIII). AAO-HNS 2021: PEATE pré-cirurgico em angulo pontocerebelar: mapeamento funcional + monitorizacao intraoperatoria. ABORL-CCF 2023 secao 14.5"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Vertigem_Tinitus_H81">
        <inputEntry id="IE_21"><text>"H81.0", "H81.1", "H93.1"</text></inputEntry>
        <inputEntry id="IE_22"><text>-</text></inputEntry>
        <inputEntry id="IE_23"><text>-</text></inputEntry>
        <inputEntry id="IE_24"><text>true</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Vertigem/tinitus com audiometria normal. PEATE investiga lesao retrococlear oculta"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID H81.x/H93.1 (Vertigem/Tinitus). ABORL-CCF 2023: PEATE detecta lesoes tronco cerebral nao identificadas por audiometria tonal. AAO-HNS 2021 secao 6.3"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_40103064">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>