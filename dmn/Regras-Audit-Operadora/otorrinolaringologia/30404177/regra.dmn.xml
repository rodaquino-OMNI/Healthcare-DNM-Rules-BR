<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30404177" name="Protese_Auditiva_Percutanea_BAHA" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30404177" name="Indicacao para Protese Auditiva Ancorada no Osso (BAHA)">
    <decisionTable id="DecisionTable_30404177" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Audiometria - Gap Osseo-Aereo (dB)">
        <inputExpression id="InputExpression_2" typeRef="string"><text>audiometria</text></inputExpression>
      </input>
      <input id="Input_3" label="Tipo de Perda Auditiva">
        <inputExpression id="InputExpression_3" typeRef="string"><text>tipoPerda</text></inputExpression>
      </input>
      <input id="Input_4" label="Tentativa AASI Convencional">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>tentativaAASI</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Neurossensorial">
        <inputEntry id="IE_1"><text>-</text></inputEntry>
        <inputEntry id="IE_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3"><text>"neurossensorial"</text></inputEntry>
        <inputEntry id="IE_4"><text>-</text></inputEntry>
        <inputEntry id="IE_5"><text>-</text></inputEntry>
        <outputEntry id="OE_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Contraindicacao: BAHA indicado apenas em perdas condutivas ou mistas. Perda neurossensorial requer implante coclear"</text></outputEntry>
        <outputEntry id="OE_3"><text>"AAO-HNS 2022: BAHA aprovado exclusivamente em perdas condutivas/mistas com via ossea preservada. Surdez neurossensorial: considerar IC. ABORL-CCF 2023 secao 11.3"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraindicacao_Gap_Insuficiente">
        <inputEntry id="IE_6"><text>-</text></inputEntry>
        <inputEntry id="IE_7"><text>"&amp;lt;15dB", "gap &amp;lt;10dB"</text></inputEntry>
        <inputEntry id="IE_8"><text>-</text></inputEntry>
        <inputEntry id="IE_9"><text>-</text></inputEntry>
        <inputEntry id="IE_10"><text>-</text></inputEntry>
        <outputEntry id="OE_4"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Contraindicacao: Gap osseo-aereo &amp;lt;15dB. Beneficio clinico insuficiente para BAHA"</text></outputEntry>
        <outputEntry id="OE_6"><text>"FDA 2019 Criteria: BAHA requer gap osseo-aereo minimo de 15dB para justificar procedimento cirurgico. Gap &amp;lt;15dB: AASI convencional mais adequado. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Atresia_CAE_Q16">
        <inputEntry id="IE_11"><text>"Q16.1", "Q16.9"</text></inputEntry>
        <inputEntry id="IE_12"><text>"&amp;gt;=20dB", "gap &amp;gt;=15dB"</text></inputEntry>
        <inputEntry id="IE_13"><text>"condutiva", "mista"</text></inputEntry>
        <inputEntry id="IE_14"><text>true</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Atresia congenita de CAE com gap osseo-aereo &amp;gt;=20dB. AASI convencional impossivel. BAHA indicado"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID Q16.x (Atresia de CAE) com audiometria gap &amp;gt;=20dB. AAO-HNS 2022 Criterio Principal: Atresia bilateral/unilateral com via ossea preservada. BAHA primeira linha. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Otite_Cronica_H66">
        <inputEntry id="IE_16"><text>"H66.1", "H66.2", "H66.3"</text></inputEntry>
        <inputEntry id="IE_17"><text>"&amp;gt;=15dB", "gap &amp;gt;=20dB"</text></inputEntry>
        <inputEntry id="IE_18"><text>"condutiva", "mista"</text></inputEntry>
        <inputEntry id="IE_19"><text>true</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Otite media cronica supurativa bilateral com otorreia recorrente. AASI convencional contraindicado. BAHA alternativa eficaz"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID H66.x (Otite cronica) com gap &amp;gt;=15dB. ANS RN 465/2021: BAHA aprovado em otorreia cronica refrataria onde AASI exacerba infeccao. AAO-HNS 2022 secao 4.2"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Malformacao_Q17">
        <inputEntry id="IE_21"><text>"Q17.2", "Q87.0"</text></inputEntry>
        <inputEntry id="IE_22"><text>"&amp;gt;=20dB", "gap &amp;gt;=15dB"</text></inputEntry>
        <inputEntry id="IE_23"><text>"condutiva", "mista"</text></inputEntry>
        <inputEntry id="IE_24"><text>true</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Malformacao congenita de orelha (Treacher Collins, microtia) com perda condutiva/mista. BAHA indicado"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID Q17.2/Q87.0 (Sindromes craniofaciais) com gap &amp;gt;=15dB. FDA 2019: BAHA aprovado em malformacoes congenitas bilaterais. Protocolo MS 2024 secao 9.1"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Sindrome_Down_Q90">
        <inputEntry id="IE_26"><text>"Q90.9"</text></inputEntry>
        <inputEntry id="IE_27"><text>"&amp;gt;=15dB", "gap &amp;gt;=20dB"</text></inputEntry>
        <inputEntry id="IE_28"><text>"condutiva", "mista"</text></inputEntry>
        <inputEntry id="IE_29"><text>true</text></inputEntry>
        <inputEntry id="IE_30"><text>true</text></inputEntry>
        <outputEntry id="OE_16"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Sindrome de Down com otite secretora recorrente e perda auditiva condutiva bilateral. BAHA recomendado"</text></outputEntry>
        <outputEntry id="OE_18"><text>"CID Q90.9 (Down) com gap &amp;gt;=15dB. AAO-HNS 2022: Criancas Down com otite secretora cronica bilateral: BAHA superior a AASI. Menor risco infeccao. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30404177">
        <inputEntry id="IE_31"><text>-</text></inputEntry>
        <inputEntry id="IE_32"><text>-</text></inputEntry>
        <inputEntry id="IE_33"><text>-</text></inputEntry>
        <inputEntry id="IE_34"><text>-</text></inputEntry>
        <inputEntry id="IE_35"><text>-</text></inputEntry>
        <outputEntry id="OE_19"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_20"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_21"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>