<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30501369" name="Septoplastia_Turbinectomia_Bilateral" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30501369" name="Indicacao para Septoplastia e Turbinectomia Bilateral">
    <decisionTable id="DecisionTable_30501369" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Escala NOSE (Obstrucao Nasal 0-100)">
        <inputExpression id="InputExpression_2" typeRef="integer"><text>escalaNOSE</text></inputExpression>
      </input>
      <input id="Input_3" label="Rinoscopia/TC Bilateral Documentada">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>rinoscopiaDocumentada</text></inputExpression>
      </input>
      <input id="Input_4" label="Tratamento Conservador (meses)">
        <inputExpression id="InputExpression_4" typeRef="integer"><text>tratamentoConservador</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_NOSE_Baixo">
        <inputEntry id="IE_1"><text>-</text></inputEntry>
        <inputEntry id="IE_2"><text>&amp;lt; 50</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>-</text></inputEntry>
        <inputEntry id="IE_5"><text>-</text></inputEntry>
        <outputEntry id="OE_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Contraindicacao: Escala NOSE &amp;lt;50. Obstrucao nasal leve/moderada nao justifica cirurgia bilateral"</text></outputEntry>
        <outputEntry id="OE_3"><text>"AAO-HNS 2022: Turbinectomia bilateral indicada apenas NOSE &amp;gt;=75 ou obstrucao moderada-grave &amp;gt;=50. Casos leves: tratamento conservador. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Obstrucao_Grave_J34">
        <inputEntry id="IE_6"><text>"J34.2", "J34.0"</text></inputEntry>
        <inputEntry id="IE_7"><text>&amp;gt;= 75</text></inputEntry>
        <inputEntry id="IE_8"><text>true</text></inputEntry>
        <inputEntry id="IE_9"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Obstrucao nasal bilateral grave (NOSE &amp;gt;=75) por desvio septal + hipertrofia cornetos. Refrataria a tratamento &amp;gt;=3 meses"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID J34.2+J34.0 com NOSE &amp;gt;=75. SBR 2024 Criterio Principal: Cirurgia combinada (septoplastia + turbinectomia) em obstrucao bilateral grave. ANS RN 465/2021 aprovacao automatica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Obstrucao_Moderada_J34">
        <inputEntry id="IE_11"><text>"J34.2", "J34.0", "J34.3"</text></inputEntry>
        <inputEntry id="IE_12"><text>&amp;gt;= 50</text></inputEntry>
        <inputEntry id="IE_13"><text>true</text></inputEntry>
        <inputEntry id="IE_14"><text>&amp;gt;= 6</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Obstrucao nasal bilateral moderada (NOSE 50-75) refrataria a tratamento prolongado &amp;gt;=6 meses"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID J34.x com NOSE 50-75. AAO-HNS 2022: Obstrucao moderada cronica &amp;gt;=6 meses com falha terapeutica: cirurgia justificada. ABORL-CCF 2023 secao 10.2"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Apneia_G47">
        <inputEntry id="IE_16"><text>"G47.3"</text></inputEntry>
        <inputEntry id="IE_17"><text>&amp;gt;= 60</text></inputEntry>
        <inputEntry id="IE_18"><text>true</text></inputEntry>
        <inputEntry id="IE_19"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Apneia do sono com obstrucao nasal bilateral anatomica. Cirurgia combinada melhora adesao ao CPAP"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID G47.3 (Apneia) com obstrucao nasal &amp;gt;=60. SBR 2024: Correcao nasal bilateral em apneia: reducao AHI em 30% + adesao CPAP. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Sinusite_Cronica_J32">
        <inputEntry id="IE_21"><text>"J32.0", "J32.4", "J32.9"</text></inputEntry>
        <inputEntry id="IE_22"><text>&amp;gt;= 50</text></inputEntry>
        <inputEntry id="IE_23"><text>true</text></inputEntry>
        <inputEntry id="IE_24"><text>&amp;gt;= 6</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Sinusite cronica recorrente secundaria a obstrucao nasal bilateral. Cirurgia combinada restaura drenagem sinusal"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID J32.x (Sinusite cronica). AAO-HNS 2022: Desvio septal + hipertrofia cornetos impedem drenagem: correcao bilateral indicada. TC demonstrando obstrucao. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30501369">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>