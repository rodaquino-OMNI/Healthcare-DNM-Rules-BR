      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Infeccao_Aguda">
        <inputEntry id="IE_1"><text>"J03.9"</text></inputEntry>
        <inputEntry id="IE_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>-</text></inputEntry>
        <inputEntry id="IE_5"><text>-</text></inputEntry>
        <outputEntry id="OE_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Contraindicacao: Amigdalite aguda em atividade. Cirurgia eletiva deve aguardar resolucao do quadro"</text></outputEntry>
        <outputEntry id="OE_3"><text>"AAO-HNS 2019 Guideline Statement 2 - Cirurgia deve ser realizada &amp;gt;2-3 semanas apos episodio agudo. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Amigdalites_Recorrentes_J35">
        <inputEntry id="IE_6"><text>"J35.0", "J35.1", "J35.3"</text></inputEntry>
        <inputEntry id="IE_7"><text>&amp;gt;= 7</text></inputEntry>
        <inputEntry id="IE_8"><text>-</text></inputEntry>
        <inputEntry id="IE_9"><text>-</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Amigdalites recorrentes ≥7 episodios/ano conforme criterio Paradise modificado"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID J35.x com frequencia documentada. AAO-HNS 2019 Statement 3 (Paradise Criteria): ≥7 episodios/ano indica cirurgia. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_SAHOS_G47">
        <inputEntry id="IE_11"><text>"G47.3"</text></inputEntry>
        <inputEntry id="IE_12"><text>-</text></inputEntry>
        <inputEntry id="IE_13"><text>"AHI &amp;gt;5", "AHI grave"</text></inputEntry>
        <inputEntry id="IE_14"><text>"III", "IV"</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Apneia obstrutiva do sono (SAHOS) com AHI patologico e hipertrofia amigdaliana obstrutiva"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID G47.3 com polissonografia (AHI &amp;gt;5). AAO-HNS 2019 Statement 5: Adenoamigdalectomia indicada em SAHOS pediatrica. Mallampati III/IV. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Hipertrofia_J35">
