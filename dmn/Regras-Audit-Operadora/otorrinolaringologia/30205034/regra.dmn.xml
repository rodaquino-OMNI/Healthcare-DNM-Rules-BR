<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30205034" name="Adenoamigdalectomia" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30205034" name="Indicacao para Adenoamigdalectomia">
    <decisionTable id="DecisionTable_30205034" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Episodios Anuais de Amigdalite">
        <inputExpression id="InputExpression_2" typeRef="integer"><text>episodiosAmigdaliteAno</text></inputExpression>
      </input>
      <input id="Input_3" label="Polissonografia (AHI Index)">
        <inputExpression id="InputExpression_3" typeRef="string"><text>polissonografia</text></inputExpression>
      </input>
      <input id="Input_4" label="Grau Mallampati">
        <inputExpression id="InputExpression_4" typeRef="string"><text>grauMallampati</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
