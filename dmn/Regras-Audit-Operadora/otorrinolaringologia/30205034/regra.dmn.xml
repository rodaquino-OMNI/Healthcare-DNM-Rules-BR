        <inputEntry id="IE_17"><text>-</text></inputEntry>
        <inputEntry id="IE_18"><text>-</text></inputEntry>
        <inputEntry id="IE_19"><text>"III", "IV"</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Hipertrofia adenoamigdaliana obstrutiva Mallampati III/IV com indicacao cirurgica"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID J35.x com Mallampati III/IV. Protocolo SBP 2024 secao 6.3 - Obstrucao grave justifica cirurgia. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Abscesso_J36">
        <inputEntry id="IE_21"><text>"J36"</text></inputEntry>
        <inputEntry id="IE_22"><text>&amp;gt;= 2</text></inputEntry>
        <inputEntry id="IE_23"><text>-</text></inputEntry>
        <inputEntry id="IE_24"><text>-</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Abscesso periamigdaliano recorrente (≥2 episodios) com indicacao de amigdalectomia eletiva"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID J36 recorrente. AAO-HNS 2019 Statement 4: Abscesso periamigdaliano recorrente indica cirurgia. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30205034">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
