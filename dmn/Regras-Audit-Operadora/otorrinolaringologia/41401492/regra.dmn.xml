<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_41401492" name="VENG Vectoeletronistagmografia" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_41401492" name="Criterios de Autorizacao - VENG Vectoeletronistagmografia">
    <decisionTable id="DecisionTable_41401492" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpr_diagnosticoConfirmado" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoConfirmado">
          <text>"Vertigem","VPPB","Doenca_Meniere","Neurite_Vestibular","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpr_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Avaliacao_Funcao_Vestibular","Diagnostico_Diferencial_Vertigem","Monitoramento_Meniere","Pre_Cirurgico_Otoneurologico","Pericia_Laboral"</text>
        </inputValues>
      </input>

      
      <input id="Input_tratamentosPrevios" label="Tratamentos Previos">
        <inputExpression id="InputExpr_tratamentosPrevios" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentosPrevios">
          <text>"Avaliacao_Clinica_ORL","Manobra_Dix_Hallpike","Audiometria","Nenhum","Tratamento_Reabilitacao_Vestibular"</text>
        </inputValues>
      </input>

      
      <input id="Input_riscoCirurgico" label="Risco Cirurgico ASA">
        <inputExpression id="InputExpr_riscoCirurgico" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="InputValues_riscoCirurgico">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoIndicacao" label="Laudo de Indicacao Assinado">
        <inputExpression id="InputExpr_laudoIndicacao" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_41401492_Contraindicacao_1">
        <description>Reprovado - Avaliacao clinica ORL nao realizada - obrigatoria antes de VENG para contexto diagnostico</description>
        <inputEntry id="IE_Contra1_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_3">
          <text>not("Avaliacao_Clinica_ORL")</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_2">
          <text>"Avaliacao clinica ORL nao realizada - obrigatoria antes de VENG para contexto diagnostico"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_3">
          <text>"ABORL-CCF 2023, Barany Society - Avaliacao clinica ORL obrigatoria antes de testes vestibulares"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_41401492_Contraindicacao_2">
        <description>Reprovado - VPPB sem falha manobras reposicionamento - VENG nao indicado para VPPB tipico responsivo</description>
        <inputEntry id="IE_Contra2_1">
          <text>"VPPB"</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_3">
          <text>not("Manobra_Dix_Hallpike")</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_2">
          <text>"VPPB sem falha manobras reposicionamento - VENG nao indicado para VPPB tipico responsivo"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_3">
          <text>"Barany Society, AAO-HNS - VPPB tipico diagnostico clinico sem necessidade VENG"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_41401492_Aprovacao_1">
        <description>Aprovado - VENG para vertigem persistente &amp;gt;4 semanas sem diagnostico definido</description>
        <inputEntry id="IE_Aprov1_1">
          <text>"Vertigem"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_2">
          <text>"Diagnostico_Diferencial_Vertigem"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_3">
          <text>"Avaliacao_Clinica_ORL","Manobra_Dix_Hallpike"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_2">
          <text>"VENG aprovado para vertigem persistente &amp;gt;4 semanas sem diagnostico definido"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_3">
          <text>"ABORL-CCF 2023, Barany Society - VENG essencial no diagnostico diferencial vertigem cronica"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_41401492_Aprovacao_2">
        <description>Aprovado - VENG para monitoramento funcao vestibular em doenca de Meniere</description>
        <inputEntry id="IE_Aprov2_1">
          <text>"Doenca_Meniere"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_2">
          <text>"Monitoramento_Meniere","Avaliacao_Funcao_Vestibular"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_3">
          <text>"Avaliacao_Clinica_ORL","Audiometria"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_2">
          <text>"VENG aprovado para monitoramento funcao vestibular em doenca de Meniere"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_3">
          <text>"AAO-HNS 2020, ABORL-CCF - VENG util no acompanhamento funcional Meniere"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_41401492_Aprovacao_3">
        <description>Aprovado - VENG pre-operatorio cirurgia otoneurologica para avaliacao funcional basal</description>
        <inputEntry id="IE_Aprov3_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_2">
          <text>"Pre_Cirurgico_Otoneurologico"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_3">
          <text>"Avaliacao_Clinica_ORL","Audiometria"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_4">
          <text>"ASA_II","ASA_III","ASA_IV"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_2">
          <text>"VENG aprovado pre-operatorio cirurgia otoneurologica para avaliacao funcional basal"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_3">
          <text>"ABORL-CCF, Barany Society - VENG basal obrigatorio antes de cirurgia vestibular"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_41401492_Aprovacao_4">
        <description>Aprovado - VENG para pericia medica laboral - documentacao objetiva funcao vestibular</description>
        <inputEntry id="IE_Aprov4_1">
          <text>"Vertigem","Neurite_Vestibular"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov4_2">
          <text>"Pericia_Laboral"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov4_3">
          <text>"Avaliacao_Clinica_ORL"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov4_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov4_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov4_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov4_2">
          <text>"VENG aprovado para pericia medica laboral - documentacao objetiva funcao vestibular"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov4_3">
          <text>"ABORL-CCF, Barany Society - VENG fundamental na documentacao pericial de disturbios vestibulares"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_41401492">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado</description>
        <inputEntry id="IE_Fallback_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Fallback_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_2">
          <text>"Dados incompletos para analise - documentacao complementar necessaria"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_3">
          <text>"ABORL-CCF 2023 - Documentacao otoneurologica completa obrigatoria para autorizacao VENG"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>