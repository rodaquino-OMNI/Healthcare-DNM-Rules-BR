<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103463" name="Otoemissoes Evocadas Transientes (TEOAE/Teste Orelhinha)" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_40103463" name="Criterios de Autorizacao - TEOAE Teste Orelhinha">
    <decisionTable id="DecisionTable_40103463" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpr_diagnosticoConfirmado" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoConfirmado">
          <text>"Triagem_Neonatal","Perda_Auditiva_Suspeita","Monitoramento","Avaliacao_Coclear","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpr_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Triagem_Universal_Neonatal","Confirmatoria_Falha_Triagem","Monitoramento_Risco","Avaliacao_Funcional","Pesquisa"</text>
        </inputValues>
      </input>

      
      <input id="Input_tratamentosPrevios" label="Tratamentos/Testes Previos">
        <inputExpression id="InputExpr_tratamentosPrevios" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentosPrevios">
          <text>"Primeiro_Teste","Reteste_Falha","Pos_UTI_Neonatal","Acompanhamento","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_riscoCirurgico" label="Classificacao ASA">
        <inputExpression id="InputExpr_riscoCirurgico" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="InputValues_riscoCirurgico">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoIndicacao" label="Laudo Indicacao Presente">
        <inputExpression id="InputExpr_laudoIndicacao" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_40103463_Contraindicacao_1">
        <description>Contraindicacao - Reteste sem falha documentada no teste inicial</description>
        <inputEntry id="IE_Contra1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>"Confirmatoria_Falha_Triagem"</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>not("Reteste_Falha")</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Reteste solicitado sem registro de falha no teste inicial - documentar laudo primeiro teste com resultado 'falha' ou 'refer' antes reteste"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"JCIH 2019 - Reteste obrigatorio apenas apos falha documentada na triagem inicial. Resultado 'passa' nao requer reteste salvo fatores risco tardios."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_40103463_Contraindicacao_2">
        <description>Contraindicacao - Solicitacao fora periodo neonatal sem indicacao especifica</description>
        <inputEntry id="IE_Contra2_1"><text>not("Triagem_Neonatal")</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>not("Triagem_Universal_Neonatal")</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>not("Primeiro_Teste")</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Solicitacao fora do periodo neonatal sem indicacao especifica - Teste Orelhinha restrito a neonatos ate 3 meses salvo indicacoes especiais (pos-UTI, meningite)"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"Lei 12.303/2010, SBP 2023 - Teste Orelhinha (TEOAE) restrito ao periodo neonatal (ate 3 meses). Apos 3 meses usar DPOAE, BERA ou audiometria comportamental."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_40103463_Aprovacao_1">
        <description>Aprovacao - Triagem auditiva neonatal universal - Lei 12.303/2010</description>
        <inputEntry id="IE_Aprov1_1"><text>"Triagem_Neonatal"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>"Triagem_Universal_Neonatal"</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>"Primeiro_Teste"</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov1_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Triagem auditiva neonatal universal aprovada - Lei Federal 12.303/2010 torna Teste Orelhinha obrigatorio para todos recem-nascidos em maternidades e hospitais"</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"Lei 12.303/2010 - Teste Orelhinha obrigatorio para todos recem-nascidos em maternidades publicas e privadas, preferencialmente antes alta hospitalar, maximo 1 mes vida."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103463_Aprovacao_2">
        <description>Aprovacao - Reteste apos falha na triagem inicial - dentro 30 dias</description>
        <inputEntry id="IE_Aprov2_1"><text>"Triagem_Neonatal","Perda_Auditiva_Suspeita"</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>"Confirmatoria_Falha_Triagem"</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>"Reteste_Falha"</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov2_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Reteste aprovado apos falha na triagem inicial - essencial para confirmacao diagnostica e reducao falsos positivos (vernix, debris, agitacao)"</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"JCIH 2019 - Reteste obrigatorio em 30 dias apos falha inicial para reduzir falsos positivos (50% normalizacao). Falha persistente encaminhar BERA diagnostico."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103463_Aprovacao_3">
        <description>Aprovacao - Monitoramento auditivo pos-UTI neonatal - alto risco</description>
        <inputEntry id="IE_Aprov3_1"><text>"Monitoramento"</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>"Monitoramento_Risco"</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>"Pos_UTI_Neonatal"</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Aprov3_5"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Monitoramento auditivo pos-UTI neonatal aprovado - alto risco para perda auditiva (ventilacao mecanica, aminoglicosideos, hiperbilirrubinemia, sepse)"</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"JCIH 2019, SBP 2023 - Neonatos pos-UTI (especialmente &amp;gt;5 dias, ventilacao mecanica, ECMO, aminoglicosideos IV) requerem monitoramento auditivo seriado ate 6 meses."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_40103463">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado em otorrinolaringologia/pediatria</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_5"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Dados incompletos para decisao automatizada - documentacao complementar necessaria. Documentar dados nascimento, teste inicial se reteste, intercorrencias neonatais"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"JCIH 2019 - Documentacao clinica completa obrigatoria: dados nascimento (peso, IG, APGAR), intercorrencias (UTI, VM, sepse), resultado teste inicial se reteste, otoscopia. Auditor otorrinolaringologista ou pediatra acionado para analise individualizada."</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>