<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30403120" name="Septoplastia" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30403120" name="Indicacao para Septoplastia">
    <decisionTable id="DecisionTable_30403120" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Sintomas Obstrutivos">
        <inputExpression id="InputExpression_2" typeRef="boolean"><text>sintomasObstrutivos</text></inputExpression>
      </input>
      <input id="Input_3" label="Rinoscopia/Nasofibroscopia Documentada">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>rinoscopiaDocumentada</text></inputExpression>
      </input>
      <input id="Input_4" label="Infeccao Ativa">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>infeccaoAtiva</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Infeccao_Ativa">
        <inputEntry id="IE_1"><text>-</text></inputEntry>
        <inputEntry id="IE_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>true</text></inputEntry>
        <inputEntry id="IE_5"><text>-</text></inputEntry>
        <outputEntry id="OE_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Contraindicacao: Infeccao nasal ativa. Cirurgia deve aguardar resolucao do quadro infeccioso"</text></outputEntry>
        <outputEntry id="OE_3"><text>"ABORL-CCF 2023 secao 7.1: Septoplastia contraindicada em rinossinusite aguda ativa. Recomenda-se aguardar 4-6 semanas pos-tratamento. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Desvio_Septal_J34">
        <inputEntry id="IE_6"><text>"J34.2"</text></inputEntry>
        <inputEntry id="IE_7"><text>true</text></inputEntry>
        <inputEntry id="IE_8"><text>true</text></inputEntry>
        <inputEntry id="IE_9"><text>false</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Desvio de septo nasal sintomatico com obstrucao documentada"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID J34.2 com sintomas obstrutivos e rinoscopia confirmatoria. AAO-HNS 2017: Septoplastia indicada em obstrucao sintomatica. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Rinossinusite_Cronica_J32">
        <inputEntry id="IE_11"><text>starts with(diagnosticoPrincipal, "J32")</text></inputEntry>
        <inputEntry id="IE_12"><text>true</text></inputEntry>
        <inputEntry id="IE_13"><text>true</text></inputEntry>
        <inputEntry id="IE_14"><text>false</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Rinossinusite cronica com componente septal obstrutivo documentado"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID J32.x com desvio septal associado. Protocolo Rinologia 2024 item 5.3: Septoplastia indicada em rinossinusite cronica refrataria com obstrucao septal. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Epistaxe_R04">
        <inputEntry id="IE_16"><text>"R04.0"</text></inputEntry>
        <inputEntry id="IE_17"><text>-</text></inputEntry>
        <inputEntry id="IE_18"><text>true</text></inputEntry>
        <inputEntry id="IE_19"><text>false</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Epistaxe recorrente secundaria a desvio septal com lesao mucosa"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID R04.0 com desvio septal traumatico documentado. ABORL-CCF 2023 secao 7.4: Septoplastia indicada em epistaxe recorrente por atrito septal. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Cefaleia_Rinogenica_G44">
        <inputEntry id="IE_21"><text>"G44.8"</text></inputEntry>
        <inputEntry id="IE_22"><text>true</text></inputEntry>
        <inputEntry id="IE_23"><text>true</text></inputEntry>
        <inputEntry id="IE_24"><text>false</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Cefaleia rinogenica por ponto de contato septal documentado"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID G44.8 com nasofibroscopia demonstrando ponto de contato. Protocolo Rinologia 2024 secao 6.2: Septoplastia indicada em cefaleia rinogenica refrataria. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30403120">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
