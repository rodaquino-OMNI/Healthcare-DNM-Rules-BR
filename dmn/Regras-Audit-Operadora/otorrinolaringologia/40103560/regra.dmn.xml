<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103560" name="Potencial Evocado P300" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  

  <decision id="Decision_40103560" name="Criterios de Autorizacao - Potencial Evocado P300">
    <decisionTable id="DecisionTable_40103560" hitPolicy="FIRST">

      
      <input id="Input_diagnosticoConfirmado" label="Diagnostico Confirmado">
        <inputExpression id="InputExpr_diagnosticoConfirmado" typeRef="string">
          <text>diagnosticoConfirmado</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoConfirmado">
          <text>"DPAC_Suspeita","Demencia_Inicial","TCE","TDAH","Outros"</text>
        </inputValues>
      </input>

      
      <input id="Input_indicacaoClinica" label="Indicacao Clinica">
        <inputExpression id="InputExpr_indicacaoClinica" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues id="InputValues_indicacaoClinica">
          <text>"Avaliacao_Cognitiva_Auditiva","Monitoramento_Demencia","Pos_TCE","Diferencial_TDAH_DPAC","Pesquisa"</text>
        </inputValues>
      </input>

      
      <input id="Input_tratamentosPrevios" label="Tratamentos Previos">
        <inputExpression id="InputExpr_tratamentosPrevios" typeRef="string">
          <text>tratamentosPrevios</text>
        </inputExpression>
        <inputValues id="InputValues_tratamentosPrevios">
          <text>"Audiometria","Avaliacao_Neuropsicologica","Testes_PAC","Nenhum","Tratamento_Cognitivo"</text>
        </inputValues>
      </input>

      
      <input id="Input_riscoCirurgico" label="Risco Cirurgico ASA">
        <inputExpression id="InputExpr_riscoCirurgico" typeRef="string">
          <text>riscoCirurgico</text>
        </inputExpression>
        <inputValues id="InputValues_riscoCirurgico">
          <text>"ASA_I","ASA_II","ASA_III","ASA_IV"</text>
        </inputValues>
      </input>

      
      <input id="Input_laudoIndicacao" label="Laudo de Indicacao Assinado">
        <inputExpression id="InputExpr_laudoIndicacao" typeRef="boolean">
          <text>laudoIndicacao</text>
        </inputExpression>
      </input>

      
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />

      
      
      

      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_40103560_Contraindicacao_1">
        <description>Reprovado - Audiometria nao realizada - obrigatoria antes de P300 para excluir deficiencia auditiva periferica</description>
        <inputEntry id="IE_Contra1_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_3">
          <text>not("Audiometria")</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra1_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_2">
          <text>"Audiometria nao realizada - obrigatoria antes de P300 para excluir deficiencia auditiva periferica"</text>
        </outputEntry>
        <outputEntry id="OE_Contra1_3">
          <text>"ABORL-CCF 2023, AAA Guidelines - Audiometria obrigatoria antes de testes cognitivos auditivos"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_40103560_Contraindicacao_2">
        <description>Reprovado - Indicacao clinica nao especificada ou sem avaliacao previa - P300 requer contexto diagnostico claro</description>
        <inputEntry id="IE_Contra2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_3">
          <text>"Nenhum"</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Contra2_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Contra2_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_2">
          <text>"Indicacao clinica nao especificada ou sem avaliacao previa - P300 requer contexto diagnostico claro"</text>
        </outputEntry>
        <outputEntry id="OE_Contra2_3">
          <text>"SBFa 2023 - P300 exame de terceiro nivel requer bateria diagnostica previa"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      
      

      <rule id="Rule_40103560_Aprovacao_1">
        <description>Aprovado - P300 para avaliacao complementar DPAC - discriminacao auditiva cognitiva</description>
        <inputEntry id="IE_Aprov1_1">
          <text>"DPAC_Suspeita"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_2">
          <text>"Avaliacao_Cognitiva_Auditiva"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_3">
          <text>"Audiometria","Testes_PAC"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov1_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov1_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_2">
          <text>"P300 aprovado para avaliacao complementar DPAC - discriminacao auditiva cognitiva"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov1_3">
          <text>"ABORL-CCF 2023, SBFa - P300 complementa bateria PAC em casos complexos DPAC"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103560_Aprovacao_2">
        <description>Aprovado - P300 para monitoramento cognitivo em demencia inicial</description>
        <inputEntry id="IE_Aprov2_1">
          <text>"Demencia_Inicial"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_2">
          <text>"Monitoramento_Demencia"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_3">
          <text>"Audiometria","Avaliacao_Neuropsicologica"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov2_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov2_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_2">
          <text>"P300 aprovado para monitoramento cognitivo em demencia inicial"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov2_3">
          <text>"AAA Guidelines, SBFa - P300 marcador eletrofisiologico util em demencias iniciais"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_40103560_Aprovacao_3">
        <description>Aprovado - P300 para avaliacao cognitiva auditiva pos-TCE</description>
        <inputEntry id="IE_Aprov3_1">
          <text>"TCE"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_2">
          <text>"Pos_TCE"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_3">
          <text>"Audiometria","Avaliacao_Neuropsicologica"</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Aprov3_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OE_Aprov3_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_2">
          <text>"P300 aprovado para avaliacao cognitiva auditiva pos-TCE"</text>
        </outputEntry>
        <outputEntry id="OE_Aprov3_3">
          <text>"AAA Guidelines - P300 sensivel para detectar alteracoes cognitivas auditivas pos-TCE"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      
      
      <rule id="Rule_Fallback_40103560">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado</description>
        <inputEntry id="IE_Fallback_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="IE_Fallback_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OE_Fallback_1">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_2">
          <text>"Dados incompletos para analise - documentacao complementar necessaria"</text>
        </outputEntry>
        <outputEntry id="OE_Fallback_3">
          <text>"ABORL-CCF 2023 - Documentacao audiologica completa obrigatoria para autorizacao P300"</text>
        </outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>

    </decisionTable>
  </decision>
</definitions>