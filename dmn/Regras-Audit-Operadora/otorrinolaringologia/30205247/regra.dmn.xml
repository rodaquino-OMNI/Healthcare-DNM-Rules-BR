<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30205247" name="Uvulopalatofaringoplastia" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30205247" name="Indicacao para Uvulopalatofaringoplastia">
    <decisionTable id="DecisionTable_30205247" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Polissonografia (AHI Index)">
        <inputExpression id="InputExpression_2" typeRef="string"><text>polissonografia</text></inputExpression>
      </input>
      <input id="Input_3" label="Tentativa CPAP (meses)">
        <inputExpression id="InputExpression_3" typeRef="integer"><text>tentativaCPAP</text></inputExpression>
      </input>
      <input id="Input_4" label="IMC (kg/m²)">
        <inputExpression id="InputExpression_4" typeRef="number"><text>imc</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Obesidade_Grave">
        <inputEntry id="IE_1"><text>-</text></inputEntry>
        <inputEntry id="IE_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>&amp;gt;= 35</text></inputEntry>
        <inputEntry id="IE_5"><text>-</text></inputEntry>
        <outputEntry id="OE_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Contraindicacao: IMC ≥35. Obesidade grave reduz eficacia cirurgica. Cirurgia bariatrica prioritaria."</text></outputEntry>
        <outputEntry id="OE_3"><text>"AASM 2020 Guidelines: IMC &amp;gt;35 associado a falha cirurgica em 70% dos casos. UPFP contraindicada. Recomenda-se cirurgia bariatrica + CPAP. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      <rule id="Rule_Contraindicacao_CPAP_Nao_Tentado">
        <inputEntry id="IE_6"><text>"G47.3"</text></inputEntry>
        <inputEntry id="IE_7"><text>-</text></inputEntry>
        <inputEntry id="IE_8"><text>&amp;lt; 3</text></inputEntry>
        <inputEntry id="IE_9"><text>-</text></inputEntry>
        <inputEntry id="IE_10"><text>-</text></inputEntry>
        <outputEntry id="OE_4"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"CPAP nao foi tentado adequadamente (&amp;lt;3 meses). Tratamento conservador deve ser priorizado."</text></outputEntry>
        <outputEntry id="OE_6"><text>"ABMS 2024 Protocolo 5.2: CPAP é tratamento de primeira linha para SAHOS. Cirurgia indicada apenas apos falha/intolerancia documentada ≥3 meses. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_SAHOS_Grave">
        <inputEntry id="IE_11"><text>"G47.3"</text></inputEntry>
        <inputEntry id="IE_12"><text>"AHI &amp;gt;30", "AHI grave"</text></inputEntry>
        <inputEntry id="IE_13"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_14"><text>&amp;lt; 35</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"SAHOS grave (AHI &amp;gt;30) com falha de CPAP documentada. IMC adequado para cirurgia."</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID G47.3 com polissonografia AHI &amp;gt;30. AASM 2020: UPFP indicada em SAHOS grave apos falha CPAP. IMC &amp;lt;35. ABORL-CCF 2023 secao 8.4"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_SAHOS_Moderada">
        <inputEntry id="IE_16"><text>"G47.3"</text></inputEntry>
        <inputEntry id="IE_17"><text>"AHI &amp;gt;15", "AHI moderada"</text></inputEntry>
        <inputEntry id="IE_18"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_19"><text>&amp;lt; 32</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"SAHOS moderada (AHI &amp;gt;15) com falha de CPAP. IMC favoravel."</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID G47.3 com polissonografia AHI 15-30. ABMS 2024: UPFP indicada em SAHOS moderada refrataria a CPAP ≥3 meses. IMC &amp;lt;32 melhora prognostico. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_SAHOS_Sintomatica">
        <inputEntry id="IE_21"><text>"G47.3"</text></inputEntry>
        <inputEntry id="IE_22"><text>"AHI &amp;gt;5"</text></inputEntry>
        <inputEntry id="IE_23"><text>&amp;gt;= 6</text></inputEntry>
        <inputEntry id="IE_24"><text>&amp;lt; 30</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"SAHOS com sintomas diurnos incapacitantes, AHI &amp;gt;5, apos tentativa prolongada de CPAP (≥6 meses)"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID G47.3 com AHI &amp;gt;5 e sintomas graves (sonolencia diurna excessiva documentada). Tentativa CPAP ≥6 meses sem sucesso. ABORL-CCF 2023 item 8.5 - Qualidade de vida justifica cirurgia"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30205247">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>