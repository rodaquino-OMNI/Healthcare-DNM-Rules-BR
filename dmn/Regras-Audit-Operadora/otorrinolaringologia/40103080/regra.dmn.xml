<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103080" name="Audiometria_Tonal_Infantil_Peep_Show" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_40103080" name="Indicacao para Audiometria Tonal Infantil Condicionada (Peep-Show)">
    <decisionTable id="DecisionTable_40103080" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Idade da Crianca (anos)">
        <inputExpression id="InputExpression_2" typeRef="integer"><text>idadeCrianca</text></inputExpression>
      </input>
      <input id="Input_3" label="Atraso de Linguagem">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>atrasoLinguagem</text></inputExpression>
      </input>
      <input id="Input_4" label="Otite Media Recorrente (&gt;=4x/ano)">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>otiteRecorrente</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Aprovado_Surdez_Infantil_H90">
        <inputEntry id="IE_1"><text>"H90.3", "H90.5", "H91.9"</text></inputEntry>
        <inputEntry id="IE_2"><text>2, 3, 4, 5</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>-</text></inputEntry>
        <inputEntry id="IE_5"><text>true</text></inputEntry>
        <outputEntry id="OE_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Suspeita de surdez em crianca 2-5 anos. Audiometria Peep-Show indicada (metodo ludico apropriado para faixa etaria)"</text></outputEntry>
        <outputEntry id="OE_3"><text>"CID H90.x/H91.x (Surdez infantil). AAO-HNS 2021: Criancas 2-5 anos cooperativas: Peep-Show metodo validado. Reforcador visual aumenta acuracia. ABORL-CCF 2023 secao 5.2"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Atraso_Linguagem_F80">
        <inputEntry id="IE_6"><text>"F80.1", "F80.9", "R48.8"</text></inputEntry>
        <inputEntry id="IE_7"><text>2, 3, 4, 5</text></inputEntry>
        <inputEntry id="IE_8"><text>true</text></inputEntry>
        <inputEntry id="IE_9"><text>-</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Atraso de linguagem em crianca pre-escolar. Audiometria Peep-Show essencial para descartar hipoacusia como causa"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID F80.x/R48.8 (Atraso linguagem). SBFa 2024: Todo atraso linguagem requer avaliacao auditiva. Perda auditiva leve (25-40dB) causa atraso fala em 70% dos casos. AAO-HNS 2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Otite_Recorrente_H66">
        <inputEntry id="IE_11"><text>"H66.0", "H66.9", "H65.2"</text></inputEntry>
        <inputEntry id="IE_12"><text>2, 3, 4, 5</text></inputEntry>
        <inputEntry id="IE_13"><text>-</text></inputEntry>
        <inputEntry id="IE_14"><text>true</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Otite media recorrente (&amp;gt;=4 episodios/ano). Audiometria Peep-Show monitora perda auditiva condutiva flutuante"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID H66.x/H65.2 (Otite recorrente). ABORL-CCF 2023: Otite recorrente: audiometria semestral. Efusao persistente &amp;gt;3 meses: perda 20-40dB. Peep-Show metodo ideal 2-5 anos. AAO-HNS 2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Pos_Meningite_G00">
        <inputEntry id="IE_16"><text>"G00.9", "G03.9", "B05.9"</text></inputEntry>
        <inputEntry id="IE_17"><text>2, 3, 4, 5</text></inputEntry>
        <inputEntry id="IE_18"><text>-</text></inputEntry>
        <inputEntry id="IE_19"><text>-</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Historia de meningite/infeccao de risco auditivo. Audiometria Peep-Show para deteccao precoce de perda auditiva pos-infecciosa"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID G00.x/B05.x (Meningite/Sarampo). AAO-HNS 2021: Pos-meningite: audiometria 1, 3, 6 meses. Perda tardia em 10% casos. Peep-Show apropriado 2-5 anos. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Risco_Neonatal_Z13">
        <inputEntry id="IE_21"><text>"Z13.5", "P07.1", "P07.3"</text></inputEntry>
        <inputEntry id="IE_22"><text>2, 3, 4, 5</text></inputEntry>
        <inputEntry id="IE_23"><text>-</text></inputEntry>
        <inputEntry id="IE_24"><text>-</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Crianca de risco neonatal (prematuro/UTI) em idade pre-escolar. Audiometria Peep-Show complementa PEATE neonatal"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID Z13.5/P07.x (Triagem/Prematuro). JCIH 2019: Criancas risco auditivo: reavaliacao 2-5 anos com audiometria comportamental (Peep-Show). Complementa PEATE neonatal. SBFa 2024"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_40103080">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>