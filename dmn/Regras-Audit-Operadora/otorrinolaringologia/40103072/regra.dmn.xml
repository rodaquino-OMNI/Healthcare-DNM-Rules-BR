<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_40103072" name="Audiometria_Tonal_Discriminacao" targetNamespace="http://camunda.org/spec/DMN/20191111/MODEL/">
  
  <decision id="Decision_40103072" name="Indicacao para Audiometria Tonal Limiar com Testes de Discriminacao">
    <decisionTable id="DecisionTable_40103072" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Queixa Auditiva (meses)">
        <inputExpression id="InputExpression_2" typeRef="integer"><text>queixaAuditiva</text></inputExpression>
      </input>
      <input id="Input_3" label="Exposicao Ocupacional Ruido">
        <inputExpression id="InputExpression_3" typeRef="boolean"><text>exposicaoRuido</text></inputExpression>
      </input>
      <input id="Input_4" label="Pre-Adaptacao AASI">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>preAdaptacaoAASI</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Aprovado_Perda_Auditiva_H90">
        <inputEntry id="IE_1"><text>"H90.3", "H90.5", "H91.1", "H91.9"</text></inputEntry>
        <inputEntry id="IE_2"><text>&amp;gt;= 1</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>-</text></inputEntry>
        <inputEntry id="IE_5"><text>true</text></inputEntry>
        <outputEntry id="OE_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Queixa de perda auditiva bilateral/unilateral. Audiometria tonal + discriminacao vocal indicada para diagnostico e graduacao"</text></outputEntry>
        <outputEntry id="OE_3"><text>"CID H90.x/H91.x (Surdez). AAO-HNS 2022 Criterio Principal: Toda queixa auditiva &amp;gt;=1 mes requer audiometria completa. Testes discriminacao essenciais para adaptacao AASI. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Pre_AASI_Z46">
        <inputEntry id="IE_6"><text>"Z46.1", "H90.3", "H91.1"</text></inputEntry>
        <inputEntry id="IE_7"><text>-</text></inputEntry>
        <inputEntry id="IE_8"><text>-</text></inputEntry>
        <inputEntry id="IE_9"><text>true</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Avaliacao pre-adaptacao de aparelho auditivo (AASI). Audiometria + discriminacao obrigatoria para selecao de ganho e potencia"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID Z46.1/H90.x (Adaptacao AASI). ABORL-CCF 2023: Audiometria completa com testes discriminacao vocal pre-AASI: determina ganho funcional esperado. Portaria MS 2073/2004 requisito obrigatorio"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_PAIR_H83">
        <inputEntry id="IE_11"><text>"H83.3", "Z57.0"</text></inputEntry>
        <inputEntry id="IE_12"><text>-</text></inputEntry>
        <inputEntry id="IE_13"><text>true</text></inputEntry>
        <inputEntry id="IE_14"><text>-</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Exposicao ocupacional a ruido. Audiometria + discriminacao para diagnostico e estadiamento de PAIR (Perda Auditiva Induzida por Ruido)"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID H83.3/Z57.0 (PAIR/Exposicao ruido). NR-7 PCMSO: Audiometria obrigatoria anual em trabalhadores expostos &amp;gt;85dB. AAO-HNS 2022: Testes discriminacao detectam lesao coclear precoce"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Tinitus_H93">
        <inputEntry id="IE_16"><text>"H93.1", "H93.9"</text></inputEntry>
        <inputEntry id="IE_17"><text>&amp;gt;= 3</text></inputEntry>
        <inputEntry id="IE_18"><text>-</text></inputEntry>
        <inputEntry id="IE_19"><text>-</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Tinitus persistente &amp;gt;=3 meses. Audiometria + discriminacao investiga perda auditiva associada (presente em 80% dos casos)"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID H93.1/H93.9 (Tinitus). ABORL-CCF 2023: Tinitus cronico &amp;gt;=3 meses: audiometria obrigatoria. 80% apresentam perda auditiva associada. AAO-HNS 2022 secao 8.1"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Presbiacusia_H91">
        <inputEntry id="IE_21"><text>"H91.1"</text></inputEntry>
        <inputEntry id="IE_22"><text>&amp;gt;= 1</text></inputEntry>
        <inputEntry id="IE_23"><text>-</text></inputEntry>
        <inputEntry id="IE_24"><text>-</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Presbiacusia (perda auditiva relacionada a idade). Audiometria + discriminacao para monitoramento e indicacao AASI"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID H91.1 (Presbiacusia). AAO-HNS 2022: Idosos &amp;gt;60 anos com queixa auditiva: audiometria anual. Testes discriminacao predizem beneficio AASI. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_40103072">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>