<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_30404061" name="Implante_Coclear" targetNamespace="http://camunda.org/schema/1.0/dmn">
  
  <decision id="Decision_30404061" name="Indicacao para Implante Coclear">
    <decisionTable id="DecisionTable_30404061" hitPolicy="FIRST">
      <input id="Input_1" label="Diagnostico Principal (CID-10)">
        <inputExpression id="InputExpression_1" typeRef="string"><text>diagnosticoPrincipal</text></inputExpression>
      </input>
      <input id="Input_2" label="Audiometria (dB - Perda Auditiva)">
        <inputExpression id="InputExpression_2" typeRef="string"><text>audiometria</text></inputExpression>
      </input>
      <input id="Input_3" label="Teste com AASI (meses)">
        <inputExpression id="InputExpression_3" typeRef="integer"><text>testeAASI</text></inputExpression>
      </input>
      <input id="Input_4" label="Tipo de Perda Auditiva">
        <inputExpression id="InputExpression_4" typeRef="string"><text>tipoPerda</text></inputExpression>
      </input>
      <input id="Input_5" label="Laudo Otorrino Anexado">
        <inputExpression id="InputExpression_5" typeRef="boolean"><text>laudoOtorrino</text></inputExpression>
      </input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string" />
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Fundamentacao" name="fundamentacao" typeRef="string" />

      
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string"><outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues></output><output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string"><outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues></output><rule id="Rule_Contraindicacao_Perda_Condutiva">
        <inputEntry id="IE_1"><text>-</text></inputEntry>
        <inputEntry id="IE_2"><text>-</text></inputEntry>
        <inputEntry id="IE_3"><text>-</text></inputEntry>
        <inputEntry id="IE_4"><text>"condutiva", "mista"</text></inputEntry>
        <inputEntry id="IE_5"><text>-</text></inputEntry>
        <outputEntry id="OE_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_2"><text>"Contraindicacao: Perda auditiva condutiva ou mista. Implante coclear indicado apenas em perda neurossensorial"</text></outputEntry>
        <outputEntry id="OE_3"><text>"Portaria MS 2776/2014: IC indicado exclusivamente em surdez neurossensorial bilateral. Perdas condutivas devem ser tratadas cirurgicamente (timpanoplastia, estapedectomia). ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"PROT"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"NEGAR"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_Surdez_Profunda_H90">
        <inputEntry id="IE_6"><text>"H90.3", "H90.5"</text></inputEntry>
        <inputEntry id="IE_7"><text>"profound", "&gt;90dB", "severe &gt;70dB"</text></inputEntry>
        <inputEntry id="IE_8"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="IE_9"><text>"neurossensorial"</text></inputEntry>
        <inputEntry id="IE_10"><text>true</text></inputEntry>
        <outputEntry id="OE_4"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_5"><text>"Surdez neurossensorial bilateral profunda (&gt;90dB) com beneficio limitado de AASI apos ≥3 meses"</text></outputEntry>
        <outputEntry id="OE_6"><text>"CID H90.3/H90.5 com audiometria &gt;90dB bilateral. Portaria MS 2776/2014 Criterio 1: Surdez profunda bilateral com ganho insuficiente de AASI. ABORL-CCF 2023 secao 12.1"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Surdez_Severa_H91">
        <inputEntry id="IE_11"><text>"H91.3", "H91.9"</text></inputEntry>
        <inputEntry id="IE_12"><text>"severe", "&gt;70dB"</text></inputEntry>
        <inputEntry id="IE_13"><text>&gt;= 6</text></inputEntry>
        <inputEntry id="IE_14"><text>"neurossensorial"</text></inputEntry>
        <inputEntry id="IE_15"><text>true</text></inputEntry>
        <outputEntry id="OE_7"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_8"><text>"Surdez neurossensorial bilateral severa (70-90dB) refrataria a AASI por ≥6 meses"</text></outputEntry>
        <outputEntry id="OE_9"><text>"CID H91.x com audiometria 70-90dB. FDA 2020 Guidelines: IC aprovado em surdez severa com discriminacao &lt;50% em testes de fala. Tentativa AASI ≥6 meses. ANS RN 465/2021"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Meningite_G00">
        <inputEntry id="IE_16"><text>"G00.9", "G03.9"</text></inputEntry>
        <inputEntry id="IE_17"><text>"&gt;70dB", "profound"</text></inputEntry>
        <inputEntry id="IE_18"><text>&gt;= 1</text></inputEntry>
        <inputEntry id="IE_19"><text>"neurossensorial"</text></inputEntry>
        <inputEntry id="IE_20"><text>true</text></inputEntry>
        <outputEntry id="OE_10"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_11"><text>"Surdez pos-meningite com risco de ossificacao coclear. Indicacao cirurgica urgente"</text></outputEntry>
        <outputEntry id="OE_12"><text>"CID G00.x (Meningite) com perda auditiva neurossensorial &gt;70dB. Portaria MS 2776/2014 Criterio Especial: Meningite requer IC precoce (&amp;lt;6 meses) para evitar ossificacao coclear. ABORL-CCF 2023"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      <rule id="Rule_Aprovado_Surdez_Congenita_H83">
        <inputEntry id="IE_21"><text>"H83.3", "Q16.5"</text></inputEntry>
        <inputEntry id="IE_22"><text>"&gt;90dB", "profound"</text></inputEntry>
        <inputEntry id="IE_23"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="IE_24"><text>"neurossensorial"</text></inputEntry>
        <inputEntry id="IE_25"><text>true</text></inputEntry>
        <outputEntry id="OE_13"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_14"><text>"Surdez congenita bilateral profunda em crianca com indicacao de IC para desenvolvimento da linguagem"</text></outputEntry>
        <outputEntry id="OE_15"><text>"CID H83.3/Q16.5 (Surdez congenita) com audiometria &gt;90dB. FDA 2020: IC pediatrico aprovado em surdez congenita profunda. Ideal antes dos 2 anos. Protocolo MS 2024 secao 8.2"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"APROVAR"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_30404061">
        <inputEntry id="IE_26"><text>-</text></inputEntry>
        <inputEntry id="IE_27"><text>-</text></inputEntry>
        <inputEntry id="IE_28"><text>-</text></inputEntry>
        <inputEntry id="IE_29"><text>-</text></inputEntry>
        <inputEntry id="IE_30"><text>-</text></inputEntry>
        <outputEntry id="OE_16"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_17"><text>"Caso nao se enquadra em criterios automaticos. Requer analise por auditor medico especializado."</text></outputEntry>
        <outputEntry id="OE_18"><text>"ANS RN 465/2021 Art. 19 - Casos atipicos devem ser avaliados individualmente por junta medica"</text></outputEntry>
      <outputEntry id="OutputEntry_Added_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_Added_5"><text>"SOLICITAR_INFO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
</definitions>