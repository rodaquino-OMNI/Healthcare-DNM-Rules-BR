<definitions  xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" id="Definitions_41203089" name="Radioterapia com acelerador linear só com fótons" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <!--
    TUSS 41203089 - Radioterapia com acelerador linear só com fótons
    Versao: 2.0.0 (2026-02-06)

    Indicacoes: Conforme protocolo ANS/TISS
    Contraindicacoes: Verificar regras especificas
    Referencias: ANS/TISS 2024, Resolucao Normativa
  -->
  <decision id="Decision_41203089" name="Definição do Número de Campos para Radioterapia Convencional">
    
    
    <decisionTable id="DecisionTable_41203089" hitPolicy="FIRST">
      
      
      <input id="Input_1" label="Número de campos disponível na literatura peer-reviewed">
        
        
        <inputExpression id="InputExpression_1" typeRef="string">
          
          
          <text>disponibilidadeLiteratura</text>
          
        
        </inputExpression>
        
      
      </input>
      
      
      
      <output id="Output_resultado" name="resultado" typeRef="string">
        <outputValues><text>"Aprovado","Reprovado","Pendente"</text></outputValues>
      </output>
      <output id="Output_observacao" name="observacao" typeRef="string"/>
      <output id="Output_fundamentacao" name="fundamentacao" typeRef="string"/>
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues>
      </output>
      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>

      <rule id="Rule_41203089_1">
        <inputEntry id="InputEntry_41203089_1">
          <text>"Sim"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_41203089_1">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_41203089_1">
          <text>"Literatura peer-reviewed disponível"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_41203089_1">
          <text>"Utilizar o número de campos descrito na literatura peer-reviewed indexada."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_41203089_1">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_41203089_1">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>

      <rule id="Rule_41203089_2">
        <inputEntry id="InputEntry_41203089_2">
          <text>"Não"</text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_41203089_2">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_41203089_2">
          <text>"Literatura não disponível - aplicar rol Unimed"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_41203089_2">
          <text>"Utilizar o número máximo de campos citados na tabela do Rol Unimed."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_41203089_2">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_41203089_2">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>

      <rule id="Rule_Default_41203089">
        <description>Regra padrao - Criterios nao atendidos</description>
        <inputEntry id="InputEntry_Default_41203089_1">
          <text></text>
        </inputEntry>
        <outputEntry id="OutputEntry_resultado_Default_41203089">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_observacao_Default_41203089">
          <text>"Aguardando informações sobre disponibilidade de literatura"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_fundamentacao_Default_41203089">
          <text>"Critérios não atendidos - avaliar disponibilidade de literatura peer-reviewed"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_alertasDesperdicio_Default_41203089">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_acaoRecomendada_Default_41203089">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      
  
    </decisionTable>
    
  
  </decision>
  

</definitions>
