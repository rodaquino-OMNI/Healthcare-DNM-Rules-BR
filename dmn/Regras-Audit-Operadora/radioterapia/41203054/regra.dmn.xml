<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_41203054" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_41203054" name="Radioterapia de intensidade modulada IMRT [LEAN TIER-2]">
    <decisionTable id="DecisionTable_41203054" hitPolicy="FIRST">
      <input id="Input_1" label="Finalidade do Tratamento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>finalidadeTratamento</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Viabilidade Dose Conformacional">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>viabilidadeDoseConformacional</text>
        </inputExpression>
      </input>
      <input id="Input_3" label="Dose Órgãos Risco">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>doseOrgaosRiscoConformacional</text>
        </inputExpression>
      </input>
      <input id="Input_4" label="Documentação Completa">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_Paliativo">
        <description>Reprovado: Tratamento paliativo não elegível para IMRT</description>
        <inputEntry id="InputEntry_1_1">
          <text>"Paliativo"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"IMRT não indicado para tratamento paliativo salvo exceções justificadas"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"ANS RN 465/2021: IMRT reservado para finalidade curativa com benefício dosimétrico comprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_1">
          <text>"ESC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_1">
          <text>"SUGERIR_ALTERNATIVA"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_DocIncompleta">
        <description>Documentação incompleta - Pendente</description>
        <inputEntry id="InputEntry_1_2">
          <text>"Curativo"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Solicitar: relatório médico, plano dosimétrico comparativo (IMRT vs conformacional), DVH órgãos de risco"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"ANS RN 465/2021: documentação comparativa obrigatória para justificar IMRT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_2">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_2">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_AprovadoInviavel">
        <description>Aprovado: Dose prescrita inviável com conformacional</description>
        <inputEntry id="InputEntry_1_3">
          <text>"Curativo"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>"Inviável"</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"IMRT indicado: dose curativa inatingível com radioterapia conformacional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"Justificativa técnica adequada conforme protocolo ANS/SBRT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_3">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_3">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_AprovadoRiscoAlto">
        <description>Aprovado: Dose alta em órgãos de risco com conformacional</description>
        <inputEntry id="InputEntry_1_4">
          <text>"Curativo"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>"Viável"</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_4">
          <text>"Alta"</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"IMRT indicado: proteção de órgãos de risco com dose excessiva na conformacional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"Benefício dosimétrico comprovado - ANS RN 465/2021"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_4">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_4">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_ReprovadoSemBeneficio">
        <description>Reprovado: Conformacional viável e segura sem benefício IMRT</description>
        <inputEntry id="InputEntry_1_5">
          <text>"Curativo"</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_5">
          <text>"Viável"</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_5">
          <text>"Aceitável"</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_5">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_5">
          <text>"IMRT não indicado: radioterapia conformacional é factível e segura sem benefício dosimétrico claro"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_5">
          <text>"ANS RN 465/2021: IMRT reservado para casos com superioridade dosimétrica comprovada"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_5">
          <text>"ESC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_5">
          <text>"SUGERIR_ALTERNATIVA"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_Fallback">
        <description>Fallback - Análise manual necessária</description>
        <inputEntry id="InputEntry_1_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_6">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_6">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_6">
          <text>"Caso não enquadrado nos critérios automatizados - requer avaliação médica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_6">
          <text>"Análise manual necessária conforme protocolo institucional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_6">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_6">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_41203054">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
