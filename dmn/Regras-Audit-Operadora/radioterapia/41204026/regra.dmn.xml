<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_41204026" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_41204026" name="Cheque-filme (verificação de isocentro) [LEAN TIER-2]">
    <decisionTable id="DecisionTable_41204026" hitPolicy="FIRST">
      <input id="Input_1" label="Técnica de Tratamento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tecnicaTratamento</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Frequência Semanal Solicitada">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>frequenciaSemanalSolicitada</text>
        </inputExpression>
      </input>
      <input id="Input_3" label="Justificativa Verificação Frequente">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>justificativaVerificacaoFrequente</text>
        </inputExpression>
      </input>
      <input id="Input_4" label="Documentação Completa">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_TecnicaSSD">
        <description>Reprovado: Técnica SSD não requer verificação de isocentro</description>
        <inputEntry id="InputEntry_1_1">
          <text>contains(., "SSD")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Cheque-filme não aplicável para técnica SSD: verificação de isocentro é dispensável"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"ANS RN 465/2021: Técnica SSD não utiliza isocentro, portanto não requer verificação por cheque-filme"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_1">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_1">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_FrequenciaExcessiva">
        <description>Reprovado: Frequência semanal excede limite técnico (máx 2/semana)</description>
        <inputEntry id="InputEntry_1_2">
          <text>not(contains(., "SSD"))</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>&gt; 2</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Frequência máxima padrão: 2 cheque-filmes por semana. Solicitação excede limite técnico sem justificativa"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"ANS RN 465/2021: Limite técnico padrão de 2 verificações semanais"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_2">
          <text>"FREQ"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_2">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_DocIncompleta">
        <description>Documentação incompleta - Pendente</description>
        <inputEntry id="InputEntry_1_3">
          <text>not(contains(., "SSD"))</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_3">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"Solicitar: plano de tratamento radioterápico, técnica utilizada, frequência semanal necessária"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"ANS RN 465/2021 exige documentação técnica para verificação de isocentro"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_3">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_3">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_AprovadoPadrao">
        <description>Aprovado: Frequência padrão (até 2/semana) para técnicas que requerem isocentro</description>
        <inputEntry id="InputEntry_1_4">
          <text>not(contains(., "SSD"))</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>&lt;= 2</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"Cheque-filme aprovado: frequência padrão de até 2 verificações semanais de isocentro"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"Dentro do protocolo ANS/TISS para verificação de isocentro"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_4">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_4">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_AprovadoJustificado">
        <description>Aprovado: Frequência excepcional com justificativa médica</description>
        <inputEntry id="InputEntry_1_5">
          <text>not(contains(., "SSD"))</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_5">
          <text>&gt; 2</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_5">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_5">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_5">
          <text>"Cheque-filme aprovado: frequência excepcional justificada por necessidade clínica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_5">
          <text>"Caso especial aprovado com justificativa técnica adequada - ANS RN 465/2021"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_5">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_5">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_Fallback">
        <description>Fallback - Análise manual necessária</description>
        <inputEntry id="InputEntry_1_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_6">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_6">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_6">
          <text>"Caso não enquadrado nos critérios automatizados - requer avaliação técnica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_6">
          <text>"Análise manual necessária conforme protocolo institucional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_6">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_6">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_41204026">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
