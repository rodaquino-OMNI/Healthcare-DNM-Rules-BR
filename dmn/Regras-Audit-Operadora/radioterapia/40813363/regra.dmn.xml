<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_40813363"
             name="Infiltracao Foraminal ou Facetaria ou Articular"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    TUSS 40813363 - Coluna vertebral: infiltracao foraminal ou facetaria ou articular
    Versao: 3.0.0 (2026-02-07)
    Especialidade: Dor / Anestesiologia / Ortopedia
    Categoria: Procedimentos Intervencionistas Coluna Vertebral

    Indicacoes Clinicas (DUT ANS 2024):
    - Dor facetaria cronica refrataria a tratamento conservador
    - Artrose facetaria documentada por imagem (TC ou RM)
    - Ate 3 segmentos por regiao anatomica (cervical, toracica, lombar)
    - Falha tratamento medicamentoso e fisioterapia por minimo 3 meses

    Contraindicacoes:
    - Cobranca concomitante de procedimento 31602126 (bloqueio para-espinhoso excludente)
    - Mais de 3 segmentos vertebrais na mesma sessao por regiao anatomica
    - Ausencia documentacao adequada de artrose facetaria
    - Mesmo segmento anatomico alvo de denervacao/rizotomia (procedimento excludente)

    Documentacao Obrigatoria:
    - Diagnostico confirmado (CID-10) com exame de imagem (TC ou RM coluna)
    - Laudo de artrose facetaria por radiologista
    - Justificativa clinica detalhada com historia clinica e exame fisico
    - Documentacao de tratamento conservador previo (medicamentos, fisioterapia)
    - Termo de consentimento informado assinado

    Referencias Normativas:
    - SBA Diretrizes Tratamento Dor Coluna 2023
    - ANS/TISS Rol de Procedimentos 2024
    - Resolucao Normativa ANS vigente
    - SBCOL Consenso Dor Facetaria 2022
  -->

  <decision id="Decision_40813363" name="Criterios de Autorizacao - Infiltracao Foraminal/Facetaria/Articular">
    <decisionTable id="DecisionTable_40813363" hitPolicy="FIRST">

      <!-- Input 1: Quantidade de Segmentos Vertebrais -->
      <input id="Input_quantidadeSegmentos" label="Quantidade de Segmentos Vertebrais">
        <inputExpression id="InputExpr_quantidadeSegmentos" typeRef="integer">
          <text>quantidadeSegmentos</text>
        </inputExpression>
      </input>

      <!-- Input 2: Documentacao de Artrose e Justificativa Clinica -->
      <input id="Input_documentacaoAdequada" label="Documentacao de Artrose e Justificativa Clinica">
        <inputExpression id="InputExpr_documentacaoAdequada" typeRef="boolean">
          <text>documentacaoAdequada</text>
        </inputExpression>
      </input>

      <!-- Input 3: Cobranca Concomitante de 31602126 -->
      <input id="Input_cobrancaConcomitante31602126" label="Cobranca Concomitante de 31602126">
        <inputExpression id="InputExpr_cobrancaConcomitante31602126" typeRef="boolean">
          <text>cobrancaConcomitante31602126</text>
        </inputExpression>
      </input>

      <!-- Input 4: Denervacao/Rizotomia no Mesmo Segmento -->
      <input id="Input_denervacaoMesmoSegmento" label="Denervacao/Rizotomia no Mesmo Segmento">
        <inputExpression id="InputExpr_denervacaoMesmoSegmento" typeRef="boolean">
          <text>denervacaoMesmoSegmento</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao Clinica" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal/Tecnica" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues><text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text></outputValues>
      </output>
      <output id="Output_acaoRecomendada" name="acaoRecomendada" typeRef="string">
        <outputValues><text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text></outputValues>
      </output>

      <!-- ============================================== -->
      <!-- CONTRAINDICATION RULES (FIRST PRIORITY)       -->
      <!-- ============================================== -->

      <rule id="Rule_40813363_Contraindicacao_1">
        <description>Quantidade de segmentos excede o limite de 3 por regiao anatomica</description>
        <inputEntry id="IE_Contra1_1"><text>&gt;3</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Quantidade de segmentos excede o limite de 3 por segmento anatomico"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"ANS/TISS: limite 3 segmentos por sessao por regiao anatomica conforme normativa"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Contraindicacao_1_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Contraindicacao_1_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_40813363_Contraindicacao_2">
        <description>Documentacao inadequada - Necessario laudo de artrose facetaria e justificativa clinica</description>
        <inputEntry id="IE_Contra2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>false</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Documentacao inadequada. Necessario laudo de artrose facetaria e justificativa clinica"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"ANS/TISS: exigencia laudo imagem confirmando artrose facetaria e indicacao clinica"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Contraindicacao_2_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Contraindicacao_2_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_40813363_Contraindicacao_3">
        <description>Procedimento 31602126 (bloqueio para-espinhoso) e excludente e nao pode ser cobrado concomitantemente</description>
        <inputEntry id="IE_Contra3_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra3_3"><text>true</text></inputEntry>
        <inputEntry id="IE_Contra3_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra3_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra3_2"><text>"Procedimento 31602126 (bloqueio para-espinhoso) e excludente e nao pode ser cobrado concomitantemente"</text></outputEntry>
        <outputEntry id="OE_Contra3_3"><text>"ANS/TISS: procedimentos excludentes conforme tabela TUSS vigente"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Contraindicacao_3_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Contraindicacao_3_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <rule id="Rule_40813363_Contraindicacao_4">
        <description>Nao deve ser remunerado no mesmo segmento anatomico alvo de denervacao/rizotomia</description>
        <inputEntry id="IE_Contra4_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra4_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra4_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra4_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Contra4_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra4_2"><text>"Nao deve ser remunerado no mesmo segmento anatomico alvo de denervacao/rizotomia"</text></outputEntry>
        <outputEntry id="OE_Contra4_3"><text>"ANS/TISS: infiltracao e denervacao/rizotomia sao excludentes no mesmo segmento"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Contraindicacao_4_4"><text>"PROT"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Contraindicacao_4_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APPROVAL RULES (AFTER CONTRAINDICATIONS)      -->
      <!-- ============================================== -->

      <rule id="Rule_40813363_Aprovacao_1">
        <description>Procedimento autorizado - ate 3 segmentos com documentacao adequada</description>
        <inputEntry id="IE_Aprov1_1"><text>&lt;=3</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>true</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>false</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Procedimento autorizado conforme criterios ANS/TISS"</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"ANS/TISS: infiltracao facetaria ate 3 segmentos com documentacao adequada"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Aprovacao_1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_40813363_Aprovacao_1_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- FALLBACK RULE - Casos nao cobertos            -->
      <!-- ============================================== -->
      <rule id="Rule_Fallback_40813363">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por auditor medico especializado</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Dados incompletos para avaliacao infiltracao facetaria"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Documentacao obrigatoria: laudo imagem artrose, justificativa clinica, numero segmentos"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_40813363_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_Rule_Fallback_40813363_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
