<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_41203135" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_41203135" name="Radioterapia estereotáxica fracionada cranial [LEAN TIER-2]">
    <decisionTable id="DecisionTable_41203135" hitPolicy="FIRST">
      <input id="Input_1" label="Lesão Intracraniana">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>lesaoIntracraniana</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Comprovação Superioridade">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>comprovacaoSuperioridade</text>
        </inputExpression>
      </input>
      <input id="Input_3" label="Documentação Completa">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>
      <input id="Input_4" label="Expectativa Vida Superior 1 Ano">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_NaoIntracraniana">
        <description>Contraindicação: Lesão não intracraniana</description>
        <inputEntry id="InputEntry_1_1">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Radioterapia estereotáxica fracionada cranial é indicada apenas para lesões intracranianas"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"ANS RN 465/2021; SBRT Guidelines; Indicação técnica restrita a lesões SNC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_1">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_1">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_ExpectativaVida">
        <description>Contraindicação: Expectativa de vida inferior a 1 ano</description>
        <inputEntry id="InputEntry_1_2">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Radioterapia estereotáxica fracionada não indicada para pacientes com expectativa de vida inferior a 1 ano"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"Protocolo ANS/SBRT: Procedimento de alta complexidade requer prognóstico favorável"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_2">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_2">
          <text>"SUGERIR_ALTERNATIVA"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_DocIncompleta">
        <description>Documentação incompleta - Pendente</description>
        <inputEntry id="InputEntry_1_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_3">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"Solicitar: relatório médico, exames de imagem (TC/RM), estudo dosimétrico comparativo vs radioterapia conformacional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"ANS RN 465/2021 exige documentação completa e comprovação de superioridade técnica para REF cranial"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_3">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_3">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_AprovadoSuperioridade">
        <description>Aprovado: Comprovação de superioridade vs conformacional</description>
        <inputEntry id="InputEntry_1_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_4">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"REF cranial aprovada: superioridade comprovada vs radioterapia conformacional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"Benefício dosimétrico comprovado conforme protocolo ANS/SBRT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_4">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_4">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_ReprovadoSemJustificativa">
        <description>Reprovado: Sem comprovação de superioridade vs conformacional</description>
        <inputEntry id="InputEntry_1_5">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_5">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_5">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_5">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_5">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_5">
          <text>"REF cranial não justificada: ausência de comprovação de superioridade vs radioterapia conformacional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_5">
          <text>"ANS RN 465/2021: REF reservada para casos com benefício dosimétrico comprovado ou impossibilidade técnica da conformacional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_5">
          <text>"ESC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_5">
          <text>"SUGERIR_ALTERNATIVA"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_Fallback">
        <description>Fallback - Análise manual necessária</description>
        <inputEntry id="InputEntry_1_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_6">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_6">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_6">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_6">
          <text>"Caso não enquadrado nos critérios automatizados - requer avaliação médica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_6">
          <text>"Análise manual necessária conforme protocolo institucional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_6">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_6">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_41203135">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
