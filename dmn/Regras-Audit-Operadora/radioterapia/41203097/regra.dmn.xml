<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_41203097" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_41203097" name="Radioterapia com unidade de telecobalto [LEAN TIER-2]">
    <decisionTable id="DecisionTable_41203097" hitPolicy="FIRST">
      <input id="Input_1" label="Indicação Clínica">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Número de Campos">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>numeroCampos</text>
        </inputExpression>
      </input>
      <input id="Input_3" label="Documentação Completa">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>
      <input id="Input_4" label="Expectativa Vida Superior 1 Ano">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>expectativaVidaSuperior1Ano</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_ExpectativaVida">
        <description>Contraindicação: Expectativa de vida inferior a 1 ano para tratamento curativo</description>
        <inputEntry id="InputEntry_1_1">
          <text>not(contains(., "paliativ"))</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_1">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Radioterapia com telecobalto não indicada para pacientes com expectativa de vida inferior a 1 ano"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"Protocolo ANS: tratamento curativo requer prognóstico favorável. Considerar radioterapia paliativa"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_1">
          <text>"PROT"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_1">
          <text>"SUGERIR_ALTERNATIVA"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_DocIncompleta">
        <description>Documentação incompleta - Pendente</description>
        <inputEntry id="InputEntry_1_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_2">
          <text>false</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Solicitar: relatório médico oncológico, plano de tratamento, número de campos baseado em literatura peer-reviewed"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"ANS RN 465/2021 exige documentação técnica completa incluindo número de campos justificado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_2">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_2">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_Aprovado">
        <description>Aprovado: Indicação adequada com documentação completa</description>
        <inputEntry id="InputEntry_1_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>&gt;= 1</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_3">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"Radioterapia com telecobalto aprovada conforme protocolo ANS"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"Indicação adequada: documentação completa, prognóstico favorável, número de campos justificado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_3">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_3">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_AprovadoPaliativo">
        <description>Aprovado: Radioterapia paliativa com prognóstico limitado</description>
        <inputEntry id="InputEntry_1_4">
          <text>contains(., "paliativ")</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>&gt;= 1</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_4">
          <text>true</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_4">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"Radioterapia paliativa com telecobalto aprovada para controle sintomático"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"Indicação paliativa adequada com finalidade de alívio sintomático - ANS RN 465/2021"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_4">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_4">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_Fallback">
        <description>Fallback - Análise manual necessária</description>
        <inputEntry id="InputEntry_1_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_4_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_5">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_5">
          <text>"Caso não enquadrado nos critérios automatizados - requer avaliação médica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_5">
          <text>"Análise manual necessária conforme protocolo institucional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_5">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_41203097">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
