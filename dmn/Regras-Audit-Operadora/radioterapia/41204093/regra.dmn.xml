<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_41204093" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.14.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0">
  <decision id="Decision_41204093" name="Sistemas de imobilização (cabeça/membros) [LEAN TIER-2]">
    <decisionTable id="DecisionTable_41204093" hitPolicy="FIRST">
      <input id="Input_1" label="Quantidade por Tratamento">
        <inputExpression id="InputExpression_1" typeRef="integer">
          <text>quantidadeTratamento</text>
        </inputExpression>
      </input>
      <input id="Input_2" label="Região Anatômica">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>regiaoAnatomica</text>
        </inputExpression>
      </input>
      <input id="Input_3" label="Documentação Completa">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="UnaryTests_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>
      <output id="Output_observacao" label="Observação" name="observacao" typeRef="string" />
      <output id="Output_fundamentacao" label="Fundamentação" name="fundamentacao" typeRef="string" />
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>
      <rule id="Rule_QuantidadeExcessiva">
        <description>Reprovado: Quantidade excede limite técnico (máximo 1 por tratamento)</description>
        <inputEntry id="InputEntry_1_1">
          <text>&gt; 1</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_1">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Quantidade máxima: 1 sistema de imobilização por tratamento. Solicitado excede limite técnico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_1">
          <text>"ANS RN 465/2021: Limite técnico de 1 sistema de imobilização por tratamento radioterápico"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_1">
          <text>"FREQ"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_1">
          <text>"NEGAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_DocIncompleta">
        <description>Documentação incompleta - Pendente</description>
        <inputEntry id="InputEntry_1_2">
          <text>&lt;= 1</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"Solicitar: relatório médico, plano de tratamento radioterápico, justificativa para sistema de imobilização (cabeça/membros)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_2">
          <text>"ANS RN 465/2021 exige documentação técnica para sistemas de imobilização"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_2">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_2">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_Aprovado">
        <description>Aprovado: Quantidade adequada (1 sistema por tratamento)</description>
        <inputEntry id="InputEntry_1_3">
          <text>&lt;= 1</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_3">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_3">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Aprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"Sistema de imobilização aprovado: máximo 1 por tratamento conforme protocolo (cabeça/membros)"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_3">
          <text>"Quantidade adequada dentro do protocolo ANS/TISS para imobilização de cabeça ou membros"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_3">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_3">
          <text>"APROVAR"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_ReprovadoQuantidadeInvalida">
        <description>Reprovado: Quantidade inválida (zero ou negativa)</description>
        <inputEntry id="InputEntry_1_4">
          <text>&lt; 0</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_4">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_4">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"Reprovado"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"Quantidade inválida: não é possível ter quantidade negativa de sistemas de imobilização"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_4">
          <text>"Dados inconsistentes: quantidade inválida"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_4">
          <text>"DOC"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_4">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
      <rule id="Rule_Fallback">
        <description>Fallback - Análise manual necessária</description>
        <inputEntry id="InputEntry_1_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_5">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_3_5">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_5">
          <text>"Pendente"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_5">
          <text>"Caso não enquadrado nos critérios automatizados - requer avaliação técnica"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_3_5">
          <text>"Análise manual necessária conforme protocolo institucional"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_4_5">
          <text>"NENHUM"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_5_5">
          <text>"SOLICITAR_INFO"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI>
    <dmndi:DMNDiagram>
      <dmndi:DMNShape dmnElementRef="Decision_41204093">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>
</definitions>
