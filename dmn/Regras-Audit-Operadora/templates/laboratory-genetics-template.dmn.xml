<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC/"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI/"
             id="Definitions_TUSS_CODE"
             name="PROCEDURE_NAME"
             namespace="http://camunda.org/schema/1.0/dmn"
             exporter="Camunda Modeler"
             exporterVersion="5.0.0">
  <!--
    TUSS TUSS_CODE - PROCEDURE_NAME
    Versao: 1.0.0 (2026-02-06)
    Especialidade: SPECIALTY_NAME
    Categoria: Laboratory Genetics

    Indicacoes Clinicas:
    - Suspeita clinica fundamentada de [condicao genetica] com criterios especificos
    - Historico familiar positivo em [N ou mais] parentes de primeiro grau
    - [Indicacao 3 com criterios quantitativos especificos]

    Contraindicacoes:
    - Ausencia de suspeita clinica ou familiar fundamentada
    - Teste ja realizado anteriormente com resultado conclusivo
    - [Contraindicacao especifica 3]

    Documentacao Obrigatoria:
    - Pedido medico com justificativa clinica detalhada
    - Arvore genealogica (pedigree) em casos de historico familiar
    - Resultados de exames previos relacionados
    - Consentimento informado para teste genetico
    - [Documento especifico adicional]

    Referencias Normativas:
    - DUT ANS 2024 - Diretrizes para Exames Geneticos
    - RN 465/2021 - Rol de Procedimentos em Genetica
    - Sociedade Brasileira de Genetica Medica - Diretrizes
    - [Protocolo clinico especifico]
  -->

  <decision id="Decision_TUSS_CODE" name="Criterios de Autorizacao - PROCEDURE_NAME">
    <decisionTable id="DecisionTable_TUSS_CODE" hitPolicy="FIRST">

      <!-- Input 1: Diagnostico de Suspeita -->
      <input id="Input_diagnosticoSuspeita" label="Diagnostico de Suspeita (CID-10)">
        <inputExpression id="InputExpr_diagnosticoSuspeita" typeRef="string">
          <text>diagnosticoSuspeita</text>
        </inputExpression>
        <inputValues id="InputValues_diagnosticoSuspeita">
          <text>"GENETIC_CONDITION_1","GENETIC_CONDITION_2","GENETIC_CONDITION_3"</text>
        </inputValues>
      </input>

      <!-- Input 2: Historico Familiar -->
      <input id="Input_historicoFamiliar" label="Historico Familiar Positivo">
        <inputExpression id="InputExpr_historicoFamiliar" typeRef="string">
          <text>historicoFamiliar</text>
        </inputExpression>
        <inputValues id="InputValues_historicoFamiliar">
          <text>"NENHUM","1_PARENTE_1GRAU","2_OU_MAIS_PARENTES_1GRAU","PARENTE_2GRAU"</text>
        </inputValues>
      </input>

      <!-- Input 3: Testes Anteriores -->
      <input id="Input_testesAnteriores" label="Testes Geneticos Anteriores">
        <inputExpression id="InputExpr_testesAnteriores" typeRef="string">
          <text>testesAnteriores</text>
        </inputExpression>
        <inputValues id="InputValues_testesAnteriores">
          <text>"NAO_REALIZADO","INCONCLUSIVO","NEGATIVO_OUTRA_MUTACAO","POSITIVO_CONCLUSIVO"</text>
        </inputValues>
      </input>

      <!-- Input 4: Justificativa Medica -->
      <input id="Input_justificativaMedica" label="Justificativa Medica Detalhada">
        <inputExpression id="InputExpr_justificativaMedica" typeRef="boolean">
          <text>justificativaMedica</text>
        </inputExpression>
      </input>

      <!-- Output 1: Resultado -->
      <output id="Output_resultado" label="Resultado" name="resultado" typeRef="string">
        <outputValues id="OutputValues_resultado">
          <text>"Aprovado","Reprovado","Pendente"</text>
        </outputValues>
      </output>

      <!-- Output 2: Observacao -->
      <output id="Output_observacao" label="Observacao" name="observacao" typeRef="string" />

      <!-- Output 3: Fundamentacao -->
      <output id="Output_fundamentacao" label="Fundamentacao Legal" name="fundamentacao" typeRef="string" />

      <!-- Output 4: Alertas de Desperdicio -->
      <output id="Output_alertasDesperdicio" label="Alertas de Desperdicio" name="alertasDesperdicio" typeRef="string">
        <outputValues id="OutputValues_alertasDesperdicio">
          <text>"NENHUM","DUP","FREQ","ESC","DOC","PROT","MULTI"</text>
        </outputValues>
      </output>

      <!-- Output 5: Acao Recomendada -->
      <output id="Output_acaoRecomendada" label="Acao Recomendada" name="acaoRecomendada" typeRef="string">
        <outputValues id="OutputValues_acaoRecomendada">
          <text>"APROVAR","NEGAR","SOLICITAR_INFO","AGUARDAR_PRAZO","SUGERIR_ALTERNATIVA"</text>
        </outputValues>
      </output>

      <!-- ============================================== -->
      <!-- CONTRAINDICATION: Teste ja realizado          -->
      <!-- MUST COME FIRST for global denial             -->
      <!-- ============================================== -->
      <rule id="Rule_TUSS_CODE_Contraindicacao_TestePositivo">
        <description>Contraindicacao - Teste ja realizado com resultado conclusivo positivo</description>
        <inputEntry id="IE_Contra1_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra1_3"><text>"POSITIVO_CONCLUSIVO"</text></inputEntry>
        <inputEntry id="IE_Contra1_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Contra1_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra1_2"><text>"Teste genetico ja realizado anteriormente com resultado conclusivo - sem indicacao para repeticao"</text></outputEntry>
        <outputEntry id="OE_Contra1_3"><text>"DUT ANS 2024 - Restricao para testes repetitivos"</text></outputEntry>
        <outputEntry id="OE_Contra1_4"><text>"DUP"</text></outputEntry>
        <outputEntry id="OE_Contra1_5"><text>"NEGAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- CONTRAINDICATION: Sem justificativa           -->
      <!-- ============================================== -->
      <rule id="Rule_TUSS_CODE_Contraindicacao_SemJustificativa">
        <description>Contraindicacao - Documentacao obrigatoria ausente</description>
        <inputEntry id="IE_Contra2_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Contra2_4"><text>false</text></inputEntry>
        <outputEntry id="OE_Contra2_1"><text>"Reprovado"</text></outputEntry>
        <outputEntry id="OE_Contra2_2"><text>"Justificativa medica detalhada eh documentacao obrigatoria para exames geneticos"</text></outputEntry>
        <outputEntry id="OE_Contra2_3"><text>"RN 465/2021 Art. 5 - Documentacao Minima para Genetica"</text></outputEntry>
        <outputEntry id="OE_Contra2_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OE_Contra2_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APPROVAL: Suspeita clinica + historico forte  -->
      <!-- ============================================== -->
      <rule id="Rule_TUSS_CODE_Aprovacao_SuspeitaHistorico2Mais">
        <description>Aprovacao - Suspeita clinica com historico familiar em 2+ parentes de 1o grau</description>
        <inputEntry id="IE_Aprov1_1"><text>"GENETIC_CONDITION_1"</text></inputEntry>
        <inputEntry id="IE_Aprov1_2"><text>"2_OU_MAIS_PARENTES_1GRAU"</text></inputEntry>
        <inputEntry id="IE_Aprov1_3"><text>"NAO_REALIZADO","INCONCLUSIVO","NEGATIVO_OUTRA_MUTACAO"</text></inputEntry>
        <inputEntry id="IE_Aprov1_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov1_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov1_2"><text>"Indicacao prioritaria - suspeita clinica com historico familiar robusto (2+ parentes 1o grau)"</text></outputEntry>
        <outputEntry id="OE_Aprov1_3"><text>"DUT ANS 2024 - Criterio de Alto Risco Familiar"</text></outputEntry>
        <outputEntry id="OE_Aprov1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprov1_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APPROVAL: Suspeita clinica + historico 1      -->
      <!-- ============================================== -->
      <rule id="Rule_TUSS_CODE_Aprovacao_SuspeitaHistorico1">
        <description>Aprovacao - Suspeita clinica com historico familiar em 1 parente de 1o grau</description>
        <inputEntry id="IE_Aprov2_1"><text>"GENETIC_CONDITION_1","GENETIC_CONDITION_2"</text></inputEntry>
        <inputEntry id="IE_Aprov2_2"><text>"1_PARENTE_1GRAU"</text></inputEntry>
        <inputEntry id="IE_Aprov2_3"><text>"NAO_REALIZADO","INCONCLUSIVO","NEGATIVO_OUTRA_MUTACAO"</text></inputEntry>
        <inputEntry id="IE_Aprov2_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov2_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_2"><text>"Indicacao secundaria - suspeita clinica com historico familiar em 1 parente de 1o grau"</text></outputEntry>
        <outputEntry id="OE_Aprov2_3"><text>"DUT ANS 2024 - Criterio de Risco Familiar Moderado"</text></outputEntry>
        <outputEntry id="OE_Aprov2_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprov2_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- APPROVAL: Teste inconclusivo anterior         -->
      <!-- ============================================== -->
      <rule id="Rule_TUSS_CODE_Aprovacao_TesteInconclusivo">
        <description>Aprovacao - Teste anterior inconclusivo com nova indicacao</description>
        <inputEntry id="IE_Aprov3_1"><text>"GENETIC_CONDITION_1","GENETIC_CONDITION_2","GENETIC_CONDITION_3"</text></inputEntry>
        <inputEntry id="IE_Aprov3_2"><text>"1_PARENTE_1GRAU","2_OU_MAIS_PARENTES_1GRAU"</text></inputEntry>
        <inputEntry id="IE_Aprov3_3"><text>"INCONCLUSIVO"</text></inputEntry>
        <inputEntry id="IE_Aprov3_4"><text>true</text></inputEntry>
        <outputEntry id="OE_Aprov3_1"><text>"Aprovado"</text></outputEntry>
        <outputEntry id="OE_Aprov3_2"><text>"Indicacao para repeticao ou complementacao - teste anterior inconclusivo"</text></outputEntry>
        <outputEntry id="OE_Aprov3_3"><text>"DUT ANS 2024 - Repeticao de Teste Inconclusivo"</text></outputEntry>
        <outputEntry id="OE_Aprov3_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Aprov3_5"><text>"APROVAR"</text></outputEntry>
      </rule>

      <!-- ============================================== -->
      <!-- FALLBACK RULE - Casos nao cobertos            -->
      <!-- ============================================== -->
      <rule id="Rule_Fallback_TUSS_CODE">
        <description>Regra padrao - Casos nao cobertos requerem analise manual por geneticista medico</description>
        <inputEntry id="IE_Fallback_1"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_2"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_3"><text>-</text></inputEntry>
        <inputEntry id="IE_Fallback_4"><text>-</text></inputEntry>
        <outputEntry id="OE_Fallback_1"><text>"Pendente"</text></outputEntry>
        <outputEntry id="OE_Fallback_2"><text>"Caso requer analise manual por geneticista medico - criterios nao atendidos automaticamente"</text></outputEntry>
        <outputEntry id="OE_Fallback_3"><text>"Processo manual de auditoria em genetica medica"</text></outputEntry>
        <outputEntry id="OE_Fallback_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OE_Fallback_5"><text>"SOLICITAR_INFO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>
</definitions>
