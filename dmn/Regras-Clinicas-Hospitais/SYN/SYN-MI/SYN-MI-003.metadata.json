{
  "ruleId": "SYN-MI-003",
  "ruleName": "Estratificacao de Risco SCA via GRACE Score",
  "category": "SYNDROME_DETECTION",
  "subcategory": "MI",
  "version": "1.0.0",
  "effectiveDate": "2026-02-06",
  "specialty": "CARDIOLOGIA",
  "severity": "HIGH",
  "timeSensitive": true,
  "maxResponseTime": "2_HOURS",
  "clinicalReferences": [
    "GRACE Risk Score Calculator 2.0",
    "ESC NSTE-ACS Guidelines 2020",
    "Fox KA et al. BMJ 2006",
    "Killip Classification"
  ],
  "inputs": [
    {
      "variable": "graceScore",
      "type": "number",
      "required": true,
      "range": [0, 300],
      "criticalThreshold": 140
    },
    {
      "variable": "classeKillip",
      "type": "number",
      "required": true,
      "allowedValues": [1, 2, 3, 4],
      "criticalThreshold": 4
    },
    {
      "variable": "alteracaoSTDinamica",
      "type": "boolean",
      "required": true
    },
    {
      "variable": "paradaCardiacaReanimada",
      "type": "boolean",
      "required": true
    }
  ],
  "outputs": [
    "nivelAlerta",
    "classificacaoSindrome",
    "protocoloAtivado",
    "acaoRequerida",
    "urgencia"
  ],
  "automationLevel": "ALERT_ONLY",
  "requiresPhysicianReview": true,
  "protocolActivation": {
    "protocol": "ACS_RISK_STRATIFICATION",
    "actions": [
      "Calcular GRACE Score completo",
      "Determinar timing da ICP",
      "Definir nivel de cuidado (UTI vs UCO vs enfermaria)",
      "Otimizar terapia antitrombinha",
      "Planejar estrategia de revascularizacao"
    ]
  },
  "mortalityImpact": "GRACE permite prever mortalidade com AUC 0.80-0.85",
  "tags": ["acs", "grace-score", "risk-stratification", "nstemi", "killip", "cardiology"]
}
