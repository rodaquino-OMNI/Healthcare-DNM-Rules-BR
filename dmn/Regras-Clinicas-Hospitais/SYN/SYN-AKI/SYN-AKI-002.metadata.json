{
  "ruleId": "SYN-AKI-002",
  "ruleName": "Deteccao de Lesao Renal Aguda via Debito Urinario",
  "category": "SYNDROME_DETECTION",
  "subcategory": "AKI",
  "version": "1.0.0",
  "effectiveDate": "2026-02-06",
  "specialty": "NEFROLOGIA",
  "severity": "CRITICAL",
  "timeSensitive": true,
  "maxResponseTime": "2_HOURS",
  "clinicalReferences": [
    "KDIGO Clinical Practice Guideline for Acute Kidney Injury (2012)",
    "Surviving Sepsis Campaign Guidelines 2021",
    "NICE Guideline NG148 - Acute Kidney Injury (2019)"
  ],
  "inputs": [
    {
      "variable": "debitoUrinario",
      "type": "number",
      "unit": "mL/kg/h",
      "required": true,
      "normalRange": [0.5, 2.0],
      "criticalThreshold": 0.5
    },
    {
      "variable": "duracaoOliguria",
      "type": "number",
      "unit": "horas",
      "required": true
    },
    {
      "variable": "anuria",
      "type": "boolean",
      "required": true,
      "description": "Ausencia completa de diurese"
    },
    {
      "variable": "cateterVesical",
      "type": "boolean",
      "required": true,
      "description": "Cateter vesical de demora instalado"
    }
  ],
  "outputs": [
    "nivelAlerta",
    "classificacaoSindrome",
    "protocoloAtivado",
    "acaoRequerida",
    "urgencia"
  ],
  "automationLevel": "ALERT_ONLY",
  "requiresPhysicianReview": true,
  "protocolActivation": {
    "protocol": "OLIGURIA_PROTOCOL",
    "actions": [
      "Instalar cateter vesical se nao presente",
      "Avaliar volemia (ecografia VCI, PVC)",
      "Descartar obstrucao urinaria (USG)",
      "Desafio de fluidos se hipovolemico",
      "Monitorar DU a cada 2h"
    ]
  },
  "mortalityImpact": "Oliguria persistente aumenta mortalidade em 2-3x",
  "tags": ["aki", "oliguria", "anuria", "urine-output", "kdigo", "nephrology"]
}
