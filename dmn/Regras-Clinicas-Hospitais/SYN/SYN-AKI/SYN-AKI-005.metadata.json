{
  "ruleId": "SYN-AKI-005",
  "ruleName": "Deteccao de Nefropatia Induzida por Contraste",
  "category": "SYNDROME_DETECTION",
  "subcategory": "AKI",
  "version": "1.0.0",
  "effectiveDate": "2026-02-06",
  "specialty": "NEFROLOGIA",
  "severity": "HIGH",
  "timeSensitive": true,
  "maxResponseTime": "24_HOURS",
  "clinicalReferences": [
    "KDIGO AKI Guidelines 2012",
    "ACR Manual on Contrast Media 2021",
    "ESC Guidelines on Contrast-Induced Nephropathy",
    "Mehran Risk Score for CIN"
  ],
  "inputs": [
    {
      "variable": "creatininaPreContraste",
      "type": "number",
      "unit": "mg/dL",
      "required": true,
      "description": "Creatinina basal antes do contraste"
    },
    {
      "variable": "creatininaPosContraste",
      "type": "number",
      "unit": "mg/dL",
      "required": false,
      "description": "Creatinina 48-72h apos contraste"
    },
    {
      "variable": "horasDesdeContraste",
      "type": "number",
      "unit": "horas",
      "required": true
    },
    {
      "variable": "tfgPreContraste",
      "type": "number",
      "unit": "mL/min/1.73m2",
      "required": true,
      "criticalThreshold": 30
    }
  ],
  "outputs": [
    "nivelAlerta",
    "classificacaoSindrome",
    "protocoloAtivado",
    "acaoRequerida",
    "urgencia"
  ],
  "automationLevel": "ALERT_ONLY",
  "requiresPhysicianReview": true,
  "protocolActivation": {
    "protocol": "CONTRAST_NEPHROPATHY_PREVENTION",
    "actions": [
      "Hidratacao SF 0.9% 1mL/kg/h 6-12h pre e pos",
      "Usar contraste iso-osmolar (iodixanol)",
      "Minimizar volume de contraste",
      "Suspender metformina 48h apos",
      "Dosar creatinina 48-72h apos"
    ]
  },
  "mortalityImpact": "NIC aumenta mortalidade hospitalar em 5-10x",
  "tags": ["aki", "contrast", "nephropathy", "cin", "prevention", "radiology"]
}
