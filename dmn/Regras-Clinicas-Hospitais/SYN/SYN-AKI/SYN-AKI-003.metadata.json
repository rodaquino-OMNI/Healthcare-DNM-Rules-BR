{
  "ruleId": "SYN-AKI-003",
  "ruleName": "Deteccao de Complicacoes Eletroliticas da AKI",
  "category": "SYNDROME_DETECTION",
  "subcategory": "AKI",
  "version": "1.0.0",
  "effectiveDate": "2026-02-06",
  "specialty": "NEFROLOGIA",
  "severity": "CRITICAL",
  "timeSensitive": true,
  "maxResponseTime": "30_MINUTES",
  "clinicalReferences": [
    "KDIGO Clinical Practice Guideline for AKI (2012)",
    "AHA Guidelines for Hyperkalemia Management (2020)",
    "Sociedade Brasileira de Nefrologia"
  ],
  "inputs": [
    {
      "variable": "potassio",
      "type": "number",
      "unit": "mEq/L",
      "required": true,
      "normalRange": [3.5, 5.0],
      "criticalThreshold": 6.5
    },
    {
      "variable": "phArterial",
      "type": "number",
      "required": true,
      "normalRange": [7.35, 7.45],
      "criticalThreshold": 7.15
    },
    {
      "variable": "bicarbonato",
      "type": "number",
      "unit": "mEq/L",
      "required": true,
      "normalRange": [22, 26],
      "criticalThreshold": 10
    },
    {
      "variable": "alteracoesEcgHipercalemia",
      "type": "boolean",
      "required": true,
      "description": "Onda T apiculada, alargamento QRS, ondas sinusoidais"
    }
  ],
  "outputs": [
    "nivelAlerta",
    "classificacaoSindrome",
    "protocoloAtivado",
    "acaoRequerida",
    "urgencia"
  ],
  "automationLevel": "ALERT_ONLY",
  "requiresPhysicianReview": true,
  "protocolActivation": {
    "protocol": "HYPERKALEMIA_EMERGENCY",
    "actions": [
      "Gluconato de calcio 10% 10mL IV (estabilizacao cardiaca)",
      "Insulina regular 10U + Glicose 50% 25g IV",
      "Salbutamol nebulizacao 10-20mg",
      "Poliestireno sulfonato de calcio VO/VR",
      "Dialise de emergencia se refratario"
    ]
  },
  "mortalityImpact": "Hipercalemia > 6.5 mEq/L pode causar arritmia fatal em minutos",
  "tags": ["aki", "hyperkalemia", "acidosis", "electrolytes", "emergency", "dialysis"]
}
