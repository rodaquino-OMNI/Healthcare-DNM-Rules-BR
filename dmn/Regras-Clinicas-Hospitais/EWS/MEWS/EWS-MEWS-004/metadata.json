{
  "ruleId": "EWS-MEWS-004",
  "version": "1.0.0",
  "createdAt": "2026-02-06",
  "category": "CLINICAL_ALERT",
  "subcategory": "EARLY_WARNING",
  "alertSystem": "MEWS",
  "name": "MEWS Respiratory Component Alert",
  "description": "Triggers alerts specifically for respiratory distress indicators within MEWS scoring.",
  "clinicalDomain": "Pulmonology / Respiratory Care",
  "evidenceBase": {
    "primarySource": "Subbe CP et al. QJM 2001;94:521-526",
    "keyFinding": "Respiratory rate most predictive MEWS component",
    "mortalityRisk": "3x increased at RR >=25"
  },
  "scoringCriteria": {
    "respiratoryRate": {
      "<9": {
        "points": 2,
        "interpretation": "Hypoventilation"
      },
      "9-14": {
        "points": 0,
        "interpretation": "Normal"
      },
      "15-20": {
        "points": 1,
        "interpretation": "Mild tachypnea"
      },
      "21-29": {
        "points": 2,
        "interpretation": "Moderate tachypnea"
      },
      ">=30": {
        "points": 3,
        "interpretation": "Severe tachypnea"
      }
    }
  },
  "inputs": [
    {
      "name": "frequenciaRespiratoria",
      "type": "number",
      "unit": "irpm"
    },
    {
      "name": "saturacaoO2",
      "type": "number",
      "unit": "%"
    },
    {
      "name": "o2Suplementar",
      "type": "boolean"
    },
    {
      "name": "doencaRespiratoriaCronica",
      "type": "boolean",
      "description": "DPOC, asthma, or other chronic respiratory disease"
    }
  ],
  "outputs": [
    {
      "name": "nivelAlerta",
      "type": "string",
      "values": [
        "Alerta",
        "Atencao",
        "Informativo",
        "OK",
        "Revisar"
      ]
    },
    {
      "name": "urgencia",
      "type": "string",
      "values": [
        "IMEDIATA",
        "URGENTE",
        "ROTINA",
        "INFORMATIVA"
      ]
    },
    {
      "name": "acaoRequerida",
      "type": "string"
    },
    {
      "name": "justificativaCientifica",
      "type": "string"
    }
  ],
  "tags": [
    "MEWS",
    "respiratory",
    "tachypnea",
    "hypoxia",
    "DPOC",
    "pulmonology"
  ],
  "relatedRules": [
    "EWS-MEWS-001",
    "EWS-NEWS-002"
  ],
  "status": "active",
  "criticality": "HIGH"
}