{
  "ruleId": "EWS-PEWS-004",
  "version": "1.0.0",
  "createdAt": "2026-02-06",
  "category": "CLINICAL_ALERT",
  "subcategory": "EARLY_WARNING",
  "alertSystem": "PEWS",
  "name": "PEWS Respiratory Component - Age-Adjusted",
  "description": "Evaluates respiratory status using age-adjusted thresholds for pediatric patients.",
  "clinicalDomain": "Pediatric Pulmonology / Emergency Medicine",
  "evidenceBase": {
    "primarySource": "Pediatric vital sign references",
    "keyFinding": "Respiratory failure is #1 cause of pediatric arrest"
  },
  "ageAdjustedRR": {
    "0-3months": "30-60/min",
    "3-6months": "25-40/min",
    "6-12months": "20-30/min",
    "1-3years": "20-30/min",
    "3-6years": "18-25/min",
    "6-12years": "16-20/min",
    ">12years": "12-18/min"
  },
  "effortScore": {
    "0": "Normal, no distress",
    "1": "Mild: slight accessory use, mild tachypnea",
    "2": "Moderate: intercostal/subcostal retractions, nasal flaring",
    "3": "Severe: apnea, gasping, grunting, severe retractions"
  },
  "inputs": [
    {
      "name": "idadeMeses",
      "type": "number"
    },
    {
      "name": "frequenciaRespiratoria",
      "type": "number",
      "unit": "/min"
    },
    {
      "name": "saturacaoO2",
      "type": "number",
      "unit": "%"
    },
    {
      "name": "esforcoRespiratorio",
      "type": "number",
      "range": "0-3"
    }
  ],
  "outputs": [
    {
      "name": "nivelAlerta",
      "type": "string",
      "values": [
        "Alerta",
        "Atencao",
        "Informativo",
        "OK",
        "Revisar"
      ]
    },
    {
      "name": "urgencia",
      "type": "string",
      "values": [
        "IMEDIATA",
        "URGENTE",
        "ROTINA",
        "INFORMATIVA"
      ]
    },
    {
      "name": "acaoRequerida",
      "type": "string"
    },
    {
      "name": "justificativaCientifica",
      "type": "string"
    }
  ],
  "tags": [
    "PEWS",
    "respiratory",
    "pediatric",
    "age-adjusted",
    "hypoxia"
  ],
  "relatedRules": [
    "EWS-PEWS-001",
    "EWS-PEWS-005"
  ],
  "status": "active",
  "criticality": "HIGH"
}