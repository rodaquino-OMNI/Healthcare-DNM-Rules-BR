<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_EWS_NEWS_003"
             name="NEWS2 Medium Risk (5-6)"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_EWS_NEWS_003" name="NEWS2 Medium Risk Evaluation">
    <decisionTable id="DecisionTable_EWS_NEWS_003" hitPolicy="FIRST">

      <input id="Input_1" label="NEWS2 Score Total">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>news2ScoreTotal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Trending Up">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>scoreTrendingUp</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Nivel Alerta" name="nivelAlerta" typeRef="string">
        <outputValues id="OutputValues_1">
          <text>"Alerta", "Atencao", "Informativo", "OK", "Revisar"</text>
        </outputValues>
      </output>

      <output id="Output_2" label="Urgencia" name="urgencia" typeRef="string">
        <outputValues id="OutputValues_2">
          <text>"IMEDIATA", "URGENTE", "ROTINA", "INFORMATIVA"</text>
        </outputValues>
      </output>

      <output id="Output_3" label="Acao Requerida" name="acaoRequerida" typeRef="string"/>
      <output id="Output_4" label="Justificativa Cientifica" name="justificativaCientifica" typeRef="string"/>

      <rule id="Rule_MediumRisk_Trending">
        <description>Medium risk score trending upward - urgent review needed</description>
        <inputEntry id="InputEntry_1_1">
          <text>[5..6]</text>
        </inputEntry>
        <inputEntry id="InputEntry_1_2">
          <text>true</text>
        </inputEntry>
        <outputEntry id="OutputEntry_1_1">
          <text>"Atencao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_1_2">
          <text>"URGENTE"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_1_3">
          <text>"Score medio-elevado (5-6) com tendencia de piora. Avaliacao por medico clinico dentro de 1 hora. Aumentar frequencia de monitorizacao para cada 1 hora. Revisar plano terapeutico."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_1_4">
          <text>"NEWS2 5-6 indica risco medio (mortalidade 3.1%). Tendencia ascendente sugere deterioracao progressiva requerendo intensificacao de cuidados antes de atingir risco alto."</text>
        </outputEntry>
      </rule>

      <rule id="Rule_MediumRisk_Stable">
        <description>Medium risk score stable - enhanced monitoring</description>
        <inputEntry id="InputEntry_2_1">
          <text>[5..6]</text>
        </inputEntry>
        <inputEntry id="InputEntry_2_2">
          <text>false</text>
        </inputEntry>
        <outputEntry id="OutputEntry_2_1">
          <text>"Atencao"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_2">
          <text>"URGENTE"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_3">
          <text>"Score medio (5-6) estavel. Avaliacao clinica urgente. Monitoramento a cada 1-2 horas. Identificar causa do score elevado e otimizar tratamento."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_2_4">
          <text>"Score medio requer atencao medica urgente para prevenir deterioracao. Risco intermediario entre baixo (1-4) e alto (â‰¥7)."</text>
        </outputEntry>
      </rule>

      <rule id="Rule_Fallback">
        <description>Score outside medium range</description>
        <inputEntry id="InputEntry_F_1">
          <text>-</text>
        </inputEntry>
        <inputEntry id="InputEntry_F_2">
          <text>-</text>
        </inputEntry>
        <outputEntry id="OutputEntry_F_1">
          <text>"Revisar"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_F_2">
          <text>"INFORMATIVA"</text>
        </outputEntry>
        <outputEntry id="OutputEntry_F_3">
          <text>"Score fora da faixa medio-risco. Consultar outras regras NEWS2."</text>
        </outputEntry>
        <outputEntry id="OutputEntry_F_4">
          <text>"Score nao se enquadra nos criterios de risco medio (5-6)."</text>
        </outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_EWS_NEWS_003">
      <dmndi:DMNShape id="DMNShape_Decision_EWS_NEWS_003" dmnElementRef="Decision_EWS_NEWS_003">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
