<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" id="Definitions_VIT_CRITICAL_BP_001" name="DRD" namespace="http://camunda.org/schema/1.0/dmn" exporter="Camunda Modeler" exporterVersion="5.32.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.7.0">
  <decision id="Decision_VIT_CRITICAL_BP_001" name="VIT-CRITICAL-BP-001: Choque Hipotensivo">
    <decisionTable id="DecisionTable_VIT_CRITICAL_BP_001" hitPolicy="FIRST">
      <input id="Input_pressaoSistolica" label="Pressão Sistólica (mmHg)">
        <inputExpression id="InputExpression_pressaoSistolica" typeRef="number">
          <text>pressaoSistolica</text>
        </inputExpression>
      </input>
      <output id="Output_nivelAlerta" label="Nível de Alerta" name="nivelAlerta" typeRef="string">
        <outputValues id="UnaryTests_nivelAlerta">
          <text>"Alerta","Atencao","Informativo","OK"</text>
        </outputValues>
      </output>
      <output id="Output_nivelRisco" label="Nível de Risco" name="nivelRisco" typeRef="string">
        <outputValues id="UnaryTests_nivelRisco">
          <text>"MUITO_ALTO","ALTO","MODERADO","BAIXO"</text>
        </outputValues>
      </output>
      <output id="Output_intervencaoRequerida" label="Intervenção Requerida" name="intervencaoRequerida" typeRef="string" />
      <output id="Output_frequenciaMonitoramento" label="Frequência de Monitoramento" name="frequenciaMonitoramento" typeRef="string" />
      <!-- Severe hypotension: SBP < 80 mmHg -->
      <rule id="DecisionRule_001">
        <inputEntry id="UnaryTests_001_01">
          <text>&lt; 80</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_001_01">
          <text>"Alerta"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_001_02">
          <text>"MUITO_ALTO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_001_03">
          <text>"CHOQUE HIPOTENSIVO. Avaliar perfusão (lactato, débito urinário). Acesso vascular calibroso. Iniciar ressuscitação volêmica. Investigar causa (hipovolêmico/cardiogênico/distributivo/obstrutivo). ACIONAR CÓDIGO AZUL."</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_001_04">
          <text>"CONTINUO"</text>
        </outputEntry>
      </rule>
      <!-- Moderate hypotension: SBP 80-90 mmHg -->
      <rule id="DecisionRule_002">
        <inputEntry id="UnaryTests_002_01">
          <text>[80..90]</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_002_01">
          <text>"Atencao"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_002_02">
          <text>"ALTO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_002_03">
          <text>"Hipotensão moderada. Avaliar sinais de perfusão inadequada. Considerar bolus de volume. Investigar causas. Acionar MD se sintomático."</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_002_04">
          <text>"Q15M"</text>
        </outputEntry>
      </rule>
      <!-- Normal range -->
      <rule id="DecisionRule_999">
        <inputEntry id="UnaryTests_999_01">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_999_01">
          <text>"OK"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_999_02">
          <text>"BAIXO"</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_999_03">
          <text>"Pressão arterial dentro da normalidade para hipotensão."</text>
        </outputEntry>
        <outputEntry id="LiteralExpression_999_04">
          <text>"Q4H"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>
