{
  "ruleId": "VIT-CRITICAL-SPO2-001",
  "category": "VIT",
  "subcategory": "CRITICAL",
  "name": "Hipoxemia Severa",
  "description": "Detecta saturação de oxigênio criticamente baixa (< 88%) indicando insuficiência respiratória grave",
  "inputs": [
    {
      "name": "saturacaoO2",
      "type": "number",
      "description": "Saturação periférica de oxigênio em percentual"
    }
  ],
  "outputs": [
    {
      "name": "nivelAlerta",
      "type": "string",
      "validValues": ["ALERTA", "ATENCAO", "INFORMATIVO", "OK"]
    },
    {
      "name": "nivelRisco",
      "type": "string",
      "validValues": ["MUITO_ALTO", "ALTO", "MODERADO", "BAIXO"]
    },
    {
      "name": "intervencaoRequerida",
      "type": "string"
    },
    {
      "name": "frequenciaMonitoramento",
      "type": "string"
    }
  ],
  "regulatoryCompliance": ["RDC-36-2013", "NR-32"],
  "clinicalReferences": ["WHO-Oxygen-Therapy-2016", "BTS-Oxygen-Guidelines-2017"],
  "version": "1.0.0",
  "status": "active",
  "criticality": "CRITICAL"
}
