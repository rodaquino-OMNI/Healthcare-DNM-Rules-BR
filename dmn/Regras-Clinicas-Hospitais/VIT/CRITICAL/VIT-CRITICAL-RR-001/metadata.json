{
  "ruleId": "VIT-CRITICAL-RR-001",
  "category": "VIT",
  "subcategory": "CRITICAL",
  "name": "Frequência Respiratória Crítica",
  "description": "Detecta frequência respiratória criticamente anormal (< 8 ou > 30 irpm) indicando falência respiratória iminente",
  "inputs": [
    {
      "name": "frequenciaRespiratoria",
      "type": "number",
      "description": "Frequência respiratória em incursões por minuto"
    }
  ],
  "outputs": [
    {
      "name": "nivelAlerta",
      "type": "string",
      "validValues": ["ALERTA", "ATENCAO", "INFORMATIVO", "OK"]
    },
    {
      "name": "nivelRisco",
      "type": "string",
      "validValues": ["MUITO_ALTO", "ALTO", "MODERADO", "BAIXO"]
    },
    {
      "name": "intervencaoRequerida",
      "type": "string"
    },
    {
      "name": "frequenciaMonitoramento",
      "type": "string"
    }
  ],
  "regulatoryCompliance": ["RDC-36-2013", "NR-32"],
  "clinicalReferences": ["ATS-Respiratory-Failure-2019", "ERS-Guidelines-2020"],
  "version": "1.0.0",
  "status": "active",
  "criticality": "CRITICAL"
}
