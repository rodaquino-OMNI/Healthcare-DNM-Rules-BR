      "validValues": ["ALERTA", "ATENCAO", "INFORMATIVO", "OK"]
    },
    {
      "name": "nivelRisco",
      "type": "string",
      "validValues": ["MUITO_ALTO", "ALTO", "MODERADO", "BAIXO"]
    },
    {
      "name": "intervencaoRequerida",
      "type": "string"
    },
    {
      "name": "frequenciaMonitoramento",
      "type": "string"
    }
  ],
  "regulatoryCompliance": ["RDC-36-2013"],
  "clinicalReferences": ["Surviving-Sepsis-Campaign-2021", "Intensive-Care-Medicine-2020"],
