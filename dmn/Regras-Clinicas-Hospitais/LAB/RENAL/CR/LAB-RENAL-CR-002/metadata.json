{
  "id": "LAB-RENAL-CR-002",
  "name": "Creatinina - Tendencia de Elevacao",
  "category": "LAB",
  "subcategory": "RENAL",
  "description": "Alerta para tendencia de elevacao de creatinina e oliguria indicando progressao de IRA",
  "version": "1.0.0",
  "created": "2026-02-06",
  "updated": "2026-02-06",
  "author": "Critical Lab Specialist",
  "status": "active",
  "priority": "CRITICAL",
  "clinicalRationale": "Criterios KDIGO para AKI incluem creatinina E diurese. Oliguria <0.3 ml/kg/h por >6h define AKI estagio 3 independente da creatinina. Deteccao precoce de tendencia permite intervencao antes de dano irreversivel.",
  "inputVariables": [
    {
      "name": "creatininaAtual",
      "type": "number",
      "unit": "mg/dL",
      "description": "Creatinina serica atual",
      "required": false
    },
    {
      "name": "creatinina24h",
      "type": "number",
      "unit": "mg/dL",
      "description": "Creatinina serica de 24h antes",
      "required": false
    },
    {
      "name": "diurese6h",
      "type": "number",
      "unit": "ml/kg/h",
      "description": "Diurese media nas ultimas 6 horas",
      "required": false,
      "normalRange": ">0.5 ml/kg/h"
    }
  ],
  "outputVariables": [
    {
      "name": "nivelAlerta",
      "type": "string",
      "values": ["Alerta", "Atencao", "OK"]
    },
    {
      "name": "urgencia",
      "type": "string",
      "values": ["IMEDIATA", "ALTA", "MEDIA", "BAIXA"]
    },
    {
      "name": "classificacao",
      "type": "string"
    },
    {
      "name": "tendencia",
      "type": "string"
    },
    {
      "name": "acaoRequerida",
      "type": "string"
    },
    {
      "name": "justificativaCientifica",
      "type": "string"
    }
  ],
  "references": [
    "KDIGO Guidelines: AKI Definition and Staging",
