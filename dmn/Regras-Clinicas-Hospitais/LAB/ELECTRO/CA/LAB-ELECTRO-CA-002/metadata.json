{
  "id": "LAB-ELECTRO-CA-002",
  "name": "Hipercalcemia Critica",
  "category": "LAB",
  "subcategory": "ELECTROLYTES",
  "description": "Alerta para niveis criticos de calcio ionizado >1.4 mmol/L ou total >12 mg/dL com risco de crise hipercalcemica",
  "version": "1.0.0",
  "created": "2026-02-06",
  "updated": "2026-02-06",
  "author": "Critical Lab Specialist",
  "status": "active",
  "priority": "CRITICAL",
  "clinicalRationale": "Crise hipercalcemica (iCa >1.5 mmol/L ou Ca total >14 mg/dL) causa alteracao de consciencia, arritmias, IRA e pancreatite. Tratamento emergencial com hidratacao agressiva, calciurese forcada e bifosfonatos. Investigacao etiologica essencial (malignidade vs hiperparatireoidismo).",
  "inputVariables": [
    {
      "name": "calcioIonizado",
      "type": "number",
      "unit": "mmol/L",
      "description": "Calcio ionizado serico",
      "required": false,
      "normalRange": "1.12-1.32 mmol/L"
    },
    {
      "name": "calcioTotalCorrigido",
      "type": "number",
      "unit": "mg/dL",
      "description": "Calcio total corrigido para albumina",
      "required": false,
      "normalRange": "8.5-10.5 mg/dL"
    }
  ],
  "outputVariables": [
    {
      "name": "nivelAlerta",
      "type": "string",
      "values": ["Alerta", "Atencao", "OK"]
    },
    {
      "name": "urgencia",
      "type": "string",
      "values": ["IMEDIATA", "ALTA", "MEDIA", "BAIXA"]
    },
    {
      "name": "classificacao",
      "type": "string"
    },
    {
      "name": "tendencia",
      "type": "string"
    },
    {
      "name": "acaoRequerida",
      "type": "string"
    },
    {
      "name": "justificativaCientifica",
      "type": "string"
    }
  ],
  "references": [
    "UpToDate: Treatment of hypercalcemia",
    "AACE/ACE Guidelines: Primary Hyperparathyroidism",
    "Critical Lab Values: Calcium >14 mg/dL requires immediate notification"
  ],
  "tags": ["critical-lab", "electrolytes", "calcium", "hypercalcemia", "malignancy", "hyperparathyroidism"]
}
