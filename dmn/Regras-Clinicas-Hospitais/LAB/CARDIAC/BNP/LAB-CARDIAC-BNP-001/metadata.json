{
  "id": "LAB-CARDIAC-BNP-001",
  "name": "BNP/NT-proBNP Elevado - Insuficiencia Cardiaca",
  "category": "LAB",
  "subcategory": "CARDIAC",
  "description": "Alerta para peptideos natriureticos elevados indicando insuficiencia cardiaca",
  "version": "1.0.0",
  "created": "2026-02-06",
  "updated": "2026-02-06",
  "author": "Critical Lab Specialist",
  "status": "active",
  "priority": "CRITICAL",
  "clinicalRationale": "BNP >500 pg/mL com dispneia tem VPP >90% para IC descompensada. NT-proBNP tem cortes ajustados por idade (<50: >450, 50-75: >900, >75: >1800 pg/mL). BNP guiado pode otimizar manejo. Outras causas incluem TEP, DRC, sepse.",
  "inputVariables": [
    {
      "name": "peptideoNatriuretico",
      "type": "number",
      "unit": "pg/mL",
      "description": "BNP ou NT-proBNP",
      "required": true,
      "normalRange": "BNP <100, NT-proBNP <300 (varia por idade)"
    },
    {
      "name": "tipoPeptideo",
      "type": "string",
      "description": "Tipo de peptideo dosado",
      "required": true,
      "values": ["BNP", "NT-proBNP"]
    },
    {
      "name": "dispneiaAguda",
      "type": "boolean",
      "description": "Presenca de dispneia aguda",
      "required": false
    }
  ],
  "outputVariables": [
    {
      "name": "nivelAlerta",
      "type": "string",
      "values": ["Alerta", "Atencao", "OK"]
    },
    {
      "name": "urgencia",
      "type": "string",
      "values": ["IMEDIATA", "ALTA", "MEDIA", "BAIXA"]
    },
    {
      "name": "classificacao",
      "type": "string"
    },
    {
      "name": "tendencia",
      "type": "string"
    },
    {
      "name": "acaoRequerida",
      "type": "string"
    },
    {
      "name": "justificativaCientifica",
      "type": "string"
    }
  ],
  "references": [
    "ESC Guidelines: Acute and Chronic Heart Failure",
    "AHA/ACC Guidelines: Management of Heart Failure",
    "Critical Lab Values: BNP >500 pg/mL requires clinical correlation"
  ],
  "tags": ["critical-lab", "cardiac", "BNP", "NT-proBNP", "heart-failure", "dyspnea"]
}
