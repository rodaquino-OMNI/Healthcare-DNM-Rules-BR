{
  "id": "LAB-CARDIAC-LAC-001",
  "name": "Lactato Elevado - Hipoperfusao Tecidual",
  "category": "LAB",
  "subcategory": "CARDIAC",
  "description": "Alerta para lactato elevado indicando hipoperfusao tecidual e choque",
  "version": "1.0.0",
  "created": "2026-02-06",
  "updated": "2026-02-06",
  "author": "Critical Lab Specialist",
  "status": "active",
  "priority": "CRITICAL",
  "clinicalRationale": "Lactato >4 mmol/L com hipotensao (PAM <65) define choque com mortalidade >40%. Clearance de lactato >10%/h e marcador de resposta ao tratamento. Lactato 2-4 mmol/L em sepse ja confere mortalidade de 20-30%.",
  "inputVariables": [
    {
      "name": "lactato",
      "type": "number",
      "unit": "mmol/L",
      "description": "Lactato arterial ou venoso",
      "required": true,
      "normalRange": "<2 mmol/L"
    },
    {
      "name": "pressaoArterialMedia",
      "type": "number",
      "unit": "mmHg",
      "description": "Pressao arterial media (PAM)",
      "required": false,
      "normalRange": ">65 mmHg"
    },
    {
      "name": "lactatoAnterior",
      "type": "number",
      "unit": "mmol/L",
      "description": "Lactato da medicao anterior para calculo de tendencia",
      "required": false
    }
  ],
  "outputVariables": [
    {
      "name": "nivelAlerta",
      "type": "string",
      "values": ["Alerta", "Atencao", "OK"]
    },
    {
      "name": "urgencia",
      "type": "string",
      "values": ["IMEDIATA", "ALTA", "MEDIA", "BAIXA"]
    },
    {
      "name": "classificacao",
      "type": "string"
    },
    {
      "name": "tendencia",
      "type": "string"
    },
    {
      "name": "acaoRequerida",
      "type": "string"
    },
    {
      "name": "justificativaCientifica",
      "type": "string"
    }
  ],
  "references": [
    "Surviving Sepsis Campaign: International Guidelines",
    "Crit Care Med: Lactate-Guided Resuscitation",
    "Critical Lab Values: Lactate >4 mmol/L requires immediate notification"
  ],
  "tags": ["critical-lab", "cardiac", "lactate", "shock", "hypoperfusion", "sepsis", "resuscitation"]
}
