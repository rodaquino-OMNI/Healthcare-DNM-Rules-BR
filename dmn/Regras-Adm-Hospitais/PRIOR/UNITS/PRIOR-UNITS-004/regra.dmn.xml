<?xml version="1.0" encoding="UTF-8"?>
<!--
  ============================================================================
  PRIOR-UNITS-004 - Transferencia de Unidades entre Guias
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: PRIOR-UNITS
  Perspectiva: HOSPITAL (Faturamento/Auditoria)

  ============================================================================
  INDICACOES (Quando PROSSEGUIR):
  ============================================================================
  - Transferencia autorizada pela operadora
  - Guias do mesmo paciente e periodo
  - Justificativa clinica presente

  ============================================================================
  CONTRAINDICACOES (Quando BLOQUEAR):
  ============================================================================
  - Transferencia nao autorizada
  - Guias de pacientes diferentes
  - Falta anuencia da operadora

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - RN 424/2017 (Autorizacao Previa)
  - RN 465/2021 (Procedimentos Regulados)
  - TISS 4.01.00 (Guia SP/SADT)
  - Lei 9.656/1998 Art. 12 (Cobertura Obrigatoria)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_PRIOR_UNITS_004"
             name="Transferencia de Unidades entre Guias"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_PRIOR_UNITS_004" name="Verificar Transferencia de Unidades entre Guias">
    <decisionTable id="DecisionTable_PRIOR_UNITS_004" hitPolicy="FIRST">

      <!-- ========== INPUTS ========== -->
      <input id="Input_1" label="Guia Origem">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>guiaOrigem</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Guia Destino">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>guiaDestino</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Unidades Transferidas">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>unidadesTransferidas</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Autorizado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>autorizado</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->

      <!-- REGRA B1: Bloqueio - Transferencia nao autorizada -->
      <rule id="Rule_Bloqueio_1">
        <description>Transferencia de unidades sem autorizacao</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>&gt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"Transferencia de unidades entre guias sem autorizacao da operadora. Faturamento bloqueado."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Solicitar anuencia da operadora para transferencia de unidades entre guias."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <!-- REGRA B2: Bloqueio - Unidades invalidas -->
      <rule id="Rule_Bloqueio_2">
        <description>Quantidade de unidades invalida</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>&lt;= 0</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_2"><text>"Quantidade de unidades transferidas invalida (zero ou negativa)."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_3"><text>"Corrigir quantidade de unidades a transferir."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <!-- REGRA A1: Alerta - Transferencia autorizada mas requer documentacao -->
      <rule id="Rule_Alerta_1">
        <description>Transferencia autorizada mas requer rastreabilidade</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&gt; 0</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_2"><text>"Transferencia de unidades autorizada. Documentacao de rastreabilidade necessaria."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_3"><text>"Documentar transferencia detalhadamente (guia origem, destino, unidades, justificativa)."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      <!-- REGRA P1: Aprovado - Sem transferencia -->
      <rule id="Rule_Aprovado_1">
        <description>Sem transferencia de unidades</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>0</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"Sem transferencia de unidades entre guias. Faturamento direto."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme guia original."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <!-- REGRA F: Fallback - Requer revisao manual -->
      <rule id="Rule_Fallback_PRIOR_UNITS_004">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Situacao de transferencia de unidades requer analise manual."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria verificar transferencia entre guias."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_PRIOR_UNITS_004">
      <dmndi:DMNShape id="DMNShape_Decision_PRIOR_UNITS_004" dmnElementRef="Decision_PRIOR_UNITS_004">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
