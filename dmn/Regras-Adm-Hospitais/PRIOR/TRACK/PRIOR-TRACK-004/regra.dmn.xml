<?xml version="1.0" encoding="UTF-8"?>
<!--
  ============================================================================
  PRIOR-TRACK-004 - Alerta Operadora com Alto Indice Negacao
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: PRIOR-TRACK
  Perspectiva: HOSPITAL (Faturamento/Auditoria)

  ============================================================================
  INDICACOES (Quando PROSSEGUIR):
  ============================================================================
  - Taxa de negacao dentro da media do setor
  - Volume de solicitacoes adequado
  - Sem desvios significativos

  ============================================================================
  CONTRAINDICACOES (Quando BLOQUEAR):
  ============================================================================
  - Taxa acima de 2x a media do setor
  - Operadora com comportamento abusivo
  - Volume alto com taxa critica

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - RN 424/2017 (Autorizacao Previa)
  - RN 465/2021 (Procedimentos Regulados)
  - RN 395/2016 (Monitoramento de Negativas - Notificacao ANS)
  - Lei 9.656/1998 Art. 12 (Cobertura Obrigatoria)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_PRIOR_TRACK_004"
             name="Alerta Operadora com Alto Indice Negacao"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_PRIOR_TRACK_004" name="Verificar Alerta Operadora com Alto Indice Negacao">
    <decisionTable id="DecisionTable_PRIOR_TRACK_004" hitPolicy="FIRST">

      <!-- ========== INPUTS ========== -->
      <input id="Input_1" label="Taxa Negacao (%)">
        <inputExpression id="InputExpression_1" typeRef="integer">
          <text>taxaNegacao</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Media Setor (%)">
        <inputExpression id="InputExpression_2" typeRef="integer">
          <text>mediaSetor</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Volume Solicitacoes">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>volumeSolicitacoes</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->

      <!-- REGRA B1: Bloqueio - Taxa 2x acima da media com volume alto -->
      <rule id="Rule_Bloqueio_1">
        <description>Taxa de negacao 2x acima da media do setor com alto volume</description>
        <inputEntry id="InputEntry_B1_1"><text>&gt; mediaSetor * 2</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>&gt; 50</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"Taxa de negacao 2x acima da media do setor com alto volume de solicitacoes. Comportamento abusivo."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Notificar ANS imediatamente (RN 395/2016). Considerar acoes legais e rescisao contratual."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"CONTRATO"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry>
      </rule>

      <!-- REGRA A1: Alerta - Taxa 50% acima da media -->
      <rule id="Rule_Alerta_1">
        <description>Taxa de negacao 50% acima da media do setor</description>
        <inputEntry id="InputEntry_A1_1"><text>&gt; mediaSetor * 1.5</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_2"><text>"Taxa de negacao 50% acima da media do setor. Operadora com comportamento desviante."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_3"><text>"Documentar casos. Preparar notificacao ANS. Avaliar necessidade de mediacao."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_4"><text>"CONTRATO"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <!-- REGRA A2: Alerta - Taxa acima da media com volume significativo -->
      <rule id="Rule_Alerta_2">
        <description>Taxa acima da media com volume significativo</description>
        <inputEntry id="InputEntry_A2_1"><text>&gt; mediaSetor</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>&gt; 30</text></inputEntry>
        <outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A2_2"><text>"Taxa de negacao acima da media do setor com volume significativo de solicitacoes."</text></outputEntry>
        <outputEntry id="OutputEntry_A2_3"><text>"Monitorar operadora mensalmente. Verificar se ha padrao sistematico de negacoes."</text></outputEntry>
        <outputEntry id="OutputEntry_A2_4"><text>"CONTRATO"</text></outputEntry>
        <outputEntry id="OutputEntry_A2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      <!-- REGRA P1: Aprovado - Taxa dentro ou abaixo da media -->
      <rule id="Rule_Aprovado_1">
        <description>Taxa de negacao dentro ou abaixo da media do setor</description>
        <inputEntry id="InputEntry_P1_1"><text>&lt;= mediaSetor</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"Taxa de negacao dentro ou abaixo da media do setor. Operadora com comportamento adequado."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Continuar monitorando taxa de negacao comparada a media do setor."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <!-- REGRA F: Fallback - Requer revisao manual -->
      <rule id="Rule_Fallback_PRIOR_TRACK_004">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Situacao de taxa de negacao vs media do setor requer analise manual."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria avaliar comparativo com media do setor."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_PRIOR_TRACK_004">
      <dmndi:DMNShape id="DMNShape_Decision_PRIOR_TRACK_004" dmnElementRef="Decision_PRIOR_TRACK_004">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
