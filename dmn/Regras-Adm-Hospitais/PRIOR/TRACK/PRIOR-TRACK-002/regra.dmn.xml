<?xml version="1.0" encoding="UTF-8"?>
<!--
  ============================================================================
  PRIOR-TRACK-002 - Historico Negacoes por Operadora
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: PRIOR-TRACK
  Perspectiva: HOSPITAL (Faturamento/Auditoria)

  ============================================================================
  INDICACOES (Quando PROSSEGUIR):
  ============================================================================
  - Taxa de negacao aceitavel
  - Operadora dentro dos parametros
  - Historico favoravel

  ============================================================================
  CONTRAINDICACOES (Quando BLOQUEAR):
  ============================================================================
  - Taxa de negacao critica (>30%)
  - Operadora com historico abusivo
  - Violacao sistematica de direitos

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - RN 424/2017 (Autorizacao Previa)
  - RN 465/2021 (Procedimentos Regulados)
  - RN 395/2016 (Monitoramento de Negativas)
  - Lei 9.656/1998 Art. 12 (Cobertura Obrigatoria)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_PRIOR_TRACK_002"
             name="Historico Negacoes por Operadora"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_PRIOR_TRACK_002" name="Verificar Historico Negacoes por Operadora">
    <decisionTable id="DecisionTable_PRIOR_TRACK_002" hitPolicy="FIRST">

      <!-- ========== INPUTS ========== -->
      <input id="Input_1" label="Taxa Negacao (%)">
        <inputExpression id="InputExpression_1" typeRef="integer">
          <text>taxaNegacao</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Operadora">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>operadora</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Limite Aceitavel (%)">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>limiteAceitavel</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->

      <!-- REGRA B1: Bloqueio - Taxa critica de negacao -->
      <rule id="Rule_Bloqueio_1">
        <description>Taxa de negacao critica (acima de 30%)</description>
        <inputEntry id="InputEntry_B1_1"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"Taxa de negacao critica (acima de 30%). Operadora com historico abusivo."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Notificar ANS (RN 395/2016). Considerar rescisao contratual ou mediacao juridica."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"CONTRATO"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry>
      </rule>

      <!-- REGRA A1: Alerta - Taxa alta de negacao -->
      <rule id="Rule_Alerta_1">
        <description>Taxa de negacao alta (20-30%)</description>
        <inputEntry id="InputEntry_A1_1"><text>[20..30]</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_2"><text>"Taxa de negacao alta (20-30%). Operadora com historico preocupante."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_3"><text>"Monitorar operadora mensalmente. Preparar dossie para eventual notificacao ANS."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_4"><text>"CONTRATO"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <!-- REGRA A2: Alerta - Taxa acima do limite aceitavel -->
      <rule id="Rule_Alerta_2">
        <description>Taxa acima do limite interno aceitavel</description>
        <inputEntry id="InputEntry_A2_1"><text>&gt; limiteAceitavel</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A2_2"><text>"Taxa de negacao acima do limite aceitavel interno do hospital."</text></outputEntry>
        <outputEntry id="OutputEntry_A2_3"><text>"Avaliar qualidade das solicitacoes. Reforcar documentacao e justificativas."</text></outputEntry>
        <outputEntry id="OutputEntry_A2_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_A2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      <!-- REGRA P1: Aprovado - Taxa aceitavel -->
      <rule id="Rule_Aprovado_1">
        <description>Taxa de negacao aceitavel</description>
        <inputEntry id="InputEntry_P1_1"><text>&lt;= limiteAceitavel</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"Taxa de negacao dentro do limite aceitavel. Operadora com historico favoravel."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Continuar monitorando taxa de negacao mensalmente."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <!-- REGRA F: Fallback - Requer revisao manual -->
      <rule id="Rule_Fallback_PRIOR_TRACK_002">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Situacao de historico de negacoes requer analise manual."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria avaliar taxa de negacao da operadora."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_PRIOR_TRACK_002">
      <dmndi:DMNShape id="DMNShape_Decision_PRIOR_TRACK_002" dmnElementRef="Decision_PRIOR_TRACK_002">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
