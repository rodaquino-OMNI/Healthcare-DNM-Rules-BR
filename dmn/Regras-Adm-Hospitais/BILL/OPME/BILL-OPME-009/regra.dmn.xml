<?xml version="1.0" encoding="UTF-8"?>
<!--
  BILL-OPME-009 - Relatorio Implante Preenchido
  Versao: 1.0.0 (2026-02-08) | HOSPITAL (Faturamento)
  
  INDICACOES: Relatorio completo com identificacao OPME, lote, medico responsavel
  CONTRAINDICACOES: Ausencia relatorio, dados incompletos, assinatura medica faltante
  REFERENCIAS: RDC 16/2013 ANVISA, IN 59/2021 ANS, CPC Art. 15
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_BILL_OPME_009"
             name="Relatorio Implante Preenchido"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_OPME_009" name="Relatorio Implante Preenchido">
    <decisionTable id="DecisionTable_BILL_OPME_009" hitPolicy="FIRST">

      <input id="Input_1" label="Tem Relatorio"><inputExpression id="InputExpression_1" typeRef="boolean"><text>temRelatorio</text></inputExpression></input>
      <input id="Input_2" label="Relatorio Completo"><inputExpression id="InputExpression_2" typeRef="boolean"><text>relatorioCompleto</text></inputExpression></input>
      <input id="Input_3" label="Tem Assinatura Medica"><inputExpression id="InputExpression_3" typeRef="boolean"><text>temAssinaturaMedica</text></inputExpression></input>
      <input id="Input_4" label="OPME Implantavel"><inputExpression id="InputExpression_4" typeRef="boolean"><text>opmeImplantavel</text></inputExpression></input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>

      <rule id="Rule_Bloqueio_1">
        <description>Bloqueio: OPME implantavel sem relatorio de implante</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - OPME implantavel sem relatorio de implante. RDC 16/2013 exige documentacao completa de rastreabilidade."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Preencher relatorio de implante com dados do OPME, lote, validade e assinatura do medico cirurgiao."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloqueio_2">
        <description>Bloqueio: Relatorio incompleto</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_2"><text>"BLOQUEADO - Relatorio de implante com dados incompletos (faltam: identificacao OPME, lote, validade ou local implantacao)."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_3"><text>"Completar dados faltantes no relatorio de implante antes de faturar."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloqueio_3">
        <description>Bloqueio: Relatorio sem assinatura medica</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B3_2"><text>"BLOQUEADO - Relatorio de implante sem assinatura do medico responsavel. CPC Art. 15 exige identificacao do profissional."</text></outputEntry>
        <outputEntry id="OutputEntry_B3_3"><text>"Obter assinatura do medico cirurgiao no relatorio de implante com CRM."</text></outputEntry>
        <outputEntry id="OutputEntry_B3_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_1">
        <description>Aprovado: Relatorio completo e assinado</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Relatorio de implante completo e assinado pelo medico responsavel."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrao."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_2">
        <description>Aprovado: OPME nao implantavel</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P2_2"><text>"APROVADO - OPME nao implantavel dispensa relatorio especifico de implante."</text></outputEntry>
        <outputEntry id="OutputEntry_P2_3"><text>"Prosseguir com faturamento conforme procedimento padrao."</text></outputEntry>
        <outputEntry id="OutputEntry_P2_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_BILL_OPME_009">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Situacao requer analise manual por nao corresponder a nenhum criterio automatico."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliacao caso a caso."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_OPME_009">
      <dmndi:DMNShape id="DMNShape_Decision_BILL_OPME_009" dmnElementRef="Decision_BILL_OPME_009">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
