<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-OPME-001 - Rastreabilidade OPME Implantavel
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-OPME
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL-OPME-001" name="Rastreabilidade OPME Implantavel" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL-OPME-001" name="Rastreabilidade OPME Implantavel">
    <decisionTable id="DecisionTable_BILL-OPME-001" hitPolicy="FIRST">

      <input id="Input_1" label="Codigo ANVISA Valido">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>codigoAnvisaValido</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Lote Registrado Prontuario">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>loteRegistradoProntuario</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dias Ate Validade">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>diasAteValidade</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Registro Paciente Completo">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>registroPacienteCompleto</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Bloqueio: OPME sem codigo ANVISA valido</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - OPME implantavel sem codigo ANVISA valido. Conforme RDC ANVISA 185/2001, produtos implantaveis requerem registro sanitario vigente. Cobranca bloqueada ate regularizacao."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CORRIGIR_ANVISA"</text></outputEntry>
<outputEntry id="Rule_Bloquear_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Bloquear_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_2">
        <description>Bloqueio: OPME sem lote no prontuario</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"BLOQUEADO - Lote do OPME nao registrado no prontuario do paciente. RDC ANVISA 16/2013 exige rastreabilidade completa. Registrar lote antes de faturar."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"REGISTRAR_LOTE"</text></outputEntry>
<outputEntry id="Rule_Bloquear_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Bloquear_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_3">
        <description>Bloqueio: OPME com validade expirada</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"BLOQUEADO - OPME com validade expirada na data do procedimento. Produto vencido nao pode ser cobrado. Verificar registro de uso e data do procedimento."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"VERIFICAR_VALIDADE"</text></outputEntry>
<outputEntry id="Rule_Bloquear_3_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Bloquear_3_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_4">
        <description>Bloqueio: Registro paciente incompleto</description>
        <inputEntry id="InputEntry_B4_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B4_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B4_3"><text>&gt;= 0</text></inputEntry>
        <inputEntry id="InputEntry_B4_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B4_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B4_2"><text>"BLOQUEADO - Registro do paciente incompleto para rastreabilidade OPME. Conforme RDC 16/2013, deve haver vinculo completo OPME-paciente-procedimento."</text></outputEntry>
<outputEntry id="OutputEntry_B4_3"><text>"REGISTRAR_LOTE"</text></outputEntry>
<outputEntry id="Rule_Bloquear_4_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Bloquear_4_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Alerta: Validade proxima de expirar (90 dias)</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>[0..90]</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"ALERTA - OPME com validade proxima de expirar (menos de 90 dias). Produto valido mas requer atencao para gestao de estoque e prioridade de uso."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="Rule_Alertar_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alertar_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Aprovado: Rastreabilidade completa e validade adequada</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>&gt; 90</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"APROVADO - OPME implantavel com rastreabilidade completa. Codigo ANVISA valido, lote registrado no prontuario, validade adequada e vinculo paciente confirmado."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_BILL-OPME-001">
        <description>Regra padrao - Dados insuficientes para decisao automatica</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"REVISAR - Dados insuficientes para validacao de rastreabilidade OPME. Verificar: codigo ANVISA, lote, validade e registro no prontuario do paciente."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"REGISTRAR_LOTE"</text></outputEntry>
<outputEntry id="Rule_Fallback_BILL-OPME-001_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_BILL-OPME-001_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL-OPME-001">
      <dmndi:DMNShape id="DMNShape_Decision_BILL-OPME-001" dmnElementRef="Decision_BILL-OPME-001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>