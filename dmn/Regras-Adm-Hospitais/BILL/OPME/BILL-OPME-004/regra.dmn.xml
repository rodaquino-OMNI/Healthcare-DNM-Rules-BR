<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-OPME-004 - Compatibilidade OPME x Procedimento TUSS
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-OPME
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_OPME_004" name="Compatibilidade OPME x Procedimento TUSS" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_OPME_004" name="Compatibilidade OPME x Procedimento TUSS">
    <decisionTable id="DecisionTable_BILL_OPME_004" hitPolicy="FIRST">

      
      <input id="Input_1" label="Codigo TUSS Procedimento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>codigoTUSS</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="OPME Compativel Procedimento">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>opmeCompativelProcedimento</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="OPME Autorizado ANS">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>opmeAutorizadoANS</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Quantidade OPME">
        <inputExpression id="InputExpression_4" typeRef="integer">
          <text>quantidadeOPME</text>
        </inputExpression>
      </input>

      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      

      
      <rule id="Rule_Bloqueio_1">
        <description>Bloqueio: OPME incompativel com procedimento TUSS</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - OPME incompativel com procedimento TUSS informado. A ortese/protese/material especial nao e adequada para o procedimento cirurgico realizado."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Verificar compatibilidade OPME x procedimento na tabela TUSS ANS. Corrigir codigo OPME ou codigo procedimento."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Bloqueio: OPME nao autorizado pela ANS para este procedimento</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_2"><text>"BLOQUEADO - OPME nao consta no Rol ANS para este procedimento especifico. Conforme RN 465/2021, apenas OPME listados na tabela ANS podem ser cobertos."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_3"><text>"Solicitar autorizacao previa da operadora para OPME fora do Rol ANS ou substituir por material autorizado."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_4"><text>"CONTRATO"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloqueio_3">
        <description>Bloqueio: Quantidade de OPME excede limite tecnico</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>&gt; 10</text></inputEntry>
        <outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B3_2"><text>"BLOQUEADO - Quantidade de OPME excede limite tecnico esperado para o procedimento. Possivel erro de lancamento ou fraude."</text></outputEntry>
        <outputEntry id="OutputEntry_B3_3"><text>"Revisar quantidade de OPME utilizada no procedimento. Solicitar justificativa medica se quantidade estiver correta."</text></outputEntry>
        <outputEntry id="OutputEntry_B3_4"><text>"VALOR"</text></outputEntry>
        <outputEntry id="OutputEntry_B3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Alerta: Quantidade de OPME proxima ao limite tecnico</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>[5..10]</text></inputEntry>
        <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Quantidade de OPME proxima ao limite tecnico esperado. Verificar se esta dentro da normalidade para o caso clinico."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_3"><text>"Revisar relatorio cirurgico para confirmar necessidade de quantidade informada de OPME."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_4"><text>"VALOR"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Aprovado: OPME compativel, autorizado e quantidade adequada</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>[1..4]</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"APROVADO - OPME compativel com procedimento TUSS, autorizado pela ANS e quantidade dentro do esperado."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrao."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_BILL_OPME_004">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Situacao requer analise manual por nao corresponder a nenhum criterio automatico."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliacao caso a caso."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_OPME_004">
      <dmndi:DMNShape id="DMNShape_Decision_BILL_OPME_004" dmnElementRef="Decision_BILL_OPME_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>