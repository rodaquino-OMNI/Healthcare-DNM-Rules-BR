<?xml version="1.0" encoding="UTF-8"?>
<!-- BILL-MATERIAL-005 - Material Padronizado vs Nao-Padronizado | V1.0.0 (2026-02-08) -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_MATERIAL_005" name="Material Padronizado vs Nao-Padronizado" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_BILL_MATERIAL_005" name="Material Padronizado vs Nao-Padronizado">
    <decisionTable id="DecisionTable_BILL_MATERIAL_005" hitPolicy="FIRST">
      <input id="Input_1" label="Material Padronizado"><inputExpression id="InputExpression_1" typeRef="boolean"><text>materialPadronizado</text></inputExpression></input>
      <input id="Input_2" label="Tem Justificativa"><inputExpression id="InputExpression_2" typeRef="boolean"><text>temJustificativa</text></inputExpression></input>
      <input id="Input_3" label="Diferenca Preco"><inputExpression id="InputExpression_3" typeRef="double"><text>diferencaPreco</text></inputExpression></input>
      <input id="Input_4" label="Material Padronizado Disponivel"><inputExpression id="InputExpression_4" typeRef="boolean"><text>materialPadronizadoDisponivel</text></inputExpression></input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>
      <rule id="Rule_Bloqueio_1"><description>Bloqueio: Material nao-padronizado com padronizado disponivel</description><inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry><inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry><inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry><inputEntry id="InputEntry_B1_4"><text>true</text></inputEntry><outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry><outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - Uso de material nao-padronizado sem justificativa clinica, havendo material padronizado disponivel."</text></outputEntry><outputEntry id="OutputEntry_B1_3"><text>"Substituir por material padronizado ou apresentar justificativa tecnica."</text></outputEntry><outputEntry id="OutputEntry_B1_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_B1_5"><text>"ALTO"</text></outputEntry></rule>
      <rule id="Rule_Bloqueio_2"><description>Bloqueio: Diferenca preco elevada sem justificativa</description><inputEntry id="InputEntry_B2_1"><text>false</text></inputEntry><inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry><inputEntry id="InputEntry_B2_3"><text>&gt; 50</text></inputEntry><inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry><outputEntry id="OutputEntry_B2_2"><text>"BLOQUEADO - Material nao-padronizado com preco 50%+ acima do padronizado sem justificativa."</text></outputEntry><outputEntry id="OutputEntry_B2_3"><text>"Apresentar justificativa tecnica ou utilizar material padronizado."</text></outputEntry><outputEntry id="OutputEntry_B2_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_B2_5"><text>"CRITICO"</text></outputEntry></rule>
      <rule id="Rule_Alerta_1"><description>Alerta: Material nao-padronizado com diferenca moderada</description><inputEntry id="InputEntry_A1_1"><text>false</text></inputEntry><inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry><inputEntry id="InputEntry_A1_3"><text>[20..50]</text></inputEntry><inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry><outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Material nao-padronizado com diferenca de preco entre 20% e 50%. Revisar necessidade."</text></outputEntry><outputEntry id="OutputEntry_A1_3"><text>"Documentar justificativa clinica e anexar ao prontuario."</text></outputEntry><outputEntry id="OutputEntry_A1_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry></rule>
      <rule id="Rule_Aprovado_1"><description>Aprovado: Material padronizado</description><inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry><inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry><inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry><inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Material padronizado conforme politica de compras do hospital."</text></outputEntry><outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrao."</text></outputEntry><outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_Aprovado_2"><description>Aprovado: Padronizado indisponivel com justificativa</description><inputEntry id="InputEntry_P2_1"><text>false</text></inputEntry><inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry><inputEntry id="InputEntry_P2_3"><text>[0..20]</text></inputEntry><inputEntry id="InputEntry_P2_4"><text>false</text></inputEntry><outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P2_2"><text>"APROVADO - Material padronizado indisponivel. Uso de nao-padronizado justificado."</text></outputEntry><outputEntry id="OutputEntry_P2_3"><text>"Prosseguir. Notificar compras sobre ruptura de estoque."</text></outputEntry><outputEntry id="OutputEntry_P2_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_P2_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_Fallback_BILL_MATERIAL_005"><description>Regra padrao</description><inputEntry id="InputEntry_F_1"><text>-</text></inputEntry><inputEntry id="InputEntry_F_2"><text>-</text></inputEntry><inputEntry id="InputEntry_F_3"><text>-</text></inputEntry><inputEntry id="InputEntry_F_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry><outputEntry id="OutputEntry_F_2"><text>"Situacao requer analise manual por nao corresponder a nenhum criterio automatico."</text></outputEntry><outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliacao caso a caso."</text></outputEntry><outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI><dmndi:DMNDiagram id="DMNDiagram_BILL_MATERIAL_005"><dmndi:DMNShape id="DMNShape_Decision_BILL_MATERIAL_005" dmnElementRef="Decision_BILL_MATERIAL_005"><dc:Bounds height="80" width="180" x="160" y="100"/></dmndi:DMNShape></dmndi:DMNDiagram></dmndi:DMNDI>
</definitions>
