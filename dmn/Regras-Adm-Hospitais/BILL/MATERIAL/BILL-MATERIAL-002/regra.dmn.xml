<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-MATERIAL-002 - Validacao de Material de Alto Custo
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-MATERIAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL-MATERIAL-002" name="Validacao de Material de Alto Custo" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL-MATERIAL-002" name="Validacao Material Alto Custo">
    <decisionTable id="DecisionTable_BILL-MATERIAL-002" hitPolicy="FIRST">

      <input id="Input_1" label="Possui Justificativa Medica">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>possuiJustificativaMedica</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Material Foi Substituido">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>materialFoiSubstituido</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Substituicao Documentada">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>substituicaoDocumentada</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo Tecnico Presente">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoTecnicoPresente</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Exige Laudo Contratual">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>exigeLaudoContratual</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Bloqueio: Material alto custo sem justificativa medica</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - Material de alto custo cobrado sem justificativa medica documentada. Conforme exigencia contratual e RN ANS, materiais MAC requerem justificativa clinica detalhada. Glosa integral prevista."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"OBTER_JUSTIFICATIVA"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_2">
        <description>Bloqueio: Substituicao de material sem documentacao</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"BLOQUEADO - Material substituto utilizado sem documentacao de intercambialidade. Operadora nao aceitara material diferente do autorizado sem registro de equivalencia e justificativa da troca."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"DOCUMENTAR_SUBSTITUICAO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_3">
        <description>Bloqueio: Ausencia de laudo tecnico exigido</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"BLOQUEADO - Contrato exige laudo tecnico para este material e o mesmo nao foi anexado. Sem laudo, a operadora glosara integralmente o item."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"ANEXAR_LAUDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Alerta: Substituicao com justificativa incompleta</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Material substituto documentado porem justificativa pode ser considerada incompleta pela auditoria da operadora. Recomenda-se revisar e complementar documentacao antes do envio."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"DOCUMENTAR_SUBSTITUICAO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Aprovado: Material original com documentacao completa</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Material de alto custo com justificativa medica documentada e laudo tecnico anexado conforme exigencia contratual. Prosseguir com faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Prosseguir_2">
        <description>Aprovado: Material sem exigencia de laudo</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"APROVADO - Material de alto custo com justificativa medica. Contrato nao exige laudo tecnico para este item. Prosseguir com faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Prosseguir_3">
        <description>Aprovado: Substituicao devidamente documentada</description>
        <inputEntry id="InputEntry_P3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"APROVADO - Material substituto com documentacao completa de intercambialidade, justificativa medica e laudo tecnico. Prosseguir com faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_3_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_3_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_BILL-MATERIAL-002">
        <description>Regra padrao - Dados insuficientes para decisao automatica</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"REVISAR - Dados insuficientes para validacao automatica de material de alto custo. Verificar: justificativa medica, registro de substituicao, laudo tecnico e exigencias contratuais."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"OBTER_JUSTIFICATIVA"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_BILL-MATERIAL-002_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_BILL-MATERIAL-002_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL-MATERIAL-002">
      <dmndi:DMNShape id="DMNShape_Decision_BILL-MATERIAL-002" dmnElementRef="Decision_BILL-MATERIAL-002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>