<?xml version="1.0" encoding="UTF-8"?>
<!-- BILL-MATERIAL-004 - Quantidade Material x Procedimento | V1.0.0 (2026-02-08) -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_MATERIAL_004" name="Quantidade Material x Procedimento" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_BILL_MATERIAL_004" name="Quantidade Material x Procedimento">
    <decisionTable id="DecisionTable_BILL_MATERIAL_004" hitPolicy="FIRST">
      <input id="Input_1" label="Quantidade Solicitada"><inputExpression id="InputExpression_1" typeRef="integer"><text>quantidadeSolicitada</text></inputExpression></input>
      <input id="Input_2" label="Quantidade Media"><inputExpression id="InputExpression_2" typeRef="double"><text>quantidadeMedia</text></inputExpression></input>
      <input id="Input_3" label="Desvio Padrao"><inputExpression id="InputExpression_3" typeRef="double"><text>desvioPadrao</text></inputExpression></input>
      <input id="Input_4" label="Tem Justificativa"><inputExpression id="InputExpression_4" typeRef="boolean"><text>temJustificativa</text></inputExpression></input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>
      <rule id="Rule_Bloqueio_1"><description>Bloqueio: Quantidade 3+ desvios sem justificativa</description><inputEntry id="InputEntry_B1_1"><text>&gt; 0</text></inputEntry><inputEntry id="InputEntry_B1_2"><text>&gt; 0</text></inputEntry><inputEntry id="InputEntry_B1_3"><text>&gt; 3</text></inputEntry><inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry><outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry><outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - Quantidade de material excede em 3+ desvios-padrao a media historica sem justificativa tecnica."</text></outputEntry><outputEntry id="OutputEntry_B1_3"><text>"Revisar quantidade solicitada ou apresentar justificativa tecnica documentada."</text></outputEntry><outputEntry id="OutputEntry_B1_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry></rule>
      <rule id="Rule_Alerta_1"><description>Alerta: Quantidade 2-3 desvios acima</description><inputEntry id="InputEntry_A1_1"><text>&gt; 0</text></inputEntry><inputEntry id="InputEntry_A1_2"><text>&gt; 0</text></inputEntry><inputEntry id="InputEntry_A1_3"><text>[2..3]</text></inputEntry><inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry><outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Quantidade de material significativamente acima da media historica (2-3 desvios-padrao)."</text></outputEntry><outputEntry id="OutputEntry_A1_3"><text>"Revisar consumo com equipe cirurgica e documentar particularidade do caso."</text></outputEntry><outputEntry id="OutputEntry_A1_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry></rule>
      <rule id="Rule_Aprovado_1"><description>Aprovado: Quantidade dentro 2 desvios</description><inputEntry id="InputEntry_P1_1"><text>&gt; 0</text></inputEntry><inputEntry id="InputEntry_P1_2"><text>&gt; 0</text></inputEntry><inputEntry id="InputEntry_P1_3"><text>[0..2]</text></inputEntry><inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Quantidade de material dentro da faixa estatistica esperada (ate 2 desvios-padrao)."</text></outputEntry><outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrao."</text></outputEntry><outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_Aprovado_2"><description>Aprovado: Excesso justificado</description><inputEntry id="InputEntry_P2_1"><text>&gt; 0</text></inputEntry><inputEntry id="InputEntry_P2_2"><text>&gt; 0</text></inputEntry><inputEntry id="InputEntry_P2_3"><text>&gt; 2</text></inputEntry><inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry><outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P2_2"><text>"APROVADO - Quantidade acima da media mas com justificativa tecnica documentada."</text></outputEntry><outputEntry id="OutputEntry_P2_3"><text>"Prosseguir com faturamento. Manter justificativa anexa para auditoria."</text></outputEntry><outputEntry id="OutputEntry_P2_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_P2_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_Fallback_BILL_MATERIAL_004"><description>Regra padrao</description><inputEntry id="InputEntry_F_1"><text>-</text></inputEntry><inputEntry id="InputEntry_F_2"><text>-</text></inputEntry><inputEntry id="InputEntry_F_3"><text>-</text></inputEntry><inputEntry id="InputEntry_F_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry><outputEntry id="OutputEntry_F_2"><text>"Situacao requer analise manual por nao corresponder a nenhum criterio automatico."</text></outputEntry><outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliacao caso a caso."</text></outputEntry><outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI><dmndi:DMNDiagram id="DMNDiagram_BILL_MATERIAL_004"><dmndi:DMNShape id="DMNShape_Decision_BILL_MATERIAL_004" dmnElementRef="Decision_BILL_MATERIAL_004"><dc:Bounds height="80" width="180" x="160" y="100"/></dmndi:DMNShape></dmndi:DMNDiagram></dmndi:DMNDI>
</definitions>
