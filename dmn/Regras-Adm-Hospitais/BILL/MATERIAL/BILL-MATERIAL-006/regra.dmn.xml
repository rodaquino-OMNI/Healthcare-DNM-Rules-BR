<?xml version="1.0" encoding="UTF-8"?>
<!-- BILL-MATERIAL-006 - Alerta Consumo Atipico | V1.0.0 (2026-02-08) -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_MATERIAL_006" name="Alerta Consumo Atipico" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_BILL_MATERIAL_006" name="Alerta Consumo Atipico">
    <decisionTable id="DecisionTable_BILL_MATERIAL_006" hitPolicy="FIRST">
      <input id="Input_1" label="Consumo Atual"><inputExpression id="InputExpression_1" typeRef="double"><text>consumoAtual</text></inputExpression></input>
      <input id="Input_2" label="Media Historica"><inputExpression id="InputExpression_2" typeRef="double"><text>mediaHistorica</text></inputExpression></input>
      <input id="Input_3" label="Percentual Variacao"><inputExpression id="InputExpression_3" typeRef="double"><text>percentualVariacao</text></inputExpression></input>
      <input id="Input_4" label="Tem Explicacao"><inputExpression id="InputExpression_4" typeRef="boolean"><text>temExplicacao</text></inputExpression></input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>
      <rule id="Rule_Bloqueio_1"><description>Bloqueio: Variacao extrema sem explicacao</description><inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry><inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry><inputEntry id="InputEntry_B1_3"><text>&gt; 200</text></inputEntry><inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry><outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry><outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - Consumo de material 200%+ acima da media historica sem explicacao. Possivel fraude ou erro."</text></outputEntry><outputEntry id="OutputEntry_B1_3"><text>"Investigar consumo atipico. Revisar lancamentos e estoques."</text></outputEntry><outputEntry id="OutputEntry_B1_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry></rule>
      <rule id="Rule_Alerta_1"><description>Alerta: Variacao elevada</description><inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry><inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry><inputEntry id="InputEntry_A1_3"><text>[100..200]</text></inputEntry><inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry><outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Consumo de material 100-200% acima da media. Verificar se ha sazonalidade ou mudanca de perfil."</text></outputEntry><outputEntry id="OutputEntry_A1_3"><text>"Revisar com gestao de suprimentos e equipe cirurgica."</text></outputEntry><outputEntry id="OutputEntry_A1_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry></rule>
      <rule id="Rule_Alerta_2"><description>Alerta: Variacao moderada sem explicacao</description><inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry><inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry><inputEntry id="InputEntry_A2_3"><text>[50..100]</text></inputEntry><inputEntry id="InputEntry_A2_4"><text>false</text></inputEntry><outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry><outputEntry id="OutputEntry_A2_2"><text>"ALERTA - Consumo 50-100% acima da media sem explicacao documentada."</text></outputEntry><outputEntry id="OutputEntry_A2_3"><text>"Documentar explicacao para consumo acima do padr√£o."</text></outputEntry><outputEntry id="OutputEntry_A2_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_A2_5"><text>"MEDIO"</text></outputEntry></rule>
      <rule id="Rule_Aprovado_1"><description>Aprovado: Consumo dentro do esperado</description><inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry><inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry><inputEntry id="InputEntry_P1_3"><text>[0..50]</text></inputEntry><inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Consumo de material dentro da faixa historica esperada (variacao ate 50%)."</text></outputEntry><outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrao."</text></outputEntry><outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_Aprovado_2"><description>Aprovado: Consumo alto com explicacao</description><inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry><inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry><inputEntry id="InputEntry_P2_3"><text>&gt; 50</text></inputEntry><inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry><outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P2_2"><text>"APROVADO - Consumo acima da media mas com explicacao documentada (ex: cirurgias complexas, emergencias)."</text></outputEntry><outputEntry id="OutputEntry_P2_3"><text>"Prosseguir. Manter documentacao anexa."</text></outputEntry><outputEntry id="OutputEntry_P2_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_P2_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_Fallback_BILL_MATERIAL_006"><description>Regra padrao</description><inputEntry id="InputEntry_F_1"><text>-</text></inputEntry><inputEntry id="InputEntry_F_2"><text>-</text></inputEntry><inputEntry id="InputEntry_F_3"><text>-</text></inputEntry><inputEntry id="InputEntry_F_4"><text>-</text></inputEntry><outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry><outputEntry id="OutputEntry_F_2"><text>"Situacao requer analise manual por nao corresponder a nenhum criterio automatico."</text></outputEntry><outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliacao caso a caso."</text></outputEntry><outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI><dmndi:DMNDiagram id="DMNDiagram_BILL_MATERIAL_006"><dmndi:DMNShape id="DMNShape_Decision_BILL_MATERIAL_006" dmnElementRef="Decision_BILL_MATERIAL_006"><dc:Bounds height="80" width="180" x="160" y="100"/></dmndi:DMNShape></dmndi:DMNDiagram></dmndi:DMNDI>
</definitions>
