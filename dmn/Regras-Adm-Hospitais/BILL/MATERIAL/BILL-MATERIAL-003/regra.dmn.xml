<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-MATERIAL-003 - Validacao de Insumos Cirurgicos
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-MATERIAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL-MATERIAL-003" name="Validacao de Insumos Cirurgicos" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL-MATERIAL-003" name="Validacao Insumos Cirurgicos">
    <decisionTable id="DecisionTable_BILL-MATERIAL-003" hitPolicy="FIRST">

      <input id="Input_1" label="Quantidade Dentro Padrao">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>quantidadeDentroPadrao</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Percentual Excedente">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>percentualExcedente</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Marca Padronizada">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>marcaPadronizada</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Justificativa Marca Presente">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>justificativaMarcaPresente</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Incluso Taxa Sala">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>inclusoTaxaSala</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Bloqueio: Quantidade excede significativamente o padrao</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 50</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - Quantidade de insumos cirurgicos excede em mais de 50% o padrao estabelecido para este procedimento. Auditoria medica exigira justificativa detalhada. Alto risco de glosa parcial ou total."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"AJUSTAR_QUANTIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_2">
        <description>Bloqueio: Insumo ja incluso em taxa de sala</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"BLOQUEADO - Insumo ja esta incluso na taxa de sala cirurgica ou pacote contratado. Cobranca adicional caracteriza duplicidade e sera glosada integralmente."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"REMOVER_DUPLICIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_3">
        <description>Bloqueio: Marca nao padronizada sem justificativa</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"BLOQUEADO - Marca de insumo nao consta na lista padronizada da operadora e nao ha justificativa documentada para uso de marca alternativa. Risco de glosa por material nao autorizado."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"JUSTIFICAR_MARCA"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Alerta: Quantidade no limite superior do padrao</description>
        <inputEntry id="InputEntry_A1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>[20..50]</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Quantidade de insumos excede entre 20% e 50% do padrao. Auditoria pode questionar. Recomenda-se anexar justificativa de intercorrencia ou complexidade do caso."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"AJUSTAR_QUANTIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      <rule id="Rule_Alertar_2">
        <description>Alerta: Marca alternativa com justificativa</description>
        <inputEntry id="InputEntry_A2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"ALERTA - Marca alternativa utilizada com justificativa documentada. Verificar se justificativa atende criterios da operadora antes do envio."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"JUSTIFICAR_MARCA"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Aprovado: Insumos dentro do padrao com marca autorizada</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>[0..20]</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Insumos cirurgicos dentro da quantidade padrao estabelecida para o procedimento, marca padronizada utilizada, sem duplicidade com taxa de sala. Prosseguir com faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Prosseguir_2">
        <description>Aprovado: Quantidade padrao exata</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>0</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"APROVADO - Consumo de insumos conforme protocolo padrao. Marca autorizada. Documentacao completa. Faturamento seguro."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_BILL-MATERIAL-003">
        <description>Regra padrao - Dados insuficientes para decisao automatica</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"REVISAR - Dados insuficientes para validacao automatica de insumos cirurgicos. Verificar: quantidade vs protocolo, marca utilizada, inclusao em taxa de sala e justificativas."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"AJUSTAR_QUANTIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_BILL-MATERIAL-003_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_BILL-MATERIAL-003_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL-MATERIAL-003">
      <dmndi:DMNShape id="DMNShape_Decision_BILL-MATERIAL-003" dmnElementRef="Decision_BILL-MATERIAL-003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>