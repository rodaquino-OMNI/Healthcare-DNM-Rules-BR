<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-MED-001 - Medicamento Alto Custo
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-MED
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL-MED-001" name="Medicamento Alto Custo" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL-MED-001" name="Medicamento Alto Custo">
    <decisionTable id="DecisionTable_BILL-MED-001" hitPolicy="FIRST">

      <input id="Input_1" label="Classificado Alto Custo">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>classificadoAltoCusto</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Protocolo Clinico Status">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>protocoloClinicoStatus</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"APROVADO", "INCOMPLETO", "AUSENTE"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Autorizacao Especifica">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>possuiAutorizacaoEspecifica</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Medicamento No Rol ANS">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>medicamentoNoRolAns</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Bloqueio: Alto custo sem protocolo clinico</description>
        <inputEntry id="InputEntry_B1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - Medicamento de alto custo sem protocolo clinico documentado. Conforme CONITEC/MS, medicamentos especializados requerem PCDT. Elaborar protocolo antes de faturar."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"COMPLETAR_PROTOCOLO"</text></outputEntry>
<outputEntry id="Rule_Bloquear_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Bloquear_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_2">
        <description>Bloqueio: Alto custo sem autorizacao especifica</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"APROVADO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"BLOQUEADO - Medicamento de alto custo sem autorizacao especifica da operadora. Conforme RN ANS 465/2021, MAT/MED de alto custo requer autorizacao previa."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"SOLICITAR_AUTORIZACAO"</text></outputEntry>
<outputEntry id="Rule_Bloquear_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Bloquear_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_3">
        <description>Bloqueio: Fora do rol ANS sem justificativa</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>"APROVADO"</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"BLOQUEADO - Medicamento de alto custo fora do rol ANS sem justificativa tecnica. Lei 14.454/2022 permite cobertura com justificativa. Documentar necessidade tecnica."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"JUSTIFICAR_TECNICO"</text></outputEntry>
<outputEntry id="Rule_Bloquear_3_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Bloquear_3_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Alerta: Protocolo clinico incompleto</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"INCOMPLETO"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Protocolo clinico presente mas incompleto. Risco de questionamento em auditoria. Completar documentacao clinica para maior seguranca no faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"COMPLETAR_PROTOCOLO"</text></outputEntry>
<outputEntry id="Rule_Alertar_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alertar_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Aprovado: Alto custo com cadeia completa</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"APROVADO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Medicamento de alto custo com documentacao completa. Protocolo clinico aprovado, autorizacao especifica confirmada e medicamento no rol ANS. Prosseguir faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Prosseguir_2">
        <description>Aprovado: Medicamento padrao (nao alto custo)</description>
        <inputEntry id="InputEntry_P2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"APROVADO - Medicamento padrao (nao classificado como alto custo) presente no rol ANS. Dispensa protocolo e autorizacao especifica. Prosseguir com faturamento normal."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"FATURAR"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_BILL-MED-001">
        <description>Regra padrao - Dados insuficientes para decisao automatica</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"REVISAR - Dados insuficientes para validacao de medicamento alto custo. Verificar: classificacao, protocolo clinico, autorizacao e status no rol ANS."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"COMPLETAR_PROTOCOLO"</text></outputEntry>
<outputEntry id="Rule_Fallback_BILL-MED-001_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_BILL-MED-001_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL-MED-001">
      <dmndi:DMNShape id="DMNShape_Decision_BILL-MED-001" dmnElementRef="Decision_BILL-MED-001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>