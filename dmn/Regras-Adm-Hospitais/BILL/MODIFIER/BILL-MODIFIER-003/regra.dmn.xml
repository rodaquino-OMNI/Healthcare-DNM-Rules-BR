<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-MODIFIER-003 - Bilateral Modifier Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-MODIFIER
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_MODIFIER_003" name="Bilateral Modifier Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_MODIFIER_003" name="Validar Modificador Bilateral">
    <decisionTable id="DecisionTable_BILL_MODIFIER_003" hitPolicy="FIRST">

      <input id="Input_1" label="Procedimento Bilateral">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>procedimentoBilateral</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Orgao Pareado">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>orgaoPareado</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Documentacao Ambos Lados">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>documentacaoAmbosLados</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Contrato Paga Bilateral">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>contratoPagaBilateral</text>
        </inputExpression>
        <inputValues><text>"200%", "150%", "130%", "100%"</text></inputValues>
      </input>

      <input id="Input_5" label="Modificador Bilateral Aplicado">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>modificadorBilateralAplicado</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear bilateral sem documentacao dos lados</description>
        <inputEntry id="InputEntry_B1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Modificador bilateral aplicado sem documentacao de ambos os lados."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Documentar procedimento em cada lado ou remover modificador bilateral."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear bilateral em orgao nao pareado</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Modificador bilateral aplicado em orgao nao pareado. Glosa certa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Remover modificador bilateral. Cobrar procedimento simples."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar bilateral sem modificador - perda receita</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>"200%", "150%"</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Procedimento bilateral documentado sem modificador. Contrato paga adicional."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Aplicar modificador bilateral para capturar receita adicional."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir bilateral completo</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>"200%", "150%", "130%"</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Bilateral com documentacao completa e modificador. Cobranca maximizada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com adicional bilateral conforme contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir contrato sem adicional bilateral</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>"100%"</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Contrato nao paga adicional bilateral. Cobrar como dois procedimentos se permitido."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Verificar se quantidade 2 e mais vantajoso que modificador."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir procedimento unilateral</description>
        <inputEntry id="InputEntry_P3_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Procedimento unilateral. Modificador bilateral nao aplicavel."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar procedimento simples."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar modificador bilateral."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar lateralidade, documentacao e contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_MODIFIER_003">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_MODIFIER_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>