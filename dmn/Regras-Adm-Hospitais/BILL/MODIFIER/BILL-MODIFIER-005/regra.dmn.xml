<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-MODIFIER-005 - Repeat Procedure Modifier Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-MODIFIER
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_MODIFIER_005" name="Repeat Procedure Modifier Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_MODIFIER_005" name="Validar Modificador Procedimento Repetido">
    <decisionTable id="DecisionTable_BILL_MODIFIER_005" hitPolicy="FIRST">

      <input id="Input_1" label="Procedimento Repetido">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>procedimentoRepetido</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Motivo Repeticao">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>motivoRepeticao</text>
        </inputExpression>
        <inputValues><text>"complicacao", "progressao_doenca", "rotina", "falha_tecnica", "qualidade_insuficiente"</text></inputValues>
      </input>

      <input id="Input_3" label="Dias Desde Procedimento Anterior">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>diasDesdeAnterior</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Justificativa Documentada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>justificativaDocumentada</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Mesmo Profissional">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>mesmoProfissional</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear repeticao por falha tecnica sem justificativa</description>
        <inputEntry id="InputEntry_B1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"falha_tecnica", "qualidade_insuficiente"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>&lt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Repeticao por falha sem justificativa. Operadora nao pagara."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Documentar justificativa detalhada ou absorver custo como garantia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear repeticao muito proxima sem razao clinica</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"rotina"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>&lt; 7</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Repeticao de rotina em menos de 7 dias. Sem cobertura."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Aguardar intervalo minimo ou documentar urgencia clinica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar repeticao por complicacao - documentar bem</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"complicacao"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&lt; 30</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Repeticao por complicacao com mesmo profissional. Auditor pode questionar."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Documentar detalhadamente que complicacao nao foi por falha tecnica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir progressao doenca documentada</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"progressao_doenca"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Repeticao por progressao de doenca bem documentada. Cobranca integral."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar normalmente. Anexar evolucao da doenca."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir complicacao com profissional diferente</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"complicacao"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Repeticao por complicacao com profissional diferente. Menos risco de glosa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar normalmente com documentacao da complicacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir repeticao rotina com intervalo adequado</description>
        <inputEntry id="InputEntry_P3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>"rotina"</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>&gt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Intervalo adequado para repeticao de rotina. Cobranca normal."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar conforme tabela."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_4">
        <description>Prosseguir procedimento nao repetido</description>
        <inputEntry id="InputEntry_P4_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P4_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P4_2"><text>"Primeiro procedimento ou sem historico recente. Sem restricao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P4_3"><text>"Faturar normalmente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_4_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_4_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar repeticao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar historico, motivo e documentacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_MODIFIER_005">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_MODIFIER_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>