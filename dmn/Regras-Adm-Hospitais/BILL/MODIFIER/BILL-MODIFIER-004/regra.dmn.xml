<!--
  ============================================================================
  BILL-MODIFIER-004 - Professional Technical Component Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-MODIFIER
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_MODIFIER_004" name="Professional Technical Component Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_MODIFIER_004" name="Validar Componente Profissional Tecnico">
    <decisionTable id="DecisionTable_BILL_MODIFIER_004" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Componente Cobrado">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoComponenteCobrado</text>
        </inputExpression>
        <inputValues><text>"global", "tecnico", "profissional"</text></inputValues>
      </input>

      <input id="Input_2" label="Medico Vinculo Hospital">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>medicoVinculoHospital</text>
        </inputExpression>
        <inputValues><text>"empregado", "cooperado", "credenciado", "autonomo"</text></inputValues>
      </input>

      <input id="Input_3" label="Equipamento Hospital">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>equipamentoHospital</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Laudo Emitido Hospital">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>laudoEmitidoHospital</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Contrato Permite Global">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>contratoPermiteGlobal</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear global sem permissao contratual</description>
        <inputEntry id="InputEntry_B1_1"><text>"global"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Cobranca global sem permissao contratual. Operadora pagara apenas tecnico."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Separar em tecnico (hospital) e profissional (medico) ou renegociar contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear tecnico sem equipamento proprio</description>
        <inputEntry id="InputEntry_B2_1"><text>"tecnico"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Cobranca de componente tecnico sem equipamento hospitalar."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Verificar propriedade do equipamento ou ajustar componente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar profissional separado quando medico empregado</description>
        <inputEntry id="InputEntry_A1_1"><text>"profissional"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"empregado"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Medico empregado com profissional separado. Contrato permite global."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Avaliar se global e mais vantajoso para o hospital."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir global com medico empregado</description>
        <inputEntry id="InputEntry_P1_1"><text>"global"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"empregado"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Cobranca global correta: medico empregado, equipamento e laudo hospitalares."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar valor global. Toda receita fica com hospital."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir tecnico com equipamento hospital</description>
        <inputEntry id="InputEntry_P2_1"><text>"tecnico"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"credenciado", "autonomo"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Componente tecnico correto: medico externo usa equipamento hospital."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar tecnico. Medico fatura profissional separado."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir apenas profissional</description>
        <inputEntry id="InputEntry_P3_1"><text>"profissional"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Apenas laudo/interpretacao. Componente profissional adequado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar profissional. Tecnico com terceiro."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar componente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar vinculo medico, equipamento e contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_MODIFIER_004">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_MODIFIER_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>