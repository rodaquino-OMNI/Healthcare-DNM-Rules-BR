<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-TIME-001 - Anesthesia Time Documentation Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-TIME
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_TIME_001" name="Anesthesia Time Documentation Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_TIME_001" name="Validar Documentacao Tempo Anestesia">
    <decisionTable id="DecisionTable_BILL_TIME_001" hitPolicy="FIRST">

      <input id="Input_1" label="Hora Inicio Documentada">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>horaInicioDocumentada</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Hora Fim Documentada">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>horaFimDocumentada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Duracao Calculada Minutos">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>duracaoCalculadaMinutos</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Duracao Cobrada Minutos">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>duracaoCobradaMinutos</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Ficha Anestesica Assinada">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>fichaAnestesicaAssinada</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear sem hora inicio ou fim</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"BLOQUEIO: Hora de inicio da anestesia nao documentada conforme CFM 1.802/2006. Impossivel calcular tempo anestesico para faturamento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Completar ficha anestesica com hora de inicio ANTES de enviar ao faturamento. Solicitar ao anestesista preenchimento retroativo se necessario."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      <rule id="Rule_Block_2">
        <description>Bloquear sem hora fim</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Hora de termino da anestesia nao documentada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Completar ficha anestesica com hora de termino."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear tempo cobrado maior que documentado</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>&lt; 60</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>&gt; 90</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_2"><text>"Tempo cobrado excede significativamente o tempo documentado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_3"><text>"Ajustar tempo cobrado para corresponder ao documentado."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_3_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_3_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_4">
        <description>Bloquear sem assinatura do anestesista</description>
        <inputEntry id="InputEntry_B4_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B4_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B4_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B4_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B4_2"><text>"BLOQUEIO: Ficha anestesica sem assinatura do anestesista responsavel. Documento invalido conforme CFM 1.802/2006 Art. 5."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B4_3"><text>"Obter assinatura do anestesista responsavel (CRM + carimbo). Verificar se anestesista esta ativo no CRM antes de enviar."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_4_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_4_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar possivel subcobranca de tempo</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&gt; 180</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>&lt; 150</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"ALERTA RECEITA: Tempo cobrado ({{duracaoCobradaMinutos}}min) menor que documentado ({{duracaoCalculadaMinutos}}min). Perda potencial de faturamento de porte anestesico."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Revisar calculo de tempo anestesico. Ajustar para tempo real documentado na ficha para maximizar faturamento legitimo conforme CBHPM."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir tempos compativeis</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>&gt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>&gt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Documentacao de tempo completa e compativel com cobranca."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com ficha anestesica anexa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar documentacao de tempo."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar ficha anestesica completa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_TIME_001">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_TIME_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>