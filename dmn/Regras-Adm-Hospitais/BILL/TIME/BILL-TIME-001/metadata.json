{
  "ruleId": "BILL-TIME-001",
  "version": "1.0.0",
  "name": "Anesthesia Time Documentation Validation",
  "category": "BILL",
  "subcategory": "TIME",
  "perspective": "HOSPITAL",
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "status": "active",
  "description": "Valida documentacao de tempo de anestesia para cobranca correta",
  "businessRationale": "Evitar glosas por falta de documentacao de tempo e capturar tempo integral",
  "riskLevel": "HIGH",
  "glosaRisk": "Tempo anestesico sem documentacao de inicio/fim",
  "inputs": [
    {"name": "horaInicioDocumentada", "type": "boolean"},
    {"name": "horaFimDocumentada", "type": "boolean"},
    {"name": "duracaoCalculadaMinutos", "type": "number"},
    {"name": "duracaoCobradaMinutos", "type": "number"},
    {"name": "fichaAnestesicaAssinada", "type": "boolean"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
    {"name": "observacao", "type": "string"},
    {"name": "acaoRecomendada", "type": "string"}
  ],
  "references": ["SBA Guidelines", "CFM Anesthesia Resolution", "TUSS Time Billing"]
}
