<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-TIME-005 - Emergency Overtime Billing Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-TIME
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_TIME_005" name="Emergency Overtime Billing Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_TIME_005" name="Validar Cobranca Adicional Noturno Emergencia">
    <decisionTable id="DecisionTable_BILL_TIME_005" hitPolicy="FIRST">

      <input id="Input_1" label="Hora Inicio Procedimento">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>horaInicioProcedimento</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Dia Semana">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>diaSemana</text>
        </inputExpression>
        <inputValues><text>"segunda", "terca", "quarta", "quinta", "sexta", "sabado", "domingo", "feriado"</text></inputValues>
      </input>

      <input id="Input_3" label="Adicional Noturno Cobrado">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>adicionalNoturnoCobrado</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Adicional Fim Semana Cobrado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>adicionalFimSemanaCobrado</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Contrato Permite Adicionais">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>contratoPermiteAdicionais</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear noturno cobrado em horario comercial</description>
        <inputEntry id="InputEntry_B1_1"><text>[8..18]</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"segunda", "terca", "quarta", "quinta", "sexta"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Adicional noturno cobrado para procedimento em horario comercial."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Remover adicional noturno. Horario 8-18h dias uteis."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear fim de semana cobrado em dia util</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"segunda", "terca", "quarta", "quinta", "sexta"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Adicional de fim de semana cobrado em dia util."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Remover adicional de fim de semana. Verificar data."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear adicionais sem permissao contratual</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_2"><text>"Adicional cobrado mas contrato nao permite. Sera glosado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_3"><text>"Remover adicional ou renegociar contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_3_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_3_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar noturno nao cobrado quando elegivel</description>
        <inputEntry id="InputEntry_A1_1"><text>&lt; 6, &gt; 22</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Procedimento noturno sem adicional cobrado. Perda de receita."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Aplicar adicional noturno conforme contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar fim de semana nao cobrado quando elegivel</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"sabado", "domingo", "feriado"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Procedimento fim de semana/feriado sem adicional. Perda de receita."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Aplicar adicional de fim de semana conforme contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir noturno corretamente aplicado</description>
        <inputEntry id="InputEntry_P1_1"><text>&lt; 6, &gt; 22</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"segunda", "terca", "quarta", "quinta", "sexta"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Adicional noturno corretamente aplicado. Horario elegivel."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com adicional noturno."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir fim de semana corretamente aplicado</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"sabado", "domingo", "feriado"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Adicional fim de semana/feriado corretamente aplicado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar com adicional."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir horario comercial sem adicional</description>
        <inputEntry id="InputEntry_P3_1"><text>[8..18]</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>"segunda", "terca", "quarta", "quinta", "sexta"</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Horario comercial em dia util. Sem adicional aplicavel."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar valor padrao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar adicionais."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar horario, dia e clausulas contratuais."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_TIME_005">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_TIME_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>