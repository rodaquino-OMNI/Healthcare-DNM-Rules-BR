<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-TIME-003 - Surgical Time Complexity Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-TIME
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_TIME_003" name="Surgical Time Complexity Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_TIME_003" name="Validar Tempo Cirurgico vs Complexidade">
    <decisionTable id="DecisionTable_BILL_TIME_003" hitPolicy="FIRST">

      <input id="Input_1" label="Porte Cirurgico">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>porteCirurgico</text>
        </inputExpression>
        <inputValues><text>"1", "2", "3", "4", "5", "6", "7", "8"</text></inputValues>
      </input>

      <input id="Input_2" label="Tempo Cirurgico Minutos">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>tempoCirurgicoMinutos</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tempo Esperado Minutos">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>tempoEsperadoMinutos</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Justificativa Desvio">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>justificativaDesvio</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear porte alto com tempo muito curto</description>
        <inputEntry id="InputEntry_B1_1"><text>"6", "7", "8"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&lt; 60</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>&gt; 180</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Porte 6-8 com tempo &lt; 60min. Incompativel sem justificativa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Revisar porte cirurgico ou documentar razao do tempo reduzido."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear porte baixo com tempo excessivo</description>
        <inputEntry id="InputEntry_B2_1"><text>"1", "2"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 240</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>&lt; 60</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Porte 1-2 com tempo &gt; 4h. Considerar upgrade de porte ou justificar."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Avaliar se porte deve ser maior ou documentar complicacoes."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar tempo muito maior que esperado</description>
        <inputEntry id="InputEntry_A1_1"><text>"3", "4", "5"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt; 300</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&lt; 150</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Tempo cirurgico 2x maior que esperado. Possivel upgrade de porte."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Avaliar se complicacoes justificam porte maior."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar tempo abaixo do esperado</description>
        <inputEntry id="InputEntry_A2_1"><text>"4", "5", "6"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>&lt; 60</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>&gt;= 120</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Tempo muito abaixo do esperado para o porte. Risco de questionamento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Documentar razao do tempo reduzido ou revisar porte."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir tempo dentro do esperado</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&gt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>&gt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Tempo cirurgico compativel com porte do procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com descricao cirurgica e tempo documentados."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir desvio com justificativa</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Desvio de tempo justificado na documentacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar com justificativa anexa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar tempo vs complexidade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar porte, tempo real e tempo esperado."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_TIME_003">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_TIME_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>