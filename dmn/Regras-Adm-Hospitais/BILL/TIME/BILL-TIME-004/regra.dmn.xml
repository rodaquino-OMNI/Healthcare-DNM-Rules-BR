<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-TIME-004 - Recovery Room Time Billing Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-TIME
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_TIME_004" name="Recovery Room Time Billing Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_TIME_004" name="Validar Cobranca Tempo SRPA">
    <decisionTable id="DecisionTable_BILL_TIME_004" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Anestesia">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoAnestesia</text>
        </inputExpression>
        <inputValues><text>"geral", "raqui", "peridural", "bloqueio", "sedacao", "local"</text></inputValues>
      </input>

      <input id="Input_2" label="Tempo SRPA Minutos">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>tempoSRPAMinutos</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Aldrete Score Alta">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>aldreteScoreAlta</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Complicacoes SRPA">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>complicacoesSRPA</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Cobranca SRPA Separada">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>cobrancaSRPASeparada</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear SRPA prolongada sem complicacao documentada</description>
        <inputEntry id="InputEntry_B1_1"><text>"geral", "raqui"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 180</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>&gt;= 9</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"SRPA &gt; 3h com Aldrete alto e sem complicacoes. Tempo incompativel."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Documentar razao da permanencia prolongada ou ajustar tempo."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear cobranca SRPA para anestesia local</description>
        <inputEntry id="InputEntry_B2_1"><text>"local"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Cobranca SRPA para anestesia local normalmente nao e coberta."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Remover cobranca SRPA ou documentar intercorrencia que justifique."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar anestesia geral sem cobranca SRPA</description>
        <inputEntry id="InputEntry_A1_1"><text>"geral"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt; 60</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Anestesia geral com tempo SRPA significativo sem cobranca separada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Verificar se contrato permite cobranca SRPA separada."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir SRPA prolongada com complicacoes</description>
        <inputEntry id="InputEntry_P1_1"><text>"geral", "raqui", "peridural"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&gt; 120</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>&lt; 9</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"SRPA prolongada justificada por complicacoes documentadas."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com documentacao de intercorrencias."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir SRPA tempo padrao</description>
        <inputEntry id="InputEntry_P2_1"><text>"geral", "raqui", "peridural"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>[30..120]</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>&gt;= 9</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Tempo SRPA dentro do esperado. Aldrete satisfatorio."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar SRPA conforme tempo."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir sedacao SRPA minima</description>
        <inputEntry id="InputEntry_P3_1"><text>"sedacao", "bloqueio"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>[15..60]</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>&gt;= 9</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Tempo SRPA adequado para sedacao/bloqueio."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar se contrato permitir."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar cobranca SRPA."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar tipo anestesia, tempo, Aldrete e contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_TIME_004">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_TIME_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>