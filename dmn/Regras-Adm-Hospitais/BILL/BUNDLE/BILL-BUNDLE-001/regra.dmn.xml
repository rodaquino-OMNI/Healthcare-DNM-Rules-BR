<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-BUNDLE-001 - Validacao de Cobranca Agrupada - Mastectomia Radical
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-BUNDLE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL-BUNDLE-001" name="Validacao de Cobranca Agrupada - Mastectomia Radical" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL-BUNDLE-001" name="Validacao Cobranca Agrupada Mastectomia">
    <decisionTable id="DecisionTable_BILL-BUNDLE-001" hitPolicy="FIRST">

      <input id="Input_1" label="Codigo TUSS Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>codigoTUSSprincipal</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"30602149"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Codigo TUSS Secundario">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>codigoTUSSsecundario</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"30602130", ""</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Contrato Permite Separar">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>contratoPermiteSeparar</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Mesmo Ato Anestesico">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>mesmoAtoAnestesico</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Bloqueio: Cobranca separada de linfadenectomia axilar inclusa na mastectomia radical</description>
        <inputEntry id="InputEntry_B1_1"><text>"30602149"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"30602130"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - Linfadenectomia axilar (30602130) esta INCLUSA no procedimento de mastectomia radical (30602149). Cobranca separada resultara em glosa. Contrato nao permite separacao."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"COBRAR_AGRUPADO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"VALOR"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloquear_2">
        <description>Bloqueio: Contrato indefinido com tentativa de cobranca separada</description>
        <inputEntry id="InputEntry_B2_1"><text>"30602149"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"30602130"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>null</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"BLOQUEADO - Status do contrato para cobranca separada nao definido. Regra padrao TUSS: linfadenectomia inclusa na mastectomia radical."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"VERIFICAR_CONTRATO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"VALOR"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Alerta: Contrato permite separacao mas atos anestesicos diferentes</description>
        <inputEntry id="InputEntry_A1_1"><text>"30602149"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"30602130"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Contrato permite cobranca separada. Atos anestesicos diferentes identificados. Verificar documentacao de tempos cirurgicos distintos."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"COBRAR_SEPARADO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"VALOR"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Aprovado: Cobranca agrupada correta - apenas mastectomia radical</description>
        <inputEntry id="InputEntry_P1_1"><text>"30602149"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>""</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Cobranca correta. Mastectomia radical (30602149) inclui linfadenectomia axilar. Sem tentativa de cobranca separada."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>""</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Prosseguir_2">
        <description>Aprovado: Contrato permite cobranca separada explicitamente</description>
        <inputEntry id="InputEntry_P2_1"><text>"30602149"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"30602130"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"APROVADO - Contrato permite cobranca separada de linfadenectomia axilar. Prosseguir com cobranca separada."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"COBRAR_SEPARADO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_BILL-BUNDLE-001">
        <description>Regra padrao - Dados insuficientes para decisao automatica</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"REVISAR - Dados insuficientes. Verificar: codigo TUSS, clausulas contratuais, registro de ato anestesico."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"VERIFICAR_CONTRATO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_BILL-BUNDLE-001_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_BILL-BUNDLE-001_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL-BUNDLE-001">
      <dmndi:DMNShape id="DMNShape_Decision_BILL-BUNDLE-001" dmnElementRef="Decision_BILL-BUNDLE-001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>