<!--
  ============================================================================
  BILL-BUNDLE-004 - Histerectomia Ooforectomia Bundle Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-BUNDLE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_BUNDLE_004" name="Histerectomia Ooforectomia Bundle Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_BUNDLE_004" name="Validar Bundle Histerectomia Ooforectomia">
    <decisionTable id="DecisionTable_BILL_BUNDLE_004" hitPolicy="FIRST">

      <input id="Input_1" label="Codigo TUSS Histerectomia">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>codigoHisterectomia</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Ooforectomia Bilateral">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>ooforectomiaBilateral</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Indicacao Oncologica">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>indicacaoOncologica</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Contrato Permite Separado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>contratoPermiteSeparado</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear cobranca separada sem indicacao oncologica quando contrato nao permite</description>
        <inputEntry id="InputEntry_B1_1"><text>"31306063", "31306055", "31306098"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Ooforectomia profilatica sem indicacao oncologica. Contrato nao permite cobranca separada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Documentar indicacao oncologica ou incluir no valor da histerectomia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar ooforectomia bilateral sem indicacao clara</description>
        <inputEntry id="InputEntry_A1_1"><text>"31306063", "31306055", "31306098"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Ooforectomia bilateral sem indicacao oncologica. Verificar justificativa medica."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Anexar justificativa medica detalhada para evitar glosa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir com indicacao oncologica</description>
        <inputEntry id="InputEntry_P1_1"><text>"31306063", "31306055", "31306098"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Indicacao oncologica documentada. Cobranca separada justificada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar histerectomia + salpingooforectomia com CID oncologico."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir apenas histerectomia</description>
        <inputEntry id="InputEntry_P2_1"><text>"31306063", "31306055", "31306098"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Histerectomia sem ooforectomia. Cobranca simples."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar apenas histerectomia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar bundle histerectomia/ooforectomia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar indicacao, lateralidade e contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_BUNDLE_004">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_BUNDLE_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>