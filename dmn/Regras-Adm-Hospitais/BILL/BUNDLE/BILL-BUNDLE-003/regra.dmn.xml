<!--
  ============================================================================
  BILL-BUNDLE-003 - Cesariana Ligadura Bundle Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-BUNDLE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_BUNDLE_003" name="Cesariana Ligadura Bundle Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_BUNDLE_003" name="Validar Bundle Cesariana Ligadura">
    <decisionTable id="DecisionTable_BILL_BUNDLE_003" hitPolicy="FIRST">

      <input id="Input_1" label="Cesariana Realizada">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>cesarianaRealizada</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Ligadura Realizada">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>ligaduraRealizada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Termo Consentimento Valido">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>termoConsentimentoValido</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Intervalo Legal Atendido">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>intervaloLegalAtendido</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Contrato Permite Cobranca Separada">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>contratoPermiteSeparada</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear ligadura sem termo de consentimento valido</description>
        <inputEntry id="InputEntry_B1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Termo de consentimento ausente ou invalido. Lei 9.263/1996 exige consentimento expresso."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Obter termo de consentimento assinado com antecedencia minima de 60 dias conforme lei."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear ligadura sem intervalo legal atendido</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Intervalo legal de 60 dias nao atendido. Risco de nao cobertura."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Documentar justificativa medica se cesaria de emergencia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear cobranca separada quando contrato nao permite</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_2"><text>"Contrato inclui ligadura na cesariana. Cobranca separada nao permitida."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_3"><text>"Cobrar apenas cesariana. Ligadura inclusa no pacote."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_3_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_3_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir cobranca separada com todos requisitos atendidos</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Todos requisitos legais atendidos. Cobranca separada permitida pelo contrato."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar cesariana (31309178) + ligadura (31303129) separadamente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir quando apenas cesariana realizada</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Cesariana sem ligadura. Cobranca simples."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar apenas cesariana (31309178)."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar bundle cesariana/ligadura."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar termo consentimento, intervalo legal e contrato."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_BUNDLE_003">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_BUNDLE_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>