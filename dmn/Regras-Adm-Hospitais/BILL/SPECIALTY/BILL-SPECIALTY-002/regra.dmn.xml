<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-SPECIALTY-002 - Medicina Nuclear PET-CT
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-SPECIALTY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_SPECIALTY_002" name="Medicina Nuclear PET-CT" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_SPECIALTY_002" name="Validar Medicina Nuclear PET-CT">
    <decisionTable id="DecisionTable_BILL_SPECIALTY_002" hitPolicy="FIRST">

      <input id="Input_1" label="Radiofarmaco Utilizado">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>radiofarmacoUtilizado</text>
        </inputExpression>
        <inputValues><text>"FDG_F18", "colina_F18", "galio_68", "fluciclovina", "psma", "outros"</text></inputValues>
      </input>

      <input id="Input_2" label="Radiofarmaco Aprovado CNEN">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>radiofarmacoAprovadoCNEN</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Indicacao Oncologica">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>indicacaoOncologica</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Justificativa Clinica Documentada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>justificativaClinicaDocumentada</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Laudo Comparativo Disponivel">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>laudoComparativoDisponivel</text>
        </inputExpression>
      </input>

      <input id="Input_6" label="Exame Estadiamento Inicial">
        <inputExpression id="InputExpression_6" typeRef="boolean">
          <text>exameEstadiamentoInicial</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Bloquear sem justificativa oncologica</description>
        <inputEntry id="InputEntry_B1_1"><text>"FDG_F18"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"PET-CT com FDG sem indicacao oncologica e sem justificativa clinica. Cobertura ANS limitada."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Documentar indicacao oncologica ou cardiologica coberta pelo rol ANS."</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear radiofarmaco nao aprovado</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Radiofarmaco nao aprovado pela CNEN ou sem registro ANVISA. Nao faturavel."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Utilizar radiofarmaco com registro vigente ou encaminhar para protocolo de pesquisa."</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear PSMA/galio sem indicacao especifica</description>
        <inputEntry id="InputEntry_B3_1"><text>"psma", "galio_68"</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"PET com PSMA ou Galio-68 requer justificativa especifica para cancer de prostata ou neuroendocrino."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"Documentar indicacao especifica conforme protocolos SBN."</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar laudo comparativo pendente</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Laudo comparativo com exame anterior nao disponivel. Importante para avaliacao de resposta."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Solicitar exames anteriores ou registrar como primeiro estadiamento."</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar indicacao cardiologica com FDG</description>
        <inputEntry id="InputEntry_A2_1"><text>"FDG_F18"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"PET-CT cardiologico com FDG. Verificar indicacao especifica (viabilidade miocardica, endocardite)."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Confirmar indicacao cardiologica coberta: viabilidade miocardica ou endocardite."</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir estadiamento oncologico inicial</description>
        <inputEntry id="InputEntry_P1_1"><text>"FDG_F18"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"PET-CT FDG para estadiamento oncologico inicial. Indicacao primaria coberta."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Faturar PET-CT com codigo TUSS correspondente ao tipo de cancer."</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir acompanhamento oncologico com comparativo</description>
        <inputEntry id="InputEntry_P2_1"><text>"FDG_F18"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"PET-CT para reestadiamento ou avaliacao de resposta com laudo comparativo disponivel."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Faturar PET-CT incluindo comparativo com exame anterior no laudo."</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir tracadores especificos para prostata</description>
        <inputEntry id="InputEntry_P3_1"><text>"psma", "colina_F18"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"PET-CT com PSMA ou Colina para cancer de prostata. Indicacao especifica documentada."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"Faturar com codigo especifico e justificativa de recidiva bioquimica."</text></outputEntry>
<outputEntry id="Rule_Proceed_3_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_3_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar PET-CT."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Verificar radiofarmaco, indicacao oncologica, justificativa e laudo comparativo."</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_SPECIALTY_002">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_SPECIALTY_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>