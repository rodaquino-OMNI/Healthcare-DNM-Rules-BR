<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-SPECIALTY-001 - Medicina Hiperbarica
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-SPECIALTY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_SPECIALTY_001" name="Medicina Hiperbarica" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_SPECIALTY_001" name="Validar Medicina Hiperbarica">
    <decisionTable id="DecisionTable_BILL_SPECIALTY_001" hitPolicy="FIRST">

      <input id="Input_1" label="Indicacao Clinica">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>indicacaoClinica</text>
        </inputExpression>
        <inputValues><text>"ferida_cronica", "pe_diabetico", "osteomielite", "queimadura", "enxerto", "descompressao", "embolia", "intoxicacao_co", "outras"</text></inputValues>
      </input>

      <input id="Input_2" label="Indicacao Prevista ANS">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>indicacaoPrevistaANS</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Numero Sessoes Solicitadas">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>numeroSessoesSolicitadas</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Sessoes Dentro Protocolo">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>sessoesDentroProtocolo</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Equipamento Certificado ANVISA">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>equipamentoCertificadoANVISA</text>
        </inputExpression>
      </input>

      <input id="Input_6" label="Laudo Medico Especialista">
        <inputExpression id="InputExpression_6" typeRef="boolean">
          <text>laudoMedicoEspecialista</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Bloquear indicacao nao prevista na ANS</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Indicacao clinica para oxigenoterapia hiperbarica nao consta no rol ANS. Glosa certa."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Verificar indicacoes cobertas ou orientar paciente para via particular."</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear equipamento nao certificado ANVISA</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Equipamento de oxigenoterapia hiperbarica sem certificacao ANVISA vigente. Nao faturavel."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Regularizar certificacao do equipamento ou encaminhar para servico certificado."</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear sem laudo de especialista</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Laudo de medico especialista em medicina hiperbarica obrigatorio para cobertura."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"Obter laudo do medico hiperbarico com indicacao e protocolo de tratamento."</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar sessoes excedendo protocolo</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&gt; 20</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Numero de sessoes excede protocolo padrao. Necessaria justificativa detalhada."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Documentar evolucao clinica e justificar sessoes adicionais."</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar indicacoes de emergencia</description>
        <inputEntry id="InputEntry_A2_1"><text>"descompressao", "embolia", "intoxicacao_co"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Indicacao de emergencia hiperbarica. Documentar hora de inicio e evolucao."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Garantir registro de urgencia e tempo porta-camara."</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir indicacao cronica padrao</description>
        <inputEntry id="InputEntry_P1_1"><text>"ferida_cronica", "pe_diabetico", "osteomielite"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>&lt;= 20</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Oxigenoterapia hiperbarica com indicacao coberta, sessoes dentro do protocolo e equipamento certificado."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Faturar sessoes conforme protocolo SBMH e contrato."</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir indicacoes de suporte cirurgico</description>
        <inputEntry id="InputEntry_P2_1"><text>"queimadura", "enxerto"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Oxigenoterapia hiperbarica como suporte a cirurgia/enxerto. Indicacao valida."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Faturar associado ao procedimento cirurgico principal."</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar oxigenoterapia hiperbarica."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Verificar indicacao, numero de sessoes, equipamento e laudo medico."</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_SPECIALTY_001">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_SPECIALTY_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>