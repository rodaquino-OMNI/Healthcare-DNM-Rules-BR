<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-UPCODE-003 - Anesthesia Time Based Coding Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-UPCODE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_UPCODE_003" name="Anesthesia Time Based Coding Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_UPCODE_003" name="Validar Cobranca Anestesia por Tempo">
    <decisionTable id="DecisionTable_BILL_UPCODE_003" hitPolicy="FIRST">

      <input id="Input_1" label="Tempo Anestesia Documentado (min)">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>tempoAnestesiaMinutos</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Unidades Anestesia Cobradas">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>unidadesAnestesiaCobradas</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tipo Anestesia">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tipoAnestesia</text>
        </inputExpression>
        <inputValues><text>"geral", "raqui", "peridural", "local", "sedacao"</text></inputValues>
      </input>

      <input id="Input_4" label="Ficha Anestesica Completa">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>fichaAnestesicaCompleta</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="ASA do Paciente">
        <inputExpression id="InputExpression_5" typeRef="number">
          <text>asaPaciente</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear sem ficha anestesica</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Cobranca de anestesia sem ficha anestesica completa. Glosa certa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Anexar ficha anestesica com horarios inicio/fim e sinais vitais."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear unidades acima do tempo documentado</description>
        <inputEntry id="InputEntry_B2_1"><text>&lt; 60</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 4</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Unidades cobradas excedem tempo documentado. Tempo &lt; 60min nao comporta &gt; 4 unidades."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Ajustar unidades para compatibilizar com tempo ou corrigir ficha."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear discrepancia extrema tempo vs unidades</description>
        <inputEntry id="InputEntry_B3_1"><text>&lt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>&gt; 2</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_2"><text>"Tempo anestesico &lt; 30min com mais de 2 unidades. Incompativel."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_3"><text>"Revisar ficha anestesica e quantidade de unidades."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_3_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_3_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar possivel subcobranca - ASA alto com poucas unidades</description>
        <inputEntry id="InputEntry_A1_1"><text>&gt; 180</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&lt; 8</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"geral"</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>&gt;= 4</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Paciente ASA 4-5 com anestesia geral &gt; 3h pode ter adicional de complexidade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Verificar se adicional por ASA elevado esta sendo cobrado."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir tempo e unidades alinhados</description>
        <inputEntry id="InputEntry_P1_1"><text>&gt;= 60</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>[4..12]</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"geral", "raqui", "peridural"</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Tempo anestesico e unidades compativeis. Ficha completa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com ficha anestesica anexa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir procedimento curto com sedacao/local</description>
        <inputEntry id="InputEntry_P2_1"><text>&lt; 60</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>[1..4]</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"local", "sedacao"</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Sedacao/local com tempo e unidades compativeis."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar conforme tabela."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar cobranca de anestesia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar ficha anestesica, tempo, tipo e unidades."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_UPCODE_003">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_UPCODE_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>