<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-UPCODE-002 - Bilateral Unilateral Coding Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-UPCODE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_UPCODE_002" name="Bilateral Unilateral Coding Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_UPCODE_002" name="Validar Codificacao Bilateral Unilateral">
    <decisionTable id="DecisionTable_BILL_UPCODE_002" hitPolicy="FIRST">

      <input id="Input_1" label="Lateralidade Documentada">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>lateralidadeDocumentada</text>
        </inputExpression>
        <inputValues><text>"unilateral_direita", "unilateral_esquerda", "bilateral", "nao_especificada"</text></inputValues>
      </input>

      <input id="Input_2" label="Codigo Cobrado Bilateral">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>codigoCobradoBilateral</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Imagem Confirma Bilateral">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>imagemConfirmaBilateral</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Descricao Cirurgica Detalha Lados">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>descricaoDetalhaLados</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear cobranca bilateral sem documentacao</description>
        <inputEntry id="InputEntry_B1_1"><text>"unilateral_direita", "unilateral_esquerda"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Codigo bilateral cobrado mas documentacao indica procedimento unilateral."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Corrigir para codigo unilateral ou atualizar documentacao se bilateral."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear bilateral sem confirmacao de imagem</description>
        <inputEntry id="InputEntry_B2_1"><text>"bilateral"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Bilateral sem imagem confirmando ambos os lados. Risco de glosa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Anexar imagem pre/pos mostrando ambos os lados tratados."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar possivel subcobranca - bilateral documentado, unilateral cobrado</description>
        <inputEntry id="InputEntry_A1_1"><text>"bilateral"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Documentacao indica bilateral mas cobrando unilateral. Possivel perda de receita."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Revisar para codigo bilateral ou quantidade 2 se aplicavel."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar lateralidade nao especificada</description>
        <inputEntry id="InputEntry_A2_1"><text>"nao_especificada"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Lateralidade nao especificada na documentacao. Risco de auditoria."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Atualizar descricao cirurgica com lateralidade especifica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir bilateral com documentacao completa</description>
        <inputEntry id="InputEntry_P1_1"><text>"bilateral"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Procedimento bilateral com documentacao completa. Cobranca correta."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar codigo bilateral com imagem e descricao anexas."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir unilateral corretamente codificado</description>
        <inputEntry id="InputEntry_P2_1"><text>"unilateral_direita", "unilateral_esquerda"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Procedimento unilateral corretamente codificado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar com lateralidade especificada."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar lateralidade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar documentacao, imagens e codigo cobrado."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_UPCODE_002">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_UPCODE_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>