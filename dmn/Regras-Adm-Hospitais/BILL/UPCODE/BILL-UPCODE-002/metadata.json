{
  "ruleId": "BILL-UPCODE-002",
  "version": "1.0.0",
  "name": "Bilateral Unilateral Coding Validation",
  "category": "BILL",
  "subcategory": "UPCODE",
  "perspective": "HOSPITAL",
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "status": "active",
  "description": "Valida codificacao correta de procedimentos bilaterais vs unilaterais",
  "businessRationale": "Maximizar receita capturando cobranca bilateral quando documentado",
  "riskLevel": "MEDIUM",
  "glosaRisk": "Cobranca bilateral sem documentacao de ambos os lados",
  "inputs": [
    {"name": "lateralidadeDocumentada", "type": "string", "values": ["unilateral_direita", "unilateral_esquerda", "bilateral", "nao_especificada"]},
    {"name": "codigoCobradoBilateral", "type": "boolean"},
    {"name": "imagemConfirmaBilateral", "type": "boolean"},
    {"name": "descricaoDetalhaLados", "type": "boolean"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
    {"name": "observacao", "type": "string"},
    {"name": "acaoRecomendada", "type": "string"}
  ],
  "references": ["TUSS Laterality Guidelines", "ANS Audit Standards"]
}
