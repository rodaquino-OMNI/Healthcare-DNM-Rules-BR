{
  "ruleId": "BILL-UPCODE-001",
  "version": "1.0.0",
  "name": "Complexity Code Documentation Validation",
  "category": "BILL",
  "subcategory": "UPCODE",
  "perspective": "HOSPITAL",
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "status": "active",
  "description": "Valida correspondencia entre codigo de complexidade e documentacao clinica",
  "businessRationale": "Proteger receita evitando downcoding indevido e garantir documentacao para sustentar complexidade",
  "riskLevel": "HIGH",
  "glosaRisk": "Complexidade cobrada sem suporte documental",
  "inputs": [
    {"name": "nivelComplexidadeCobrado", "type": "string", "values": ["baixa", "media", "alta", "muito_alta"]},
    {"name": "tempoCirurgicoMinutos", "type": "number"},
    {"name": "complicacoesIntraoperatorias", "type": "boolean"},
    {"name": "descricaoCirurgicaDetalhada", "type": "boolean"},
    {"name": "asaScorePaciente", "type": "number", "description": "ASA Physical Status Classification (1-5)"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
    {"name": "observacao", "type": "string"},
    {"name": "acaoRecomendada", "type": "string"}
  ],
  "references": ["ASA Physical Status Classification", "TUSS Complexity Guidelines", "ANS Audit Standards"]
}
