<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-UPCODE-001 - Complexity Code Documentation Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-UPCODE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_UPCODE_001" name="Complexity Code Documentation Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_UPCODE_001" name="Validar Codigo Complexidade">
    <decisionTable id="DecisionTable_BILL_UPCODE_001" hitPolicy="FIRST">

      <input id="Input_1" label="Nivel Complexidade Cobrado">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>nivelComplexidadeCobrado</text>
        </inputExpression>
        <inputValues><text>"baixa", "media", "alta", "muito_alta"</text></inputValues>
      </input>

      <input id="Input_2" label="Tempo Cirurgico Minutos">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>tempoCirurgicoMinutos</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Complicacoes Intraoperatorias">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>complicacoesIntraoperatorias</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Descricao Cirurgica Detalhada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>descricaoCirurgicaDetalhada</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="ASA Score Paciente">
        <inputExpression id="InputExpression_5" typeRef="number">
          <text>asaScorePaciente</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear complexidade muito alta sem documentacao suporte</description>
        <inputEntry id="InputEntry_B1_1"><text>"muito_alta"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&lt; 120</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>&lt; 3</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"BLOQUEIO: Complexidade muito alta (CBHPM porte 8+) cobrada sem evidencias. Tempo &lt;120min, sem complicacoes, sem descricao detalhada e ASA &lt;3. Documentar conforme CFM 1.821/2007 Art. 5."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Anexar descricao cirurgica detalhada com justificativa tecnica. Documentar tempo real, intercorrencias e ASA pre-operatorio."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear alta complexidade com cirurgia curta sem complicacoes</description>
        <inputEntry id="InputEntry_B2_1"><text>"alta"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&lt; 60</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>&lt; 3</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"BLOQUEIO: Alta complexidade incompativel com tempo &lt;60min sem intercorrencias. Risco de downcoding pela operadora conforme CBHPM porte anestesico."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Reclassificar para complexidade media ou anexar relatorio medico detalhando fatores de risco/complicacoes que justifiquem complexidade alta."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar possivel subcobranca - ASA alto com complexidade baixa</description>
        <inputEntry id="InputEntry_A1_1"><text>"baixa", "media"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt; 180</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>&gt;= 3</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"ALERTA RECEITA: Possivel subcobranca detectada. ASA &gt;=3, tempo &gt;3h e complicacoes documentadas sugerem complexidade maior que a cobrada. Potencial perda de receita."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Revisar classificacao para upgrade de complexidade. Anexar ASA pre-operatorio, tempo total e relatorio de complicacoes para justificar codigo de maior porte."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir complexidade muito alta com documentacao completa</description>
        <inputEntry id="InputEntry_P1_1"><text>"muito_alta"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&gt;= 180</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>&gt;= 3</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Complexidade muito alta justificada. Tempo, ASA e documentacao suportam."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com codigo de alta complexidade. Manter documentacao anexa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir alta complexidade com complicacoes documentadas</description>
        <inputEntry id="InputEntry_P2_1"><text>"alta"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>&gt;= 90</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Alta complexidade justificada por complicacoes intraoperatorias documentadas."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar com documentacao de complicacoes e tempo cirurgico."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir complexidade padrao</description>
        <inputEntry id="InputEntry_P3_1"><text>"baixa", "media"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>&lt;= 120</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>&lt; 3</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Complexidade adequada para o perfil do procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar conforme codigo selecionado."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar nivel de complexidade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar tempo, ASA, complicacoes e descricao cirurgica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_UPCODE_001">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_UPCODE_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>