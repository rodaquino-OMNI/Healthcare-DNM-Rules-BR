<!--
  ============================================================================
  BILL-UPCODE-004 - ICU Standard Admission Coding Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-UPCODE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_UPCODE_004" name="ICU Standard Admission Coding Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_UPCODE_004" name="Validar Cobranca UTI vs Enfermaria">
    <decisionTable id="DecisionTable_BILL_UPCODE_004" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Leito Cobrado">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoLeitoCobrado</text>
        </inputExpression>
        <inputValues><text>"uti_adulto", "uti_pediatrica", "uti_neonatal", "semi_intensiva", "enfermaria"</text></inputValues>
      </input>

      <input id="Input_2" label="SAPS Score">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>sapsScore</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Ventilacao Mecanica">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>ventilacaoMecanica</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Drogas Vasoativas">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>drogasVasoativas</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Evolucao Medica Diaria">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>evolucaoMedicaDiaria</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear UTI sem criterios clinicos</description>
        <inputEntry id="InputEntry_B1_1"><text>"uti_adulto", "uti_pediatrica"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&lt; 20</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Diaria UTI sem criterios: SAPS baixo, sem VM, sem DVA. Alto risco glosa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Revisar para semi-intensiva ou enfermaria, ou documentar criterio de gravidade."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear UTI sem evolucao diaria</description>
        <inputEntry id="InputEntry_B2_1"><text>"uti_adulto", "uti_pediatrica", "uti_neonatal"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Diarias UTI sem evolucao medica diaria documentada. Obrigatorio."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Completar prontuario com evolucao de cada dia antes de faturar."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar paciente grave em enfermaria</description>
        <inputEntry id="InputEntry_A1_1"><text>"enfermaria"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt;= 40</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"SAPS &gt;= 40 em enfermaria. Verificar se paciente deveria estar em UTI."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Avaliar transferencia para UTI ou documentar motivo permanencia enfermaria."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar semi-intensiva com criterios de UTI</description>
        <inputEntry id="InputEntry_A2_1"><text>"semi_intensiva"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>&gt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Paciente em semi com VM. Pode qualificar para diaria UTI se vaga disponivel."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Avaliar upgrade para UTI ou justificar permanencia em semi."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_2_4"><text>"VALOR"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir UTI com criterios completos</description>
        <inputEntry id="InputEntry_P1_1"><text>"uti_adulto", "uti_pediatrica"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&gt;= 25</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"UTI com criterios clinicos: VM e SAPS adequado. Evolucao presente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar diarias UTI com documentacao de suporte."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir UTI com drogas vasoativas</description>
        <inputEntry id="InputEntry_P2_1"><text>"uti_adulto", "uti_pediatrica"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"UTI justificada por uso de drogas vasoativas. Criterio de gravidade atendido."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar diarias UTI. Documentar drogas na prescricao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir enfermaria padrao</description>
        <inputEntry id="InputEntry_P3_1"><text>"enfermaria"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>&lt; 30</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Diaria enfermaria adequada para perfil do paciente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar diarias enfermaria."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar tipo de diaria."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar SAPS, suporte ventilat√≥rio, DVA e evolucoes."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_UPCODE_004">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_UPCODE_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>