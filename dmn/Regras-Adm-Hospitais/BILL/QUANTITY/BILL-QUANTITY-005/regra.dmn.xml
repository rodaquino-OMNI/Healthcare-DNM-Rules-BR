<!--
  ============================================================================
  BILL-QUANTITY-005 - Multiple Session Billing Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-QUANTITY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_QUANTITY_005" name="Multiple Session Billing Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_QUANTITY_005" name="Validar Cobranca Multiplas Sessoes">
    <decisionTable id="DecisionTable_BILL_QUANTITY_005" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Procedimento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoProcedimento</text>
        </inputExpression>
        <inputValues><text>"radioterapia", "fisioterapia", "dialise", "infusao", "curativo"</text></inputValues>
      </input>

      <input id="Input_2" label="Sessoes Mesmo Dia">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>sessoesMesmoDia</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Intervalo Minimo Respeitado">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>intervaloMinimoRespeitado</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Regioes Anatomicas Diferentes">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>regioesAnatomicasDiferentes</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Prescricao Justifica Multiplas">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>prescricaoJustificaMultiplas</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear multiplas sessoes sem justificativa</description>
        <inputEntry id="InputEntry_B1_1"><text>"fisioterapia"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 2</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Mais de 2 sessoes de fisioterapia no mesmo dia sem justificativa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Limitar a 2 sessoes ou documentar regioes anatomicas diferentes."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear dialise multipla sem intervalo</description>
        <inputEntry id="InputEntry_B2_1"><text>"dialise"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 1</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Multiplas sessoes de dialise no mesmo dia sem intervalo minimo."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Dialise requer intervalo. Documentar situacao especial se aplicavel."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar multiplas infusoes</description>
        <inputEntry id="InputEntry_A1_1"><text>"infusao"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt; 2</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Multiplas infusoes no mesmo dia. Verificar se SADTs diferentes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Confirmar que cada infusao tem codigo TUSS distinto."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir radioterapia multiplas fracoes</description>
        <inputEntry id="InputEntry_P1_1"><text>"radioterapia"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>[1..2]</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Radioterapia com multiplas fracoes em regioes diferentes. Permitido."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar cada fracao com campos de tratamento especificados."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir fisioterapia regioes diferentes</description>
        <inputEntry id="InputEntry_P2_1"><text>"fisioterapia"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>2</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"2 sessoes fisioterapia em regioes anatomicas diferentes. Permitido."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar especificando regiao de cada sessao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir multiplos curativos</description>
        <inputEntry id="InputEntry_P3_1"><text>"curativo"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>&gt;= 1</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Multiplos curativos em lesoes diferentes. Cobranca por lesao permitida."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar cada curativo com localizacao especifica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_4">
        <description>Prosseguir sessao unica</description>
        <inputEntry id="InputEntry_P4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_2"><text>1</text></inputEntry>
        <inputEntry id="InputEntry_P4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P4_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P4_2"><text>"Sessao unica no dia. Sem restricao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P4_3"><text>"Faturar normalmente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_4_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_4_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar multiplas sessoes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar tipo, quantidade, intervalo e justificativa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_QUANTITY_005">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_QUANTITY_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>