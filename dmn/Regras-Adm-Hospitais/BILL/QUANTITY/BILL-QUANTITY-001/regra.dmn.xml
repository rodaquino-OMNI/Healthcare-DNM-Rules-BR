<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-QUANTITY-001 - Quantity Procedure Type Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-QUANTITY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_QUANTITY_001" name="Quantity Procedure Type Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_QUANTITY_001" name="Validar Quantidade por Tipo Procedimento">
    <decisionTable id="DecisionTable_BILL_QUANTITY_001" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Procedimento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoProcedimento</text>
        </inputExpression>
        <inputValues><text>"cirurgia", "consulta", "exame", "terapia", "diaria"</text></inputValues>
      </input>

      <input id="Input_2" label="Quantidade Cobrada">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>quantidadeCobrada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Periodo Dias">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>periodoDias</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Documentacao Quantidade">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>documentacaoQuantidade</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear cirurgia com quantidade maior que 1</description>
        <inputEntry id="InputEntry_B1_1"><text>"cirurgia"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 1</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"BLOQUEIO: Cirurgia com quantidade &gt;1 sem documentacao de bilateralidade/multiplicidade conforme CBHPM Art. 14. Glosa certa se enviado assim."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Ajustar para quantidade 1 OU usar codigo TUSS especifico para bilateral (ex: terminacao -B) OU documentar procedimentos multiplos com descricao operatoria."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear mais de 1 consulta no mesmo dia</description>
        <inputEntry id="InputEntry_B2_1"><text>"consulta"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 1</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>1</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"BLOQUEIO: Mais de uma consulta por dia para mesmo paciente conforme regras ANS. Glosa automatica de consultas excedentes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Manter apenas uma consulta por dia OU documentar especialidades DIFERENTES com CRM distintos para justificar multiplas consultas."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear diarias maior que periodo</description>
        <inputEntry id="InputEntry_B3_1"><text>"diaria"</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>&lt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_2"><text>"Quantidade de diarias maior que dias de internacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_3"><text>"Ajustar quantidade para corresponder ao periodo de internacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_3_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_3_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar terapia com quantidade alta</description>
        <inputEntry id="InputEntry_A1_1"><text>"terapia"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt; 20</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&lt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Alta quantidade de sessoes de terapia. Pode necessitar autorizacao especial."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Verificar se quantidade esta dentro do autorizado pelo plano."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir cirurgia quantidade 1</description>
        <inputEntry id="InputEntry_P1_1"><text>"cirurgia"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>1</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Cirurgia com quantidade padrao. Cobranca correta."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar normalmente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir exame com quantidade documentada</description>
        <inputEntry id="InputEntry_P2_1"><text>"exame"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>&gt;= 1</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Exames com quantidade documentada. Cobranca aprovada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar com laudos anexos."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir diarias corretas</description>
        <inputEntry id="InputEntry_P3_1"><text>"diaria"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>[1..30]</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>&gt;= 1</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Quantidade de diarias dentro do periodo de internacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar diarias conforme internacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar quantidade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar tipo procedimento, quantidade e documentacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_QUANTITY_001">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_QUANTITY_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>