{
  "ruleId": "BILL-QUANTITY-001",
  "version": "1.0.0",
  "name": "Quantity Procedure Type Validation",
  "category": "BILL",
  "subcategory": "QUANTITY",
  "perspective": "HOSPITAL",
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "status": "active",
  "description": "Valida quantidade cobrada conforme tipo de procedimento",
  "businessRationale": "Evitar glosas por quantidade incompativel com tipo de procedimento",
  "riskLevel": "MEDIUM",
  "glosaRisk": "Quantidade superior ao permitido para o tipo",
  "inputs": [
    {"name": "tipoProcedimento", "type": "string", "values": ["cirurgia", "consulta", "exame", "terapia", "diaria"]},
    {"name": "quantidadeCobrada", "type": "number"},
    {"name": "periodoDias", "type": "number"},
    {"name": "documentacaoQuantidade", "type": "boolean"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
    {"name": "observacao", "type": "string"},
    {"name": "acaoRecomendada", "type": "string"}
  ],
  "references": ["TUSS Quantity Rules", "ANS Billing Guidelines"]
}
