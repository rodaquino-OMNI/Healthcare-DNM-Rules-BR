<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-QUANTITY-002 - Material Quantity Standard Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-QUANTITY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_QUANTITY_002" name="Material Quantity Standard Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_QUANTITY_002" name="Validar Quantidade Material vs Padrao">
    <decisionTable id="DecisionTable_BILL_QUANTITY_002" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Material">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoMaterial</text>
        </inputExpression>
        <inputValues><text>"opme", "medicamento", "material_consumo", "gases"</text></inputValues>
      </input>

      <input id="Input_2" label="Quantidade Cobrada">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>quantidadeCobrada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Quantidade Padrao Procedimento">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>quantidadePadrao</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Percentual Desvio">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>percentualDesvio</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Justificativa Excesso">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>justificativaExcesso</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear OPME com excesso extremo sem justificativa</description>
        <inputEntry id="InputEntry_B1_1"><text>"opme"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>&gt; 100</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"OPME com desvio &gt; 100% do padrao sem justificativa. Glosa certa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Documentar motivo do consumo elevado ou ajustar quantidade."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear medicamento com excesso extremo</description>
        <inputEntry id="InputEntry_B2_1"><text>"medicamento"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>&gt; 150</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Medicamento com consumo 150% acima do padrao. Verificar erro de cobranca."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Revisar prescricao e consumo real. Ajustar se necessario."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar OPME com excesso moderado</description>
        <inputEntry id="InputEntry_A1_1"><text>"opme"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>[30..100]</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"OPME com desvio 30-100% do padrao. Preparar justificativa preventiva."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Documentar razao clinica para consumo acima do padrao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar possivel subcobranca - material abaixo do padrao</description>
        <inputEntry id="InputEntry_A2_1"><text>"opme", "material_consumo"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>&lt; -30</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Material cobrado 30% abaixo do padrao. Verificar se houve subcobranca."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Revisar consumo real e ajustar se materiais nao foram cobrados."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir dentro da faixa padrao</description>
        <inputEntry id="InputEntry_P1_1"><text>"opme", "medicamento", "material_consumo"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>[-30..30]</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Consumo de material dentro da faixa padrao (+/- 30%). Aprovado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar materiais normalmente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir excesso com justificativa</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Consumo acima do padrao com justificativa documentada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar com justificativa anexa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir gases</description>
        <inputEntry id="InputEntry_P3_1"><text>"gases"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>&gt; 0</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Gases cobrados por volume. Verificar registro de fluxometro."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar conforme registro de consumo."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar quantidade de material."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar tipo, quantidade e padrao do procedimento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_QUANTITY_002">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_QUANTITY_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>