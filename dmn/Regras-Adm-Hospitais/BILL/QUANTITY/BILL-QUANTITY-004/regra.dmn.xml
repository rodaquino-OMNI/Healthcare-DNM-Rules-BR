<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-QUANTITY-004 - Days Hospitalization Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-QUANTITY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_QUANTITY_004" name="Days Hospitalization Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_QUANTITY_004" name="Validar Dias Internacao">
    <decisionTable id="DecisionTable_BILL_QUANTITY_004" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Internacao">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoInternacao</text>
        </inputExpression>
        <inputValues><text>"cirurgica", "clinica", "obstetrica", "pediatrica", "psiquiatrica"</text></inputValues>
      </input>

      <input id="Input_2" label="Dias Cobrados">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>diasCobrados</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Media Permanencia DRG">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>mediaPermanenciaDRG</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Complicacoes Documentadas">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>complicacoesDocumentadas</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Alta Programada">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>altaProgramada</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear permanencia muito acima da media sem justificativa</description>
        <inputEntry id="InputEntry_B1_1"><text>"cirurgica", "clinica"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>&lt;= 10</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Permanencia 3x acima da media DRG sem complicacoes documentadas."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Documentar justificativa para permanencia prolongada ou revisar dias."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear obstetrica acima do esperado</description>
        <inputEntry id="InputEntry_B2_1"><text>"obstetrica"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 10</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>&lt;= 4</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Internacao obstetrica &gt; 10 dias sem complicacoes. Media e 2-4 dias."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Documentar complicacoes que justifiquem permanencia prolongada."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar permanencia muito curta para procedimento complexo</description>
        <inputEntry id="InputEntry_A1_1"><text>"cirurgica"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&lt; 2</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&gt;= 5</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Permanencia muito abaixo da media DRG. Verificar se hospital day ou ambulatorial."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Confirmar se cobranca como hospital day e mais vantajosa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar aproximando limite sem alta programada</description>
        <inputEntry id="InputEntry_A2_1"><text>"cirurgica", "clinica"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>&gt;= 20</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>&lt;= 10</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Permanencia elevada sem alta programada. Risco de questionamento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Documentar plano terapeutico e previsao de alta."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir permanencia elevada com complicacoes</description>
        <inputEntry id="InputEntry_P1_1"><text>"cirurgica", "clinica"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&gt; 15</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Permanencia prolongada justificada por complicacoes documentadas."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com documentacao de complicacoes. Considerar CID secundario."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir cirurgica padrao</description>
        <inputEntry id="InputEntry_P2_1"><text>"cirurgica"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>[1..15]</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Permanencia cirurgica dentro da faixa esperada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar diarias conforme tipo de acomodacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir obstetrica padrao</description>
        <inputEntry id="InputEntry_P3_1"><text>"obstetrica"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>[2..5]</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Permanencia obstetrica dentro do esperado (2-4 dias)."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar diarias padrao para parto."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_4">
        <description>Prosseguir psiquiatrica</description>
        <inputEntry id="InputEntry_P4_1"><text>"psiquiatrica"</text></inputEntry>
        <inputEntry id="InputEntry_P4_2"><text>[7..30]</text></inputEntry>
        <inputEntry id="InputEntry_P4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P4_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P4_2"><text>"Permanencia psiquiatrica dentro do esperado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P4_3"><text>"Faturar com laudos de evolucao psiquiatrica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_4_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_4_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar dias de internacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar tipo, dias, DRG e documentacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_QUANTITY_004">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_QUANTITY_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>