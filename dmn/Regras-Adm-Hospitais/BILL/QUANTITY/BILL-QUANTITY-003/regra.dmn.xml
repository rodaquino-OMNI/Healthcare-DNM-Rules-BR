<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  BILL-QUANTITY-003 - Session Quantity Limits Validation
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-QUANTITY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_QUANTITY_003" name="Session Quantity Limits Validation" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_QUANTITY_003" name="Validar Limites Quantidade Sessoes">
    <decisionTable id="DecisionTable_BILL_QUANTITY_003" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Terapia">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoTerapia</text>
        </inputExpression>
        <inputValues><text>"fisioterapia", "fonoaudiologia", "terapia_ocupacional", "psicologia", "hemodialise", "quimioterapia"</text></inputValues>
      </input>

      <input id="Input_2" label="Sessoes Cobradas Mes">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>sessoesCobradasMes</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Limite Contratual">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>limiteContratual</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Autorizacao Especial">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>autorizacaoEspecial</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="CID Cronico">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>cidCronico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Block_1">
        <description>Bloquear sessoes acima do limite sem autorizacao</description>
        <inputEntry id="InputEntry_B1_1"><text>"fisioterapia", "fonoaudiologia", "terapia_ocupacional", "psicologia"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 20</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>&lt;= 20</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Sessoes excedem limite contratual de 20/mes sem autorizacao especial."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar autorizacao para sessoes excedentes ou ajustar quantidade."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear hemodialise acima do protocolo</description>
        <inputEntry id="InputEntry_B2_1"><text>"hemodialise"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 15</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Hemodialise acima de 15 sessoes/mes sem autorizacao. Protocolo padrao 12-13."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Justificar sessoes adicionais com indicacao clinica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Block_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar aproximando limite contratual</description>
        <inputEntry id="InputEntry_A1_1"><text>"fisioterapia", "fonoaudiologia", "terapia_ocupacional", "psicologia"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>[18..20]</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>20</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Aproximando do limite de sessoes. Considerar solicitar autorizacao preventiva."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Iniciar processo de autorizacao se paciente necessitar mais sessoes."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alert_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir cronico com autorizacao especial</description>
        <inputEntry id="InputEntry_P1_1"><text>"fisioterapia", "fonoaudiologia", "terapia_ocupacional"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&gt; 20</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_2"><text>"Paciente cronico com autorizacao especial para sessoes ilimitadas."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P1_3"><text>"Faturar com numero da autorizacao especial."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir dentro dos limites</description>
        <inputEntry id="InputEntry_P2_1"><text>"fisioterapia", "fonoaudiologia", "terapia_ocupacional", "psicologia"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>&lt;= 20</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>&gt;= 20</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_2"><text>"Quantidade de sessoes dentro do limite contratual."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P2_3"><text>"Faturar sessoes normalmente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir hemodialise padrao</description>
        <inputEntry id="InputEntry_P3_1"><text>"hemodialise"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>[12..15]</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_2"><text>"Hemodialise dentro do protocolo padrao (3x/semana)."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P3_3"><text>"Faturar sessoes de hemodialise."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Proceed_4">
        <description>Prosseguir quimioterapia conforme protocolo</description>
        <inputEntry id="InputEntry_P4_1"><text>"quimioterapia"</text></inputEntry>
        <inputEntry id="InputEntry_P4_2"><text>&gt; 0</text></inputEntry>
        <inputEntry id="InputEntry_P4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_P4_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_P4_2"><text>"Quimioterapia segue protocolo oncologico. Sem limite de sessoes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_P4_3"><text>"Faturar conforme APAC oncologica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Proceed_4_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Proceed_4_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar limite de sessoes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar tipo terapia, limite contratual e autorizacoes."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_QUANTITY_003">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_QUANTITY_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>