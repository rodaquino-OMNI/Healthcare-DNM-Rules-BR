  "inputs": [
    {"name": "protocoloQuimio", "type": "string", "values": ["conitec", "nccn", "esmo", "institucional", "off_label"]},
    {"name": "medicamentosInclusosConforme", "type": "boolean"},
    {"name": "medicamentoNaoProtocolo", "type": "boolean"},
    {"name": "suporteTerapeuticoAdicional", "type": "boolean"},
    {"name": "autorizacaoMedicamentoExtra", "type": "boolean"},
    {"name": "justificativaClinicaDocumentada", "type": "boolean"}
