<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-BUNDLE-EXT-003 - Pacote Oncologia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-BUNDLE-EXT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_BUNDLE_EXT_003" name="Pacote Oncologia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_BUNDLE_EXT_003" name="Validar Pacote Oncologia">
    <decisionTable id="DecisionTable_BILL_BUNDLE_EXT_003" hitPolicy="FIRST">

      <input id="Input_1" label="Protocolo Quimioterapia">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>protocoloQuimio</text>
        </inputExpression>
        <inputValues><text>"conitec", "nccn", "esmo", "institucional", "off_label"</text></inputValues>
      </input>

      <input id="Input_2" label="Medicamentos Inclusos Conforme">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>medicamentosInclusosConforme</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Medicamento Nao Protocolo">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>medicamentoNaoProtocolo</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Suporte Terapeutico Adicional">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>suporteTerapeuticoAdicional</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Autorizacao Medicamento Extra">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>autorizacaoMedicamentoExtra</text>
        </inputExpression>
      </input>

      <input id="Input_6" label="Justificativa Clinica Documentada">
        <inputExpression id="InputExpression_6" typeRef="boolean">
          <text>justificativaClinicaDocumentada</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Bloquear medicamento nao protocolo sem autorizacao</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Medicamento oncologico fora do protocolo sem autorizacao previa. Alta probabilidade de glosa."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Obter autorizacao via junta medica ou substituir por medicamento do protocolo."</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear off-label sem justificativa documentada</description>
        <inputEntry id="InputEntry_B2_1"><text>"off_label"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Uso off-label sem justificativa clinica documentada. Negativa garantida."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Documentar evidencia cientifica e justificativa clinica detalhada."</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar suporte terapeutico adicional</description>
        <inputEntry id="InputEntry_A1_1"><text>"conitec", "nccn", "esmo"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Suporte terapeutico adicional pode nao estar incluso no pacote. Verificar cobertura."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Documentar necessidade clinica do suporte adicional."</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar protocolo institucional sem respaldo CONITEC</description>
        <inputEntry id="InputEntry_A2_1"><text>"institucional"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Protocolo institucional pode ser questionado por operadora. Ter evidencias cientificas disponiveis."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Preparar justificativa com referencias bibliograficas."</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir protocolo CONITEC conforme</description>
        <inputEntry id="InputEntry_P1_1"><text>"conitec"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Pacote oncologico conforme protocolo CONITEC. Cobranca segura."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Faturar conforme pacote oncologico negociado."</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir com medicamento extra autorizado</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Medicamento adicional autorizado com justificativa documentada."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Faturar pacote mais medicamento autorizado separadamente."</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar pacote oncologico."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Verificar protocolo, medicamentos e suporte terapeutico."</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_BUNDLE_EXT_003">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_BUNDLE_EXT_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>