<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-BUNDLE-EXT-005 - Pacote Day Clinic
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-BUNDLE-EXT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_BUNDLE_EXT_005" name="Pacote Day Clinic" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_BUNDLE_EXT_005" name="Validar Pacote Day Clinic">
    <decisionTable id="DecisionTable_BILL_BUNDLE_EXT_005" hitPolicy="FIRST">

      <input id="Input_1" label="Procedimento Elegivel Day Clinic">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>procedimentoElegivelDayClinic</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tempo Permanencia Horas">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>tempoPermanenciaHoras</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tempo Maximo Contratado">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>tempoMaximoContratado</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Recurso Adicional Sala">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>recursoAdicionalSala</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Conversao Internacao">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>conversaoInternacao</text>
        </inputExpression>
      </input>

      <input id="Input_6" label="Complicacao Pos Procedimento">
        <inputExpression id="InputExpression_6" typeRef="boolean">
          <text>complicacaoPosProcedimento</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Bloquear procedimento nao elegivel para day clinic</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Procedimento nao elegivel para regime de day clinic conforme ANVISA RDC 50."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Converter para internacao hospitalar ou revisar indicacao."</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear tempo excedido sem conversao para internacao</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 12</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Permanencia superior a 12 horas requer conversao para internacao."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Converter cobranca para regime de internacao hospitalar."</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear tempo excedido do contratado</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Tempo de permanencia excede limite contratual do pacote day clinic."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"Solicitar autorizacao de extensao ou ajustar cobranca."</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar recursos adicionais de sala</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&lt;= 12</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Recursos adicionais de sala podem nao estar inclusos no pacote day clinic."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Verificar se recursos extras estao cobertos ou cobrar separadamente."</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar complicacao pos procedimento</description>
        <inputEntry id="InputEntry_A2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Complicacao pos procedimento pode justificar conversao para internacao."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Avaliar necessidade de internacao e documentar complicacao."</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir conversao para internacao</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Conversao para internacao autorizada. Cobrar conforme regime hospitalar."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Faturar como internacao hospitalar com justificativa de conversao."</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir pacote day clinic padrao</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>&lt;= 8</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Pacote day clinic dentro dos parametros. Cobranca conforme."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Faturar pacote day clinic padrao."</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar pacote day clinic."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Verificar elegibilidade, tempo e recursos utilizados."</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_BUNDLE_EXT_005">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_BUNDLE_EXT_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>