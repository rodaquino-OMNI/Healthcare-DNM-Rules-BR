<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-BUNDLE-EXT-002 - Pacote Ortopedia Complexa
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-BUNDLE-EXT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_BUNDLE_EXT_002" name="Pacote Ortopedia Complexa" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_BUNDLE_EXT_002" name="Validar Pacote Ortopedia Complexa">
    <decisionTable id="DecisionTable_BILL_BUNDLE_EXT_002" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Protese">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoProtese</text>
        </inputExpression>
        <inputValues><text>"quadril_total", "quadril_parcial", "joelho_total", "joelho_parcial", "coluna", "ombro"</text></inputValues>
      </input>

      <input id="Input_2" label="Material Auxiliar Previsto">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>materialAuxiliarPrevisto</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Material Nao Previsto">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>materialNaoPrevisto</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Honorario Equipe Conforme">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>honorarioEquipeConforme</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Autorizacao Material Adicional">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>autorizacaoMaterialAdicional</text>
        </inputExpression>
      </input>

      <input id="Input_6" label="Protese Compativel Procedimento">
        <inputExpression id="InputExpression_6" typeRef="boolean">
          <text>proteseCompativelProcedimento</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Bloquear material nao previsto sem autorizacao</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Material auxiliar nao previsto no pacote ortopedico sem autorizacao previa. Glosa certa."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Obter autorizacao retroativa ou absorver custo adicional."</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear protese incompativel com procedimento</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Tipo de protese cobrada incompativel com procedimento realizado. Verificar codigo TUSS correto."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Corrigir tipo de protese ou procedimento na cobranca."</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar honorarios de equipe divergentes</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Honorarios da equipe cirurgica divergem do valor contratado. Risco de glosa parcial."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Verificar tabela contratual de honorarios e ajustar se necessario."</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar coluna com multiplos materiais auxiliares</description>
        <inputEntry id="InputEntry_A2_1"><text>"coluna"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Cirurgia de coluna com multiplos materiais. Documentar necessidade clinica de cada item."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Anexar justificativa clinica e imagens pre e pos-operatorias."</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir pacote completo com extras autorizados</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Pacote ortopedico completo com materiais adicionais autorizados. Cobranca conforme."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Faturar pacote mais materiais autorizados separadamente."</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir pacote ortopedico padrao</description>
        <inputEntry id="InputEntry_P2_1"><text>"quadril_total", "quadril_parcial", "joelho_total", "joelho_parcial"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Cobranca do pacote ortopedico dentro dos parametros contratuais."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Faturar conforme pacote negociado."</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar pacote ortopedico."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Verificar tipo de protese, materiais auxiliares e honorarios."</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_BUNDLE_EXT_002">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_BUNDLE_EXT_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>