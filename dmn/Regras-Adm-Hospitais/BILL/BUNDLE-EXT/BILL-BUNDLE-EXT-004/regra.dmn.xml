<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  BILL-BUNDLE-EXT-004 - Pacote Maternidade
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: BILL-BUNDLE-EXT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_BILL_BUNDLE_EXT_004" name="Pacote Maternidade" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_BILL_BUNDLE_EXT_004" name="Validar Pacote Maternidade">
    <decisionTable id="DecisionTable_BILL_BUNDLE_EXT_004" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Parto">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoParto</text>
        </inputExpression>
        <inputValues><text>"normal", "cesarea_eletiva", "cesarea_urgencia", "forceps", "vacuo"</text></inputValues>
      </input>

      <input id="Input_2" label="Intercorrencias Documentadas">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>intercorrenciasDocumentadas</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Intercorrencia Coberta no Pacote">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>intercorrenciaCobertaPacote</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Dias Internacao">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>diasInternacao</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Dias Excedidos Autorizados">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>diasExcedidosAutorizados</text>
        </inputExpression>
      </input>

      <input id="Input_6" label="UTI Neonatal Necessaria">
        <inputExpression id="InputExpression_6" typeRef="boolean">
          <text>utiNeonatalNecessaria</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Bloquear intercorrencia nao coberta no pacote</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Intercorrencia obstetrica nao prevista no pacote maternidade. Cobranca separada requer autorizacao."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Solicitar autorizacao para intercorrencia ou incluir em aditivo contratual."</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear dias excedidos cesarea sem autorizacao</description>
        <inputEntry id="InputEntry_B2_1"><text>"cesarea_eletiva", "cesarea_urgencia"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>&gt; 3</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Cesarea com mais de 3 dias sem autorizacao de prorrogacao. Diarias excedentes serao glosadas."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Obter autorizacao de prorrogacao com justificativa clinica."</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear dias excedidos parto normal sem autorizacao</description>
        <inputEntry id="InputEntry_B3_1"><text>"normal", "forceps", "vacuo"</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>&gt; 2</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Parto normal com mais de 2 dias sem autorizacao de prorrogacao."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"Justificar necessidade clinica de internacao prolongada."</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar necessidade de UTI neonatal</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"UTI neonatal geralmente nao inclusa no pacote maternidade. Cobrar separadamente."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Verificar contrato e abrir guia separada para UTI neonatal."</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar dias excedidos com autorizacao</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>&gt; 4</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Internacao prolongada autorizada. Documentar evolucao clinica diaria."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Manter registros de evolucao e justificativa para cada dia adicional."</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir pacote cesarea padrao</description>
        <inputEntry id="InputEntry_P1_1"><text>"cesarea_eletiva", "cesarea_urgencia"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>&lt;= 3</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Pacote cesarea dentro dos parametros. Cobranca conforme contrato."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Faturar pacote maternidade cesarea padrao."</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir pacote parto normal padrao</description>
        <inputEntry id="InputEntry_P2_1"><text>"normal", "forceps", "vacuo"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>&lt;= 2</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Pacote parto normal dentro dos parametros. Cobranca conforme."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Faturar pacote maternidade parto normal."</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar pacote maternidade."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Verificar tipo de parto, dias de internacao e intercorrencias."</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_BILL_BUNDLE_EXT_004">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_BILL_BUNDLE_EXT_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>