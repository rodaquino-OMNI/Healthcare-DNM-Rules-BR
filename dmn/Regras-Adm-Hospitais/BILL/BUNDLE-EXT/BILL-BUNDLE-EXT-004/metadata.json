{
  "ruleId": "BILL-BUNDLE-EXT-004",
  "version": "1.0.0",
  "name": "Pacote Maternidade",
  "category": "BILL",
  "subcategory": "BUNDLE-EXT",
  "perspective": "HOSPITAL",
  "tier": "TIER3",
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "status": "active",
  "description": "Valida cobranca de pacotes de maternidade incluindo tipo de parto, intercorrencias e dias de internacao",
  "businessRationale": "Otimizar receita em partos garantindo cobranca correta de pacotes e extras autorizados",
  "riskLevel": "MEDIUM",
  "glosaRisk": "Intercorrencias nao cobertas ou dias excedidos sem autorizacao",
  "inputs": [
    {"name": "tipoParto", "type": "string", "values": ["normal", "cesarea_eletiva", "cesarea_urgencia", "forceps", "vacuo"]},
    {"name": "intercorrenciasDocumentadas", "type": "boolean"},
    {"name": "intercorrenciaCobertaPacote", "type": "boolean"},
    {"name": "diasInternacao", "type": "number"},
    {"name": "diasExcedidosAutorizados", "type": "boolean"},
    {"name": "utiNeonatalNecessaria", "type": "boolean"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
    {"name": "observacao", "type": "string"},
    {"name": "acaoRecomendada", "type": "string"}
  ],
  "references": ["RN ANS 465", "Lei 9656/98", "Parto Humanizado Guidelines", "TISS Obstetricia"]
}
