<?xml version="1.0" encoding="UTF-8"?>
<!--
  ============================================================================
  CRED-LICENSE-003 - Habilitação Específica (Radioterapia, Nuclear)
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: CRED-LICENSE
  Perspectiva: HOSPITAL (Administrativo - Licenças Profissionais)

  ============================================================================
  INDICACOES (Quando PROSSEGUIR):
  ============================================================================
  - Habilitação específica válida para o procedimento
  - Certificado vigente e reconhecido
  - Profissional autorizado pelo órgão competente

  ============================================================================
  CONTRAINDICACOES (Quando BLOQUEAR):
  ============================================================================
  - Sem habilitação específica requerida
  - Certificado vencido ou não reconhecido
  - Procedimento requer habilitação especial não comprovada

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - Resolução CFM específica por área
  - Portaria MS habilitações CNEN (radioterapia, medicina nuclear)
  - RDC ANVISA específicas
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_CRED_LICENSE_003"
             name="Habilitação Específica (Radioterapia, Nuclear)"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_CRED_LICENSE_003" name="Habilitação Específica">
    <decisionTable id="DecisionTable_CRED_LICENSE_003" hitPolicy="FIRST">

      <!-- ========== INPUTS ========== -->
      <input id="Input_1" label="Habilitação Específica">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>habilitacaoEspecifica</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tipo Habilitação">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>tipoHabilitacao</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"RADIOTERAPIA", "NUCLEAR", "HEMODINAMICA"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Certificado Válido">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>certificadoValido</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->

      <!-- REGRA B1: Bloqueio - Sem habilitação específica -->
      <rule id="Rule_Bloqueio_1">
        <description>Profissional sem habilitação específica requerida</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"Profissional não possui habilitação específica requerida para o procedimento."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Obter habilitação específica ou designar profissional habilitado."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"CRED"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry>
      </rule>

      <!-- REGRA B2: Bloqueio - Certificado inválido -->
      <rule id="Rule_Bloqueio_2">
        <description>Certificado vencido ou não reconhecido</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_2"><text>"Certificado de habilitação específica vencido ou não reconhecido."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_3"><text>"Renovar certificado de habilitação junto ao órgão competente."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_4"><text>"CRED"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_5"><text>"CRITICO"</text></outputEntry>
      </rule>

      <!-- REGRA P1: Aprovado - Habilitação válida -->
      <rule id="Rule_Aprovado_1">
        <description>Profissional com habilitação específica válida</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"Profissional possui habilitação específica válida para o procedimento."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrão."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <!-- REGRA F: Fallback - Requer revisão manual -->
      <rule id="Rule_Fallback_CRED_LICENSE_003">
        <description>Regra padrão - Requer avaliação manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Situação requer análise manual por não corresponder a nenhum critério automático."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliação caso a caso."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CRED_LICENSE_003">
      <dmndi:DMNShape id="DMNShape_Decision_CRED_LICENSE_003" dmnElementRef="Decision_CRED_LICENSE_003">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
