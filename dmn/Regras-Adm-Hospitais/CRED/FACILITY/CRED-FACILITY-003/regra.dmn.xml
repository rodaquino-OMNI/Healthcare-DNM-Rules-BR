<?xml version="1.0" encoding="UTF-8"?>
<!--
  ============================================================================
  CRED-FACILITY-003 - Cadastro CNES Serviços Compatíveis
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: CRED-FACILITY
  Perspectiva: HOSPITAL (Administrativo - Cadastro CNES)

  ============================================================================
  INDICACOES (Quando PROSSEGUIR):
  ============================================================================
  - Serviço cadastrado no CNES
  - Procedimento compatível com CNES
  - CNES atualizado nos últimos 12 meses

  ============================================================================
  CONTRAINDICACOES (Quando BLOQUEAR):
  ============================================================================
  - Serviço não cadastrado no CNES
  - Procedimento incompatível com serviços CNES
  - CNES desatualizado há mais de 24 meses

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - Portaria MS/GM 1646/2015 - CNES
  - Cadastro DATASUS
  - RN 412/2016 ANS
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_CRED_FACILITY_003"
             name="Cadastro CNES Serviços Compatíveis"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_CRED_FACILITY_003" name="CNES Serviços Compatíveis">
    <decisionTable id="DecisionTable_CRED_FACILITY_003" hitPolicy="FIRST">

      <!-- ========== INPUTS ========== -->
      <input id="Input_1" label="Serviço CNES">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>servicoCNES</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Procedimento Compatível">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>procedimentoCompativel</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dias Última Atualização">
        <inputExpression id="InputExpression_3" typeRef="integer">
          <text>ultimaAtualizacao</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->

      <!-- REGRA B1: Bloqueio - Serviço não cadastrado -->
      <rule id="Rule_Bloqueio_1">
        <description>Serviço não cadastrado no CNES</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"Serviço não cadastrado no CNES. Hospital não autorizado para executar."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Cadastrar serviço no CNES antes de executar procedimentos."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"CRED"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry>
      </rule>

      <!-- REGRA B2: Bloqueio - Procedimento incompatível -->
      <rule id="Rule_Bloqueio_2">
        <description>Procedimento incompatível com serviços CNES</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_2"><text>"Procedimento não compatível com os serviços cadastrados no CNES."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_3"><text>"Atualizar CNES com serviços compatíveis ou não executar procedimento."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_4"><text>"CRED"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <!-- REGRA A1: Alerta - CNES desatualizado -->
      <rule id="Rule_Alerta_1">
        <description>CNES desatualizado há mais de 1 ano</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&gt; 365</text></inputEntry>
        <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_2"><text>"CNES desatualizado há mais de 1 ano. Risco de inconsistências cadastrais."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_3"><text>"Programar atualização do CNES nos próximos 60 dias."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      <!-- REGRA P1: Aprovado - Serviço compatível e CNES atualizado -->
      <rule id="Rule_Aprovado_1">
        <description>Serviço CNES compatível e atualizado</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>&lt;= 365</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"Serviço cadastrado no CNES e compatível com o procedimento."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrão."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <!-- REGRA F: Fallback - Requer revisão manual -->
      <rule id="Rule_Fallback_CRED_FACILITY_003">
        <description>Regra padrão - Requer avaliação manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Situação requer análise manual por não corresponder a nenhum critério automático."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliação caso a caso."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_CRED_FACILITY_003">
      <dmndi:DMNShape id="DMNShape_Decision_CRED_FACILITY_003" dmnElementRef="Decision_CRED_FACILITY_003">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
