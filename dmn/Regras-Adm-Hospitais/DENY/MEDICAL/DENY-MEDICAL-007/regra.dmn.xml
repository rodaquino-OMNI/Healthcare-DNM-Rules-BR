<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-MEDICAL-007 - Urgencia Emergencia Justificativa
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-MEDICAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_MEDICAL_007" name="Urgencia Emergencia Justificativa" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_DENY_MEDICAL_007" name="Verificar Urgencia Emergencia">
    <decisionTable id="DecisionTable_DENY_MEDICAL_007" hitPolicy="FIRST">
      <input id="Input_1" label="Caracterizado Urgencia/Emergencia">
        <inputExpression id="InputExpression_1" typeRef="boolean"><text>caracterizadoUrgencia</text></inputExpression>
      </input>
      <input id="Input_2" label="Tem Justificativa Medica">
        <inputExpression id="InputExpression_2" typeRef="boolean"><text>temJustificativaMedica</text></inputExpression>
      </input>
      <input id="Input_3" label="Tipo Atendimento">
        <inputExpression id="InputExpression_3" typeRef="string"><text>tipoAtendimento</text></inputExpression>
      </input>
      <input id="Input_4" label="CID Compativel">
        <inputExpression id="InputExpression_4" typeRef="boolean"><text>cidCompativel</text></inputExpression>
      </input>
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Urgencia sem caracterizacao adequada</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>"Urgencia", "Emergencia"</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA PROVAVEL: Urgencia/Emergencia nao caracterizada adequadamente. Pode ser contestado."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>
      <rule id="Rule_Alert_1">
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO MEDIO: CID pode nao ser compativel com urgencia/emergencia."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"MEDIO"</text></outputEntry>
      </rule>
      <rule id="Rule_Proceed_1">
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Urgencia/Emergencia caracterizada adequadamente. CID compativel."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>
      <rule id="Rule_Proceed_2">
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"Eletivo"</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Atendimento eletivo. Nao exige caracterizacao de urgencia."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"BAIXO"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback">
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar caracterizacao de urgencia/emergencia."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>