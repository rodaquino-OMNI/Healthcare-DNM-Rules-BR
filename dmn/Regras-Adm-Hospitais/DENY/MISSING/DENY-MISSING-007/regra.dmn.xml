<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-MISSING-007 - Documentacao Suporte Faltante
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-MISSING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_MISSING_007" name="Documentacao Suporte Faltante" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_MISSING_007" name="Verificar Documentacao Suporte">
    <decisionTable id="DecisionTable_DENY_MISSING_007" hitPolicy="FIRST">

      <input id="Input_1" label="Exige Documentacao Extra">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>exigeDocumentacaoExtra</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Documentacao Anexada">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>documentacaoAnexada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tipo Documentacao">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tipoDocumentacao</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Especialidade">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>especialidade</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Documentacao obrigatoria nao anexada</description>
        <inputEntry id="InputEntry_B1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Documentacao de suporte obrigatoria nao foi anexada. Nao faturar sem os documentos exigidos."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Oncologia sem exames de estadiamento</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"Estadiamento"</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>"Oncologia"</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO ALTO: Procedimento oncologico sem documentacao de estadiamento pode ser questionado."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Cirurgia sem termo de consentimento</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>"Consentimento"</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"RISCO MEDIO: Termo de consentimento nao anexado. Recomendado ter no prontuario."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Documentacao de suporte presente</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Documentacao de suporte presente e completa."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Procedimento nao exige documentacao extra</description>
        <inputEntry id="InputEntry_P2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Procedimento nao exige documentacao de suporte adicional."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_MISSING_007">
        <description>Caso padrao - verificar documentacao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar necessidade de documentacao de suporte adicional."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_MISSING_007_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_MISSING_007">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_MISSING_007" dmnElementRef="Decision_DENY_MISSING_007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>