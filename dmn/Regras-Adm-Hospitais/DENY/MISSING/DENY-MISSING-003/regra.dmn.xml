<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-MISSING-003 - Guia TISS Incompleta
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-MISSING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_MISSING_003" name="Guia TISS Incompleta" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_MISSING_003" name="Verificar Completude Guia TISS">
    <decisionTable id="DecisionTable_DENY_MISSING_003" hitPolicy="FIRST">

      <input id="Input_1" label="Guia Completa">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>guiaCompleta</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tipo Guia">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>tipoGuia</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Campos Faltantes">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>camposFaltantes</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Quantidade Campos Faltantes">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>qtdCamposFaltantes</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Muitos campos obrigatorios faltantes</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>&gt; 3</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Guia TISS com mais de 3 campos obrigatorios faltantes. Sera rejeitada automaticamente."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Campo critico obrigatorio faltante</description>
        <inputEntry id="InputEntry_B2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>"CID", "TUSS", "Carteirinha", "Data"</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"GLOSA CERTA: Campo critico (CID/TUSS/Carteirinha/Data) faltante na guia TISS. Preencher antes de enviar."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Poucos campos faltantes mas requer atencao</description>
        <inputEntry id="InputEntry_A1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>[1..3]</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO MEDIO: Guia TISS com campos faltantes. Completar antes do envio para evitar rejeicao."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Guia TISS completamente preenchida</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Guia TISS completa. Todos os campos obrigatorios preenchidos."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_MISSING_003">
        <description>Caso padrao - verificar completude</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Revisar completude da guia TISS antes do envio."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_MISSING_003_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_MISSING_003">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_MISSING_003" dmnElementRef="Decision_DENY_MISSING_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>