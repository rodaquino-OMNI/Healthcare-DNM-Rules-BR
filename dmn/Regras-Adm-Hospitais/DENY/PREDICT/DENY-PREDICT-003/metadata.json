  "status": "active",
  "specialty": ["Ortopedia", "Traumatologia"],
  "description": "Detecta risco de glosa por ausencia de RM/Artro-RM em procedimentos ortopedicos",
  "denialRiskFactors": [
    "Ausencia de exame de imagem",
    "Artroscopia sem RM previo",
    "Cirurgia ligamentar sem comprovacao por imagem"
  ],
  "requiredDocuments": [
    "Ressonancia Magnetica",
    "Artro-RM",
    "Laudo Radiologico"
  ],
  "inputs": [
    {"name": "codigoTUSS", "type": "string", "description": "Codigo TUSS do procedimento"},
    {"name": "especialidade", "type": "string", "description": "Especialidade medica"},
    {"name": "temImagemAnexada", "type": "boolean", "description": "Indica se imagem esta anexada"},
    {"name": "tipoImagem", "type": "string", "description": "Tipo do exame de imagem"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
    {"name": "observacao", "type": "string"},
    {"name": "riscoDenial", "type": "string", "values": ["BAIXO", "MEDIO", "ALTO", "CRITICO"]},
    {"name": "documentacaoFaltante", "type": "string"}
  ],
  "references": [
    "RN 465/2021 ANS",
    "Diretrizes SBOT",
    "TUSS 2024"
  ],
  "historicalDenialRate": 0.80,
  "tags": ["ortopedia", "imagem", "rm", "artroscopia", "glosa"]
}
