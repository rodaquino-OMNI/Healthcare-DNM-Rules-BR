{
  "ruleId": "DENY-PREDICT-003",
  "name": "Ortopedia Imagem Faltante",
  "category": "DENY",
  "subcategory": "DENY-PREDICT",
  "version": "1.0.0",
  "createdAt": "2026-02-06",
