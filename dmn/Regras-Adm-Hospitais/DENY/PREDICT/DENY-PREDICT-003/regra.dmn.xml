<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-PREDICT-003 - Ortopedia Imagem Faltante
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-PREDICT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_PREDICT_003" name="Ortopedia Imagem Faltante" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_PREDICT_003" name="Verificar Imagem Ortopedia">
    <decisionTable id="DecisionTable_DENY_PREDICT_003" hitPolicy="FIRST">

      <input id="Input_1" label="Codigo TUSS">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>codigoTUSS</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Especialidade">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>especialidade</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tem Imagem Anexada">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>temImagemAnexada</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Tipo Imagem">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>tipoImagem</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Artroscopia sem exame de imagem - glosa certa</description>
        <inputEntry id="InputEntry_B1_1"><text>"31503082", "31503090", "31503104"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Artroscopia requer RM ou Artro-RM previo. Operadoras exigem imagem para autorizacao."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Reconstrucao ligamentar sem RM</description>
        <inputEntry id="InputEntry_B2_1"><text>"31503112", "31503120", "31503139"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"Ortopedia"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"GLOSA CERTA: Reconstrucao de LCA/LCP exige RM com evidencia de lesao. Anexar imagem antes de faturar."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Imagem anexada mas nao e RM</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"Ortopedia"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>"RX", "Radiografia"</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO MEDIO: Apenas radiografia anexada. Alguns procedimentos exigem RM. Verificar exigencia do convenio."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Imagem adequada anexada</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"Ortopedia"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>"RM", "Artro-RM", "Ressonancia"</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Exame de imagem adequado (RM/Artro-RM) presente. Baixo risco de glosa."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_PREDICT_003">
        <description>Caso padrao - requer revisao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar manualmente necessidade de exame de imagem para este procedimento ortopedico."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_PREDICT_003_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_PREDICT_003">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_PREDICT_003" dmnElementRef="Decision_DENY_PREDICT_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>