<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-PREDICT-007 - Tratamento Conservador Gap
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-PREDICT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_PREDICT_007" name="Tratamento Conservador Gap" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_PREDICT_007" name="Verificar Tratamento Conservador">
    <decisionTable id="DecisionTable_DENY_PREDICT_007" hitPolicy="FIRST">

      <input id="Input_1" label="Codigo TUSS">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>codigoTUSS</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Especialidade">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>especialidade</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tem Tratamento Conservador">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>temTratamentoConservador</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Tipo Procedimento">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>tipoProcedimento</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Cirurgia de coluna sem tratamento conservador documentado</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"Ortopedia", "Neurocirurgia"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>"Cirurgia Coluna"</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Cirurgia de coluna exige documentacao de tratamento conservador previo (fisioterapia minimo 6 semanas). Operadoras negam sem esta documentacao."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Artroscopia sem tentativa de tratamento clinico</description>
        <inputEntry id="InputEntry_B2_1"><text>"31503082", "31503090"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"GLOSA PROVAVEL: Artroscopia exige documentacao de falha de tratamento conservador. Anexar historico de fisioterapia/medicamentoso."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Cirurgia eletiva sem documentacao de tratamento previo</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>"Cirurgia Eletiva"</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO ALTO: Cirurgia eletiva sem documentacao de tratamento conservador previo pode ser questionada. Recomendado anexar historico."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Documentacao de tratamento conservador presente</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Tratamento conservador documentado. Falha terapeutica comprovada justifica procedimento cirurgico."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Procedimento de emergencia nao exige conservador</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>"Emergencia", "Urgencia"</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Procedimento de emergencia/urgencia. Tratamento conservador previo nao e exigido."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_PREDICT_007">
        <description>Caso padrao - requer revisao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar manualmente se procedimento exige documentacao de tratamento conservador previo."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_PREDICT_007_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_PREDICT_007">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_PREDICT_007" dmnElementRef="Decision_DENY_PREDICT_007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>