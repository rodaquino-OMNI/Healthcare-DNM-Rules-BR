<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-PREDICT-006 - CID TUSS Mismatch
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-PREDICT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_PREDICT_006" name="CID TUSS Mismatch" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_PREDICT_006" name="Verificar Compatibilidade CID-TUSS">
    <decisionTable id="DecisionTable_DENY_PREDICT_006" hitPolicy="FIRST">

      <input id="Input_1" label="Codigo TUSS">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>codigoTUSS</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Diagnostico CID10">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>diagnosticoCID10</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Especialidade">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>especialidade</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Compatibilidade Verificada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>compatibilidadeVerificada</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Quimioterapia com CID nao-oncologico</description>
        <inputEntry id="InputEntry_B1_1"><text>"40801012", "40801020", "40801039"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>not("C00".."C97", "D00".."D09")</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>"Oncologia"</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Procedimento oncologico requer CID de neoplasia (C00-C97 ou D00-D09). CID informado nao justifica o procedimento."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Cirurgia ortopedica com CID clinico incompativel</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"Z00".."Z99"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>"Ortopedia"</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"GLOSA PROVAVEL: CID de capitulo Z (fatores de saude) nao justifica procedimento cirurgico. Revisar diagnostico."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>CID muito generico para o procedimento</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"R00".."R99"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO ALTO: CID de sinais e sintomas (R00-R99) pode nao justificar procedimento. Operadoras frequentemente questionam."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Compatibilidade CID-TUSS confirmada</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Compatibilidade CID-TUSS verificada. Baixo risco de glosa por incompatibilidade."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_PREDICT_006">
        <description>Caso padrao - requer verificacao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar manualmente compatibilidade entre CID-10 e codigo TUSS."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_PREDICT_006_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_PREDICT_006">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_PREDICT_006" dmnElementRef="Decision_DENY_PREDICT_006">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>