  "description": "Detecta risco de glosa por ausencia de laudo histopatologico em procedimentos de mastologia",
  "denialRiskFactors": [
    "Ausencia de confirmacao diagnostica",
    "Procedimento oncologico sem biopsia",
    "Falta de laudo anatomopatologico"
  ],
  "requiredDocuments": [
    "Laudo Histopatologico",
    "Resultado de Biopsia",
    "Anatomopatologico"
  ],
  "inputs": [
    {"name": "codigoTUSS", "type": "string", "description": "Codigo TUSS do procedimento"},
    {"name": "especialidade", "type": "string", "description": "Especialidade medica"},
    {"name": "temLaudoHistopatologico", "type": "boolean", "description": "Indica se laudo histopatologico esta anexado"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
