<!--
  ============================================================================
  DENY-PREDICT-004 - Ortopedia Idade Limite
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-PREDICT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_PREDICT_004" name="Ortopedia Idade Limite" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_PREDICT_004" name="Verificar Idade Limite Ortopedia">
    <decisionTable id="DecisionTable_DENY_PREDICT_004" hitPolicy="FIRST">

      <input id="Input_1" label="Codigo TUSS">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>codigoTUSS</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Idade Paciente">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>idadePaciente</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tipo Procedimento">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tipoProcedimento</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Tem Justificativa Clinica">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>temJustificativaClinica</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Protese total de quadril em paciente menor de 50 anos sem justificativa</description>
        <inputEntry id="InputEntry_B1_1"><text>"30715024", "30715032"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&lt; 50</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>"Artroplastia Total Quadril"</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA PROVAVEL: Protese de quadril em paciente &lt;50 anos exige justificativa clinica detalhada. Operadoras questionam indicacao em jovens."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Protese de joelho em paciente maior de 85 anos</description>
        <inputEntry id="InputEntry_B2_1"><text>"30715059", "30715067"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 85</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"GLOSA PROVAVEL: Protese em paciente &gt;85 anos requer avaliacao de risco cirurgico e justificativa. Alto risco de questionamento."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Idade atipica para procedimento</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>[50..55]</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"Artroplastia Total Quadril", "Artroplastia Total Joelho"</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO MEDIO: Idade do paciente esta na faixa limite. Recomendado incluir justificativa clinica preventiva."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Idade dentro da faixa normal para protese</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>[55..85]</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Idade do paciente dentro da faixa esperada para procedimento. Baixo risco de questionamento."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Justificativa clinica presente para idade atipica</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Justificativa clinica presente para indicacao. Documentacao adequada."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_PREDICT_004">
        <description>Caso padrao - requer revisao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar manualmente se idade do paciente e adequada para o procedimento."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_PREDICT_004_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_PREDICT_004">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_PREDICT_004" dmnElementRef="Decision_DENY_PREDICT_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>