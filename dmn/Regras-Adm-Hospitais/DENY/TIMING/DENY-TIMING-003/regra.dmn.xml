<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-TIMING-003 - Recurso 10 Dias
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-TIMING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_TIMING_003" name="Recurso 10 Dias" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_TIMING_003" name="Verificar Prazo Recurso">
    <decisionTable id="DecisionTable_DENY_TIMING_003" hitPolicy="FIRST">

      <input id="Input_1" label="Dias Desde Negativa">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasDesdeNegativa</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tipo Recurso">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>tipoRecurso</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Documentacao Recurso Completa">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>documentacaoRecursoCompleta</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Instancia Recurso">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>instanciaRecurso</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Prazo de 10 dias para recurso excedido</description>
        <inputEntry id="InputEntry_B1_1"><text>&gt; 10</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>"Primeira"</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Prazo de 10 dias uteis para recurso excedido. Perda de direito de recurso."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Prazo de recurso expirando</description>
        <inputEntry id="InputEntry_A1_1"><text>[8..10]</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO CRITICO: Prazo de recurso expirando em ate 2 dias. Protocolar imediatamente."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Recurso dentro do prazo</description>
        <inputEntry id="InputEntry_P1_1"><text>&lt; 8</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Recurso dentro do prazo de 10 dias. Documentacao completa."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_TIMING_003">
        <description>Caso padrao - verificar prazo</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar prazo de recurso."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_TIMING_003_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_TIMING_003">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_TIMING_003" dmnElementRef="Decision_DENY_TIMING_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>