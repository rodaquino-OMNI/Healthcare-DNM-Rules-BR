<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-TIMING-007 - Resposta Deadline Tracking
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-TIMING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_TIMING_007" name="Resposta Deadline Tracking" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_DENY_TIMING_007" name="Verificar Prazo Resposta">
    <decisionTable id="DecisionTable_DENY_TIMING_007" hitPolicy="FIRST">
      <input id="Input_1" label="Dias Aguardando Resposta">
        <inputExpression id="InputExpression_1" typeRef="number"><text>diasAguardandoResposta</text></inputExpression>
      </input>
      <input id="Input_2" label="Prazo Resposta Operadora">
        <inputExpression id="InputExpression_2" typeRef="number"><text>prazoRespostaOperadora</text></inputExpression>
      </input>
      <input id="Input_3" label="Tipo Solicitacao">
        <inputExpression id="InputExpression_3" typeRef="string"><text>tipoSolicitacao</text></inputExpression>
      </input>
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Alert_1">
        <description>Operadora nao respondeu no prazo - tacita aprovacao</description>
        <inputEntry id="InputEntry_A1_1"><text>&gt; 21</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>21</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"Autorizacao"</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"ACAO: Operadora excedeu prazo de 21 dias. Tacita aprovacao conforme RN 395."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"BAIXO"</text></outputEntry>
      </rule>
      <rule id="Rule_Alert_2">
        <inputEntry id="InputEntry_A2_1"><text>[18..21]</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>21</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"ATENCAO: Prazo de resposta da operadora expirando."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"MEDIO"</text></outputEntry>
      </rule>
      <rule id="Rule_Proceed_1">
        <inputEntry id="InputEntry_P1_1"><text>&lt; 18</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Aguardando resposta dentro do prazo."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback">
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar prazo de resposta."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
</definitions>