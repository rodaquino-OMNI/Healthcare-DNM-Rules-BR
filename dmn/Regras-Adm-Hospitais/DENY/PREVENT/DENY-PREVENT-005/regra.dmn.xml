<!--
  DENY-PREVENT-005 - Prescricao Medica Legivel e Completa
  Versao: 1.0.0 (2026-02-08) | Categoria: DENY-PREVENT | Perspectiva: HOSPITAL (Auditoria Administrativa)
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_PREVENT_005" name="Prescricao Medica Legivel e Completa" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_DENY_PREVENT_005" name="Prescricao Medica Legivel e Completa">
    <decisionTable id="DecisionTable_DENY_PREVENT_005" hitPolicy="FIRST">
      <input id="Input_1" label="Prescricao Completa"><inputExpression id="InputExpression_1" typeRef="boolean"><text>prescricaoCompleta</text></inputExpression></input>
      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>
      <rule id="Rule_1"><description>Prescricao incompleta - bloqueio</description><inputEntry id="InputEntry_1_1"><text>false</text></inputEntry><outputEntry id="OutputEntry_1_1"><text>"Bloquear"</text></outputEntry><outputEntry id="OutputEntry_1_2"><text>"RISCO ALTO: Prescricao medica ilegivel ou incompleta (Lei 5.991/73, CFM 1.246/1988)."</text></outputEntry><outputEntry id="OutputEntry_1_3"><text>"Solicitar prescricao legivel e completa. Verificar assinatura e carimbo medico."</text></outputEntry><outputEntry id="OutputEntry_1_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_1_5"><text>"ALTO"</text></outputEntry></rule>
      <rule id="Rule_2"><description>Prescricao completa - prosseguir</description><inputEntry id="InputEntry_2_1"><text>true</text></inputEntry><outputEntry id="OutputEntry_2_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_2_2"><text>"Prescricao medica legivel e completa. Conformidade legal."</text></outputEntry><outputEntry id="OutputEntry_2_3"><text>"Prosseguir com faturamento padrao."</text></outputEntry><outputEntry id="OutputEntry_2_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_2_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_3"><description>Fallback</description><inputEntry id="InputEntry_3_1"><text>-</text></inputEntry><outputEntry id="OutputEntry_3_1"><text>"Revisar"</text></outputEntry><outputEntry id="OutputEntry_3_2"><text>"Situacao requer analise manual."</text></outputEntry><outputEntry id="OutputEntry_3_3"><text>"Encaminhar para auditoria."</text></outputEntry><outputEntry id="OutputEntry_3_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_3_5"><text>"MEDIO"</text></outputEntry></rule>
      
    </decisionTable>
  </decision>
  <dmndi:DMNDI><dmndi:DMNDiagram id="DMNDiagram_DENY_PREVENT_005"><dmndi:DMNShape id="DMNShape_DENY_PREVENT_005" dmnElementRef="Decision_DENY_PREVENT_005"><dc:Bounds height="80" width="180" x="160" y="100"/></dmndi:DMNShape></dmndi:DMNDiagram></dmndi:DMNDI>
</definitions>
