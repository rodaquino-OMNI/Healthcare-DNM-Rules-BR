{
  "ruleCode": "DENY-APPEAL-004",
  "ruleName": "Arbitragem",
  "category": "DENY",
  "subcategory": "APPEAL",
  "specialty": "Juridico",
  "version": "1.0.0",
  "status": "active",
  "hitPolicy": "FIRST",
  "perspective": "HOSPITAL",
  "createdAt": "2026-02-06T00:00:00Z",
  "updatedAt": "2026-02-06T00:00:00Z",
  "inputs": [
    {
      "name": "clausulaArbitral",
      "type": "boolean",
      "label": "Clausula Arbitral Existente",
      "required": true,
      "description": "Indica se ha clausula compromissoria no contrato"
    },
    {
      "name": "camaraEscolhida",
      "type": "string",
      "label": "Status da Camara de Arbitragem",
      "required": true,
      "allowedValues": ["DEFINIDA", "EM_NEGOCIACAO", "NAO_DEFINIDA"],
      "description": "Situacao da escolha da camara arbitral"
    },
    {
      "name": "depositoInicial",
      "type": "string",
      "label": "Status do Deposito Inicial",
      "required": true,
      "allowedValues": ["REALIZADO", "PENDENTE", "NAO_REALIZADO"],
      "description": "Situacao do deposito inicial para arbitragem"
    },
    {
      "name": "valorDisputa",
      "type": "number",
      "label": "Valor da Disputa (R$)",
      "required": true,
      "description": "Valor total em discussao no procedimento",
      "validation": {"min": 0}
    },
    {
      "name": "materiaArbitravel",
      "type": "boolean",
      "label": "Materia Arbitravel",
      "required": true,
      "description": "Indica se a materia e passivel de arbitragem"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "label": "Resultado",
      "allowedValues": ["Prosseguir", "Bloquear", "Alertar", "Revisar"],
      "description": "Decisao sobre inicio da arbitragem"
    },
    {
      "name": "observacao",
      "type": "string",
      "label": "Observacao",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "acaoRecomendada",
      "type": "string",
      "label": "Acao Recomendada",
      "allowedValues": ["INICIAR_ARBITRAGEM", "AJUIZAR_JUDICIAL", "REALIZAR_DEPOSITO", "NEGOCIAR_CAMARA"],
      "description": "Acao recomendada para tratamento"
    },
    {
      "name": "custoEstimado",
      "type": "string",
      "label": "Custo Estimado",
      "allowedValues": ["BAIXO", "MEDIO", "ALTO"],
      "description": "Estimativa de custo do procedimento"
    }
  ],
  "indications": [
    "Clausula arbitral existente no contrato",
    "Camara de arbitragem escolhida por ambas as partes",
    "Deposito inicial realizado",
    "Materia arbitravel (direitos patrimoniais disponiveis)"
  ],
  "contraindications": [
    "Ausencia de clausula arbitral e valor abaixo de R$ 100.000",
    "Deposito inicial nao realizado",
    "Materia nao arbitravel (questoes regulatorias)"
  ],
  "references": [
    "Lei 9307/1996 - Lei de Arbitragem",
    "Lei 13129/2015 - Reforma da Lei de Arbitragem",
    "CPC Art. 485 - Competencia-competencia"
  ],
  "tags": ["arbitragem", "clausula", "camara", "deposito", "disputa"],
  "compliance": {
