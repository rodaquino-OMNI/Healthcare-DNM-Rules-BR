<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  DENY-APPEAL-004 - Arbitragem
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-APPEAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_APPEAL_004" name="Arbitragem" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_APPEAL_004" name="Avaliacao Arbitragem">
    <decisionTable id="DecisionTable_DENY_APPEAL_004" hitPolicy="FIRST">

      
      <input id="Input_1" label="Clausula Arbitral">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>clausulaArbitral</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Camara Escolhida">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>camaraEscolhida</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"DEFINIDA", "EM_NEGOCIACAO", "NAO_DEFINIDA"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Deposito Inicial">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>depositoInicial</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"REALIZADO", "PENDENTE", "NAO_REALIZADO"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Valor da Disputa">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>valorDisputa</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Materia Arbitravel">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>materiaArbitravel</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Sem clausula arbitral e valor abaixo de R$ 100.000</description>
        <inputEntry id="InputEntry_C1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>&lt; 100000</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Sem clausula arbitral e valor inferior a R$ 100.000. Custos de arbitragem inviabilizam o procedimento."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"AJUIZAR_JUDICIAL"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_C1_4"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Materia nao arbitravel conforme Lei 9307/1996</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Materia nao arbitravel. Lei 9307/1996 Art. 1 limita arbitragem a direitos patrimoniais disponiveis."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"AJUIZAR_JUDICIAL"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_C2_4"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Deposito inicial pendente de realizacao</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"DEFINIDA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Deposito inicial pendente. Realizar deposito para dar prosseguimento ao procedimento arbitral."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"REALIZAR_DEPOSITO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_4"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Arbitragem elegivel com todos requisitos atendidos</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"DEFINIDA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"REALIZADO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>&gt;= 100000</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Arbitragem elegivel. Clausula valida, camara definida, deposito realizado e materia arbitravel."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"INICIAR_ARBITRAGEM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_4"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Clausula arbitral valida - prosseguir com negociacoes</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"EM_NEGOCIACAO", "NAO_DEFINIDA"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>&gt;= 100000</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Clausula arbitral valida mas camara nao definida. Negociar escolha da camara com operadora."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"NEGOCIAR_CAMARA"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_P2_4"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_APPEAL_004">
        <description>Caso nao atenda criterios automaticos - Revisar</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Procedimento arbitral requer analise juridica. Avaliar clausula, custos e viabilidade."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"AJUIZAR_JUDICIAL"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_004_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_4"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_APPEAL_004">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_APPEAL_004" dmnElementRef="Decision_DENY_APPEAL_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>