<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  DENY-APPEAL-006 - Acao Coletiva
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-APPEAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_APPEAL_006" name="Acao Coletiva" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_APPEAL_006" name="Avaliacao Acao Coletiva">
    <decisionTable id="DecisionTable_DENY_APPEAL_006" hitPolicy="FIRST">

      
      <input id="Input_1" label="Classe Prejudicada">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>classePrejudicada</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"IDENTIFICADA", "PARCIAL", "NAO_IDENTIFICADA"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Representacao Legal">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>representacaoLegal</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"FORMAL", "PARCIAL", "INEXISTENTE"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Abrangencia">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>abrangencia</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"NACIONAL", "REGIONAL", "LOCAL", "PONTUAL"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Numero Prestadores Afetados">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>numeroPrestadoresAfetados</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Entidade de Classe">
        <inputExpression id="InputExpression_5" typeRef="string">
          <text>entidadeClasse</text>
        </inputExpression>
        <inputValues id="InputValues_5">
          <text>"SINDICATO", "ASSOCIACAO", "FEDERACAO", "NENHUMA"</text>
        </inputValues>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Ausencia de representacao formal da classe</description>
        <inputEntry id="InputEntry_C1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>"INEXISTENTE"</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>"NENHUMA"</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Sem representacao formal da classe. Lei 7347/1985 exige legitimidade ativa para ACP."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"BUSCAR_REPRESENTACAO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Demanda pontual sem caracteristica coletiva</description>
        <inputEntry id="InputEntry_C2_1"><text>"NAO_IDENTIFICADA"</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>"PONTUAL"</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>&lt; 5</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Demanda pontual sem interesse coletivo caracterizado. Ajuizar acao individual."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"AJUIZAR_INDIVIDUAL"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Representacao legal parcial requer complementacao</description>
        <inputEntry id="InputEntry_A1_1"><text>"IDENTIFICADA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Representacao legal parcial. Formalizar adesao junto a entidade de classe antes de prosseguir."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"BUSCAR_REPRESENTACAO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Acao coletiva elegivel com todos requisitos</description>
        <inputEntry id="InputEntry_P1_1"><text>"IDENTIFICADA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"FORMAL"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"NACIONAL", "REGIONAL"</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>&gt;= 10</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>"SINDICATO", "ASSOCIACAO", "FEDERACAO"</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Acao coletiva elegivel. Classe identificada, representacao formal e abrangencia significativa."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"ADERIR_ACAO_COLETIVA"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Acao coletiva local com representacao adequada</description>
        <inputEntry id="InputEntry_P2_1"><text>"IDENTIFICADA"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"FORMAL"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"LOCAL"</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>&gt;= 5</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>"SINDICATO", "ASSOCIACAO"</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Acao coletiva local elegivel. Verificar com entidade de classe o andamento."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"ADERIR_ACAO_COLETIVA"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_APPEAL_006">
        <description>Caso nao atenda criterios automaticos - Revisar</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Participacao em acao coletiva requer analise. Verificar classe, representacao e abrangencia."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"AGUARDAR_DECISAO"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_006_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_006_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_APPEAL_006">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_APPEAL_006" dmnElementRef="Decision_DENY_APPEAL_006">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>