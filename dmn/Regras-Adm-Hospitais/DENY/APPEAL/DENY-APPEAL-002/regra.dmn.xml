<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  DENY-APPEAL-002 - Reclamacao ANS
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-APPEAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_APPEAL_002" name="Reclamacao ANS" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_APPEAL_002" name="Avaliacao Reclamacao ANS">
    <decisionTable id="DecisionTable_DENY_APPEAL_002" hitPolicy="FIRST">

      
      <input id="Input_1" label="Protocolo ANS">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>protocoloANS</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"VALIDO", "PENDENTE", "INEXISTENTE", "EXPIRADO"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Materias Reclamacao">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>materiasReclamacao</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"GLOSA_INDEVIDA", "NEGATIVA_COBERTURA", "DESCREDENCIAMENTO", "REAJUSTE", "OUTROS"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Prazos Resposta (dias)">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>prazosResposta</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Via Interna Esgotada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>viaInternaEsgotada</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Reincidencia Operadora">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>reincidenciaOperadora</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Ausencia de protocolo valido para ANS</description>
        <inputEntry id="InputEntry_C1_1"><text>"INEXISTENTE"</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Protocolo ANS inexistente. ANS RN 388/2015 exige registro previo para acompanhamento."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"GERAR_PROTOCOLO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Via administrativa interna ainda disponivel</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Via administrativa interna nao esgotada. Completar tramite interno antes de escalar a ANS."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"ESGOTAR_VIA_INTERNA"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Protocolo pendente de resposta da ANS</description>
        <inputEntry id="InputEntry_A1_1"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Protocolo ANS pendente de resposta. Aguardar prazo regulamentar antes de nova demanda."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"AGUARDAR_RESPOSTA"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Via interna esgotada com reincidencia da operadora</description>
        <inputEntry id="InputEntry_P1_1"><text>"VALIDO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"GLOSA_INDEVIDA", "NEGATIVA_COBERTURA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Reclamacao ANS elegivel. Via interna esgotada e operadora reincidente. Registrar como denuncia."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"ABRIR_RECLAMACAO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Via interna esgotada - reclamacao padrao</description>
        <inputEntry id="InputEntry_P2_1"><text>"VALIDO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Reclamacao ANS elegivel. Via interna esgotada. Registrar como demanda de prestador."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"ABRIR_RECLAMACAO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_APPEAL_002">
        <description>Caso nao atenda criterios automaticos - Revisar</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Reclamacao ANS requer analise manual. Verificar protocolo, materias e esgotamento de via interna."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"ESGOTAR_VIA_INTERNA"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_002_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_002_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_APPEAL_002">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_APPEAL_002" dmnElementRef="Decision_DENY_APPEAL_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>