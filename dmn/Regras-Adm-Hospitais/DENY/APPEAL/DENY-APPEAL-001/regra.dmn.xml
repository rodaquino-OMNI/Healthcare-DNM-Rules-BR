<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  DENY-APPEAL-001 - Recurso Administrativo
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-APPEAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_APPEAL_001" name="Recurso Administrativo" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_APPEAL_001" name="Avaliacao Recurso Administrativo">
    <decisionTable id="DecisionTable_DENY_APPEAL_001" hitPolicy="FIRST">

      
      <input id="Input_1" label="Prazo Recurso (dias restantes)">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>prazoRecurso</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Documentacao Anexada">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>documentacaoAnexada</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"COMPLETA", "PARCIAL", "INSUFICIENTE"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Fundamentacao Legal">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>fundamentacaoLegal</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"FORTE", "MODERADA", "FRACA", "INEXISTENTE"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Valor em Discussao">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>valorDiscussao</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Instancia">
        <inputExpression id="InputExpression_5" typeRef="string">
          <text>instancia</text>
        </inputExpression>
        <inputValues id="InputValues_5">
          <text>"PRIMEIRA", "SEGUNDA", "FINAL"</text>
        </inputValues>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Prazo de recurso expirado</description>
        <inputEntry id="InputEntry_C1_1"><text>&lt;= 0</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Prazo de recurso administrativo expirado. ANS RN 395/2016 estabelece prazo peremptorio."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"DESISTIR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Recurso em instancia final - via administrativa esgotada</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>"FINAL"</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Via administrativa esgotada. Considerar reclamacao ANS ou acao judicial."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"ESCALAR_ANS"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Prazo de recurso em situacao critica</description>
        <inputEntry id="InputEntry_A1_1"><text>[1..3]</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Prazo de recurso critico (menos de 3 dias). Priorizar protocolo imediato mesmo com documentacao parcial."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"INTERPOR_RECURSO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Recurso elegivel com documentacao completa</description>
        <inputEntry id="InputEntry_P1_1"><text>&gt; 3</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"COMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"FORTE", "MODERADA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>&gt;= 100</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>"PRIMEIRA", "SEGUNDA"</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Recurso administrativo elegivel. Documentacao completa e fundamentacao adequada."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"INTERPOR_RECURSO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Recurso com documentacao parcial - tempo para complementar</description>
        <inputEntry id="InputEntry_P2_1"><text>&gt; 5</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"FORTE", "MODERADA"</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>&gt;= 500</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>"PRIMEIRA"</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Recurso elegivel mas documentacao incompleta. Completar documentacao antes do prazo."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"COMPLEMENTAR_DOCS"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_APPEAL_001">
        <description>Caso nao atenda criterios automaticos - Revisar</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Recurso administrativo requer analise manual. Avaliar prazo, documentacao e fundamentacao."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"COMPLEMENTAR_DOCS"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_001_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_001_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_APPEAL_001">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_APPEAL_001" dmnElementRef="Decision_DENY_APPEAL_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>