<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  DENY-APPEAL-003 - Acao Judicial
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-APPEAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_APPEAL_003" name="Acao Judicial" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_APPEAL_003" name="Avaliacao Acao Judicial">
    <decisionTable id="DecisionTable_DENY_APPEAL_003" hitPolicy="FIRST">

      
      <input id="Input_1" label="Advogado Contratado">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>advogadoContratado</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Peticao Inicial">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>peticaoInicial</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"PRONTA", "EM_ELABORACAO", "PENDENTE"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Prova Tecnica">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>provaTecnica</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"COMPLETA", "PARCIAL", "INCOMPLETA", "INEXISTENTE"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Valor da Causa">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>valorCausa</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Precedentes">
        <inputExpression id="InputExpression_5" typeRef="string">
          <text>precedentes</text>
        </inputExpression>
        <inputValues id="InputValues_5">
          <text>"FAVORAVEIS", "NEUTROS", "DESFAVORAVEIS", "INEXISTENTES"</text>
        </inputValues>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Ausencia de prova tecnica adequada</description>
        <inputEntry id="InputEntry_C1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>"INEXISTENTE"</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Prova tecnica inexistente. CPC Art. 464 exige prova pericial para questoes tecnicas de saude."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"AGUARDAR_PROVA"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Sem representacao legal contratada</description>
        <inputEntry id="InputEntry_C2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Sem advogado contratado. Capacidade postulatoria obrigatoria para acoes judiciais."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"CONTRATAR_ADVOGADO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Prova tecnica incompleta - risco processual</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"INCOMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Prova tecnica incompleta. Risco de inversao do onus da prova. Completar acervo probatorio."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"AGUARDAR_PROVA"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Acao elegivel com prova robusta e precedentes favoraveis</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"PRONTA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"COMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>&gt;= 10000</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>"FAVORAVEIS"</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Acao judicial elegivel. Prova tecnica completa, precedentes favoraveis e valor significativo."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"AJUIZAR_ACAO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Acao elegivel com prova robusta</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"PRONTA", "EM_ELABORACAO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"COMPLETA", "PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>&gt;= 50000</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>"FAVORAVEIS", "NEUTROS"</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Acao judicial elegivel. Valor elevado justifica ajuizamento mesmo com precedentes neutros."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"AJUIZAR_ACAO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_APPEAL_003">
        <description>Caso nao atenda criterios automaticos - Revisar</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Acao judicial requer analise do departamento juridico. Avaliar prova, custas e precedentes."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"CONTRATAR_ADVOGADO"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_003_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_DENY_APPEAL_003_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_APPEAL_003">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_APPEAL_003" dmnElementRef="Decision_DENY_APPEAL_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>