<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-DUPLICATE-005 - Bilateral Unilateral Duplicidade
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-DUPLICATE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_DUPLICATE_005" name="Bilateral Unilateral Duplicidade" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_DUPLICATE_005" name="Verificar Lateralidade">
    <decisionTable id="DecisionTable_DENY_DUPLICATE_005" hitPolicy="FIRST">

      <input id="Input_1" label="Lateralidade Cobrada">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>lateralidadeCobrada</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Lateralidade Anterior">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>lateralidadeAnterior</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Mesmo Procedimento">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>mesmoProcedimento</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Mesma Data">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>mesmaData</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Unilateral quando bilateral ja foi cobrado</description>
        <inputEntry id="InputEntry_B1_1"><text>"Unilateral", "Direito", "Esquerdo"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"Bilateral"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Procedimento bilateral ja cobrado. Nao cobrar unilateral adicional."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bilateral quando ambos unilaterais ja foram cobrados</description>
        <inputEntry id="InputEntry_B2_1"><text>"Bilateral"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"Unilateral", "Direito", "Esquerdo"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"GLOSA CERTA: Unilateral ja cobrado. Nao cobrar bilateral adicional."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Mesmo lado cobrado duas vezes</description>
        <inputEntry id="InputEntry_A1_1"><text>"Direito"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"Direito"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO CRITICO: Mesmo lado (Direito) cobrado duas vezes. Verificar se nao e duplicidade."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Lado esquerdo cobrado duas vezes</description>
        <inputEntry id="InputEntry_A2_1"><text>"Esquerdo"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"Esquerdo"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"RISCO CRITICO: Mesmo lado (Esquerdo) cobrado duas vezes. Verificar se nao e duplicidade."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Lados diferentes - pode ser bilateral</description>
        <inputEntry id="InputEntry_P1_1"><text>"Direito"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"Esquerdo"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Procedimento bilateral (lados diferentes). Cobranca adequada."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_DUPLICATE_005">
        <description>Caso padrao - verificar lateralidade</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar lateralidade do procedimento."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_DUPLICATE_005_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_DUPLICATE_005">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_DUPLICATE_005" dmnElementRef="Decision_DENY_DUPLICATE_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>