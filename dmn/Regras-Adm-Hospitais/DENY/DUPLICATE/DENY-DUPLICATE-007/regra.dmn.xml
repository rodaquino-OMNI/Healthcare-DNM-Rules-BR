<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-DUPLICATE-007 - Duplicidade Cross-Facility
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-DUPLICATE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_DUPLICATE_007" name="Duplicidade Cross-Facility" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_DUPLICATE_007" name="Verificar Cross-Facility">
    <decisionTable id="DecisionTable_DENY_DUPLICATE_007" hitPolicy="FIRST">

      <input id="Input_1" label="Mesmo Procedimento Outro Local">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>mesmoProcedimentoOutroLocal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Mesma Data">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>mesmaData</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Mesmo Paciente">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>mesmoPaciente</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Tem Justificativa">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>temJustificativa</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Mesmo procedimento em outro local na mesma data</description>
        <inputEntry id="InputEntry_B1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Procedimento ja realizado em outro estabelecimento na mesma data. Caracteriza duplicidade."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Possivel duplicidade com justificativa</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO ALTO: Procedimento em outro local na mesma data. Justificativa presente mas pode ser questionado."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Nao ha procedimento em outro local</description>
        <inputEntry id="InputEntry_P1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Nenhum procedimento similar em outro estabelecimento. Sem risco de duplicidade cross-facility."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Procedimento em outro local mas data diferente</description>
        <inputEntry id="InputEntry_P2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Procedimento em outro local mas em data diferente. Verificar intervalo minimo se aplicavel."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_DUPLICATE_007">
        <description>Caso padrao - verificar historico</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar historico de procedimentos em outros estabelecimentos."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_DUPLICATE_007_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_DUPLICATE_007">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_DUPLICATE_007" dmnElementRef="Decision_DENY_DUPLICATE_007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>