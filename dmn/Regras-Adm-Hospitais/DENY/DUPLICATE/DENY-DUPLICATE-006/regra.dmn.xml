<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  DENY-DUPLICATE-006 - Multiplas Sessoes Mesmo Dia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-DUPLICATE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_DENY_DUPLICATE_006" name="Multiplas Sessoes Mesmo Dia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_DENY_DUPLICATE_006" name="Verificar Multiplas Sessoes">
    <decisionTable id="DecisionTable_DENY_DUPLICATE_006" hitPolicy="FIRST">

      <input id="Input_1" label="Quantidade Sessoes Dia">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>quantidadeSessoesDia</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Maximo Permitido">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>maximoPermitido</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tipo Terapia">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tipoTerapia</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Tem Justificativa">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>temJustificativa</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Sessoes acima do limite diario sem justificativa</description>
        <inputEntry id="InputEntry_B1_1"><text>&gt; 2</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"GLOSA CERTA: Multiplas sessoes no mesmo dia acima do limite. Exige justificativa clinica."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Fisioterapia limita a 1 sessao por dia</description>
        <inputEntry id="InputEntry_B2_1"><text>&gt; 1</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>1</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>"Fisioterapia"</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"GLOSA CERTA: Fisioterapia permite apenas 1 sessao por dia. Sessoes adicionais serao glosadas."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Block_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Sessoes no limite diario</description>
        <inputEntry id="InputEntry_A1_1"><text>2</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"RISCO MEDIO: Duas sessoes no mesmo dia. Pode ser questionado dependendo do tipo de terapia."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Alert_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Sessao unica no dia</description>
        <inputEntry id="InputEntry_P1_1"><text>1</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Sessao unica no dia. Sem restricao de quantidade."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Proceed_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_DENY_DUPLICATE_006">
        <description>Caso padrao - verificar quantidade</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar quantidade de sessoes e limite diario."</text></outputEntry>
        <outputEntry id=""><text></text></outputEntry>
<outputEntry id="OutputEntry_Fallback_DENY_DUPLICATE_006_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_DENY_DUPLICATE_006">
      <dmndi:DMNShape id="DMNShape_Decision_DENY_DUPLICATE_006" dmnElementRef="Decision_DENY_DUPLICATE_006">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>