<?xml version="1.0" encoding="UTF-8"?>
<!--
  DENY-PAYER-007 - Auditor Externo com Perfil Restritivo
  Versao: 1.0.0 (2026-02-08)
  Categoria: DENY-PAYER | Perspectiva: HOSPITAL (Faturamento)
  Refs: RN ANS 465/2021, CFM Resolucao 1.614/2001
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_DENY_PAYER_007"
             name="Auditor Externo com Perfil Restritivo"
             targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_DENY_PAYER_007" name="Auditor Externo com Perfil Restritivo">
    <decisionTable id="DecisionTable_DENY_PAYER_007" hitPolicy="FIRST">
      <input id="Input_1" label="Auditor Externo"><inputExpression id="InputExpression_1" typeRef="boolean"><text>auditorExterno</text></inputExpression></input>
      <input id="Input_2" label="Perfil Auditor"><inputExpression id="InputExpression_2" typeRef="string"><text>perfilAuditor</text></inputExpression><inputValues id="InputValues_2"><text>"FLEXIVEL", "MODERADO", "RESTRITIVO"</text></inputValues></input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>
      <rule id="Rule_Bloqueio_1"><description>Auditor externo restritivo - risco critico</description><inputEntry id="InputEntry_B1_1"><text>true</text></inputEntry><inputEntry id="InputEntry_B1_2"><text>"RESTRITIVO"</text></inputEntry><outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry><outputEntry id="OutputEntry_B1_2"><text>"RISCO CRITICO: Auditor externo RESTRITIVO identificado. Historico de glosas tecnicamente questionaveis. Revisao exaustiva obrigatoria."</text></outputEntry><outputEntry id="OutputEntry_B1_3"><text>"Acionar comite tecnico de auditoria medica. Revisar integralmente justificativas clinicas. Preparar parecer tecnico fundamentado."</text></outputEntry><outputEntry id="OutputEntry_B1_4"><text>"CRED"</text></outputEntry><outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry></rule>
      <rule id="Rule_Alerta_1"><description>Auditor externo moderado - risco medio</description><inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry><inputEntry id="InputEntry_A1_2"><text>"MODERADO"</text></inputEntry><outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry><outputEntry id="OutputEntry_A1_2"><text>"RISCO MEDIO: Auditor externo MODERADO. Exige conformidade tecnica rigorosa mas aceita justificativas fundamentadas."</text></outputEntry><outputEntry id="OutputEntry_A1_3"><text>"Auditor medico deve revisar justificativas clinicas. Verificar conformidade com protocolos assistenciais."</text></outputEntry><outputEntry id="OutputEntry_A1_4"><text>"DOC"</text></outputEntry><outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry></rule>
      <rule id="Rule_Aprovado_1"><description>Auditor externo flexivel - risco baixo</description><inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry><inputEntry id="InputEntry_P1_2"><text>"FLEXIVEL"</text></inputEntry><outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P1_2"><text>"Auditor externo FLEXIVEL. Aceita justificativas clinicas razoaveis e documentacao basica. Baixo risco de glosa."</text></outputEntry><outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrao. Manter checklist basico."</text></outputEntry><outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_Aprovado_2"><description>Sem auditor externo</description><inputEntry id="InputEntry_P2_1"><text>false</text></inputEntry><inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry><outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P2_2"><text>"Sem auditor externo. Auditoria interna da operadora tende a ser mais previsivel. Risco moderado."</text></outputEntry><outputEntry id="OutputEntry_P2_3"><text>"Prosseguir com faturamento conforme procedimento padrao."</text></outputEntry><outputEntry id="OutputEntry_P2_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_P2_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_Fallback_DENY_PAYER_007"><description>Regra padrao</description><inputEntry id="InputEntry_F_1"><text>-</text></inputEntry><inputEntry id="InputEntry_F_2"><text>-</text></inputEntry><outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry><outputEntry id="OutputEntry_F_2"><text>"Situacao requer analise manual. Verificar historico do auditor."</text></outputEntry><outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa."</text></outputEntry><outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry></rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI><dmndi:DMNDiagram id="DMNDiagram_DENY_PAYER_007"><dmndi:DMNShape id="DMNShape_Decision_DENY_PAYER_007" dmnElementRef="Decision_DENY_PAYER_007"><dc:Bounds height="80" width="180" x="160" y="100"/></dmndi:DMNShape></dmndi:DMNDiagram></dmndi:DMNDI>
</definitions>
