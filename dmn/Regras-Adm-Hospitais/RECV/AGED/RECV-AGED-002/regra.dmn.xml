<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  RECV-AGED-002 - Recebiveis 91-180 Dias - Escalacao
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: RECV-AGED
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_RECV_AGED_002" name="Recebiveis 91-180 Dias - Escalacao" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_RECV_AGED_002" name="Acao para Recebiveis 91-180 Dias">
    <decisionTable id="DecisionTable_RECV_AGED_002" hitPolicy="FIRST">

      
      <input id="Input_1" label="Dias em Aberto">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasEmAberto</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Valor Recebivel">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>valorRecebivel</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Status Cobranca">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>statusCobranca</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"PENDENTE", "EM_CONTATO", "PROMESSA_PAGAMENTO", "CONTESTADO", "PARCIAL", "ESCALADO"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Tentativas de Cobranca">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>tentativasCobranca</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Aging abaixo do range desta regra</description>
        <inputEntry id="InputEntry_C1_1"><text>&lt; 91</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Aging &lt; 91 dias. Usar regra RECV-AGED-001."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Aging acima do range desta regra</description>
        <inputEntry id="InputEntry_C2_1"><text>&gt; 180</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Aging &gt; 180 dias. Usar regra RECV-AGED-003."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Aging 91-120 dias - Escalar para gestao</description>
        <inputEntry id="InputEntry_A1_1"><text>[91..120]</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>not("ESCALADO")</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>&gt;= 2</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Aging 91-120 dias com multiplas tentativas. Escalar para gerencia. Avaliar acao comercial direta."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Aging 121-180 dias - Revisar provisao</description>
        <inputEntry id="InputEntry_A2_1"><text>[121..180]</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>"PENDENTE", "EM_CONTATO"</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Aging 121-180 dias. OBRIGATORIO: Revisar provisao. Considerar provisao parcial ou total conforme probabilidade."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"PROVISIONAR"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Ja escalado - aguardar resolucao</description>
        <inputEntry id="InputEntry_A3_1"><text>[91..180]</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>"ESCALADO"</text></inputEntry>
        <inputEntry id="InputEntry_A3_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_A3_2"><text>"Caso ja escalado. Monitorar resolucao pela gestao. Atualizar status em 7 dias."</text></outputEntry>
<outputEntry id="OutputEntry_A3_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_3_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_3_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_4">
        <description>Alto valor requer escalacao imediata</description>
        <inputEntry id="InputEntry_A4_1"><text>[91..180]</text></inputEntry>
        <inputEntry id="InputEntry_A4_2"><text>&gt;= 50000</text></inputEntry>
        <inputEntry id="InputEntry_A4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A4_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A4_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A4_2"><text>"URGENTE: Recebivel de alto valor (&gt;= R$ 50.000) com aging critico. Escalar para diretoria. Acionar juridico se necessario."</text></outputEntry>
<outputEntry id="OutputEntry_A4_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_4_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_4_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_5">
        <description>Promessa de pagamento - monitorar cumprimento</description>
        <inputEntry id="InputEntry_A5_1"><text>[91..180]</text></inputEntry>
        <inputEntry id="InputEntry_A5_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A5_3"><text>"PROMESSA_PAGAMENTO"</text></inputEntry>
        <inputEntry id="InputEntry_A5_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A5_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A5_2"><text>"Promessa de pagamento com aging elevado. Monitorar rigorosamente. Se nao cumprir em 15 dias, escalar."</text></outputEntry>
<outputEntry id="OutputEntry_A5_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_5_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_5_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_RECV_AGED_002">
        <description>Caso padrao - Escalar e revisar provisao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Recebivel em faixa critica (91-180 dias). Escalar para gestao e revisar provisao."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"PROVISIONAR"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_RECV_AGED_002_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_RECV_AGED_002_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_RECV_AGED_002">
      <dmndi:DMNShape id="DMNShape_Decision_RECV_AGED_002" dmnElementRef="Decision_RECV_AGED_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>