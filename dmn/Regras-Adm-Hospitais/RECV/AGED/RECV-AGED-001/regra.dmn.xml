<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  RECV-AGED-001 - Recebiveis 0-90 Dias - Acompanhamento Ativo
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: RECV-AGED
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_RECV_AGED_001" name="Recebiveis 0-90 Dias - Acompanhamento Ativo" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_RECV_AGED_001" name="Acao para Recebiveis 0-90 Dias">
    <decisionTable id="DecisionTable_RECV_AGED_001" hitPolicy="FIRST">

      
      <input id="Input_1" label="Dias em Aberto">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasEmAberto</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Valor Recebivel">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>valorRecebivel</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Status Cobranca">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>statusCobranca</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"PENDENTE", "EM_CONTATO", "PROMESSA_PAGAMENTO", "CONTESTADO", "PARCIAL"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Tentativas de Cobranca">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>tentativasCobranca</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Aging fora do range desta regra</description>
        <inputEntry id="InputEntry_C1_1"><text>&gt; 90</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Aging &gt; 90 dias. Usar regra RECV-AGED-002 ou RECV-AGED-003."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Aging 0-30 dias - Acompanhamento padrao</description>
        <inputEntry id="InputEntry_A1_1"><text>[0..30]</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>0</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Recebivel recente (0-30 dias). Acompanhamento padrao. Aguardar ciclo normal de pagamento da operadora."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Aging 31-60 dias - Primeiro contato</description>
        <inputEntry id="InputEntry_A2_1"><text>[31..60]</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>0</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Aging 31-60 dias. Realizar primeiro contato com operadora. Verificar status do processamento."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Aging 61-90 dias - Segundo contato e alerta</description>
        <inputEntry id="InputEntry_A3_1"><text>[61..90]</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>"PENDENTE", "EM_CONTATO"</text></inputEntry>
        <inputEntry id="InputEntry_A3_4"><text>&lt;= 1</text></inputEntry>
<outputEntry id="OutputEntry_A3_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A3_2"><text>"Aging 61-90 dias. Segundo contato URGENTE conforme RN 412/2016. Formalizar cobranca por escrito via protocolo TISS. Prazo para resposta: 15 dias uteis."</text></outputEntry>
<outputEntry id="OutputEntry_A3_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_3_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_4">
        <description>Com promessa de pagamento - monitorar</description>
        <inputEntry id="InputEntry_A4_1"><text>[0..90]</text></inputEntry>
        <inputEntry id="InputEntry_A4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A4_3"><text>"PROMESSA_PAGAMENTO"</text></inputEntry>
        <inputEntry id="InputEntry_A4_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A4_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_A4_2"><text>"Promessa de pagamento registrada. Monitorar cumprimento. Reagendar follow-up em 7 dias."</text></outputEntry>
<outputEntry id="OutputEntry_A4_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_4_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_4_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_5">
        <description>Valor alto requer atencao especial</description>
        <inputEntry id="InputEntry_A5_1"><text>[31..90]</text></inputEntry>
        <inputEntry id="InputEntry_A5_2"><text>&gt;= 10000</text></inputEntry>
        <inputEntry id="InputEntry_A5_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A5_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A5_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A5_2"><text>"Recebivel de alto valor (&gt;= R$ 10.000). Escalar para gerencia comercial. Contato direto com gestor de relacionamento da operadora."</text></outputEntry>
<outputEntry id="OutputEntry_A5_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_5_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_5_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_6">
        <description>Recebivel contestado pela operadora</description>
        <inputEntry id="InputEntry_A6_1"><text>[0..90]</text></inputEntry>
        <inputEntry id="InputEntry_A6_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A6_3"><text>"CONTESTADO"</text></inputEntry>
        <inputEntry id="InputEntry_A6_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A6_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A6_2"><text>"Recebivel contestado. Encaminhar para analise de glosa. Verificar motivo da contestacao."</text></outputEntry>
<outputEntry id="OutputEntry_A6_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_6_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_6_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_RECV_AGED_001">
        <description>Caso padrao - Acompanhamento ativo</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Manter acompanhamento ativo. Registrar contatos e atualizar status."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_RECV_AGED_001_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_RECV_AGED_001_5"><text>"BAIXO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_RECV_AGED_001">
      <dmndi:DMNShape id="DMNShape_Decision_RECV_AGED_001" dmnElementRef="Decision_RECV_AGED_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>