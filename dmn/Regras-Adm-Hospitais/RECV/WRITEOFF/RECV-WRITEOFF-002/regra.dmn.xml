<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  RECV-WRITEOFF-002 - Provisao Deducao Fiscal
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: RECV-WRITEOFF
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_RECV_WRITEOFF_002" name="Provisao Deducao Fiscal" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_RECV_WRITEOFF_002" name="Avaliacao Provisao Deducao Fiscal">
    <decisionTable id="DecisionTable_RECV_WRITEOFF_002" hitPolicy="FIRST">

      
      <input id="Input_1" label="Valor da Baixa">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>valorBaixa</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Status Documentacao">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>documentacaoSuporte</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"COMPLETA", "PARCIAL", "AUSENTE"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Classificacao Fiscal">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>classificacaoFiscal</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"DEDUTIVEL", "NAO_DEDUTIVEL", "PARCIALMENTE_DEDUTIVEL", "PENDENTE_ANALISE"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Tem Protesto">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>temProtesto</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Partes Relacionadas">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>partesRelacionadas</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Documentacao de suporte ausente - Bloqueio obrigatorio</description>
        <inputEntry id="InputEntry_C1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>"AUSENTE"</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Documentacao de suporte ausente. Deducao fiscal requer comprovacao dos esforcos de cobranca (RIR Art. 347)."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"COMPLEMENTAR_DOCUMENTACAO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="OutputEntry_C1_4"><text>"CRITICO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Credito entre partes relacionadas - Nao dedutivel</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Credito entre partes relacionadas nao elegivel para deducao fiscal (Lei 9430/96 Art. 9 par. 6)."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"INDEDUTIVEL"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="OutputEntry_C2_4"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_3">
        <description>Classificacao fiscal ja definida como nao dedutivel</description>
        <inputEntry id="InputEntry_C3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C3_3"><text>"NAO_DEDUTIVEL"</text></inputEntry>
        <inputEntry id="InputEntry_C3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C3_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_C3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C3_2"><text>"Credito classificado como nao dedutivel. Registrar como despesa indedutivel no LALUR."</text></outputEntry>
<outputEntry id="OutputEntry_C3_3"><text>"INDEDUTIVEL"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_3_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="OutputEntry_C3_4"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Documentacao parcial - Risco de glosa fiscal</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Documentacao parcial apresenta risco de glosa em fiscalizacao. Complementar antes de deduzir."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"COMPLEMENTAR_DOCUMENTACAO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="OutputEntry_A1_4"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_2">
        <description>Pendente analise sem protesto do titulo</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"COMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>"PENDENTE_ANALISE"</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Titulo nao protestado. Para creditos acima de R$ 15.000, protesto e recomendado para seguranca fiscal."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"CONSULTAR_TRIBUTARIO"</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="OutputEntry_A2_4"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Documentacao completa com titulo protestado</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"COMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"DEDUTIVEL"</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Credito com documentacao completa e titulo protestado. Elegivel para deducao integral no IRPJ/CSLL."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DEDUZIR_IRPJ_CSLL"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P1_4"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Documentacao completa ja classificada como dedutivel</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"COMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"DEDUTIVEL"</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Credito com documentacao completa e classificacao dedutivel. Proceder com deducao no IRPJ/CSLL."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DEDUZIR_IRPJ_CSLL"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P2_4"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Credito parcialmente dedutivel</description>
        <inputEntry id="InputEntry_P3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>"COMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>"PARCIALMENTE_DEDUTIVEL"</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"Credito parcialmente dedutivel. Segregar parcela dedutivel e registrar diferenca como indedutivel no LALUR."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"DEDUZIR_IRPJ_CSLL"</text></outputEntry>
<outputEntry id="Rule_Aprovado_3_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_P3_4"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_RECV_WRITEOFF_002">
        <description>Caso nao atenda criterios automaticos - Consultar area tributaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Credito requer analise da area tributaria. Verificar enquadramento legal e documentacao de suporte."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"CONSULTAR_TRIBUTARIO"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_WRITEOFF_002_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_F_4"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_RECV_WRITEOFF_002">
      <dmndi:DMNShape id="DMNShape_Decision_RECV_WRITEOFF_002" dmnElementRef="Decision_RECV_WRITEOFF_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>