<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  RECV-REWORK-004 - Roteamento de Rework Multiplo
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: RECV-REWORK
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_RECV_REWORK_004" name="Roteamento de Rework Multiplo" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_RECV_REWORK_004" name="Roteamento de Correcao Multipla">
    <decisionTable id="DecisionTable_RECV_REWORK_004" hitPolicy="FIRST">

      
      <input id="Input_1" label="Tipo Correcao">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoCorrecao</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"DOCUMENTACAO", "CODIFICACAO", "JUSTIFICATIVA_CLINICA", "MULTIPLO"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Complexidade Correcao">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>complexidadeCorrecao</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"SIMPLES", "MEDIA", "COMPLEXA"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Tem Modelo Correcao">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>temModeloCorrecao</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Prazo Reapresentacao (dias)">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>prazoReapresentacao</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Esta regra so trata correcoes multiplas</description>
        <inputEntry id="InputEntry_C1_1"><text>not("MULTIPLO")</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Tipo de correcao nao e multiplo. Encaminhar para regra especifica."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Prazo de reapresentacao expirado</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>&lt;= 0</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Prazo de reapresentacao expirado."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"PROVISIONAR"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_3">
        <description>Correcao multipla complexa sem prazo viavel</description>
        <inputEntry id="InputEntry_C3_1"><text>"MULTIPLO"</text></inputEntry>
        <inputEntry id="InputEntry_C3_2"><text>"COMPLEXA"</text></inputEntry>
        <inputEntry id="InputEntry_C3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C3_4"><text>&lt; 20</text></inputEntry>
<outputEntry id="OutputEntry_C3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C3_2"><text>"Prazo insuficiente para correcao multipla complexa. Considerar aceitar glosa."</text></outputEntry>
<outputEntry id="OutputEntry_C3_3"><text>"ACEITAR"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Contraindicacao_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Correcao multipla simples com modelos disponiveis</description>
        <inputEntry id="InputEntry_A1_1"><text>"MULTIPLO"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"SIMPLES"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>&gt;= 20</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Correcao multipla simples. Iniciar fluxo paralelo: Faturamento + Codificacao. SLA total: 7 dias uteis."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Correcao multipla media complexidade</description>
        <inputEntry id="InputEntry_A2_1"><text>"MULTIPLO"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"MEDIA"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>&gt;= 25</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Correcao multipla media. Fluxo sequencial: Documentacao -&gt; Codificacao -&gt; Validacao. SLA: 12 dias uteis."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Correcao multipla complexa com prazo adequado</description>
        <inputEntry id="InputEntry_A3_1"><text>"MULTIPLO"</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>"COMPLEXA"</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_4"><text>&gt;= 40</text></inputEntry>
<outputEntry id="OutputEntry_A3_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A3_2"><text>"Correcao multipla complexa. Requer coordenacao entre equipes. Designar gestor de caso. SLA: 20 dias uteis."</text></outputEntry>
<outputEntry id="OutputEntry_A3_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_3_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_4">
        <description>Correcao multipla media com prazo apertado</description>
        <inputEntry id="InputEntry_A4_1"><text>"MULTIPLO"</text></inputEntry>
        <inputEntry id="InputEntry_A4_2"><text>"MEDIA"</text></inputEntry>
        <inputEntry id="InputEntry_A4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A4_4"><text>[15..25[</text></inputEntry>
<outputEntry id="OutputEntry_A4_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A4_2"><text>"URGENTE: Correcao multipla com prazo apertado. Priorizar e paralelizar etapas onde possivel."</text></outputEntry>
<outputEntry id="OutputEntry_A4_3"><text>"RECORRER"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_4_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Aprovado_4_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_RECV_REWORK_004">
        <description>Caso padrao - Avaliar viabilidade</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Avaliar viabilidade da correcao multipla. Considerar custo x beneficio e prazo disponivel."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"SOLICITAR_PARECER"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_RECV_REWORK_004_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_RECV_REWORK_004_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_RECV_REWORK_004">
      <dmndi:DMNShape id="DMNShape_Decision_RECV_REWORK_004" dmnElementRef="Decision_RECV_REWORK_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>