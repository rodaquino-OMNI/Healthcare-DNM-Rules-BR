<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  RECV-NEGO-005 - Baixa Parcial
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: RECV-NEGO
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_RECV_NEGO_005" name="Baixa Parcial" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_RECV_NEGO_005" name="Avaliacao Baixa Parcial">
    <decisionTable id="DecisionTable_RECV_NEGO_005" hitPolicy="FIRST">

      
      <input id="Input_1" label="Valor Total">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>valorTotal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Valor Recebido">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>valorRecebido</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Saldo Remanescente">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>saldoRemanescente</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Garantia Saldo">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>garantiaSaldo</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Status Operadora">
        <inputExpression id="InputExpression_5" typeRef="string">
          <text>statusOperadora</text>
        </inputExpression>
        <inputValues id="InputValues_5">
          <text>"ATIVA", "INTERVENCAO", "LIQUIDACAO", "FALENCIA"</text>
        </inputValues>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Recebimento inferior a 50% sem garantia para saldo</description>
        <inputEntry id="InputEntry_C1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>"ATIVA", "INTERVENCAO"</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Baixa parcial sem garantia para saldo remanescente nao permitida. Exigir garantia ou pagamento integral."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"EXIGIR_GARANTIA"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Operadora em liquidacao - provisionar 100%</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>"LIQUIDACAO", "FALENCIA"</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Operadora em liquidacao/falencia. Aceitar qualquer valor e provisionar 100% do saldo como perda provavel (CPC 25)."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"PROVISIONAR_SALDO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Recebimento entre 50% e 70% requer analise</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>"INTERVENCAO"</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Operadora em intervencao. Aceitar baixa parcial com garantia e provisionar 50% do saldo."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"PROVISIONAR_SALDO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Recebimento de pelo menos 70% com operadora ativa</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>"ATIVA"</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Baixa parcial aprovada com garantia para saldo. Operadora ativa - risco controlado."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"ACEITAR_PARCIAL"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_RECV_NEGO_005">
        <description>Caso nao atenda criterios automaticos - Revisar</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Proposta de baixa parcial requer analise manual. Avaliar valores, garantias e status da operadora."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"EXIGIR_GARANTIA"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_NEGO_005_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_NEGO_005_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_RECV_NEGO_005">
      <dmndi:DMNShape id="DMNShape_Decision_RECV_NEGO_005" dmnElementRef="Decision_RECV_NEGO_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>