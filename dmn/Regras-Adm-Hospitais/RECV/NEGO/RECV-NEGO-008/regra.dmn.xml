<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  RECV-NEGO-008 - Acordo Bilateral
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: RECV-NEGO
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_RECV_NEGO_008" name="Acordo Bilateral" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_RECV_NEGO_008" name="Avaliacao Acordo Bilateral">
    <decisionTable id="DecisionTable_RECV_NEGO_008" hitPolicy="FIRST">

      
      <input id="Input_1" label="Parte Credora Formalizada">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>parteCredora</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"FORMALIZADA", "PARCIAL", "NAO_FORMALIZADA"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Parte Devedora Formalizada">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>parteDevedora</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"FORMALIZADA", "PARCIAL", "NAO_FORMALIZADA"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Mediador Neutro">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>mediadorNeutro</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"PRESENTE", "AUSENTE", "NAO_NECESSARIO"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Termos do Acordo">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>termosAcordo</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"CLAROS", "AMBIGUOS", "INCOMPLETOS"</text>
        </inputValues>
      </input>

      <input id="Input_5" label="Valor do Acordo">
        <inputExpression id="InputExpression_5" typeRef="number">
          <text>valorAcordo</text>
        </inputExpression>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Uma ou ambas as partes sem formalizacao</description>
        <inputEntry id="InputEntry_C1_1"><text>"NAO_FORMALIZADA"</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Parte credora (hospital) sem formalizacao adequada. CC Art. 104 exige capacidade e forma prescrita."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"COMPLETAR_FORMALIZACAO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Parte devedora sem formalizacao</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>"NAO_FORMALIZADA"</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Parte devedora (operadora) sem formalizacao adequada. Acordo nao pode ser formalizado."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"COMPLETAR_FORMALIZACAO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Formalizacao parcial de uma das partes</description>
        <inputEntry id="InputEntry_A1_1"><text>"PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Formalizacao parcial do hospital. Completar documentacao antes de prosseguir com acordo."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"COMPLETAR_FORMALIZACAO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_2">
        <description>Formalizacao parcial da operadora</description>
        <inputEntry id="InputEntry_A2_1"><text>"FORMALIZADA"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Formalizacao parcial da operadora. Exigir completude antes de assinar acordo."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"COMPLETAR_FORMALIZACAO"</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Ambas partes formalizadas com termos claros</description>
        <inputEntry id="InputEntry_P1_1"><text>"FORMALIZADA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"FORMALIZADA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>"CLAROS"</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Acordo bilateral aprovado. Ambas as partes formalizadas e termos claros. CC Art. 840 atendido."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"APROVAR_ACORDO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_RECV_NEGO_008">
        <description>Caso nao atenda criterios automaticos - Revisar</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Acordo bilateral requer analise manual. Verificar formalizacao, termos e representacao das partes."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"INCLUIR_MEDIADOR"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_NEGO_008_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_NEGO_008_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_RECV_NEGO_008">
      <dmndi:DMNShape id="DMNShape_Decision_RECV_NEGO_008" dmnElementRef="Decision_RECV_NEGO_008">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>