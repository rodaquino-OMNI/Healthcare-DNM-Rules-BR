<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  RECV-NEGO-004 - Acordo Judicial
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: RECV-NEGO
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_RECV_NEGO_004" name="Acordo Judicial" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_RECV_NEGO_004" name="Avaliacao Acordo Judicial">
    <decisionTable id="DecisionTable_RECV_NEGO_004" hitPolicy="FIRST">

      
      <input id="Input_1" label="Numero Processo">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>processoNumero</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Valor Acao">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>valorAcao</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Proposta Conciliacao (%)">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>propostaConciliacao</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Probabilidade Exito">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>probabilidadeExito</text>
        </inputExpression>
        <inputValues id="InputValues_4">
          <text>"ALTA", "MEDIA", "BAIXA", "INCERTA"</text>
        </inputValues>
      </input>

      <input id="Input_5" label="Fase Processual">
        <inputExpression id="InputExpression_5" typeRef="string">
          <text>faseProcessual</text>
        </inputExpression>
        <inputValues id="InputValues_5">
          <text>"INICIAL", "INSTRUCAO", "SENTENCA", "RECURSO", "EXECUCAO"</text>
        </inputValues>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Proposta inferior a 40% do valor da acao</description>
        <inputEntry id="InputEntry_C1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>&lt; 40</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>"ALTA", "MEDIA"</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Proposta de acordo inferior a 40% do valor da acao com probabilidade de exito favoravel. Rejeitar e prosseguir com processo."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"REJEITAR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Alta probabilidade de exito com proposta abaixo de 70%</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>&gt; 50000</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>&lt; 70</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>"ALTA"</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>"SENTENCA", "RECURSO", "EXECUCAO"</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Processo em fase avancada com alta probabilidade de exito. Aguardar desfecho processual."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"REJEITAR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Proposta entre 40% e 60% requer analise</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>[40..60]</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Proposta de acordo entre 40% e 60% requer analise do departamento juridico. Avaliar custas e riscos."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"CONSULTAR_JURIDICO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Proposta acima de 70% em processo inicial</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>&gt;= 70</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>"MEDIA", "BAIXA", "INCERTA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>"INICIAL", "INSTRUCAO"</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Proposta de pelo menos 70% em fase inicial do processo. Acordo recomendado para evitar custas."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"ACEITAR_ACORDO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Proposta acima de 60% com baixa probabilidade de exito</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>&gt;= 60</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>"BAIXA"</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Proposta de pelo menos 60% com baixa probabilidade de exito no processo. Acordo recomendado."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"ACEITAR_ACORDO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_RECV_NEGO_004">
        <description>Caso nao atenda criterios automaticos - Consultar juridico</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Proposta de acordo judicial requer analise do departamento juridico. Avaliar merito, custas e precedentes."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"CONSULTAR_JURIDICO"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_NEGO_004_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_NEGO_004_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_RECV_NEGO_004">
      <dmndi:DMNShape id="DMNShape_Decision_RECV_NEGO_004" dmnElementRef="Decision_RECV_NEGO_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>