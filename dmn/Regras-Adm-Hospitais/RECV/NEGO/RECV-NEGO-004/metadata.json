{
  "ruleCode": "RECV-NEGO-004",
  "ruleName": "Acordo Judicial",
  "category": "RECV",
  "subcategory": "NEGO",
  "specialty": "Juridico",
  "version": "1.0.0",
  "status": "active",
  "hitPolicy": "FIRST",
  "perspective": "HOSPITAL",
  "createdAt": "2026-02-06T00:00:00Z",
  "updatedAt": "2026-02-06T00:00:00Z",
  "inputs": [
    {
      "name": "processoNumero",
      "type": "string",
      "label": "Numero do Processo",
      "required": true,
      "description": "Numero CNJ do processo judicial"
    },
    {
      "name": "valorAcao",
      "type": "number",
      "label": "Valor da Acao (R$)",
      "required": true,
      "description": "Valor total da causa",
      "validation": {"min": 0}
    },
    {
      "name": "propostaConciliacao",
      "type": "number",
      "label": "Proposta de Conciliacao (%)",
      "required": true,
      "description": "Percentual do valor oferecido em acordo",
      "validation": {"min": 0, "max": 100}
    },
    {
      "name": "probabilidadeExito",
      "type": "string",
      "label": "Probabilidade de Exito",
      "required": true,
      "allowedValues": ["ALTA", "MEDIA", "BAIXA", "INCERTA"],
      "description": "Avaliacao juridica da probabilidade de exito"
    },
    {
      "name": "faseProcessual",
      "type": "string",
      "label": "Fase Processual",
      "required": true,
      "allowedValues": ["INICIAL", "INSTRUCAO", "SENTENCA", "RECURSO", "EXECUCAO"],
      "description": "Fase atual do processo judicial"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "label": "Resultado",
      "allowedValues": ["Prosseguir", "Bloquear", "Alertar", "Revisar"],
      "description": "Decisao sobre aceitacao do acordo"
    },
    {
      "name": "observacao",
      "type": "string",
      "label": "Observacao",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "acaoRecomendada",
      "type": "string",
      "label": "Acao Recomendada",
      "allowedValues": ["ACEITAR_ACORDO", "REJEITAR", "CONTRAPROPOSTA", "CONSULTAR_JURIDICO"],
      "description": "Acao recomendada para tratamento"
    },
    {
      "name": "propostaMinimaSugerida",
      "type": "number",
      "label": "Proposta Minima Sugerida (%)",
      "description": "Percentual minimo aceitavel para acordo"
    }
  ],
  "indications": [
    "Proposta de pelo menos 60% do valor da acao",
    "Processo com risco de prescricao ou perecimento",
    "Custas processuais elevadas vs valor em discussao",
    "Operadora em recuperacao judicial"
  ],
  "contraindications": [
    "Proposta inferior a 40% do valor da acao",
    "Processo com alta probabilidade de exito total",
    "Tese juridica consolidada favoravel ao hospital"
  ],
  "references": [
    "CPC Art. 334 - Audiencia de conciliacao",
    "CPC Art. 487 - Resolucao do merito",
    "Lei 9656/1998 - Planos de saude"
  ],
  "tags": ["negociacao", "acordo", "judicial", "conciliacao", "processo"],
  "compliance": {
    "regulations": ["CPC", "Lei 9656/1998"],
    "auditRequired": true,
    "retentionPeriodDays": 3650
  },
  "metrics": {
    "expectedSuccessRate": 0.65,
    "avgProcessingTimeDays": 7
  },
  "changelog": [
    {
      "version": "1.0.0",
      "date": "2026-02-06",
      "changes": "Versao inicial - Regra de acordo judicial",
      "author": "Sistema"
    }
  ]
}
