<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  RECV-NEGO-007 - Reestruturacao Contrato
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: RECV-NEGO
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_RECV_NEGO_007" name="Reestruturacao Contrato" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_RECV_NEGO_007" name="Avaliacao Reestruturacao Contratual">
    <decisionTable id="DecisionTable_RECV_NEGO_007" hitPolicy="FIRST">

      
      <input id="Input_1" label="Contrato Original">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>contratoOriginal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Termo Aditivo">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>termoAditivo</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"FORMALIZADO", "PENDENTE", "INEXISTENTE"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Novas Condicoes">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>novasCondicoes</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"BENEFICAS", "NEUTRAS", "PREJUDICIAIS"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Representacao Legal">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>representacaoLegal</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Status Regulatorio">
        <inputExpression id="InputExpression_5" typeRef="string">
          <text>statusRegulatorio</text>
        </inputExpression>
        <inputValues id="InputValues_5">
          <text>"REGULAR", "ALERTA", "INTERVENCAO", "SUSPENSA"</text>
        </inputValues>
      </input>

      
      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Contraindicacao_1">
        <description>Ausencia de termo aditivo formal</description>
        <inputEntry id="InputEntry_C1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_2"><text>"INEXISTENTE"</text></inputEntry>
        <inputEntry id="InputEntry_C1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_C1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C1_2"><text>"Reestruturacao contratual sem termo aditivo formal nao permitida. CC Art. 472 exige forma escrita para modificacao."</text></outputEntry>
<outputEntry id="OutputEntry_C1_3"><text>"EXIGIR_FORMALIZACAO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Contraindicacao_2">
        <description>Operadora em intervencao ou suspensa</description>
        <inputEntry id="InputEntry_C2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_C2_5"><text>"INTERVENCAO", "SUSPENSA"</text></inputEntry>
<outputEntry id="OutputEntry_C2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_C2_2"><text>"Operadora em intervencao ou suspensa. Reestruturacao depende de autorizacao da ANS."</text></outputEntry>
<outputEntry id="OutputEntry_C2_3"><text>"CONSULTAR_JURIDICO"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Contraindicacao_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Termo aditivo pendente de formalizacao</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Termo aditivo pendente de formalizacao. Definir prazo para conclusao antes de prosseguir."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"EXIGIR_FORMALIZACAO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"VALOR"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Reestruturacao formalizada com condicoes beneficas</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"FORMALIZADO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"BENEFICAS"</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>"REGULAR"</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Reestruturacao contratual aprovada. Termo aditivo formalizado com condicoes beneficas ao hospital."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"APROVAR_REESTRUTURACAO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Reestruturacao formalizada com condicoes neutras</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"FORMALIZADO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"NEUTRAS"</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>"REGULAR", "ALERTA"</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Reestruturacao contratual aprovada. Termo aditivo formalizado com condicoes neutras."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"APROVAR_REESTRUTURACAO"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Aprovado_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_RECV_NEGO_007">
        <description>Caso nao atenda criterios automaticos - Consultar juridico</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Solicitacao de reestruturacao requer analise juridica. Verificar termos, condicoes e representacao."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"CONSULTAR_JURIDICO"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_NEGO_007_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_RECV_NEGO_007_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_RECV_NEGO_007">
      <dmndi:DMNShape id="DMNShape_Decision_RECV_NEGO_007" dmnElementRef="Decision_RECV_NEGO_007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>