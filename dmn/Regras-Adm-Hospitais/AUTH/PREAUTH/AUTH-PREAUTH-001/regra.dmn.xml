<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-PREAUTH-001 - Verificacao Existencia Autorizacao Previa
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-PREAUTH
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_PREAUTH_001" name="Verificacao Existencia Autorizacao Previa" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_PREAUTH_001" name="Verificar Existencia de Autorizacao Previa">
    <decisionTable id="DecisionTable_AUTH_PREAUTH_001" hitPolicy="FIRST">

      <input id="Input_1" label="Tem Autorizacao Previa">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>temAutorizacaoPrevia</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Numero Guia">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>numeroGuia</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tipo Guia">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tipoGuia</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"SADT", "INTERNACAO", "CONSULTA", "HONORARIOS"</text>
        </inputValues>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Procedimento sem autorizacao previa registrada</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Procedimento nao possui autorizacao previa. Faturamento bloqueado ate regularizacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar autorizacao junto a operadora antes de prosseguir com faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Autorizacao existente mas numero da guia nao informado</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>null</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Numero da guia de autorizacao nao informado. Obrigatorio para faturamento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Inserir numero da guia de autorizacao no sistema."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Autorizacao valida com guia SADT</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"SADT"</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Autorizacao previa valida. Guia SADT presente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com faturamento conforme guia autorizada."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Autorizacao valida com guia de internacao</description>
        <inputEntry id="InputEntry_A2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>"INTERNACAO"</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Autorizacao previa valida. Guia de Internacao presente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com faturamento de internacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Autorizacao valida com outros tipos de guia</description>
        <inputEntry id="InputEntry_A3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_2"><text>"Autorizacao previa valida. Numero de guia presente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_3"><text>"Prosseguir com faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_PREAUTH_001">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Situacao de autorizacao requer analise manual."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria verificar status de autorizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_PREAUTH_001_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_PREAUTH_001_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_PREAUTH_001">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_PREAUTH_001" dmnElementRef="Decision_AUTH_PREAUTH_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>