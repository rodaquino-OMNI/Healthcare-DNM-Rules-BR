<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-PREAUTH-004 - Validacao Quantidade Autorizada
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-PREAUTH
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_PREAUTH_004" name="Validacao Quantidade Autorizada" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_PREAUTH_004" name="Validar Quantidade Autorizada">
    <decisionTable id="DecisionTable_AUTH_PREAUTH_004" hitPolicy="FIRST">

      <input id="Input_1" label="Quantidade Autorizada">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>quantidadeAutorizada</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Quantidade Realizada">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>quantidadeRealizada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Quantidade Ja Faturada">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>quantidadeJaFaturada</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Saldo Disponivel">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>saldoDisponivel</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Saldo de autorizacao zerado</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>&lt;= 0</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Saldo de autorizacao esgotado. Quantidade autorizada ja utilizada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar nova autorizacao com quantidade adicional."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Quantidade realizada excede saldo disponivel</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Quantidade realizada excede o saldo disponivel na autorizacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Ajustar quantidade ou solicitar autorizacao complementar."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Saldo de autorizacao ficara baixo apos faturamento</description>
        <inputEntry id="InputEntry_AL1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>[1..2]</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Saldo de autorizacao baixo. Considerar solicitar complemento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Verificar necessidade de autorizacao complementar."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Saldo de autorizacao suficiente</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>&gt; 2</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Quantidade dentro do limite autorizado. Saldo disponivel."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_PREAUTH_004">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Nao foi possivel validar quantidade autorizada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar quantidades na guia e no lancamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_PREAUTH_004_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_PREAUTH_004_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_PREAUTH_004">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_PREAUTH_004" dmnElementRef="Decision_AUTH_PREAUTH_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>