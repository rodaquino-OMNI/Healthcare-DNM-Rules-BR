{
  "ruleId": "AUTH-PREAUTH-004",
  "ruleName": "Validacao Quantidade Autorizada",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "PREAUTH",
  "perspective": "HOSPITAL",
  "description": "Verifica se a quantidade realizada esta dentro do limite autorizado",
  "inputs": [
    {
      "name": "quantidadeAutorizada",
      "type": "number",
      "description": "Quantidade total autorizada na guia"
    },
    {
      "name": "quantidadeRealizada",
      "type": "number",
      "description": "Quantidade do procedimento realizada"
    },
    {
      "name": "quantidadeJaFaturada",
      "type": "number",
      "description": "Quantidade ja faturada anteriormente"
    },
    {
      "name": "saldoDisponivel",
      "type": "number",
      "description": "Saldo disponivel na autorizacao"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "Manual TISS 4.0",
    "RN 465/2021"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
