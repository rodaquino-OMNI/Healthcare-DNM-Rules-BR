<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-PREAUTH-003 - Verificacao Compatibilidade Guia e Procedimento
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-PREAUTH
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_PREAUTH_003" name="Verificacao Compatibilidade Guia e Procedimento" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_PREAUTH_003" name="Verificar Compatibilidade Guia-Procedimento">
    <decisionTable id="DecisionTable_AUTH_PREAUTH_003" hitPolicy="FIRST">

      <input id="Input_1" label="Numero Guia">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>numeroGuia</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Codigo TUSS Autorizado">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>codigoTUSSAutorizado</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Codigo TUSS Realizado">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>codigoTUSSRealizado</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Codigos Compativeis">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>codigosCompativeis</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Numero da guia nao informado</description>
        <inputEntry id="InputEntry_B1_1"><text>null</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Numero da guia de autorizacao nao informado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Inserir numero da guia antes de prosseguir."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Codigo TUSS realizado diferente do autorizado</description>
        <inputEntry id="InputEntry_B2_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Procedimento realizado nao corresponde ao autorizado na guia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Solicitar nova autorizacao para o procedimento realizado ou corrigir lancamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Codigo TUSS realizado compativel com autorizado</description>
        <inputEntry id="InputEntry_A1_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Procedimento realizado corresponde ao autorizado na guia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_PREAUTH_003">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Nao foi possivel validar compatibilidade guia-procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar manualmente codigos na guia e no lancamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_PREAUTH_003_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_PREAUTH_003_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_PREAUTH_003">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_PREAUTH_003" dmnElementRef="Decision_AUTH_PREAUTH_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>