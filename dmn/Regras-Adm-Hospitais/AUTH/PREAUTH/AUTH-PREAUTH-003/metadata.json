{
  "ruleId": "AUTH-PREAUTH-003",
  "ruleName": "Verificacao Compatibilidade Guia e Procedimento",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "PREAUTH",
  "perspective": "HOSPITAL",
  "description": "Verifica se o numero da guia corresponde ao procedimento realizado",
  "inputs": [
    {
      "name": "numeroGuia",
      "type": "string",
      "description": "Numero da guia de autorizacao"
    },
    {
      "name": "codigoTUSSAutorizado",
      "type": "string",
      "description": "Codigo TUSS do procedimento autorizado"
    },
    {
      "name": "codigoTUSSRealizado",
      "type": "string",
      "description": "Codigo TUSS do procedimento efetivamente realizado"
    },
    {
      "name": "codigosCompativeis",
      "type": "boolean",
      "description": "Indica se os codigos sao compativeis"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "Manual TISS 4.0",
    "RN 465/2021"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
