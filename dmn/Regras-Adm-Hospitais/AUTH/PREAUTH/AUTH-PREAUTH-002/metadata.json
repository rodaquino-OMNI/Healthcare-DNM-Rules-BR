{
  "ruleId": "AUTH-PREAUTH-002",
  "ruleName": "Validacao Vigencia da Autorizacao",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "PREAUTH",
  "perspective": "HOSPITAL",
  "description": "Valida se a autorizacao esta dentro do prazo de validade",
  "inputs": [
    {
      "name": "dataAutorizacao",
      "type": "date",
      "description": "Data em que a autorizacao foi emitida"
    },
    {
      "name": "dataValidade",
      "type": "date",
      "description": "Data de vencimento da autorizacao"
    },
    {
      "name": "diasAteVencimento",
      "type": "number",
      "description": "Numero de dias ate o vencimento da autorizacao"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "RN 259/2011 ANS",
    "RN 424/2017"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
