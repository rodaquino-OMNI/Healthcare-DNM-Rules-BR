<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-PREAUTH-002 - Validacao Vigencia da Autorizacao
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-PREAUTH
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_PREAUTH_002" name="Validacao Vigencia da Autorizacao" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_PREAUTH_002" name="Validar Vigencia da Autorizacao">
    <decisionTable id="DecisionTable_AUTH_PREAUTH_002" hitPolicy="FIRST">

      <input id="Input_1" label="Data Autorizacao">
        <inputExpression id="InputExpression_1" typeRef="date">
          <text>dataAutorizacao</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Data Validade">
        <inputExpression id="InputExpression_2" typeRef="date">
          <text>dataValidade</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dias Ate Vencimento">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>diasAteVencimento</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Autorizacao com data de validade expirada</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>&lt; 0</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Autorizacao expirada. Data de validade ultrapassada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar nova autorizacao junto a operadora."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Autorizacao proxima do vencimento (7 dias)</description>
        <inputEntry id="InputEntry_AL1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>[0..7]</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Autorizacao vencendo em breve. Priorizar realizacao do procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Agendar procedimento com urgencia ou solicitar extensao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_2">
        <description>Autorizacao vencendo em 15 dias</description>
        <inputEntry id="InputEntry_AL2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL2_3"><text>]7..15]</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_2"><text>"Autorizacao vence em 15 dias. Monitorar agendamento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_3"><text>"Verificar agenda para realizacao do procedimento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Autorizacao com mais de 15 dias de validade</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&gt; 15</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Autorizacao dentro do prazo de validade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com agendamento normal."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_PREAUTH_002">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Nao foi possivel determinar vigencia da autorizacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar datas de autorizacao e validade manualmente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_PREAUTH_002_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_PREAUTH_002_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_PREAUTH_002">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_PREAUTH_002" dmnElementRef="Decision_AUTH_PREAUTH_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>