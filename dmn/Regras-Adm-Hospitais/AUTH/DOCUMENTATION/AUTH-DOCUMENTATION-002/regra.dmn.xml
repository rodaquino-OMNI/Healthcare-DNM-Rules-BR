<!--
  ============================================================================
  AUTH-DOCUMENTATION-002 - Verificacao Exames Pre-Requisito
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-DOCUMENTATION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_DOCUMENTATION_002" name="Verificacao Exames Pre-Requisito" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_DOCUMENTATION_002" name="Verificar Exames Pre-Requisito">
    <decisionTable id="DecisionTable_AUTH_DOCUMENTATION_002" hitPolicy="FIRST">

      <input id="Input_1" label="Tem Exame Pre-Requisito">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>temExamePreRequisito</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Exames Validos">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>examesValidos</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dias Desde Exame">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>diasDesdeExame</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Prazo Validade Exame">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>prazoValidadeExame</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Exames pre-requisito obrigatorios ausentes</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Exames pre-requisito obrigatorios nao anexados."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Realizar e anexar exames pre-requisito antes da solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Exames pre-requisito com validade expirada</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Exames pre-requisito com validade expirada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Repetir exames pre-requisito e anexar resultados atualizados."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Exames proximos do prazo de validade</description>
        <inputEntry id="InputEntry_AL1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>[1..7]</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Exames pre-requisito proximos da data de vencimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Priorizar solicitacao de autorizacao antes do vencimento dos exames."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Exames pre-requisito validos e anexados</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>&gt; 7</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Exames pre-requisito validos e anexados."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com solicitacao de autorizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_DOCUMENTATION_002">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar requisitos de exames pre-requisito."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Consultar protocolo clinico para exames requeridos."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_002_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_002_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_DOCUMENTATION_002">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_DOCUMENTATION_002" dmnElementRef="Decision_AUTH_DOCUMENTATION_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>