<!--
  ============================================================================
  AUTH-DOCUMENTATION-001 - Verificacao Laudo Medico Anexado
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-DOCUMENTATION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_DOCUMENTATION_001" name="Verificacao Laudo Medico Anexado" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_DOCUMENTATION_001" name="Verificar Laudo Medico Anexado">
    <decisionTable id="DecisionTable_AUTH_DOCUMENTATION_001" hitPolicy="FIRST">

      <input id="Input_1" label="Tem Laudo Medico">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>temLaudoMedico</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Requer Laudo">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>requerLaudo</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tipo Procedimento">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tipoProcedimento</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"CIRURGIA", "EXAME", "TERAPIA", "INTERNACAO", "CONSULTA"</text>
        </inputValues>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Cirurgia requer laudo e nao esta anexado</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>"CIRURGIA"</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Laudo medico obrigatorio para cirurgia nao anexado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Anexar laudo medico completo antes de prosseguir."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Internacao requer laudo e nao esta anexado</description>
        <inputEntry id="InputEntry_B2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>"INTERNACAO"</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Laudo medico obrigatorio para internacao nao anexado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Anexar laudo medico justificando internacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Exame requer laudo e nao esta anexado</description>
        <inputEntry id="InputEntry_AL1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>"EXAME"</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Laudo medico solicitado para exame nao anexado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Anexar laudo medico ou justificativa para exame."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Laudo medico anexado conforme requerido</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Laudo medico anexado conforme requerido."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com solicitacao de autorizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Procedimento nao requer laudo medico</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Procedimento nao requer laudo medico."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_DOCUMENTATION_001">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar necessidade de laudo medico."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Consultar requisitos de documentacao do procedimento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_001_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_001_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_DOCUMENTATION_001">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_DOCUMENTATION_001" dmnElementRef="Decision_AUTH_DOCUMENTATION_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>