{
  "ruleId": "AUTH-DOCUMENTATION-005",
  "ruleName": "Verificacao Documentacao Suporte Completa",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "DOCUMENTATION",
  "perspective": "HOSPITAL",
  "description": "Verifica se toda a documentacao de suporte esta completa",
  "inputs": [
    {
      "name": "temLaudoMedico",
      "type": "boolean",
      "description": "Indica se laudo medico esta anexado"
    },
    {
      "name": "temExamePreRequisito",
      "type": "boolean",
      "description": "Indica se exames pre-requisito estao anexados"
    },
    {
      "name": "temJustificativaMedica",
      "type": "boolean",
      "description": "Indica se justificativa medica esta presente"
    },
    {
      "name": "temCIDConfirmado",
      "type": "boolean",
      "description": "Indica se CID esta confirmado"
    },
    {
      "name": "documentosObrigatoriosFaltantes",
      "type": "number",
      "description": "Numero de documentos obrigatorios faltantes"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "RN 465/2021 ANS",
    "Manual TISS 4.0"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
