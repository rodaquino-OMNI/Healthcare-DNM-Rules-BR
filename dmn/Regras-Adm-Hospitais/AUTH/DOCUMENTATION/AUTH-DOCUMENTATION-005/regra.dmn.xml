<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-DOCUMENTATION-005 - Verificacao Documentacao Suporte Completa
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-DOCUMENTATION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_DOCUMENTATION_005" name="Verificacao Documentacao Suporte Completa" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_DOCUMENTATION_005" name="Verificar Documentacao Suporte Completa">
    <decisionTable id="DecisionTable_AUTH_DOCUMENTATION_005" hitPolicy="FIRST">

      <input id="Input_1" label="Tem Laudo Medico">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>temLaudoMedico</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tem Exame Pre-Requisito">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>temExamePreRequisito</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tem Justificativa Medica">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>temJustificativaMedica</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Tem CID Confirmado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>temCIDConfirmado</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Documentos Obrigatorios Faltantes">
        <inputExpression id="InputExpression_5" typeRef="number">
          <text>documentosObrigatoriosFaltantes</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Multiplos documentos obrigatorios faltantes</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>&gt;= 3</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Documentacao muito incompleta. Multiplos documentos obrigatorios faltantes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Completar toda a documentacao antes de solicitar autorizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Sem laudo medico e sem justificativa</description>
        <inputEntry id="InputEntry_B2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Laudo medico e justificativa ausentes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Anexar laudo medico e/ou justificativa clinica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Alguns documentos obrigatorios faltantes</description>
        <inputEntry id="InputEntry_AL1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_5"><text>[1..2]</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Documentacao parcialmente incompleta. Verificar documentos faltantes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Completar documentos faltantes antes de submeter."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Toda documentacao de suporte completa</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>0</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Documentacao de suporte completa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com solicitacao de autorizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Nenhum documento obrigatorio faltante</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>0</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Todos os documentos obrigatorios presentes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_DOCUMENTATION_005">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar checklist de documentacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Revisar todos os documentos de suporte necessarios."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_005_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_005_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_DOCUMENTATION_005">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_DOCUMENTATION_005" dmnElementRef="Decision_AUTH_DOCUMENTATION_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>