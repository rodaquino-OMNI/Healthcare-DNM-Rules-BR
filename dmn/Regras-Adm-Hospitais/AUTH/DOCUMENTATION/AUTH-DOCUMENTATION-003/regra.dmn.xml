<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-DOCUMENTATION-003 - Verificacao Justificativa Medica Completa
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-DOCUMENTATION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_DOCUMENTATION_003" name="Verificacao Justificativa Medica Completa" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_DOCUMENTATION_003" name="Verificar Justificativa Medica Completa">
    <decisionTable id="DecisionTable_AUTH_DOCUMENTATION_003" hitPolicy="FIRST">

      <input id="Input_1" label="Tem Justificativa Medica">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>temJustificativaMedica</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Justificativa Completa">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>justificativaCompleta</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Indicacao Clinica Clara">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>indicacaoClinicaClara</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Procedimento Alta Complexidade">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>procedimentoAltaComplexidade</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Procedimento de alta complexidade sem justificativa medica</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Justificativa medica obrigatoria para procedimento de alta complexidade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar ao medico justificativa clinica detalhada."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Justificativa incompleta para alta complexidade</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Justificativa medica incompleta para procedimento de alta complexidade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Complementar justificativa com indicacao clinica detalhada."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Indicacao clinica nao esta clara</description>
        <inputEntry id="InputEntry_AL1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Indicacao clinica nao esta clara na justificativa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Revisar e esclarecer indicacao clinica na justificativa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Justificativa medica completa e indicacao clara</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Justificativa medica completa com indicacao clinica clara."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com solicitacao de autorizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Procedimento baixa complexidade nao requer justificativa detalhada</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Procedimento de baixa complexidade. Justificativa detalhada nao obrigatoria."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_DOCUMENTATION_003">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar requisitos de justificativa medica."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Consultar DUT para requisitos de documentacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_003_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_003_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_DOCUMENTATION_003">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_DOCUMENTATION_003" dmnElementRef="Decision_AUTH_DOCUMENTATION_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>