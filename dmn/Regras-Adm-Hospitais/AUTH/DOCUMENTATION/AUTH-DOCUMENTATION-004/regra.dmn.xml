<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-DOCUMENTATION-004 - Verificacao Codigo Diagnostico Confirmado
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-DOCUMENTATION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_DOCUMENTATION_004" name="Verificacao Codigo Diagnostico Confirmado" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_DOCUMENTATION_004" name="Verificar Codigo Diagnostico Confirmado">
    <decisionTable id="DecisionTable_AUTH_DOCUMENTATION_004" hitPolicy="FIRST">

      <input id="Input_1" label="Tem CID Confirmado">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>temCIDConfirmado</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="CID Valido">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>cidValido</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="CID Compativel Procedimento">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>cidCompativelProcedimento</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Requer CID Especifico">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>requerCIDEspecifico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>CID obrigatorio nao informado</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Codigo CID-10 obrigatorio nao informado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Inserir codigo CID-10 do diagnostico."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Codigo CID-10 invalido</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Codigo CID-10 informado e invalido ou nao existe."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Corrigir codigo CID-10 do diagnostico."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_3">
        <description>CID incompativel com procedimento solicitado</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_2"><text>"Codigo CID-10 nao e compativel com o procedimento solicitado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B3_3"><text>"Revisar CID ou justificar indicacao atipica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_3_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_3_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>CID valido e compativel com procedimento</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Codigo CID-10 valido e compativel com procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Procedimento nao requer CID especifico</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Procedimento nao requer CID especifico."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_DOCUMENTATION_004">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar codigo CID-10 do diagnostico."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Validar CID junto a equipe medica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_004_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_DOCUMENTATION_004_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_DOCUMENTATION_004">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_DOCUMENTATION_004" dmnElementRef="Decision_AUTH_DOCUMENTATION_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>