{
  "ruleId": "AUTH-URGENCY-001",
  "ruleName": "Bypass de Emergencia RN 424",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "URGENCY",
  "perspective": "HOSPITAL",
  "description": "Aplica bypass de autorizacao para emergencias conforme RN 424/2017",
  "inputs": [
    {
      "name": "caraterAtendimento",
      "type": "string",
      "description": "Carater do atendimento (ELETIVO, URGENCIA, EMERGENCIA)"
    },
    {
      "name": "riscoVida",
      "type": "boolean",
      "description": "Indica se ha risco de vida"
    },
    {
      "name": "temAutorizacaoPrevia",
      "type": "boolean",
      "description": "Indica se ja tem autorizacao previa"
    },
    {
      "name": "documentacaoEmergencia",
      "type": "boolean",
      "description": "Indica se ha documentacao de emergencia"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "RN 424/2017 ANS",
    "Lei 9656/98 Art. 35-C"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
