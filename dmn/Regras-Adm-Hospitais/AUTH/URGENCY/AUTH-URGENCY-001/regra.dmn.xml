<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-URGENCY-001 - Bypass de Emergencia RN 424
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-URGENCY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_URGENCY_001" name="Bypass de Emergencia RN 424" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_URGENCY_001" name="Aplicar Bypass de Emergencia">
    <decisionTable id="DecisionTable_AUTH_URGENCY_001" hitPolicy="FIRST">

      <input id="Input_1" label="Carater Atendimento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>caraterAtendimento</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"ELETIVO", "URGENCIA", "EMERGENCIA"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Risco de Vida">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>riscoVida</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tem Autorizacao Previa">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>temAutorizacaoPrevia</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Documentacao Emergencia">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>documentacaoEmergencia</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Aprovado_1">
        <description>Emergencia com risco de vida - bypass automatico</description>
        <inputEntry id="InputEntry_A1_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Bypass de emergencia aplicado conforme RN 424/2017. Risco de vida documentado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com atendimento. Regularizar autorizacao em ate 24h."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Emergencia com documentacao adequada</description>
        <inputEntry id="InputEntry_A2_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Bypass de emergencia aplicado. Documentacao de emergencia presente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com atendimento. Solicitar autorizacao retroativa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Emergencia sem documentacao adequada</description>
        <inputEntry id="InputEntry_AL1_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Atendimento de emergencia requer documentacao comprobatoria."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Documentar caracterizacao da emergencia para bypass."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_1">
        <description>Atendimento eletivo nao pode usar bypass de emergencia</description>
        <inputEntry id="InputEntry_B1_1"><text>"ELETIVO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Atendimento eletivo nao pode utilizar bypass de emergencia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar autorizacao previa pelo fluxo normal."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Ja possui autorizacao previa</description>
        <inputEntry id="InputEntry_A3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A3_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_2"><text>"Autorizacao previa ja existe. Bypass nao necessario."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_3"><text>"Prosseguir com fluxo normal de autorizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_URGENCY_001">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Avaliar aplicabilidade de bypass de emergencia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar carater do atendimento e documentacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_URGENCY_001_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_URGENCY_001_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_URGENCY_001">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_URGENCY_001" dmnElementRef="Decision_AUTH_URGENCY_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>