{
  "ruleId": "AUTH-URGENCY-005",
  "ruleName": "Validacao Extensao de Emergencia",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "URGENCY",
  "perspective": "HOSPITAL",
  "description": "Valida extensao de atendimento de emergencia alem das primeiras 12h",
  "inputs": [
    {
      "name": "horasPermanencia",
      "type": "number",
      "description": "Horas de permanencia no atendimento de emergencia"
    },
    {
      "name": "justificativaExtensao",
      "type": "boolean",
      "description": "Indica se ha justificativa para extensao"
    },
    {
      "name": "transicaoParaInternacao",
      "type": "boolean",
      "description": "Indica se ha transicao para internacao"
    },
    {
      "name": "autorizacaoInternacao",
      "type": "boolean",
      "description": "Indica se internacao foi autorizada"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "RN 424/2017 ANS",
    "Lei 9656/98"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
