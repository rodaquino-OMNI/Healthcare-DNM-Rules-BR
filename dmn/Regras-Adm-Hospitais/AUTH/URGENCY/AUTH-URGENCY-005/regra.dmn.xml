<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-URGENCY-005 - Validacao Extensao de Emergencia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-URGENCY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_URGENCY_005" name="Validacao Extensao de Emergencia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_URGENCY_005" name="Validar Extensao de Emergencia">
    <decisionTable id="DecisionTable_AUTH_URGENCY_005" hitPolicy="FIRST">

      <input id="Input_1" label="Horas Permanencia">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>horasPermanencia</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Justificativa Extensao">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>justificativaExtensao</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Transicao para Internacao">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>transicaoParaInternacao</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Autorizacao Internacao">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>autorizacaoInternacao</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Aprovado_1">
        <description>Dentro das primeiras 12h de observacao</description>
        <inputEntry id="InputEntry_A1_1"><text>&lt;= 12</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Dentro do periodo inicial de observacao de emergencia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com atendimento de emergencia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Extensao de emergencia com justificativa</description>
        <inputEntry id="InputEntry_A2_1"><text>&gt; 12</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Extensao de emergencia justificada clinicamente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com cobranca de observacao prolongada."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Transicao para internacao autorizada</description>
        <inputEntry id="InputEntry_A3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A3_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_2"><text>"Transicao de emergencia para internacao autorizada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_3"><text>"Prosseguir com faturamento de internacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Extensao alem de 12h sem justificativa</description>
        <inputEntry id="InputEntry_AL1_1"><text>&gt; 12</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Permanencia alem de 12h requer justificativa clinica."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Documentar justificativa para extensao de observacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_1">
        <description>Transicao para internacao sem autorizacao</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Transicao para internacao requer autorizacao especifica."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar autorizacao de internacao antes de prosseguir."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_URGENCY_005">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar necessidade de extensao de emergencia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Analisar permanencia e necessidade de autorizacao adicional."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_URGENCY_005_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_URGENCY_005_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_URGENCY_005">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_URGENCY_005" dmnElementRef="Decision_AUTH_URGENCY_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>