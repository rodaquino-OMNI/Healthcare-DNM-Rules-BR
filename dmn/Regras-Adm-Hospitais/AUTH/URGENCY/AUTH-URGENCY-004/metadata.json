{
  "ruleId": "AUTH-URGENCY-004",
  "ruleName": "Documentacao Pos-Emergencia",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "URGENCY",
  "perspective": "HOSPITAL",
  "description": "Valida documentacao pos-emergencia e prazo de regularizacao",
  "inputs": [
    {
      "name": "tipoAtendimentoOrigem",
      "type": "string",
      "description": "Tipo do atendimento de origem (EMERGENCIA, URGENCIA)"
    },
    {
      "name": "horasDesdeAtendimento",
      "type": "number",
      "description": "Horas desde o atendimento de emergencia"
    },
    {
      "name": "documentacaoCompleta",
      "type": "boolean",
      "description": "Indica se documentacao esta completa"
    },
    {
      "name": "regularizacaoSolicitada",
      "type": "boolean",
      "description": "Indica se regularizacao foi solicitada"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "RN 424/2017 ANS",
    "Manual TISS 4.0"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
