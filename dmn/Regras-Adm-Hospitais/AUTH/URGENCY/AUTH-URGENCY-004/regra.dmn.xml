  Categoria: AUTH-URGENCY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_URGENCY_004" name="Documentacao Pos-Emergencia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_URGENCY_004" name="Validar Documentacao Pos-Emergencia">
    <decisionTable id="DecisionTable_AUTH_URGENCY_004" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Atendimento Origem">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoAtendimentoOrigem</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"EMERGENCIA", "URGENCIA"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Horas Desde Atendimento">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>horasDesdeAtendimento</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Documentacao Completa">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>documentacaoCompleta</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Regularizacao Solicitada">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>regularizacaoSolicitada</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Aprovado_1">
        <description>Documentacao pos-emergencia completa e regularizada</description>
        <inputEntry id="InputEntry_A1_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Documentacao pos-emergencia completa. Regularizacao solicitada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Prazo de regularizacao proximo de expirar</description>
        <inputEntry id="InputEntry_AL1_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>[20..24]</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"URGENTE: Prazo de 24h para regularizacao proximo de expirar."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Solicitar regularizacao imediatamente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_2">
        <description>Documentacao pos-emergencia incompleta</description>
        <inputEntry id="InputEntry_AL2_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_AL2_2"><text>&lt; 20</text></inputEntry>
        <inputEntry id="InputEntry_AL2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_2"><text>"Documentacao pos-emergencia incompleta. Prazo em andamento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_3"><text>"Completar documentacao de emergencia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_1">
        <description>Prazo de regularizacao expirado</description>
        <inputEntry id="InputEntry_B1_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 24</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Prazo de 24h para regularizacao expirado sem solicitacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Justificar atraso e solicitar regularizacao extraordinaria."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_URGENCY_004">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar documentacao pos-emergencia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Consultar prontuario e documentacao do atendimento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_URGENCY_004_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_URGENCY_004_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_URGENCY_004">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_URGENCY_004" dmnElementRef="Decision_AUTH_URGENCY_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>