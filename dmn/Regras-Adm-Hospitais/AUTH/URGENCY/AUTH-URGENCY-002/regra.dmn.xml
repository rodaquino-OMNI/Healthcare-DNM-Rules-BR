<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-URGENCY-002 - Autorizacao Urgencia 24h
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-URGENCY
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_URGENCY_002" name="Autorizacao Urgencia 24h" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_URGENCY_002" name="Processar Autorizacao Urgencia 24h">
    <decisionTable id="DecisionTable_AUTH_URGENCY_002" hitPolicy="FIRST">

      <input id="Input_1" label="Carater Atendimento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>caraterAtendimento</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"ELETIVO", "URGENCIA", "EMERGENCIA"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Horas Desde Solicitacao">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>horasDesdesolicitacao</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Resposta Operadora">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>respostaOperadora</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"APROVADO", "NEGADO", "PENDENTE", "SEM_RESPOSTA"</text>
        </inputValues>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Aprovado_1">
        <description>Urgencia autorizada pela operadora</description>
        <inputEntry id="InputEntry_A1_1"><text>"URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"APROVADO"</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Urgencia autorizada pela operadora."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com atendimento e faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Sem resposta apos 24h - autorizacao tacita</description>
        <inputEntry id="InputEntry_A2_1"><text>"URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>&gt; 24</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>"SEM_RESPOSTA"</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Autorizacao tacita por ausencia de resposta em 24h (RN 259/2011)."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Documentar ausencia de resposta e prosseguir com atendimento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Aguardando resposta dentro do prazo</description>
        <inputEntry id="InputEntry_AL1_1"><text>"URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>&lt;= 24</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>"PENDENTE", "SEM_RESPOSTA"</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Aguardando resposta da operadora. Prazo de 24h em andamento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Monitorar resposta e cobrar operadora se necessario."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_1">
        <description>Urgencia negada pela operadora</description>
        <inputEntry id="InputEntry_B1_1"><text>"URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>"NEGADO"</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Urgencia negada pela operadora."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Analisar motivo e avaliar recurso ou nova solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_URGENCY_002">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar status de autorizacao de urgencia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Consultar sistema da operadora para status atualizado."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_URGENCY_002_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_URGENCY_002_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_URGENCY_002">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_URGENCY_002" dmnElementRef="Decision_AUTH_URGENCY_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>