<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-TIMING-004 - Bloqueio por Autorizacao Expirada
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-TIMING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_TIMING_004" name="Bloqueio por Autorizacao Expirada" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_TIMING_004" name="Bloquear Autorizacao Expirada">
    <decisionTable id="DecisionTable_AUTH_TIMING_004" hitPolicy="FIRST">

      <input id="Input_1" label="Dias Ate Vencimento">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasAteVencimento</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Extensao Aprovada">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>extensaoAprovada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Carater Atendimento">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>caraterAtendimento</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"ELETIVO", "URGENCIA", "EMERGENCIA"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Regularizacao Posterior">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>regularizacaoPosterior</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Aprovado_1">
        <description>Autorizacao vencida mas extensao aprovada</description>
        <inputEntry id="InputEntry_A1_1"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Autorizacao vencida mas extensao foi aprovada pela operadora."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Utilizar numero da extensao no faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Atendimento de emergencia com regularizacao posterior</description>
        <inputEntry id="InputEntry_A2_1"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Atendimento de emergencia com autorizacao regularizada posteriormente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Anexar documentacao de regularizacao ao faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_1">
        <description>Autorizacao vencida sem extensao para procedimento eletivo</description>
        <inputEntry id="InputEntry_B1_1"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>"ELETIVO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Autorizacao expirada. Procedimento eletivo nao pode ser faturado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar nova autorizacao para o procedimento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Atendimento de urgencia aguardando regularizacao</description>
        <inputEntry id="InputEntry_AL1_1"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>"URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Atendimento de urgencia com autorizacao vencida. Regularizacao necessaria."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Solicitar regularizacao retroativa junto a operadora."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Autorizacao dentro da validade</description>
        <inputEntry id="InputEntry_A3_1"><text>&gt;= 0</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_2"><text>"Autorizacao dentro do prazo de validade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_3"><text>"Prosseguir com faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_TIMING_004">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Situacao de autorizacao expirada requer analise manual."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar possibilidade de extensao ou regularizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_TIMING_004_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_TIMING_004_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_TIMING_004">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_TIMING_004" dmnElementRef="Decision_AUTH_TIMING_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>