{
  "ruleId": "AUTH-TIMING-004",
  "ruleName": "Bloqueio por Autorizacao Expirada",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "TIMING",
  "perspective": "HOSPITAL",
  "description": "Bloqueia faturamento quando autorizacao esta expirada, exceto casos de extensao ou emergencia",
  "inputs": [
    {
      "name": "diasAteVencimento",
      "type": "number",
      "description": "Numero de dias ate o vencimento (negativo = vencido)"
    },
    {
      "name": "extensaoAprovada",
      "type": "boolean",
      "description": "Indica se extensao foi aprovada"
    },
    {
      "name": "caraterAtendimento",
      "type": "string",
      "description": "Carater do atendimento (ELETIVO, URGENCIA, EMERGENCIA)"
    },
    {
      "name": "regularizacaoPosterior",
      "type": "boolean",
      "description": "Indica se houve regularizacao posterior"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "RN 259/2011 ANS",
    "RN 424/2017"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
