<!--
  ============================================================================
  AUTH-TIMING-003 - Alerta Vencimento Proximo
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-TIMING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_TIMING_003" name="Alerta Vencimento Proximo" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_TIMING_003" name="Alertar Vencimento Proximo">
    <decisionTable id="DecisionTable_AUTH_TIMING_003" hitPolicy="FIRST">

      <input id="Input_1" label="Dias Ate Vencimento">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasAteVencimento</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Procedimento Agendado">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>procedimentoAgendado</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Extensao Solicitada">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>extensaoSolicitada</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Autorizacao ja vencida</description>
        <inputEntry id="InputEntry_B1_1"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Autorizacao ja vencida."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar nova autorizacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Autorizacao vence em ate 2 dias</description>
        <inputEntry id="InputEntry_AL1_1"><text>[0..2]</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"URGENTE: Autorizacao vence em ate 2 dias. Acao imediata necessaria."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Agendar procedimento imediatamente ou solicitar extensao urgente."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_2">
        <description>Autorizacao vence em ate 7 dias</description>
        <inputEntry id="InputEntry_AL2_1"><text>[3..7]</text></inputEntry>
        <inputEntry id="InputEntry_AL2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL2_3"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_2"><text>"Autorizacao vence em ate 7 dias. Priorizar agendamento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_3"><text>"Agendar procedimento ou iniciar solicitacao de extensao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Autorizacao proxima do vencimento mas ja agendado</description>
        <inputEntry id="InputEntry_A1_1"><text>[0..7]</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Autorizacao proxima do vencimento porem procedimento ja agendado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Confirmar realizacao do procedimento na data agendada."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Autorizacao proxima do vencimento mas extensao ja solicitada</description>
        <inputEntry id="InputEntry_A2_1"><text>[0..7]</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Extensao de autorizacao ja solicitada."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Acompanhar resposta da solicitacao de extensao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Autorizacao com prazo confortavel</description>
        <inputEntry id="InputEntry_A3_1"><text>&gt; 7</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_2"><text>"Autorizacao dentro do prazo de validade."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_3"><text>"Prosseguir com agendamento normal."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_TIMING_003">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Situacao de vencimento requer analise manual."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar prazo e status de agendamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_TIMING_003_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_TIMING_003_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_TIMING_003">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_TIMING_003" dmnElementRef="Decision_AUTH_TIMING_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>