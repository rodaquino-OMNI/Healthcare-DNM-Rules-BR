<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-TIMING-005 - Validacao Timing de Solicitacao de Extensao
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-TIMING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_TIMING_005" name="Validacao Timing de Solicitacao de Extensao" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_TIMING_005" name="Validar Timing de Solicitacao de Extensao">
    <decisionTable id="DecisionTable_AUTH_TIMING_005" hitPolicy="FIRST">

      <input id="Input_1" label="Dias Ate Vencimento">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasAteVencimento</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Extensao Ja Solicitada">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>extensaoJaSolicitada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dias Desde Solicitacao">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>diasDesdeSolicitacao</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Prazo Resposta Operadora">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>prazoRespostaOperadora</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Autorizacao vencida e extensao nao foi solicitada</description>
        <inputEntry id="InputEntry_B1_1"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Autorizacao vencida e extensao nao foi solicitada a tempo."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar nova autorizacao. Extensao nao mais viavel."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Extensao solicitada tardiamente ainda em analise</description>
        <inputEntry id="InputEntry_AL1_1"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Extensao solicitada apos vencimento. Aguardando resposta da operadora."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Cobrar resposta urgente da operadora sobre extensao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_2">
        <description>Vencimento proximo e extensao nao solicitada</description>
        <inputEntry id="InputEntry_AL2_1"><text>[0..5]</text></inputEntry>
        <inputEntry id="InputEntry_AL2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_2"><text>"Autorizacao vence em breve. Solicitar extensao imediatamente."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_3"><text>"Iniciar processo de extensao com urgencia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Extensao ja solicitada dentro do prazo</description>
        <inputEntry id="InputEntry_A1_1"><text>&gt;= 0</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Extensao de autorizacao ja solicitada dentro do prazo."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Acompanhar resposta da operadora."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Prazo confortavel para solicitar extensao se necessario</description>
        <inputEntry id="InputEntry_A2_1"><text>&gt; 5</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Prazo adequado para solicitar extensao se necessario."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Avaliar necessidade de extensao e solicitar com antecedencia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_TIMING_005">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Situacao de extensao requer analise manual."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar status de extensao e prazos."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_TIMING_005_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_TIMING_005_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_TIMING_005">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_TIMING_005" dmnElementRef="Decision_AUTH_TIMING_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>