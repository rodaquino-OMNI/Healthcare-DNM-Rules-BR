<!--
  ============================================================================
  AUTH-TIMING-001 - Autorizacao Dentro do Periodo de Validade
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-TIMING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_TIMING_001" name="Autorizacao Dentro do Periodo de Validade" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_TIMING_001" name="Verificar Autorizacao no Periodo de Validade">
    <decisionTable id="DecisionTable_AUTH_TIMING_001" hitPolicy="FIRST">

      <input id="Input_1" label="Dias Desde Autorizacao">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasDesdeAutorizacao</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Dias Ate Vencimento">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>diasAteVencimento</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Prazo Validade Dias">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>prazoValidadeDias</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Atendimento apos vencimento da autorizacao</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Atendimento realizado apos vencimento da autorizacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Solicitar nova autorizacao ou justificar atraso junto a operadora."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Atendimento anterior a data de autorizacao</description>
        <inputEntry id="InputEntry_B2_1"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Data do atendimento anterior a data de autorizacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Verificar datas ou solicitar autorizacao retroativa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Atendimento dentro do periodo de validade</description>
        <inputEntry id="InputEntry_A1_1"><text>&gt;= 0</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt;= 0</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Atendimento dentro do periodo de validade da autorizacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com faturamento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_TIMING_001">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Nao foi possivel determinar validade temporal."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Verificar datas de autorizacao e atendimento."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_TIMING_001_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_TIMING_001_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_TIMING_001">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_TIMING_001" dmnElementRef="Decision_AUTH_TIMING_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>