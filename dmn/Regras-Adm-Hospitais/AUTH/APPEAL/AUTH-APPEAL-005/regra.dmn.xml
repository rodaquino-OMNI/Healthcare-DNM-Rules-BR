<!--
  ============================================================================
  AUTH-APPEAL-005 - Elegibilidade Segunda Instancia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-APPEAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_APPEAL_005" name="Elegibilidade Segunda Instancia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_APPEAL_005" name="Verificar Elegibilidade Segunda Instancia">
    <decisionTable id="DecisionTable_AUTH_APPEAL_005" hitPolicy="FIRST">

      <input id="Input_1" label="Instancia Atual">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>instanciaAtual</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Recurso Primeira Instancia Negado">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>recursoPrimeiraInstanciaNegado</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dias Desde Resposta">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>diasDesdeResposta</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Tem Novos Argumentos">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>temNovosArgumentos</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Ja atingiu ultima instancia administrativa</description>
        <inputEntry id="InputEntry_B1_1"><text>&gt;= 3</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Ultima instancia administrativa atingida. Nao cabe mais recurso."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Considerar via judicial ou ANS."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"PRAZO"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Prazo para segunda instancia expirado</description>
        <inputEntry id="InputEntry_B2_1"><text>1</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"Prazo para recurso de segunda instancia expirado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Considerar nova solicitacao ou via alternativa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"PRAZO"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Segunda instancia sem novos argumentos</description>
        <inputEntry id="InputEntry_AL1_1"><text>1</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>&lt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Recurso de segunda instancia sem novos argumentos pode ser indeferido."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Buscar novos argumentos ou evidencias antes de prosseguir."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"PRAZO"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Elegivel para recurso de segunda instancia</description>
        <inputEntry id="InputEntry_A1_1"><text>1</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&lt;= 30</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Elegivel para recurso de segunda instancia com novos argumentos."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Preparar e protocolar recurso de segunda instancia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Ainda em primeira instancia</description>
        <inputEntry id="InputEntry_A2_1"><text>0</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Ainda em fase de primeira instancia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Aguardar resposta da primeira instancia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_APPEAL_005">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar elegibilidade para proxima instancia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Analisar historico de recursos e prazos."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_APPEAL_005_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_APPEAL_005_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_APPEAL_005">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_APPEAL_005" dmnElementRef="Decision_AUTH_APPEAL_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>