<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-APPEAL-004 - Roteamento Negativa Administrativa vs Medica
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-APPEAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_APPEAL_004" name="Roteamento Negativa Administrativa vs Medica" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_APPEAL_004" name="Rotear Tipo de Negativa">
    <decisionTable id="DecisionTable_AUTH_APPEAL_004" hitPolicy="FIRST">

      <input id="Input_1" label="Motivo Negativa">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>motivoNegativa</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tipo Negativa">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>tipoNegativa</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"ADMINISTRATIVA", "MEDICA", "TECNICA"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Envolve Julgamento Clinico">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>envolveJulgamentoClinico</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Aprovado_1">
        <description>Negativa administrativa - rotear para setor administrativo</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"ADMINISTRATIVA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Negativa administrativa. Recurso deve ser tratado pelo setor de autorizacoes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Encaminhar para equipe de autorizacoes resolver pendencia."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Negativa medica - rotear para auditoria medica</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"MEDICA"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Negativa medica. Recurso requer parecer de auditor medico."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Encaminhar para auditoria medica elaborar contra-parecer."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Negativa tecnica - rotear para analise tecnica</description>
        <inputEntry id="InputEntry_A3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>"TECNICA"</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_2"><text>"Negativa tecnica. Verificar codificacao e documentacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_3"><text>"Revisar aspectos tecnicos antes de protocolar recurso."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>Envolve julgamento clinico mas nao classificado como medico</description>
        <inputEntry id="InputEntry_AL1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>"ADMINISTRATIVA"</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Negativa envolve julgamento clinico. Reclassificar como medica."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Envolver auditoria medica na analise do recurso."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"PRAZO"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_APPEAL_004">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Classificar tipo de negativa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Analisar motivo para determinar roteamento correto."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_APPEAL_004_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_APPEAL_004_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_APPEAL_004">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_APPEAL_004" dmnElementRef="Decision_AUTH_APPEAL_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>