<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-APPEAL-002 - Verificacao Nova Documentacao Disponivel
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-APPEAL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_APPEAL_002" name="Verificacao Nova Documentacao Disponivel" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_APPEAL_002" name="Verificar Nova Documentacao Disponivel">
    <decisionTable id="DecisionTable_AUTH_APPEAL_002" hitPolicy="FIRST">

      <input id="Input_1" label="Tem Nova Documentacao">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>temNovaDocumentacao</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Documentacao Relevante">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>documentacaoRelevante</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tipo Nova Documentacao">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>tipoNovaDocumentacao</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"LAUDO", "EXAME", "PARECER", "ARTIGO", "OUTRO"</text>
        </inputValues>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Alerta_1">
        <description>Recurso sem nova documentacao</description>
        <inputEntry id="InputEntry_AL1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Recurso sem nova documentacao pode ter baixa chance de sucesso."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Obter documentacao adicional antes de protocolar recurso."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"PRAZO"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_2">
        <description>Nova documentacao pode nao ser relevante</description>
        <inputEntry id="InputEntry_AL2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_AL2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL2_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL2_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_2"><text>"Nova documentacao pode nao ser relevante para o motivo da negativa."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL2_3"><text>"Revisar documentacao e alinhar com motivo da negativa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_2_4"><text>"PRAZO"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_2_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Nova documentacao com parecer medico relevante</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"PARECER"</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Parecer medico adicional fortalece o recurso."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com protocolo do recurso."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Nova documentacao com laudo ou exame relevante</description>
        <inputEntry id="InputEntry_A2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>"LAUDO", "EXAME"</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Nova documentacao clinica relevante disponivel."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com protocolo do recurso."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_3">
        <description>Outra documentacao relevante disponivel</description>
        <inputEntry id="InputEntry_A3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A3_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_2"><text>"Nova documentacao relevante disponivel para recurso."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A3_3"><text>"Prosseguir com protocolo do recurso."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_3_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_3_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_APPEAL_002">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar documentacao para recurso."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Avaliar documentacao disponivel com equipe medica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_APPEAL_002_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_APPEAL_002_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_APPEAL_002">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_APPEAL_002" dmnElementRef="Decision_AUTH_APPEAL_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>