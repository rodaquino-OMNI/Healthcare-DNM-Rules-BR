{
  "ruleId": "AUTH-APPEAL-002",
  "ruleName": "Verificacao Nova Documentacao Disponivel",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "APPEAL",
  "perspective": "HOSPITAL",
  "description": "Verifica se ha nova documentacao disponivel para apoiar o recurso",
  "inputs": [
    {
      "name": "temNovaDocumentacao",
      "type": "boolean",
      "description": "Indica se ha nova documentacao"
    },
    {
      "name": "documentacaoRelevante",
      "type": "boolean",
      "description": "Indica se documentacao e relevante"
    },
    {
      "name": "tipoNovaDocumentacao",
      "type": "string",
      "description": "Tipo da nova documentacao"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "RN 395/2016 ANS",
    "Protocolos de Recurso"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
