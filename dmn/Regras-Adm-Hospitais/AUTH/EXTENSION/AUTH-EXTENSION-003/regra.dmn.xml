<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  AUTH-EXTENSION-003 - Extensao Prazo Vencimento
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-EXTENSION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_EXTENSION_003" name="Extensao Prazo Vencimento" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_EXTENSION_003" name="Avaliar Extensao de Prazo de Vencimento">
    <decisionTable id="DecisionTable_AUTH_EXTENSION_003" hitPolicy="FIRST">

      <input id="Input_1" label="Dias Restantes">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasRestantes</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Motivo Extensao">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>motivoExtensao</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"INTERCORRENCIA", "ATRASO_OPERADORA", "COMPLEMENTACAO", "REAGENDAMENTO", "OUTRO"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Aprovacao Gerencia">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>aprovacaoGerencia</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloqueio_1">
        <description>Extensao prematura sem justificativa gerencial</description>
        <inputEntry id="InputEntry_B1_1"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Extensao solicitada com mais de 30 dias restantes sem aprovacao gerencial."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Aguardar prazo se aproximar ou obter aprovacao gerencial para extensao antecipada."</text></outputEntry>
<outputEntry id="Rule_Bloqueio_1_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Bloqueio_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Extensao sem motivo justificado</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"OUTRO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Motivo de extensao nao especificado ou invalido."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Especificar motivo valido ou obter aprovacao gerencial."</text></outputEntry>
<outputEntry id="Rule_Bloqueio_2_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Bloqueio_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Prazo de vencimento critico</description>
        <inputEntry id="InputEntry_A1_1"><text>&lt; 15</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Autorizacao com menos de 15 dias para vencimento. Acao urgente necessaria."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Priorizar solicitacao de extensao junto a operadora."</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Extensao por intercorrencia medica</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"INTERCORRENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Extensao por intercorrencia medica aprovada automaticamente."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com solicitacao de extensao junto a operadora."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Extensao por atraso da operadora</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"ATRASO_OPERADORA"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Extensao por atraso da operadora. Registrar protocolo de reclamacao."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Protocolar reclamacao e solicitar extensao automatica."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_3">
        <description>Extensao aprovada pela gerencia</description>
        <inputEntry id="InputEntry_P3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"Extensao aprovada pela gerencia. Prosseguir com solicitacao."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"Registrar aprovacao gerencial e solicitar extensao."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_3_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_3_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_AUTH_EXTENSION_003">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Extensao de prazo requer analise manual."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Encaminhar para gerencia avaliar necessidade de extensao."</text></outputEntry>
<outputEntry id="Rule_Fallback_AUTH_EXTENSION_003_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_AUTH_EXTENSION_003_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_EXTENSION_003">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_EXTENSION_003" dmnElementRef="Decision_AUTH_EXTENSION_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>