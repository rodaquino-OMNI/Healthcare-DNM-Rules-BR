<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  AUTH-EXTENSION-007 - Extensao Transplante
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-EXTENSION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_EXTENSION_007" name="Extensao Transplante" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_EXTENSION_007" name="Avaliar Extensao de Acompanhamento de Transplante">
    <decisionTable id="DecisionTable_AUTH_EXTENSION_007" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Transplante">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoTransplante</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"RENAL", "HEPATICO", "CARDIACO", "MEDULA", "CORNEA", "OUTRO"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Fase Acompanhamento">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>faseAcompanhamento</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"IMEDIATA", "PRECOCE", "TARDIA", "MANUTENCAO"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Complicacoes">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>complicacoes</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Justificativa Extensao">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>justificativaExtensao</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloqueio_1">
        <description>Fase imediata requer justificativa para extensao</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"IMEDIATA"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Fase imediata de transplante sem justificativa para extensao."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Documentar necessidade de extensao ou aguardar passagem para fase seguinte."</text></outputEntry>
<outputEntry id="Rule_Bloqueio_1_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Bloqueio_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Fase tardia sem complicacoes documentadas</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"TARDIA"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Fase tardia sem complicacoes nao justifica extensao."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Transferir para acompanhamento ambulatorial regular."</text></outputEntry>
<outputEntry id="Rule_Bloqueio_2_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Bloqueio_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Complicacoes em fase tardia</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"TARDIA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Complicacoes em fase tardia de transplante. Avaliar extensao de cobertura."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Documentar complicacoes e solicitar parecer da equipe de transplante."</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_2">
        <description>Fase de manutencao requer revisao periodica</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"MANUTENCAO"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Paciente em fase de manutencao. Verificar necessidade de extensao."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Agendar revisao periodica com equipe de transplante."</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Complicacoes em fase imediata pos-transplante</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"IMEDIATA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Extensao aprovada por complicacoes em fase imediata de transplante."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com autorizacao de tratamento das complicacoes."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Acompanhamento de fase precoce</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"PRECOCE"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Extensao de fase precoce pos-transplante aprovada automaticamente."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Prosseguir com autorizacao de acompanhamento precoce."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_3">
        <description>Extensao com justificativa adequada</description>
        <inputEntry id="InputEntry_P3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"Extensao de transplante aprovada com justificativa documentada."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"Prosseguir com autorizacao conforme justificativa."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_3_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_3_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_AUTH_EXTENSION_007">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Extensao de transplante requer analise da equipe especializada."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Encaminhar para junta de transplantes avaliar extensao."</text></outputEntry>
<outputEntry id="Rule_Fallback_AUTH_EXTENSION_007_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_AUTH_EXTENSION_007_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_EXTENSION_007">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_EXTENSION_007" dmnElementRef="Decision_AUTH_EXTENSION_007">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>