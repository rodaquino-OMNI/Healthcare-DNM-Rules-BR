<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  AUTH-EXTENSION-005 - Extensao Oncologia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-EXTENSION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_EXTENSION_005" name="Extensao Oncologia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_EXTENSION_005" name="Avaliar Extensao de Tratamento Oncologico">
    <decisionTable id="DecisionTable_AUTH_EXTENSION_005" hitPolicy="FIRST">

      <input id="Input_1" label="Estadiamento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>estadiamento</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"I", "II", "III", "IV", "INDEFINIDO"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Protocolo Quimio">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>protocoloQuimio</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"ATIVO", "CONCLUIDO", "PAUSADO", "ALTERADO"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Resposta Tratamento">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>respostaTratamento</text>
        </inputExpression>
        <inputValues id="InputValues_3">
          <text>"COMPLETA", "PARCIAL", "ESTAVEL", "PROGRESSAO"</text>
        </inputValues>
      </input>

      <input id="Input_4" label="Reavaliacao Recente">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>reavaliacaoRecente</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloqueio_1">
        <description>Progressao da doenca sem reavaliacao medica</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>"PROGRESSAO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Progressao da doenca detectada sem reavaliacao. Extensao bloqueada."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Solicitar reavaliacao oncologica para definir novo protocolo."</text></outputEntry>
<outputEntry id="Rule_Bloqueio_1_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Bloqueio_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Protocolo finalizado sem nova indicacao</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"CONCLUIDO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>"COMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Protocolo oncologico concluido com resposta completa. Nao ha indicacao de extensao."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Encerrar autorizacao ou solicitar novo laudo para manutencao."</text></outputEntry>
<outputEntry id="Rule_Bloqueio_2_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Bloqueio_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Resposta estavel requer monitoramento</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"ESTAVEL"</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Resposta estavel ao tratamento. Avaliar continuidade do protocolo."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Agendar reavaliacao oncologica para definir conduta."</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_2">
        <description>Estadiamento avancado em tratamento</description>
        <inputEntry id="InputEntry_A2_1"><text>"IV"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Estadiamento IV em tratamento ativo. Monitoramento intensivo necessario."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Priorizar avaliacoes periodicas e suporte multidisciplinar."</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Tratamento com resposta parcial ou completa</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Resposta parcial ao tratamento. Extensao aprovada para continuidade."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com autorizacao do ciclo seguinte."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Progressao com reavaliacao e novo protocolo</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"ALTERADO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"PROGRESSAO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Novo protocolo definido apos progressao. Extensao aprovada."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Prosseguir com autorizacao do novo protocolo."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_3">
        <description>Tratamento de manutencao apos resposta</description>
        <inputEntry id="InputEntry_P3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>"PAUSADO"</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"Manutencao oncologica com reavaliacao recente aprovada."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"Prosseguir com autorizacao de manutencao."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_3_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_3_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_AUTH_EXTENSION_005">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Extensao oncologica requer analise manual especializada."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Encaminhar para junta medica oncologica avaliar extensao."</text></outputEntry>
<outputEntry id="Rule_Fallback_AUTH_EXTENSION_005_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_AUTH_EXTENSION_005_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_EXTENSION_005">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_EXTENSION_005" dmnElementRef="Decision_AUTH_EXTENSION_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>