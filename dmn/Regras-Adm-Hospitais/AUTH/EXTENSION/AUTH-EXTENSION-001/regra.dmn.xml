<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  AUTH-EXTENSION-001 - Prorrogacao Urgencia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-EXTENSION
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_EXTENSION_001" name="Prorrogacao Urgencia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_EXTENSION_001" name="Avaliar Prorrogacao de Urgencia">
    <decisionTable id="DecisionTable_AUTH_EXTENSION_001" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Urgencia">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>tipoUrgencia</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Dias Solicitados">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>diasSolicitados</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Justificativa Medica">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>justificativaMedica</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloqueio_1">
        <description>Urgencia falsa com solicitacao acima de 30 dias</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Prorrogacao nao caracteriza urgencia e excede 30 dias. RN 424/2017 nao aplicavel."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Solicitar autorizacao regular via canal eletivo. Nao aprovar como urgencia."</text></outputEntry>
<outputEntry id="Rule_Bloqueio_1_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Bloqueio_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>Ausencia de justificativa medica para prorrogacao</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>&gt; 15</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Prorrogacao acima de 15 dias requer justificativa medica obrigatoria."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Anexar laudo medico justificando necessidade de prorrogacao."</text></outputEntry>
<outputEntry id="Rule_Bloqueio_2_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Bloqueio_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_1">
        <description>Prorrogacao acima de 15 dias requer atencao</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt; 15</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Prorrogacao de urgencia acima de 15 dias. Verificar necessidade clinica."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Revisar justificativa medica e confirmar continuidade do carater de urgencia."</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Alerta_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alerta_2">
        <description>Caso eletivo com prorrogacao superior a 15 dias</description>
        <inputEntry id="InputEntry_A2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>[16..30]</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Prorrogacao eletiva entre 16-30 dias com justificativa. Avaliar reclassificacao."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Verificar se caso pode ser reclassificado para urgencia conforme RN 424/2017."</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_alertasConformidade"><text>"PRAZO"</text></outputEntry>
<outputEntry id="Rule_Alerta_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Urgencia verdadeira com prorrogacao ate 15 dias</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&lt;= 15</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Prorrogacao de urgencia dentro do prazo padrao. Aprovada automaticamente."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com autorizacao de prorrogacao."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Caso eletivo com prorrogacao ate 15 dias</description>
        <inputEntry id="InputEntry_P2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>&lt;= 15</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Prorrogacao eletiva dentro do limite aceitavel."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Prosseguir com autorizacao padrao."</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Prosseguir_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_AUTH_EXTENSION_001">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Situacao de prorrogacao requer analise manual."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria medica avaliar necessidade de prorrogacao."</text></outputEntry>
<outputEntry id="Rule_Fallback_AUTH_EXTENSION_001_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_AUTH_EXTENSION_001_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_EXTENSION_001">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_EXTENSION_001" dmnElementRef="Decision_AUTH_EXTENSION_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>