{
  "ruleId": "AUTH-CODING-005",
  "ruleName": "Validacao Logica de Combinacao de Codigos",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "CODING",
  "perspective": "HOSPITAL",
  "description": "Valida se a combinacao de codigos TUSS e permitida",
  "inputs": [
    {
      "name": "codigoTUSSPrincipal",
      "type": "string",
      "description": "Codigo TUSS do procedimento principal"
    },
    {
      "name": "codigoTUSSSecundario",
      "type": "string",
      "description": "Codigo TUSS do procedimento secundario"
    },
    {
      "name": "combinacaoPermitida",
      "type": "boolean",
      "description": "Indica se combinacao e permitida"
    },
    {
      "name": "procedimentosExcludentes",
      "type": "boolean",
      "description": "Indica se procedimentos sao excludentes"
    },
    {
      "name": "requerJustificativa",
      "type": "boolean",
      "description": "Indica se combinacao requer justificativa"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "CBHPM",
    "TUSS 2024"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
