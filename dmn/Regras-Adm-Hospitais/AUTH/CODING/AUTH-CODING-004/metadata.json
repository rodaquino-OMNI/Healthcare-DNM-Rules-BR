{
  "ruleId": "AUTH-CODING-004",
  "ruleName": "Validacao Codigo na Lista Cobertura ANS",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "CODING",
  "perspective": "HOSPITAL",
  "description": "Valida se o codigo TUSS consta no Rol ANS e e coberto pelo plano",
  "inputs": [
    {
      "name": "codigoTUSS",
      "type": "string",
      "description": "Codigo TUSS do procedimento"
    },
    {
      "name": "constaRolANS",
      "type": "boolean",
      "description": "Indica se codigo consta no Rol ANS"
    },
    {
      "name": "cobertoPeloPlano",
      "type": "boolean",
      "description": "Indica se e coberto pelo plano do beneficiario"
    },
    {
      "name": "segmentacaoPlano",
      "type": "string",
      "description": "Segmentacao do plano do beneficiario"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "RN 465/2021 ANS",
    "TUSS 2024"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
