<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-CODING-001 - Validacao CID Compativel com Indicacao
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-CODING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_CODING_001" name="Validacao CID Compativel com Indicacao" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_CODING_001" name="Validar CID Compativel com Indicacao">
    <decisionTable id="DecisionTable_AUTH_CODING_001" hitPolicy="FIRST">

      <input id="Input_1" label="Diagnostico CID-10">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>diagnosticoCID10</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Codigo TUSS">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>codigoTUSS</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Diagnostico Compativel">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>diagnosticoCompativel</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Possui DUT">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>possuiDUT</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>CID-10 nao informado</description>
        <inputEntry id="InputEntry_B1_1"><text>null</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Codigo CID-10 obrigatorio nao informado."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Inserir codigo CID-10 do diagnostico."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Bloqueio_2">
        <description>CID-10 incompativel conforme DUT</description>
        <inputEntry id="InputEntry_B2_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_2"><text>"CID-10 nao consta na DUT como indicacao para este procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B2_3"><text>"Revisar diagnostico ou justificar indicacao off-label."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_2_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_2_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>CID-10 aparentemente incompativel (sem DUT definida)</description>
        <inputEntry id="InputEntry_AL1_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"CID-10 aparentemente incompativel com procedimento. Verificar indicacao."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Anexar justificativa medica para indicacao atipica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>CID-10 compativel com indicacao do procedimento</description>
        <inputEntry id="InputEntry_A1_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"CID-10 compativel com indicacao do procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_CODING_001">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar compatibilidade CID-procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Consultar DUT ou tabela de indicacoes."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_CODING_001_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_CODING_001_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_CODING_001">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_CODING_001" dmnElementRef="Decision_AUTH_CODING_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>