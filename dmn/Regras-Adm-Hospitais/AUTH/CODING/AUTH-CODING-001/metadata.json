{
  "ruleId": "AUTH-CODING-001",
  "ruleName": "Validacao CID Compativel com Indicacao",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "CODING",
  "perspective": "HOSPITAL",
  "description": "Valida se o CID-10 e compativel com a indicacao do procedimento",
  "inputs": [
    {
      "name": "diagnosticoCID10",
      "type": "string",
      "description": "Codigo CID-10 do diagnostico"
    },
    {
      "name": "codigoTUSS",
      "type": "string",
      "description": "Codigo TUSS do procedimento"
    },
    {
      "name": "diagnosticoCompativel",
      "type": "boolean",
      "description": "Indica se diagnostico e compativel com procedimento"
    },
    {
      "name": "possuiDUT",
      "type": "boolean",
      "description": "Indica se procedimento possui DUT definida"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "CID-10 OMS",
    "TUSS 2024",
    "DUT ANS"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
