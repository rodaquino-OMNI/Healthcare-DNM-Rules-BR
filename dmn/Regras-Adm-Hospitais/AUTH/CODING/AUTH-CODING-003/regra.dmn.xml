<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  AUTH-CODING-003 - Validacao Diagnostico Secundario de Suporte
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: AUTH-CODING
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_AUTH_CODING_003" name="Validacao Diagnostico Secundario de Suporte" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_AUTH_CODING_003" name="Validar Diagnostico Secundario de Suporte">
    <decisionTable id="DecisionTable_AUTH_CODING_003" hitPolicy="FIRST">

      <input id="Input_1" label="CID Principal">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>cidPrincipal</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="CID Secundario">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>cidSecundario</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="CIDs Coerentes">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>cidsCoerentes</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Comorbidade Contraindicante">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>comorbidadeContraindicante</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string" />
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string" />

      
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</outputValues></output><output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues>"BAIXO", "MEDIO", "ALTO", "CRITICO"</outputValues></output><rule id="Rule_Bloqueio_1">
        <description>Comorbidade contraindica o procedimento</description>
        <inputEntry id="InputEntry_B1_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>true</text></inputEntry>
        
      <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_2"><text>"Diagnostico secundario indica comorbidade que contraindica o procedimento."</text></outputEntry>
        
      <outputEntry id="OutputEntry_B1_3"><text>"Revisar indicacao considerando comorbidades."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Bloqueio_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Bloqueio_1_5"><text>"ALTO"</text></outputEntry></rule>

      
      <rule id="Rule_Alerta_1">
        <description>CIDs aparentemente incoerentes</description>
        <inputEntry id="InputEntry_AL1_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_AL1_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_AL1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_AL1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_AL1_1"><text>"Alertar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_2"><text>"Diagnosticos principal e secundario aparentam incoerencia."</text></outputEntry>
        
      <outputEntry id="OutputEntry_AL1_3"><text>"Revisar codificacao ou adicionar justificativa."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Alerta_1_4"><text>"DOC"</text></outputEntry>
      <outputEntry id="OutputEntry_Alerta_1_5"><text>"MEDIO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_1">
        <description>Diagnosticos coerentes e sem contraindicacao</description>
        <inputEntry id="InputEntry_A1_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
        
      <outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_2"><text>"Diagnosticos principal e secundario coerentes."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A1_3"><text>"Prosseguir com solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_1_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_1_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Aprovado_2">
        <description>Sem diagnostico secundario informado</description>
        <inputEntry id="InputEntry_A2_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>null</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_A2_1"><text>"Prosseguir"</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_2"><text>"Diagnostico secundario nao informado. Nao obrigatorio."</text></outputEntry>
        
      <outputEntry id="OutputEntry_A2_3"><text>"Prosseguir com solicitacao."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Aprovado_2_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Aprovado_2_5"><text>"BAIXO"</text></outputEntry></rule>

      
      <rule id="Rule_Fallback_AUTH_CODING_003">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        
      <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_2"><text>"Verificar coerencia entre diagnosticos."</text></outputEntry>
        
      <outputEntry id="OutputEntry_F_3"><text>"Validar CIDs com equipe medica."</text></outputEntry>
      
      <outputEntry id="OutputEntry_Fallback_AUTH_CODING_003_4"><text>"NENHUM"</text></outputEntry>
      <outputEntry id="OutputEntry_Fallback_AUTH_CODING_003_5"><text>"MEDIO"</text></outputEntry></rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_AUTH_CODING_003">
      <dmndi:DMNShape id="DMNShape_Decision_AUTH_CODING_003" dmnElementRef="Decision_AUTH_CODING_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>