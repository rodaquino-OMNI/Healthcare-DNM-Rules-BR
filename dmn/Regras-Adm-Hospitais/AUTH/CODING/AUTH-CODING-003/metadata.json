{
  "ruleId": "AUTH-CODING-003",
  "ruleName": "Validacao Diagnostico Secundario de Suporte",
  "version": "1.0.0",
  "category": "AUTH",
  "subcategory": "CODING",
  "perspective": "HOSPITAL",
  "description": "Valida se o diagnostico secundario apoia a indicacao do procedimento",
  "inputs": [
    {
      "name": "cidPrincipal",
      "type": "string",
      "description": "Codigo CID-10 do diagnostico principal"
    },
    {
      "name": "cidSecundario",
      "type": "string",
      "description": "Codigo CID-10 do diagnostico secundario"
    },
    {
      "name": "cidsCoerentes",
      "type": "boolean",
      "description": "Indica se os CIDs sao coerentes entre si"
    },
    {
      "name": "comorbidadeContraindicante",
      "type": "boolean",
      "description": "Indica se comorbidade contraindica procedimento"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string"
    },
    {
      "name": "acaoRecomendada",
      "type": "string"
    }
  ],
  "references": [
    "CID-10 OMS",
    "Protocolos Clinicos"
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06"
}
