<?xml version="1.0" encoding="UTF-8"?>
<!-- PRICE-PACKAGE-005 - Intercorrencia Fora Pacote | V1.0.0 (2026-02-08) | HOSPITAL -->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_PRICE_PACKAGE_005" name="Intercorrencia Fora Pacote" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_PRICE_PACKAGE_005" name="Intercorrencia Fora Pacote">
    <decisionTable id="DecisionTable_PRICE_PACKAGE_005" hitPolicy="FIRST">
      <input id="Input_1" label="Temintercorrencia"><inputExpression id="InputExpression_1" typeRef="boolean"><text>temIntercorrencia</text></inputExpression></input>
      <input id="Input_2" label="Intercorrenciadocumentada"><inputExpression id="InputExpression_2" typeRef="boolean"><text>intercorrenciaDocumentada</text></inputExpression></input>
      <input id="Input_3" label="Valorintercorrencia"><inputExpression id="InputExpression_3" typeRef="double"><text>valorIntercorrencia</text></inputExpression></input>
      <input id="Input_4" label="Dentroprazo"><inputExpression id="InputExpression_4" typeRef="boolean"><text>dentroPrazo</text></inputExpression></input>
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>
      <rule id="Rule_Bloqueio_1"><description>Intercorrencia nao documentada ou fora prazo pacote</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry><inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry><inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry><inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry><outputEntry id="OutputEntry_B1_2"><text>"BLOQUEADO - Intercorrencia nao documentada ou fora prazo pacote."</text></outputEntry><outputEntry id="OutputEntry_B1_3"><text>"Revisar criterios contratuais e solicitar documentacao adicional."</text></outputEntry><outputEntry id="OutputEntry_B1_4"><text>"CONTRATO"</text></outputEntry><outputEntry id="OutputEntry_B1_5"><text>"ALTO"</text></outputEntry>
      </rule>
      <rule id="Rule_Alerta_1"><description>Intercorrencia documentada mas valor elevado</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry><inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry><inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry><inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry><outputEntry id="OutputEntry_A1_2"><text>"ALERTA - Intercorrencia documentada mas valor elevado."</text></outputEntry><outputEntry id="OutputEntry_A1_3"><text>"Revisar documentacao e validar conformidade contratual."</text></outputEntry><outputEntry id="OutputEntry_A1_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry>
      </rule>
      <rule id="Rule_Aprovado_1"><description>Intercorrencia fora pacote devidamente caracterizada</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry><inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry><inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry><inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_P1_2"><text>"APROVADO - Intercorrencia fora pacote devidamente caracterizada."</text></outputEntry><outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrao."</text></outputEntry><outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>
      <rule id="Rule_Fallback_PRICE_PACKAGE_005"><description>Regra padrao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry><inputEntry id="InputEntry_F_2"><text>-</text></inputEntry><inputEntry id="InputEntry_F_3"><text>-</text></inputEntry><inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry><outputEntry id="OutputEntry_F_2"><text>"Situacao requer analise manual por nao corresponder a nenhum criterio automatico."</text></outputEntry><outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliacao caso a caso."</text></outputEntry><outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>
    </decisionTable>
  </decision>
  <dmndi:DMNDI><dmndi:DMNDiagram id="DMNDiagram_PRICE_PACKAGE_005"><dmndi:DMNShape id="DMNShape_Decision_PRICE_PACKAGE_005" dmnElementRef="Decision_PRICE_PACKAGE_005"><dc:Bounds height="80" width="180" x="160" y="100"/></dmndi:DMNShape></dmndi:DMNDiagram></dmndi:DMNDI>
</definitions>
