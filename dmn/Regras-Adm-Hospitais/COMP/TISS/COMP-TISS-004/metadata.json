{
  "ruleId": "COMP-TISS-004",
  "ruleName": "Validacao Formato Codigos",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "TISS",
  "perspective": "HOSPITAL",
  "description": "Valida formato de codigos utilizados na guia TISS (TUSS, CID, CNES, CNPJ, CPF)",
  "inputs": [
    {
      "name": "codigoTussValido",
      "type": "boolean",
      "description": "Indica se codigo TUSS possui formato valido (8 digitos)"
    },
    {
      "name": "codigoCidValido",
      "type": "boolean",
      "description": "Indica se codigo CID-10 possui formato valido"
    },
    {
      "name": "formatoCodigoValido",
      "type": "boolean",
      "description": "Indica se demais codigos possuem formato valido"
    },
    {
      "name": "digitoVerificadorOk",
      "type": "boolean",
      "description": "Indica se digitos verificadores de CNPJ/CPF estao corretos"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para correcao"
    }
  ],
  "regulations": [
    {
      "code": "TUSS-2024",
      "name": "Tabela TUSS",
      "source": "ANS",
      "format": "8 digitos numericos"
    },
    {
      "code": "CID-10",
      "name": "Classificacao Internacional de Doencas",
      "source": "OMS",
      "format": "Letra + digitos"
    }
  ],
