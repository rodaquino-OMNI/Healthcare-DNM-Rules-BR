<?xml version="1.0" encoding="UTF-8"?>
<!--
  ============================================================================
  COMP-TISS-007 - Versão TISS Compatível com Operadora
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-TISS
  Perspectiva: HOSPITAL (Administrativo - Faturamento)

  ============================================================================
  INDICACOES (Quando PROSSEGUIR):
  ============================================================================
  - Versão TISS do lote corresponde à versão exigida pela operadora
  - Compatibilidade verificada e confirmada no cadastro de convênios
  - Operadora aceita a versão TISS enviada

  ============================================================================
  CONTRAINDICACOES (Quando BLOQUEAR):
  ============================================================================
  - Versão TISS não compatível com a exigida pela operadora
  - Operadora não aceita a versão TISS do lote
  - Incompatibilidade detectada no cadastro de convênios

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - RN 412/2016 ANS - Padrão TISS
  - Tabela de Versões TISS ANS
  - Cadastro de Convênios e Versões Aceitas
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_COMP_TISS_007"
             name="Versão TISS Compatível com Operadora"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_TISS_007" name="Versão TISS Compatível com Operadora">
    <decisionTable id="DecisionTable_COMP_TISS_007" hitPolicy="FIRST">

      <!-- ========== INPUTS ========== -->
      <input id="Input_1" label="Versão TISS">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>versaoTISS</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"3.05", "4.00", "4.01"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="Versão Operadora">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>versaoOperadora</text>
        </inputExpression>
        <inputValues id="InputValues_2">
          <text>"3.05", "4.00", "4.01"</text>
        </inputValues>
      </input>

      <input id="Input_3" label="Compatível">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>compativel</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->

      <!-- REGRA B1: Bloqueio - Versão TISS incompatível -->
      <rule id="Rule_Bloqueio_1">
        <description>Versão TISS não corresponde à exigida pela operadora</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"Versão TISS do lote não é compatível com a versão exigida pela operadora."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Converter lote para a versão TISS aceita pela operadora antes de reenviar."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"CRITICO"</text></outputEntry>
      </rule>

      <!-- REGRA A1: Alerta - Versões diferentes mas compatível marcado como true -->
      <rule id="Rule_Alerta_1">
        <description>Versões diferentes mas cadastro indica compatibilidade</description>
        <inputEntry id="InputEntry_A1_1"><text>not("4.01")</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"4.01"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_2"><text>"Versão TISS do lote difere da versão padrão da operadora, mas compatibilidade foi confirmada."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_3"><text>"Monitorar aceitação do lote pela operadora e validar compatibilidade cadastral."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      <!-- REGRA P1: Aprovado - Versões idênticas e compatível -->
      <rule id="Rule_Aprovado_1">
        <description>Versão TISS do lote corresponde à operadora</description>
        <inputEntry id="InputEntry_P1_1"><text>"4.01"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"4.01"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"Versão TISS do lote está compatível com a versão exigida pela operadora."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com faturamento conforme procedimento padrão."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <!-- REGRA P2: Aprovado - Compatível marcado como true -->
      <rule id="Rule_Aprovado_2">
        <description>Compatibilidade confirmada no cadastro</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P2_2"><text>"Compatibilidade confirmada entre versão TISS do lote e exigência da operadora."</text></outputEntry>
        <outputEntry id="OutputEntry_P2_3"><text>"Prosseguir com faturamento conforme procedimento padrão."</text></outputEntry>
        <outputEntry id="OutputEntry_P2_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <!-- REGRA F: Fallback - Requer revisão manual -->
      <rule id="Rule_Fallback_COMP_TISS_007">
        <description>Regra padrão - Requer avaliação manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Situação requer análise manual por não corresponder a nenhum critério automático."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para auditoria administrativa para avaliação caso a caso."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_TISS_007">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_TISS_007" dmnElementRef="Decision_COMP_TISS_007">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
