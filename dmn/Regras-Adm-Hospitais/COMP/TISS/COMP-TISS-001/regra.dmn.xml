<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-TISS-001 - Validacao Formato Guia TISS
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-TISS
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_TISS_001" name="Validacao Formato Guia TISS" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_TISS_001" name="Validacao Formato Guia TISS">
    <decisionTable id="DecisionTable_COMP_TISS_001" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Guia">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoGuia</text>
        </inputExpression>
        <inputValues><text>"SP_SADT", "CONSULTA", "INTERNACAO", "HONORARIOS", "RESUMO_INTERNACAO", "ODONTOLOGICO"</text></inputValues>
      </input>

      <input id="Input_2" label="Formato Valido">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>formatoValido</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Estrutura XML Correta">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>estruturaXmlCorreta</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Schema Validado">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>schemaValidado</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Tipo de guia nao reconhecido pelo padrao TISS</description>
        <inputEntry id="InputEntry_B1_1"><text>not("SP_SADT", "CONSULTA", "INTERNACAO", "HONORARIOS", "RESUMO_INTERNACAO", "ODONTOLOGICO")</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Tipo de guia nao reconhecido pelo padrao TISS. Utilizar tipo valido: SP_SADT, CONSULTA, INTERNACAO, HONORARIOS, RESUMO_INTERNACAO ou ODONTOLOGICO."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Formato da guia nao atende padrao TISS</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Formato da guia nao atende padrao TISS. Corrigir estrutura conforme especificacao ANS vigente."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Estrutura XML mal formada</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Estrutura XML mal formada. Verificar tags, namespaces e encoding UTF-8."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Schema XSD nao validado</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"XML bem formado mas nao validou contra schema XSD TISS. Verificar versao do schema e elementos obrigatorios."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Guia SP/SADT com formato valido</description>
        <inputEntry id="InputEntry_P1_1"><text>"SP_SADT"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Guia SP/SADT com formato TISS valido. Schema validado. Prosseguir com envio."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Guia Internacao com formato valido</description>
        <inputEntry id="InputEntry_P2_1"><text>"INTERNACAO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Guia de Internacao com formato TISS valido. Schema validado. Prosseguir com envio."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_3">
        <description>Guia com formato TISS valido</description>
        <inputEntry id="InputEntry_P3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"Guia TISS com formato valido, estrutura XML correta e schema validado. Pronto para envio."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_3_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_3_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_TISS_001">
        <description>Caso padrao - Revisar formato da guia</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar tipo de guia, formato, estrutura XML e validacao contra schema XSD TISS."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_TISS_001_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_TISS_001_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_TISS_001">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_TISS_001" dmnElementRef="Decision_COMP_TISS_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>