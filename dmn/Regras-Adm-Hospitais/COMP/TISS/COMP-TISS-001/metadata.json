{
  "ruleId": "COMP-TISS-001",
  "ruleName": "Validacao Formato Guia TISS",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "TISS",
  "perspective": "HOSPITAL",
  "description": "Valida formato da guia TISS conforme tipo (SP/SADT, Internacao, etc) e conformidade com schema XSD",
  "inputs": [
    {
      "name": "tipoGuia",
      "type": "string",
      "values": ["SP_SADT", "CONSULTA", "INTERNACAO", "HONORARIOS", "RESUMO_INTERNACAO", "ODONTOLOGICO"],
      "description": "Tipo de guia TISS sendo gerada"
    },
    {
      "name": "formatoValido",
      "type": "boolean",
      "description": "Indica se formato atende especificacao TISS"
    },
    {
      "name": "estruturaXmlCorreta",
      "type": "boolean",
      "description": "Indica se XML esta bem formado"
    },
    {
      "name": "schemaValidado",
      "type": "boolean",
      "description": "Indica se XML validou contra schema XSD TISS"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para envio"
    }
  ],
  "regulations": [
    {
      "code": "TISS-4.01.00",
      "name": "Padrao TISS - Versao 4.01.00",
      "source": "ANS"
    },
    {
      "code": "RN-305-2012",
      "name": "Padrao TISS Obrigatorio",
      "source": "ANS"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
