<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-TISS-003 - Validacao Campos Obrigatorios TISS
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-TISS
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_TISS_003" name="Validacao Campos Obrigatorios TISS" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_TISS_003" name="Validacao Campos Obrigatorios">
    <decisionTable id="DecisionTable_COMP_TISS_003" hitPolicy="FIRST">

      <input id="Input_1" label="Dados Beneficiario Completos">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>dadosBeneficiarioCompletos</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Dados Prestador Completos">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>dadosPrestadorCompletos</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Dados Procedimento Completos">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>dadosProcedimentoCompletos</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Campos Obrigatorios Preenchidos">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>camposObrigatoriosPreenchidos</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Dados do beneficiario incompletos</description>
        <inputEntry id="InputEntry_B1_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Dados do beneficiario incompletos. Verificar: numero carteirinha, nome, data nascimento, CPF."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Dados do prestador incompletos</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Dados do prestador incompletos. Verificar: CNES, CNPJ, razao social, codigo na operadora."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Dados do procedimento incompletos</description>
        <inputEntry id="InputEntry_B3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Dados do procedimento incompletos. Verificar: codigo TUSS, descricao, quantidade, data execucao."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Alguns campos obrigatorios nao preenchidos</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Campos obrigatorios adicionais nao preenchidos. Verificar: CID-10, justificativa clinica, autorizacao previa."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Todos campos obrigatorios preenchidos</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Todos campos obrigatorios TISS preenchidos corretamente. Guia pronta para envio."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_TISS_003">
        <description>Caso padrao - Revisar campos obrigatorios</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar preenchimento de campos obrigatorios: beneficiario, prestador, procedimento e dados clinicos."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_TISS_003_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_TISS_003_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_TISS_003">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_TISS_003" dmnElementRef="Decision_COMP_TISS_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>