<?xml version="1.0" encoding="UTF-8"?>
<!--
  ============================================================================
  COMP-TISS-006 - Validacao Estrutural XML TISS
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-TISS
  Perspectiva: HOSPITAL (Compliance & Standards)

  ============================================================================
  INDICACOES (Quando PROSSEGUIR):
  ============================================================================
  - XML bem formado (sintaxe correta)
  - Encoding UTF-8 conforme especificacao
  - Namespaces TISS declarados corretamente

  ============================================================================
  CONTRAINDICACOES (Quando BLOQUEAR):
  ============================================================================
  - XML mal formado (tags nao fechadas, caracteres invalidos)
  - Encoding diferente de UTF-8
  - Namespaces ausentes ou incorretos

  ============================================================================
  REFERENCIAS NORMATIVAS:
  ============================================================================
  - RN ANS 412/2016 (Padrao TISS)
  - XML 1.0 Specification (W3C)
  - Guia de Implementacao TISS v4.01
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
             id="Definitions_COMP_TISS_006"
             name="Validacao Estrutural XML TISS"
             targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_TISS_006" name="Validacao Estrutural XML TISS">
    <decisionTable id="DecisionTable_COMP_TISS_006" hitPolicy="FIRST">

      <input id="Input_1" label="Versao TISS">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>versaoTISS</text>
        </inputExpression>
        <inputValues id="InputValues_1">
          <text>"3.05", "4.00", "4.01"</text>
        </inputValues>
      </input>

      <input id="Input_2" label="XML Valido">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>xmlValido</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Campos Obrigatorios Completos">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>camposObrigatoriosCompletos</text>
        </inputExpression>
      </input>

      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <rule id="Rule_Bloqueio_1">
        <description>XML mal formado ou com erros estruturais</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_2"><text>"XML TISS mal formado. Verificar tags, atributos, caracteres especiais e encoding UTF-8 conforme RN ANS 412/2016."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_3"><text>"Corrigir estrutura XML usando validador TISS. Verificar parser log para identificar erro especifico (linha/coluna)."</text></outputEntry>
        <outputEntry id="OutputEntry_B1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Bloqueio_2">
        <description>Campos obrigatorios ausentes</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>false</text></inputEntry>
        <outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_2"><text>"Campos obrigatorios ausentes no XML TISS. Todos elementos marcados como obrigatorios devem estar presentes conforme Guia de Implementacao."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_3"><text>"Revisar schema XSD TISS correspondente a versao. Preencher todos campos obrigatorios antes de reenvio."</text></outputEntry>
        <outputEntry id="OutputEntry_B2_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_B2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      <rule id="Rule_Alerta_1">
        <description>Versao TISS antiga ainda suportada</description>
        <inputEntry id="InputEntry_A1_1"><text>"3.05"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_2"><text>"Versao TISS 3.05 ainda aceita mas descontinuada. Recomenda-se migracao para TISS 4.01 conforme calendario ANS."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_3"><text>"Planejar migracao para TISS 4.01. Verificar data limite de suporte a versao 3.05 com operadora."</text></outputEntry>
        <outputEntry id="OutputEntry_A1_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_A1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Aprovado_1">
        <description>XML TISS estruturalmente valido</description>
        <inputEntry id="InputEntry_P1_1"><text>"4.00", "4.01"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_2"><text>"XML TISS estruturalmente valido. Versao atualizada, campos obrigatorios completos. Pronto para envio."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_3"><text>"Prosseguir com envio de lote TISS. Garantir assinatura digital se requerida pela operadora."</text></outputEntry>
        <outputEntry id="OutputEntry_P1_4"><text>"NENHUM"</text></outputEntry>
        <outputEntry id="OutputEntry_P1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      <rule id="Rule_Fallback_COMP_TISS_006">
        <description>Regra padrao - Requer avaliacao manual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
        <outputEntry id="OutputEntry_F_2"><text>"Validacao estrutural XML TISS requer analise manual. Verificar versao, conformidade e completude."</text></outputEntry>
        <outputEntry id="OutputEntry_F_3"><text>"Encaminhar para analise tecnica. Executar validacao completa contra schema XSD TISS oficial."</text></outputEntry>
        <outputEntry id="OutputEntry_F_4"><text>"DOC"</text></outputEntry>
        <outputEntry id="OutputEntry_F_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_TISS_006">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_TISS_006" dmnElementRef="Decision_COMP_TISS_006">
        <dc:Bounds height="80" width="180" x="160" y="100"/>
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>
