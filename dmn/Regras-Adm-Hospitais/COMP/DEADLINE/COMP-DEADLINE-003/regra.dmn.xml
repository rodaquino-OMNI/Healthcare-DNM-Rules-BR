<!--
  ============================================================================
  COMP-DEADLINE-003 - Prazo Recurso Primeira Instancia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-DEADLINE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_DEADLINE_003" name="Prazo Recurso Primeira Instancia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_DEADLINE_003" name="Validacao Prazo Recurso Primeira Instancia">
    <decisionTable id="DecisionTable_COMP_DEADLINE_003" hitPolicy="FIRST">

      <input id="Input_1" label="Dias Uteis Desde Negativa">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasUteisDesdeNegativa</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Fundamentacao Tecnica">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>fundamentacaoTecnica</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Parecer Medico Anexado">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>parecerMedicoAnexado</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Prazo de 10 dias uteis para recurso excedido</description>
        <inputEntry id="InputEntry_B1_1"><text>&gt; 10</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Prazo de 10 dias uteis para recurso de primeira instancia EXCEDIDO. Avaliar recurso de segunda instancia ou acao judicial."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Alerta critico - Menos de 2 dias uteis</description>
        <inputEntry id="InputEntry_A1_1"><text>[9..10]</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"URGENTE: Menos de 2 dias uteis para vencimento do prazo de recurso. Protocolar IMEDIATAMENTE."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Alerta proximidade - Menos de 5 dias uteis</description>
        <inputEntry id="InputEntry_A2_1"><text>[6..8]</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Atencao: Menos de 5 dias uteis para vencimento do prazo de recurso. Preparar fundamentacao tecnica."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_3">
        <description>Fundamentacao tecnica nao elaborada</description>
        <inputEntry id="InputEntry_A3_1"><text>[0..5]</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A3_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A3_2"><text>"Fundamentacao tecnica nao elaborada. Recurso requer argumentacao tecnica solida baseada em protocolos e diretrizes."</text></outputEntry>
<outputEntry id="OutputEntry_A3_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_4">
        <description>Parecer medico nao anexado</description>
        <inputEntry id="InputEntry_A4_1"><text>[0..5]</text></inputEntry>
        <inputEntry id="InputEntry_A4_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A4_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A4_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A4_2"><text>"Parecer medico nao anexado. Obter parecer do medico assistente para fortalecer o recurso."</text></outputEntry>
<outputEntry id="OutputEntry_A4_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_4_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_4_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Pronto para protocolar recurso</description>
        <inputEntry id="InputEntry_P1_1"><text>[0..5]</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Recurso de primeira instancia dentro do prazo com fundamentacao e parecer medico. Protocolar junto ao convenio."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_DEADLINE_003">
        <description>Caso padrao - Revisar recurso</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar data da negativa, fundamentacao tecnica e parecer medico para recurso."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_DEADLINE_003_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_DEADLINE_003_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_DEADLINE_003">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_DEADLINE_003" dmnElementRef="Decision_COMP_DEADLINE_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>