<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-DEADLINE-004 - Prazo Recurso Segunda Instancia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-DEADLINE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_DEADLINE_004" name="Prazo Recurso Segunda Instancia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_DEADLINE_004" name="Validacao Prazo Recurso Segunda Instancia">
    <decisionTable id="DecisionTable_COMP_DEADLINE_004" hitPolicy="FIRST">

      <input id="Input_1" label="Dias Uteis Desde Negativa R1">
        <inputExpression id="InputExpression_1" typeRef="number">
          <text>diasUteisDesdeNegativaR1</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Novas Evidencias">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>novasEvidencias</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Valor Conta Significativo">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>valorContaSignificativo</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Parecer Juridico">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>parecerJuridico</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Prazo de 10 dias uteis para recurso segunda instancia excedido</description>
        <inputEntry id="InputEntry_B1_1"><text>&gt; 10</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Prazo de 10 dias uteis para recurso de segunda instancia EXCEDIDO. Via administrativa esgotada. Avaliar acao judicial."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Alerta critico - Menos de 2 dias uteis</description>
        <inputEntry id="InputEntry_A1_1"><text>[9..10]</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"URGENTE: Menos de 2 dias uteis. Ultima chance administrativa. Protocolar recurso IMEDIATAMENTE."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Sem novas evidencias e valor nao significativo</description>
        <inputEntry id="InputEntry_A2_1"><text>[0..8]</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Sem novas evidencias e valor nao significativo. Avaliar custo-beneficio de prosseguir com recurso vs registrar perda."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_3">
        <description>Valor significativo sem parecer juridico</description>
        <inputEntry id="InputEntry_A3_1"><text>[0..8]</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A3_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A3_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A3_2"><text>"Valor significativo sem parecer juridico. Obter parecer para avaliar viabilidade de acao judicial apos esgotamento administrativo."</text></outputEntry>
<outputEntry id="OutputEntry_A3_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Pronto para recurso com novas evidencias</description>
        <inputEntry id="InputEntry_P1_1"><text>[0..8]</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Recurso segunda instancia com novas evidencias. Boa probabilidade de reversao. Protocolar."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Valor significativo com suporte juridico</description>
        <inputEntry id="InputEntry_P2_1"><text>[0..8]</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Recurso segunda instancia com valor significativo e parecer juridico. Prosseguir e preparar para eventual acao judicial."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_DEADLINE_004">
        <description>Caso padrao - Revisar recurso segunda instancia</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar data negativa R1, novas evidencias, valor da conta e parecer juridico."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_DEADLINE_004_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_DEADLINE_004_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_DEADLINE_004">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_DEADLINE_004" dmnElementRef="Decision_COMP_DEADLINE_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>