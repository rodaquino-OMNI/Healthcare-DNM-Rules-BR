{
  "ruleId": "COMP-DEADLINE-002",
  "ruleName": "Prazo Reapresentacao (30 dias)",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "DEADLINE",
  "perspective": "HOSPITAL",
  "description": "Valida prazo de 30 dias para reapresentacao apos glosa",
  "inputs": [
    {
      "name": "diasDesdeGlosa",
      "type": "number",
      "description": "Quantidade de dias desde a notificacao da glosa"
    },
    {
      "name": "correcoesRealizadas",
      "type": "boolean",
      "description": "Indica se correcoes foram realizadas"
    },
    {
      "name": "documentacaoAnexada",
      "type": "boolean",
      "description": "Indica se documentacao complementar foi anexada"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para vencimento do prazo"
    }
  ],
  "regulations": [
    {
      "code": "RN-566-2022",
      "name": "Prazos de Reapresentacao",
      "source": "ANS",
      "deadline": 30
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
