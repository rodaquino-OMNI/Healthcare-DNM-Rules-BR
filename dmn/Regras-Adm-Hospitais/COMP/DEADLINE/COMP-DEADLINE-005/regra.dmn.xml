<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-DEADLINE-005 - Prazo Recebimento Resposta
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-DEADLINE
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_DEADLINE_005" name="Prazo Recebimento Resposta" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_DEADLINE_005" name="Validacao Prazo Recebimento Resposta">
    <decisionTable id="DecisionTable_COMP_DEADLINE_005" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Solicitacao">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoSolicitacao</text>
        </inputExpression>
        <inputValues><text>"COBRANCA", "REAPRESENTACAO", "RECURSO_R1", "RECURSO_R2"</text></inputValues>
      </input>

      <input id="Input_2" label="Dias Desde Envio">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>diasDesdeEnvio</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Resposta Recebida">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>respostaRecebida</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Prosseguir_1">
        <description>Cobranca sem resposta em 30 dias - Aceite tacito</description>
        <inputEntry id="InputEntry_P1_1"><text>"COBRANCA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&gt; 30</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Cobranca sem resposta em 30 dias. ACEITE TACITO aplicavel. Registrar protocolo e considerar conta como aceita."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Reapresentacao sem resposta em 15 dias - Aceite tacito</description>
        <inputEntry id="InputEntry_P2_1"><text>"REAPRESENTACAO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>&gt; 15</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Reapresentacao sem resposta em 15 dias. ACEITE TACITO aplicavel. Considerar glosa revertida."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_3">
        <description>Recurso sem resposta em 15 dias - Aceite tacito</description>
        <inputEntry id="InputEntry_P3_1"><text>"RECURSO_R1", "RECURSO_R2"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>&gt; 15</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"Recurso sem resposta em 15 dias. ACEITE TACITO aplicavel. Considerar recurso deferido."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_3_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_3_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Cobranca proxima do prazo de resposta</description>
        <inputEntry id="InputEntry_A1_1"><text>"COBRANCA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>[25..30]</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Cobranca proxima do prazo de 30 dias para resposta. Monitorar para aplicar aceite tacito."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Reapresentacao/Recurso proximo do prazo de resposta</description>
        <inputEntry id="InputEntry_A2_1"><text>"REAPRESENTACAO", "RECURSO_R1", "RECURSO_R2"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>[12..15]</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Solicitacao proxima do prazo de 15 dias para resposta. Monitorar para aplicar aceite tacito."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_4">
        <description>Resposta do convenio recebida</description>
        <inputEntry id="InputEntry_P4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P4_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P4_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P4_2"><text>"Resposta do convenio recebida. Analisar conteudo e tomar acao apropriada."</text></outputEntry>
<outputEntry id="OutputEntry_P4_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_4_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_4_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Revisar_1">
        <description>Dentro do prazo - Aguardando resposta</description>
        <inputEntry id="InputEntry_R1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_R1_2"><text>[0..24]</text></inputEntry>
        <inputEntry id="InputEntry_R1_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_R1_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_R1_2"><text>"Aguardando resposta do convenio dentro do prazo regulamentar. Monitorar periodicamente."</text></outputEntry>
<outputEntry id="OutputEntry_R1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Revisar_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Revisar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_DEADLINE_005">
        <description>Caso padrao - Revisar status resposta</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar tipo de solicitacao, data de envio e status de resposta do convenio."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_DEADLINE_005_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_DEADLINE_005_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_DEADLINE_005">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_DEADLINE_005" dmnElementRef="Decision_COMP_DEADLINE_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>