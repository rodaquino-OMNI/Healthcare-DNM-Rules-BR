{
  "ruleId": "COMP-DEADLINE-001",
  "ruleName": "Prazo Submissao Cobranca (180 dias)",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "DEADLINE",
  "perspective": "HOSPITAL",
  "description": "Valida prazo de 180 dias para submissao de cobranca inicial ao convenio",
  "inputs": [
    {
      "name": "diasDesdeAtendimento",
      "type": "number",
      "description": "Quantidade de dias desde a data do atendimento"
    },
    {
      "name": "tipoSolicitacao",
      "type": "string",
      "values": ["COBRANCA_INICIAL", "REAPRESENTACAO", "RECURSO"],
      "description": "Tipo de solicitacao de cobranca"
    },
    {
      "name": "prazoRegulamentar",
      "type": "number",
      "description": "Prazo regulamentar aplicavel em dias"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para vencimento do prazo"
    }
  ],
  "regulations": [
    {
      "code": "RN-566-2022",
      "name": "Prazos de Autorizacao e Cobranca",
      "source": "ANS",
      "deadlines": {
        "cobranca_inicial": 180,
        "reapresentacao": 30
      }
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
