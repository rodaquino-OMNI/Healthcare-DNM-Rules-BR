<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-COUNCIL-001 - Registro CRM Medico
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-COUNCIL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_COUNCIL_001" name="Registro CRM Medico" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_COUNCIL_001" name="Validacao Registro CRM">
    <decisionTable id="DecisionTable_COMP_COUNCIL_001" hitPolicy="FIRST">

      <input id="Input_1" label="Situacao CRM">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>situacaoCRM</text>
        </inputExpression>
        <inputValues><text>"ATIVO", "SUSPENSO", "CASSADO", "INTERDITADO", "CANCELADO"</text></inputValues>
      </input>

      <input id="Input_2" label="RQE Valido">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>rqeValido</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Anuidade em Dia">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>anuidadeEmDia</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Registro CRM cassado - Bloqueio obrigatorio</description>
        <inputEntry id="InputEntry_B1_1"><text>"CASSADO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Registro CRM cassado pelo Conselho Regional de Medicina. Profissional impedido de exercer medicina. Bloqueio imediato de atendimentos."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Registro CRM suspenso - Bloqueio</description>
        <inputEntry id="InputEntry_B2_1"><text>"SUSPENSO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Registro CRM suspenso. Profissional temporariamente impedido de atender. Verificar prazo e motivo da suspensao."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Interdicao cautelar vigente - Bloqueio</description>
        <inputEntry id="InputEntry_B3_1"><text>"INTERDITADO"</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Medico sob interdicao cautelar. Conforme Resolucao CFM, profissional nao pode atender pacientes. Aguardar decisao final."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_4">
        <description>Registro CRM cancelado - Bloqueio</description>
        <inputEntry id="InputEntry_B4_1"><text>"CANCELADO"</text></inputEntry>
        <inputEntry id="InputEntry_B4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B4_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B4_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B4_2"><text>"Registro CRM cancelado (a pedido ou por falecimento). Profissional sem habilitacao legal. Bloqueio de faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_B4_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_4_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_4_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Anuidade CRM pendente - Regularizar</description>
        <inputEntry id="InputEntry_A1_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Anuidade CRM pendente de pagamento. Regularizar para evitar suspensao do registro conforme Resolucao CFM 2.148/2016."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>RQE nao registrado para especialidade - Alerta</description>
        <inputEntry id="InputEntry_A2_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"RQE (Registro de Qualificacao de Especialista) nao registrado. Medico nao pode atender como especialista. Verificar Resolucao CFM 2.162/2017."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Registro CRM ativo e regular</description>
        <inputEntry id="InputEntry_P1_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Registro CRM ativo, RQE valido e anuidade em dia. Medico habilitado para atendimento e faturamento autorizado."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_COUNCIL_001">
        <description>Caso padrao - Revisao manual necessaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Status do registro CRM requer verificacao manual. Consultar portal do CRM da UF para confirmar situacao cadastral."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_COUNCIL_001_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_COUNCIL_001_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_COUNCIL_001">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_COUNCIL_001" dmnElementRef="Decision_COMP_COUNCIL_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>