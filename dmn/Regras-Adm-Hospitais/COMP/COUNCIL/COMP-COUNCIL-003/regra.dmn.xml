<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-COUNCIL-003 - Registro CRF Farmacia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-COUNCIL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_COUNCIL_003" name="Registro CRF Farmacia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_COUNCIL_003" name="Validacao Registro CRF">
    <decisionTable id="DecisionTable_COMP_COUNCIL_003" hitPolicy="FIRST">

      <input id="Input_1" label="Situacao CRF">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>situacaoCRF</text>
        </inputExpression>
        <inputValues><text>"ATIVO", "SUSPENSO", "CASSADO", "CANCELADO", "PENDENTE"</text></inputValues>
      </input>

      <input id="Input_2" label="RT Farmacia Regular">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>rtFarmaciaRegular</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Status Licenca Especial">
        <inputExpression id="InputExpression_3" typeRef="string">
          <text>statusLicencaEspecial</text>
        </inputExpression>
        <inputValues><text>"VIGENTE", "VENCIDA", "NAO_APLICAVEL", "EM_RENOVACAO"</text></inputValues>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Registro CRF cassado - Bloqueio obrigatorio</description>
        <inputEntry id="InputEntry_B1_1"><text>"CASSADO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Registro CRF cassado. Farmaceutico impedido de exercer. Farmacia hospitalar sem habilitacao legal. Bloqueio imediato."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Registro CRF suspenso - Bloqueio</description>
        <inputEntry id="InputEntry_B2_1"><text>"SUSPENSO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Registro CRF suspenso. Farmaceutico temporariamente impedido. Verificar motivo junto ao CRF regional."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Farmacia sem responsavel tecnico - Bloqueio</description>
        <inputEntry id="InputEntry_B3_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Farmacia hospitalar sem Responsavel Tecnico regular. Conforme Lei 3.820/1960, operacao irregular. Regularizar RT."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_4">
        <description>Licenca especial vencida para controlados - Bloqueio</description>
        <inputEntry id="InputEntry_B4_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_B4_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B4_3"><text>"VENCIDA"</text></inputEntry>
<outputEntry id="OutputEntry_B4_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B4_2"><text>"Licenca especial ANVISA vencida para substancias controladas. Portaria 344/98. Dispensacao de controlados bloqueada."</text></outputEntry>
<outputEntry id="OutputEntry_B4_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_4_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_4_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Licenca especial em renovacao - Acompanhamento</description>
        <inputEntry id="InputEntry_A1_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>"EM_RENOVACAO"</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Licenca especial em processo de renovacao. Acompanhar tramitacao na ANVISA. Operacao permitida com protocolo."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Registro CRF com pendencias - Regularizar</description>
        <inputEntry id="InputEntry_A2_1"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Registro CRF com pendencias. Regularizar anuidade ou documentacao para evitar suspensao."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Farmacia com CRF e licenca especial regulares</description>
        <inputEntry id="InputEntry_P1_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>"VIGENTE"</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Registro CRF ativo, RT regular e licenca especial vigente. Farmacia hospitalar autorizada para todas as dispensacoes."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Farmacia sem necessidade de licenca especial</description>
        <inputEntry id="InputEntry_P2_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>"NAO_APLICAVEL"</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Registro CRF ativo e RT regular. Farmacia nao dispensa controlados. Operacao autorizada."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_COUNCIL_003">
        <description>Caso padrao - Revisao manual necessaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Status do registro CRF requer verificacao manual. Consultar CRF e verificar licenca especial ANVISA."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_COUNCIL_003_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_COUNCIL_003_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_COUNCIL_003">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_COUNCIL_003" dmnElementRef="Decision_COMP_COUNCIL_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>