<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-COUNCIL-005 - Registro CRP Psicologia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-COUNCIL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_COUNCIL_005" name="Registro CRP Psicologia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_COUNCIL_005" name="Validacao Registro CRP">
    <decisionTable id="DecisionTable_COMP_COUNCIL_005" hitPolicy="FIRST">

      <input id="Input_1" label="Situacao CRP">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>situacaoCRP</text>
        </inputExpression>
        <inputValues><text>"ATIVO", "SUSPENSO", "CASSADO", "CANCELADO", "PENDENTE"</text></inputValues>
      </input>

      <input id="Input_2" label="Especialidade Registrada">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>especialidadeRegistrada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Registro Atualizado">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>registroAtualizado</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Registro CRP cassado - Bloqueio obrigatorio</description>
        <inputEntry id="InputEntry_B1_1"><text>"CASSADO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Registro CRP cassado. Psicologo impedido de exercer profissao. Conforme Lei 5.766/1971, bloqueio imediato de atendimentos."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Registro CRP suspenso - Bloqueio</description>
        <inputEntry id="InputEntry_B2_1"><text>"SUSPENSO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Registro CRP suspenso. Psicologo temporariamente impedido de atender. Verificar motivo da suspensao junto ao CRP regional."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Registro CRP cancelado - Bloqueio</description>
        <inputEntry id="InputEntry_B3_1"><text>"CANCELADO"</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Registro CRP cancelado. Profissional sem habilitacao legal para exercicio da psicologia. Bloqueio de faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Especialidade nao registrada - Verificar</description>
        <inputEntry id="InputEntry_A1_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Psicologo sem titulo de especialista registrado. Conforme Resolucao CFP 013/2007, verificar necessidade para area de atuacao hospitalar."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Registro CRP desatualizado - Regularizar</description>
        <inputEntry id="InputEntry_A2_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Registro CRP com dados desatualizados. Atualizar cadastro junto ao CRP regional para evitar pendencias."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_3">
        <description>Registro CRP pendente - Regularizar</description>
        <inputEntry id="InputEntry_A3_1"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A3_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A3_2"><text>"Registro CRP com pendencias. Regularizar anuidade ou documentacao para evitar suspensao."</text></outputEntry>
<outputEntry id="OutputEntry_A3_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Psicologo com registro e especialidade regulares</description>
        <inputEntry id="InputEntry_P1_1"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Registro CRP ativo, especialidade registrada e dados atualizados. Psicologo plenamente habilitado. Faturamento autorizado."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_COUNCIL_005">
        <description>Caso padrao - Revisao manual necessaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Status do registro CRP requer verificacao manual. Consultar portal do CRP para confirmar situacao e especialidades."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_COUNCIL_005_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_COUNCIL_005_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_COUNCIL_005">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_COUNCIL_005" dmnElementRef="Decision_COMP_COUNCIL_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>