{
  "ruleId": "COMP-COUNCIL-005",
  "ruleName": "Registro CRP Psicologia",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "COUNCIL",
  "perspective": "HOSPITAL",
  "description": "Valida status do registro CRP, especialidade registrada e atualizacao cadastral",
  "inputs": [
    {
      "name": "situacaoCRP",
      "type": "string",
      "values": ["ATIVO", "SUSPENSO", "CASSADO", "CANCELADO", "PENDENTE"],
      "description": "Situacao do registro no CRP"
    },
    {
      "name": "especialidadeRegistrada",
      "type": "boolean",
      "description": "Indica se possui titulo de especialista registrado"
    },
    {
      "name": "registroAtualizado",
      "type": "boolean",
      "description": "Indica se dados cadastrais estao atualizados"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para acao"
    }
  ],
  "regulations": [
    {
      "code": "LEI-4119-1962",
      "name": "Profissao de Psicologo",
      "source": "Federal"
    },
    {
      "code": "LEI-5766-1971",
      "name": "CFP/CRP",
      "source": "Federal"
    },
    {
      "code": "RES-CFP-010-2005",
      "name": "Codigo de Etica Profissional",
      "source": "CFP"
    },
    {
      "code": "RES-CFP-013-2007",
      "name": "Titulo de Especialista",
      "source": "CFP"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
