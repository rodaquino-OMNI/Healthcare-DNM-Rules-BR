{
  "ruleId": "COMP-COUNCIL-002",
  "ruleName": "Registro COREN Enfermagem",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "COUNCIL",
  "perspective": "HOSPITAL",
  "description": "Valida status do registro COREN, categoria do profissional e responsabilidade tecnica",
  "inputs": [
    {
      "name": "categoriaCOREN",
      "type": "string",
      "values": ["ENFERMEIRO", "TECNICO", "AUXILIAR", "OBSTETRA"],
      "description": "Categoria profissional de enfermagem"
    },
    {
      "name": "situacaoRegistro",
      "type": "string",
      "values": ["ATIVO", "SUSPENSO", "CASSADO", "CANCELADO", "PENDENTE"],
      "description": "Situacao do registro no COREN"
    },
    {
      "name": "responsabilidadeTecnicaRegular",
      "type": "boolean",
      "description": "Indica se RT esta regular (aplicavel a enfermeiros)"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
