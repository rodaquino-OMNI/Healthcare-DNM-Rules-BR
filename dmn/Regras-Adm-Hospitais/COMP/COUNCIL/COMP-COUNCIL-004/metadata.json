{
  "ruleId": "COMP-COUNCIL-004",
  "ruleName": "Registro CREFITO Fisioterapia",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "COUNCIL",
  "perspective": "HOSPITAL",
  "description": "Valida status do registro CREFITO, categoria profissional e area de atuacao",
  "inputs": [
    {
      "name": "categoriaCREFITO",
      "type": "string",
      "values": ["FISIOTERAPEUTA", "TERAPEUTA_OCUPACIONAL"],
      "description": "Categoria profissional no CREFITO"
    },
    {
      "name": "situacaoRegistro",
      "type": "string",
      "values": ["ATIVO", "SUSPENSO", "CASSADO", "CANCELADO", "PENDENTE"],
      "description": "Situacao do registro no CREFITO"
    },
    {
      "name": "areaAtuacaoRegular",
      "type": "boolean",
      "description": "Indica se area de atuacao esta registrada"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para acao"
    }
  ],
  "regulations": [
    {
      "code": "DL-938-1969",
      "name": "Profissoes de Fisioterapeuta e TO",
      "source": "Federal"
    },
    {
      "code": "LEI-6316-1975",
      "name": "COFFITO/CREFITO",
      "source": "Federal"
    },
    {
      "code": "RES-COFFITO-424-2013",
      "name": "Reconhecimento de Especialidades",
      "source": "COFFITO"
    },
    {
      "code": "RES-COFFITO-387-2011",
      "name": "Atribuicoes do Fisioterapeuta",
      "source": "COFFITO"
    }
  ],
