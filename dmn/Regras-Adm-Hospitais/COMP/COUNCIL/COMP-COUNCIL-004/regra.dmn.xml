<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-COUNCIL-004 - Registro CREFITO Fisioterapia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-COUNCIL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_COUNCIL_004" name="Registro CREFITO Fisioterapia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_COUNCIL_004" name="Validacao Registro CREFITO">
    <decisionTable id="DecisionTable_COMP_COUNCIL_004" hitPolicy="FIRST">

      <input id="Input_1" label="Categoria CREFITO">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>categoriaCREFITO</text>
        </inputExpression>
        <inputValues><text>"FISIOTERAPEUTA", "TERAPEUTA_OCUPACIONAL"</text></inputValues>
      </input>

      <input id="Input_2" label="Situacao Registro">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>situacaoRegistro</text>
        </inputExpression>
        <inputValues><text>"ATIVO", "SUSPENSO", "CASSADO", "CANCELADO", "PENDENTE"</text></inputValues>
      </input>

      <input id="Input_3" label="Area Atuacao Regular">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>areaAtuacaoRegular</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Registro CREFITO cassado - Bloqueio obrigatorio</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"CASSADO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Registro CREFITO cassado. Profissional impedido de exercer fisioterapia/TO. Conforme Lei 6.316/1975, bloqueio imediato."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Registro CREFITO suspenso - Bloqueio</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"SUSPENSO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Registro CREFITO suspenso. Profissional temporariamente impedido. Verificar motivo junto ao CREFITO regional."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Atuacao em area nao registrada - Bloqueio</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Profissional atuando em area de especialidade nao registrada no CREFITO. Conforme Resolucao COFFITO 424/2013, regularizar registro."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_4">
        <description>Registro CREFITO cancelado - Bloqueio</description>
        <inputEntry id="InputEntry_B4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B4_2"><text>"CANCELADO"</text></inputEntry>
        <inputEntry id="InputEntry_B4_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B4_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B4_2"><text>"Registro CREFITO cancelado. Profissional sem habilitacao legal. Bloqueio de atendimentos e faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_B4_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_4_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_4_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Registro CREFITO com pendencias - Regularizar</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Registro CREFITO com pendencias. Regularizar anuidade ou documentacao para evitar suspensao."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Fisioterapeuta com registro regular</description>
        <inputEntry id="InputEntry_P1_1"><text>"FISIOTERAPEUTA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Registro CREFITO ativo e area de atuacao regular. Fisioterapeuta habilitado. Faturamento autorizado."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Terapeuta Ocupacional com registro regular</description>
        <inputEntry id="InputEntry_P2_1"><text>"TERAPEUTA_OCUPACIONAL"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"ATIVO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Registro CREFITO ativo. Terapeuta Ocupacional habilitado. Faturamento autorizado."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_COUNCIL_004">
        <description>Caso padrao - Revisao manual necessaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Status do registro CREFITO requer verificacao manual. Consultar CREFITO para confirmar situacao e especialidades."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_COUNCIL_004_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_COUNCIL_004_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_COUNCIL_004">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_COUNCIL_004" dmnElementRef="Decision_COMP_COUNCIL_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>