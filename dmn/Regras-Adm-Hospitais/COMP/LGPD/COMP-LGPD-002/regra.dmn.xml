<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-LGPD-002 - Anonimizacao Dados Pesquisa
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-LGPD
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_LGPD_002" name="Anonimizacao Dados Pesquisa" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_LGPD_002" name="Validacao Anonimizacao para Pesquisa">
    <decisionTable id="DecisionTable_COMP_LGPD_002" hitPolicy="FIRST">

      <input id="Input_1" label="Destino Dados">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>destinoDados</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Tipo Uso">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>tipoUso</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Tem Anonimizacao">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>temAnonimizacao</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Nivel Anonimizacao">
        <inputExpression id="InputExpression_4" typeRef="string">
          <text>nivelAnonimizacao</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Compartilhamento Externo">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>compartilhamentoExterno</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Uso para pesquisa sem anonimizacao - Bloqueio LGPD Art. 13</description>
        <inputEntry id="InputEntry_B1_1"><text>"PESQUISA"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Uso de dados para pesquisa requer anonimizacao conforme LGPD Art. 13. Aplicar tecnicas de anonimizacao antes de prosseguir."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Compartilhamento externo sem anonimizacao completa</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>"PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Compartilhamento externo com anonimizacao parcial apresenta risco de reidentificacao. Completar anonimizacao conforme LGPD Art. 12."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Uso comercial de dados sensiveis de saude</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>"COMERCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Uso comercial de dados sensiveis de saude sem anonimizacao viola LGPD Art. 11. Obter consentimento especifico ou anonimizar dados."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Anonimizacao parcial para uso interno - Revisar riscos</description>
        <inputEntry id="InputEntry_A1_1"><text>"INTERNO"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>"PARCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Anonimizacao parcial para uso interno. Avaliar risco de reidentificacao e considerar anonimizacao completa para maior seguranca."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Uso estatistico com pseudonimizacao - Documentar processo</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"ESTATISTICO"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>"PSEUDONIMIZADO"</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Dados pseudonimizados para uso estatistico. Manter chave de reversao em ambiente seguro e documentar controles de acesso."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Anonimizacao completa - Uso permitido</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>"COMPLETA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Dados completamente anonimizados. LGPD nao se aplica a dados anonimizados (Art. 12). Uso para pesquisa e estatistica permitido."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Uso assistencial interno - Permitido por base legal</description>
        <inputEntry id="InputEntry_P2_1"><text>"INTERNO"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>"ASSISTENCIAL"</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Uso assistencial interno para tutela da saude (LGPD Art. 7, VIII e Art. 11, II, f). Manter registro de acesso."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_LGPD_002">
        <description>Caso padrao - Revisao manual necessaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Cenario de anonimizacao nao previsto. Encaminhar para DPO avaliar conformidade LGPD Art. 12 e Art. 13."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_LGPD_002_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_LGPD_002_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_LGPD_002">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_LGPD_002" dmnElementRef="Decision_COMP_LGPD_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>