<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-LGPD-004 - Portabilidade Dados Paciente
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-LGPD
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_LGPD_004" name="Portabilidade Dados Paciente" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_LGPD_004" name="Validacao Portabilidade de Dados">
    <decisionTable id="DecisionTable_COMP_LGPD_004" hitPolicy="FIRST">

      <input id="Input_1" label="Solicitacao Portabilidade">
        <inputExpression id="InputExpression_1" typeRef="boolean">
          <text>solicitacaoPortabilidade</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Identidade Verificada">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>identidadeVerificada</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Formato Padrao Disponivel">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>formatoPadraoDisponivel</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Dias Desde Solicitacao">
        <inputExpression id="InputExpression_4" typeRef="number">
          <text>diasDesdeSolicitacao</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Destino Portabilidade">
        <inputExpression id="InputExpression_5" typeRef="string">
          <text>destinoPortabilidade</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Prazo de 15 dias excedido - Violacao LGPD Art. 18</description>
        <inputEntry id="InputEntry_B1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>&gt; 15</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"URGENTE: Prazo de 15 dias para portabilidade excedido (LGPD Art. 18, par. 3). Providenciar imediatamente e documentar justificativa para ANPD."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Solicitacao sem verificacao de identidade do titular</description>
        <inputEntry id="InputEntry_B2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Identidade do titular nao verificada. Exigir documento de identificacao antes de prosseguir para evitar vazamento de dados (LGPD Art. 6, VII)."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Prazo proximo ao limite - 75% consumido</description>
        <inputEntry id="InputEntry_A1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>[11..15]</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"ATENCAO: Prazo de portabilidade proximo ao limite. Restam menos de 5 dias para atender solicitacao conforme LGPD Art. 18, par. 3."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Formato estruturado nao disponivel - Adaptar dados</description>
        <inputEntry id="InputEntry_A2_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>&lt;= 10</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Dados nao disponiveis em formato estruturado padrao. Converter para formato interoperavel (JSON, XML, CSV) conforme LGPD Art. 40."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_3">
        <description>Portabilidade para operadora - Formato TISS</description>
        <inputEntry id="InputEntry_A3_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A3_5"><text>"OPERADORA"</text></inputEntry>
<outputEntry id="OutputEntry_A3_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A3_2"><text>"Portabilidade para operadora de saude. Verificar compatibilidade com padrao TISS e requisitos ANS RN 389/2015."</text></outputEntry>
<outputEntry id="OutputEntry_A3_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Solicitacao valida - Processar portabilidade</description>
        <inputEntry id="InputEntry_P1_1"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>&lt;= 10</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Solicitacao de portabilidade validada. Preparar dados em formato estruturado e enviar ao destino indicado dentro do prazo legal."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Sem solicitacao de portabilidade ativa</description>
        <inputEntry id="InputEntry_P2_1"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Nenhuma solicitacao de portabilidade ativa. Dados mantidos conforme politica de retencao vigente."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_LGPD_004">
        <description>Caso padrao - Revisao manual necessaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Solicitacao de portabilidade requer analise manual. Encaminhar para DPO avaliar viabilidade tecnica e prazo."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_LGPD_004_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_LGPD_004_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_LGPD_004">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_LGPD_004" dmnElementRef="Decision_COMP_LGPD_004">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>