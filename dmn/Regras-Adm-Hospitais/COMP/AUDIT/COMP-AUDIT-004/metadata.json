{
  "ruleId": "COMP-AUDIT-004",
  "ruleName": "Validacao de Timestamp",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "AUDIT",
  "perspective": "HOSPITAL",
  "description": "Valida formato e precisao de timestamps para ordenacao cronologica confiavel",
  "inputs": [
    {
      "name": "timestampValido",
      "type": "boolean",
      "description": "Indica se timestamp e valido"
    },
    {
      "name": "formatoIso8601",
      "type": "boolean",
      "description": "Indica se formato segue padrao ISO 8601"
    },
    {
      "name": "fusoHorarioDefinido",
      "type": "boolean",
      "description": "Indica se fuso horario esta definido"
    },
    {
      "name": "sincronizadoNtp",
      "type": "boolean",
      "description": "Indica se servidor esta sincronizado via NTP"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para correcao"
    }
  ],
  "regulations": [
    {
      "code": "ISO-8601",
      "name": "Representacao de Datas e Horas",
      "source": "ISO"
    },
    {
      "code": "RFC-5905",
      "name": "Network Time Protocol (NTP)",
      "source": "IETF"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
