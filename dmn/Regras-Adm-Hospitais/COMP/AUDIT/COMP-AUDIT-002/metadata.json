{
  "ruleId": "COMP-AUDIT-002",
  "ruleName": "Controle de Versao de Documentos",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "AUDIT",
  "perspective": "HOSPITAL",
  "description": "Valida controle de versao de documentos para rastreabilidade de alteracoes",
  "inputs": [
    {
      "name": "versaoDocumento",
      "type": "string",
      "description": "Versao atual do documento"
    },
    {
      "name": "versoesAnterioresPreservadas",
      "type": "boolean",
      "description": "Indica se versoes anteriores estao preservadas"
    },
    {
      "name": "autorAlteracaoRegistrado",
      "type": "boolean",
      "description": "Indica se autor de cada alteracao esta registrado"
    },
    {
      "name": "diffDisponivel",
      "type": "boolean",
      "description": "Indica se comparacao entre versoes esta disponivel"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para correcao"
    }
  ],
  "regulations": [
    {
      "code": "CFM-1821-2007",
      "name": "Prontuario Eletronico - Versionamento",
      "source": "CFM"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
