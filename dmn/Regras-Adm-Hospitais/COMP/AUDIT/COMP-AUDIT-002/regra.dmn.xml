<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-AUDIT-002 - Controle de Versao de Documentos
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-AUDIT
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_AUDIT_002" name="Controle de Versao de Documentos" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_AUDIT_002" name="Validacao Controle de Versao">
    <decisionTable id="DecisionTable_COMP_AUDIT_002" hitPolicy="FIRST">

      <input id="Input_1" label="Versao Documento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>versaoDocumento</text>
        </inputExpression>
      </input>

      <input id="Input_2" label="Versoes Anteriores Preservadas">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>versoesAnterioresPreservadas</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Autor Alteracao Registrado">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>autorAlteracaoRegistrado</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Diff Disponivel">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>diffDisponivel</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Versao de documento invalida ou ausente</description>
        <inputEntry id="InputEntry_B1_1"><text>null</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Versao de documento NAO DEFINIDA. Obrigatorio para controle de alteracoes. Atribuir versao."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Versoes anteriores nao preservadas</description>
        <inputEntry id="InputEntry_B2_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Versoes anteriores NAO PRESERVADAS. Historico perdido. Ativar versionamento com backup."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Autor da alteracao nao registrado</description>
        <inputEntry id="InputEntry_A1_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Autor da alteracao nao registrado. Impossivel rastrear responsavel. Configurar registro de autor."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Comparacao entre versoes indisponivel</description>
        <inputEntry id="InputEntry_A2_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Comparacao entre versoes (diff) indisponivel. Util para auditoria. Considerar implementar."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Controle de versao completo e funcional</description>
        <inputEntry id="InputEntry_P1_1"><text>not(null)</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Controle de versao completo. Historico preservado, autores registrados, diff disponivel."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_AUDIT_002">
        <description>Caso padrao - Revisar controle de versao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar configuracao de controle de versao: versionamento, backup, autoria e diff."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_AUDIT_002_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_AUDIT_002_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_AUDIT_002">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_AUDIT_002" dmnElementRef="Decision_COMP_AUDIT_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>