{
  "ruleId": "COMP-ACCRED-006",
  "ruleName": "Habilitacao Ministerio da Saude",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "ACCRED",
  "perspective": "HOSPITAL",
  "description": "Valida status da habilitacao MS para servicos de alta complexidade, portaria vigente e requisitos tecnicos",
  "inputs": [
    {
      "name": "tipoHabilitacao",
      "type": "string",
      "values": ["ONCOLOGIA", "UTI", "ALTA_COMPLEXIDADE", "TRANSPLANTE", "CARDIOVASCULAR", "NEUROLOGIA"],
      "description": "Tipo de habilitacao MS"
    },
    {
      "name": "statusPortaria",
      "type": "string",
      "values": ["VIGENTE", "REVOGADA", "SUSPENSA", "EM_RENOVACAO", "NAO_HABILITADO"],
      "description": "Status da portaria de habilitacao"
    },
    {
      "name": "requisitosPendentes",
      "type": "number",
      "description": "Quantidade de requisitos tecnicos pendentes de adequacao"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para acao"
    }
  ],
  "regulations": [
    {
      "code": "PORTARIA-MS-2395-2011",
      "name": "Habilitacao em Oncologia",
      "source": "MS"
    },
    {
      "code": "PORTARIA-MS-482-2014",
      "name": "Habilitacao UTI",
      "source": "MS"
    },
    {
      "code": "PORTARIA-MS-930-2012",
      "name": "Servicos de Alta Complexidade",
      "source": "MS"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
