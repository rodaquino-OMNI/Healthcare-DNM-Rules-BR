<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-ACCRED-001 - Certificacao ONA
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-ACCRED
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_ACCRED_001" name="Certificacao ONA" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_ACCRED_001" name="Validacao Certificacao ONA">
    <decisionTable id="DecisionTable_COMP_ACCRED_001" hitPolicy="FIRST">

      <input id="Input_1" label="Nivel ONA">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>nivelONA</text>
        </inputExpression>
        <inputValues><text>"NIVEL_1", "NIVEL_2", "NIVEL_3", "SEM_CERTIFICACAO", "SUSPENSA"</text></inputValues>
      </input>

      <input id="Input_2" label="Dias ate Validade">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>diasAteValidade</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Nao Conformidades Criticas">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>naoConformidadesCriticas</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Certificacao ONA expirada - Bloqueio obrigatorio</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&lt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Certificacao ONA expirada. Necessario renovacao imediata conforme Manual ONA. Prestador pode ter restricoes com operadoras."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Certificacao ONA suspensa - Bloqueio</description>
        <inputEntry id="InputEntry_B2_1"><text>"SUSPENSA"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Certificacao ONA suspensa pela acreditadora. Verificar motivo da suspensao e plano de regularizacao."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Nao conformidades criticas pendentes - Bloqueio</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>&gt; 3</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Excesso de nao conformidades criticas (&gt;3). Risco a seguranca do paciente. Acoes corretivas imediatas necessarias."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Certificacao vencendo em 90 dias - Alerta</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>[0..90]</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&lt;= 3</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Certificacao ONA vence em menos de 90 dias. Agendar auditoria de recertificacao. Iniciar processo de renovacao."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Nao conformidades pendentes - Monitoramento</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>&gt; 90</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>[1..3]</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Existem nao conformidades pendentes de resolucao. Acompanhar plano de acao e prazos de correcao."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Certificacao ONA Nivel 3 - Excelencia</description>
        <inputEntry id="InputEntry_P1_1"><text>"NIVEL_3"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>&gt; 90</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>0</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Certificacao ONA Nivel 3 (Excelencia) vigente. Maximo padrao de qualidade. Faturamento autorizado sem restricoes."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Certificacao ONA Nivel 1 ou 2 vigente</description>
        <inputEntry id="InputEntry_P2_1"><text>"NIVEL_1", "NIVEL_2"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>&gt; 90</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>0</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Certificacao ONA vigente e sem pendencias. Hospital acreditado. Faturamento autorizado."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_ACCRED_001">
        <description>Caso padrao - Revisao manual necessaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Status de acreditacao ONA requer verificacao manual. Conferir certificado, validade e relatorio de nao conformidades."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ACCRED_001_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ACCRED_001_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_ACCRED_001">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_ACCRED_001" dmnElementRef="Decision_COMP_ACCRED_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>