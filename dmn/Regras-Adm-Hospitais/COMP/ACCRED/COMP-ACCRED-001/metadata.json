{
  "ruleId": "COMP-ACCRED-001",
  "ruleName": "Certificacao ONA (Organizacao Nacional de Acreditacao)",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "ACCRED",
  "perspective": "HOSPITAL",
  "description": "Valida status da certificacao ONA do hospital, nivel de acreditacao, validade e nao conformidades pendentes",
  "inputs": [
    {
      "name": "nivelONA",
      "type": "string",
      "values": ["NIVEL_1", "NIVEL_2", "NIVEL_3", "SEM_CERTIFICACAO", "SUSPENSA"],
      "description": "Nivel de certificacao ONA do hospital"
    },
    {
      "name": "diasAteValidade",
      "type": "number",
      "description": "Dias restantes ate expiracao da certificacao"
    },
    {
      "name": "naoConformidadesCriticas",
      "type": "number",
      "description": "Quantidade de nao conformidades criticas pendentes"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para acao"
    }
  ],
  "regulations": [
    {
      "code": "MANUAL-ONA-2022",
      "name": "Manual da Organizacao Nacional de Acreditacao",
      "source": "ONA"
    },
    {
      "code": "RDC-63-2011",
      "name": "Requisitos de Boas Praticas de Funcionamento",
      "source": "ANVISA"
    },
    {
      "code": "PORTARIA-MS-529-2013",
      "name": "Programa Nacional de Seguranca do Paciente",
      "source": "MS"
    },
    {
      "code": "NBR-ISO-9001-2015",
      "name": "Sistema de Gestao da Qualidade",
      "source": "ABNT"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
