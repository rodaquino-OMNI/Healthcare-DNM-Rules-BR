{
  "ruleId": "COMP-ACCRED-009",
  "ruleName": "Cadastro Conselhos Profissionais (Estabelecimento)",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "ACCRED",
  "perspective": "HOSPITAL",
  "description": "Valida cadastro do estabelecimento junto aos conselhos profissionais, situacao cadastral e pendencias",
  "inputs": [
    {
      "name": "conselhoRegional",
      "type": "string",
      "values": ["CRM", "COREN", "CRF", "CREFITO", "CRP", "CRO", "CRBM"],
      "description": "Conselho regional de fiscalizacao"
    },
    {
      "name": "situacaoCadastral",
      "type": "string",
      "values": ["REGULAR", "IRREGULAR", "SUSPENSO", "PENDENTE", "CANCELADO"],
      "description": "Situacao cadastral do estabelecimento"
    },
    {
      "name": "pendenciasDocumentais",
      "type": "number",
      "description": "Quantidade de pendencias documentais"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para acao"
    }
  ],
  "regulations": [
    {
      "code": "LEI-3268-1957",
      "name": "Conselhos de Medicina",
      "source": "Federal"
    },
    {
      "code": "LEI-5905-1973",
      "name": "COFEN/COREN",
      "source": "Federal"
    },
    {
      "code": "LEI-3820-1960",
      "name": "Conselhos de Farmacia",
      "source": "Federal"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
