<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-ACCRED-009 - Cadastro Conselhos Profissionais
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-ACCRED
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_ACCRED_009" name="Cadastro Conselhos Profissionais" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_ACCRED_009" name="Validacao Cadastro Conselhos">
    <decisionTable id="DecisionTable_COMP_ACCRED_009" hitPolicy="FIRST">

      <input id="Input_1" label="Conselho Regional">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>conselhoRegional</text>
        </inputExpression>
        <inputValues><text>"CRM", "COREN", "CRF", "CREFITO", "CRP", "CRO", "CRBM"</text></inputValues>
      </input>

      <input id="Input_2" label="Situacao Cadastral">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>situacaoCadastral</text>
        </inputExpression>
        <inputValues><text>"REGULAR", "IRREGULAR", "SUSPENSO", "PENDENTE", "CANCELADO"</text></inputValues>
      </input>

      <input id="Input_3" label="Pendencias Documentais">
        <inputExpression id="InputExpression_3" typeRef="number">
          <text>pendenciasDocumentais</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Cadastro no conselho cancelado - Bloqueio</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"CANCELADO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Cadastro do estabelecimento no conselho profissional cancelado. Hospital sem autorizacao para prestacao de servicos na area."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Cadastro suspenso pelo conselho - Bloqueio</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"SUSPENSO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Cadastro do estabelecimento suspenso pelo conselho profissional. Verificar motivo e providenciar regularizacao."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Situacao cadastral irregular - Bloqueio</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>"IRREGULAR"</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Situacao cadastral irregular junto ao conselho. Pendencias impedem funcionamento regular. Providenciar regularizacao."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Pendencias documentais existentes - Alerta</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>"REGULAR", "PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>&gt; 0</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Existem pendencias documentais junto ao conselho profissional. Regularizar para evitar suspensao do cadastro."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Cadastro pendente de regularizacao - Alerta</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>"PENDENTE"</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>0</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Cadastro pendente de atualizacao junto ao conselho. Acompanhar tramitacao e fornecer documentos solicitados."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Cadastro regular no conselho</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>"REGULAR"</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>0</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Cadastro do estabelecimento regular junto ao conselho profissional. Responsabilidade tecnica definida. Operacao autorizada."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_ACCRED_009">
        <description>Caso padrao - Revisao manual necessaria</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Status do cadastro no conselho requer verificacao manual. Conferir situacao, responsabilidade tecnica e pendencias."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ACCRED_009_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ACCRED_009_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_ACCRED_009">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_ACCRED_009" dmnElementRef="Decision_COMP_ACCRED_009">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>