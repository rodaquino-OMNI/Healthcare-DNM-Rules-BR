{
  "ruleId": "COMP-ACCRED-004",
  "ruleName": "Cadastro CNES (Cadastro Nacional de Estabelecimentos de Saude)",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "ACCRED",
  "perspective": "HOSPITAL",
  "description": "Valida status do CNES, atualizacao mensal e servicos cadastrados",
  "inputs": [
    {
      "name": "statusCNES",
      "type": "string",
      "values": ["ATIVO", "INATIVO", "BLOQUEADO", "DESATUALIZADO"],
      "description": "Status atual do CNES"
    },
    {
      "name": "diasDesdeAtualizacao",
      "type": "number",
      "description": "Dias desde ultima atualizacao da FichaComplementar"
    },
    {
      "name": "servicosNaoCadastrados",
      "type": "number",
      "description": "Quantidade de servicos prestados sem cadastro no CNES"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para acao"
    }
  ],
  "regulations": [
    {
      "code": "PORTARIA-MS-1646-2015",
      "name": "Cadastro Nacional de Estabelecimentos de Saude",
      "source": "MS"
    },
    {
      "code": "PORTARIA-MS-2488-2011",
      "name": "Politica Nacional de Atencao Basica",
      "source": "MS"
    },
    {
      "code": "RN-389-2015",
      "name": "Credenciamento de Prestadores",
      "source": "ANS"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
