{
  "ruleId": "COMP-ACCRED-008",
  "ruleName": "Alvara de Funcionamento Municipal",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "ACCRED",
  "perspective": "HOSPITAL",
  "description": "Valida status do alvara de funcionamento, tipo de licenca, orgao emissor e validade",
  "inputs": [
    {
      "name": "tipoAlvara",
      "type": "string",
      "values": ["FUNCIONAMENTO", "BOMBEIROS", "HABITE_SE", "SANITARIO", "AMBIENTAL"],
      "description": "Tipo de alvara ou licenca"
    },
    {
      "name": "statusAlvara",
      "type": "string",
      "values": ["VIGENTE", "VENCIDO", "SUSPENSO", "CASSADO", "EM_RENOVACAO"],
      "description": "Status atual do alvara"
    },
    {
      "name": "diasAteValidade",
      "type": "number",
      "description": "Dias restantes ate vencimento do alvara"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para acao"
    }
  ],
  "regulations": [
    {
      "code": "CODIGO-POSTURAS-MUNICIPAL",
      "name": "Codigo de Posturas Municipal",
      "source": "Municipal"
    },
    {
      "code": "IT-BOMBEIROS",
      "name": "Instrucoes Tecnicas do Corpo de Bombeiros",
      "source": "Estadual"
    },
    {
      "code": "LC-123-2006",
      "name": "Lei do Simples Nacional",
      "source": "Federal"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
