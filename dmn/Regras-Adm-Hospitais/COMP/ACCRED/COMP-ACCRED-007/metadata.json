{
  "ruleId": "COMP-ACCRED-007",
  "ruleName": "Credenciamento Operadora de Saude",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "ACCRED",
  "perspective": "HOSPITAL",
  "description": "Valida status do credenciamento com operadora, contrato vigente e documentacao contratual",
  "inputs": [
    {
      "name": "statusContrato",
      "type": "string",
      "values": ["VIGENTE", "ENCERRADO", "SUSPENSO", "RESCINDIDO", "EM_RENOVACAO"],
      "description": "Status do contrato de credenciamento"
    },
    {
      "name": "diasAteVencimento",
      "type": "number",
      "description": "Dias restantes ate vencimento do contrato"
    },
    {
      "name": "documentacaoPendente",
      "type": "boolean",
      "description": "Indica se ha documentacao contratual pendente"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para acao"
    }
  ],
  "regulations": [
    {
      "code": "RN-363-2014",
      "name": "Contrato Prestador-Operadora",
      "source": "ANS"
    },
    {
      "code": "RN-259-2011",
      "name": "Prazo de Pagamento",
      "source": "ANS"
    },
    {
      "code": "LEI-9656-1998",
      "name": "Lei dos Planos de Saude",
      "source": "Federal"
    },
    {
      "code": "RN-124-2006",
      "name": "Referencia de Prestadores",
      "source": "ANS"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
