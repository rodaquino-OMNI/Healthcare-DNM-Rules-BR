<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-ANS-003 - Obrigacao Cobertura Emergencia
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-ANS
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_ANS_003" name="Obrigacao Cobertura Emergencia" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_ANS_003" name="Validacao Cobertura Emergencia">
    <decisionTable id="DecisionTable_COMP_ANS_003" hitPolicy="FIRST">

      <input id="Input_1" label="Carater Atendimento">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>caraterAtendimento</text>
        </inputExpression>
        <inputValues><text>"EMERGENCIA", "URGENCIA", "ELETIVO"</text></inputValues>
      </input>

      <input id="Input_2" label="Risco Imediato de Vida">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>riscoImediatoVida</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Lesao Irreparavel">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>lesaoIrreparavel</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Em Carencia">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>emCarencia</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Prosseguir_1">
        <description>Emergencia com risco imediato de vida - Art. 35-C</description>
        <inputEntry id="InputEntry_P1_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Emergencia com risco imediato de vida. Cobertura OBRIGATORIA conforme Art. 35-C Lei 9.656/98. Atender imediatamente."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Lesao irreparavel - Art. 35-C</description>
        <inputEntry id="InputEntry_P2_1"><text>"EMERGENCIA", "URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Risco de lesao irreparavel. Cobertura OBRIGATORIA conforme Art. 35-C. Atender independente de carencia."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Urgencia em carencia - Cobertura limitada</description>
        <inputEntry id="InputEntry_A1_1"><text>"URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Urgencia em periodo de carencia. Cobertura obrigatoria nas primeiras 12 horas. Apos, verificar cobertura contratual."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_1">
        <description>Procedimento eletivo em carencia</description>
        <inputEntry id="InputEntry_B1_1"><text>"ELETIVO"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Procedimento eletivo em periodo de carencia. Art. 35-C nao aplicavel. Aguardar fim da carencia ou cobranca particular."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_3">
        <description>Urgencia sem carencia - Atender normalmente</description>
        <inputEntry id="InputEntry_P3_1"><text>"URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"Urgencia sem restricao de carencia. Atendimento coberto. Solicitar autorizacao conforme RN 566/2022."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_3_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_3_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_ANS_003">
        <description>Caso padrao - Revisar classificacao</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar classificacao do atendimento e aplicabilidade do Art. 35-C Lei 9.656/98."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ANS_003_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ANS_003_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_ANS_003">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_ANS_003" dmnElementRef="Decision_COMP_ANS_003">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>