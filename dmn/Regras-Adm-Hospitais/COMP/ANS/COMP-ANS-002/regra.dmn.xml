<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-ANS-002 - Prazo Resposta Autorizacao
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-ANS
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_ANS_002" name="Prazo Resposta Autorizacao" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_ANS_002" name="Validacao Prazo Resposta Autorizacao">
    <decisionTable id="DecisionTable_COMP_ANS_002" hitPolicy="FIRST">

      <input id="Input_1" label="Tipo Solicitacao">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>tipoSolicitacao</text>
        </inputExpression>
        <inputValues><text>"EMERGENCIA", "URGENCIA", "INTERNACAO_ELETIVA", "ALTA_COMPLEXIDADE", "DEMAIS"</text></inputValues>
      </input>

      <input id="Input_2" label="Prazo Resposta (dias)">
        <inputExpression id="InputExpression_2" typeRef="number">
          <text>prazoResposta</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Resposta Recebida">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>respostaRecebida</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Emergencia sem resposta imediata - Autorizacao tacita</description>
        <inputEntry id="InputEntry_B1_1"><text>"EMERGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>&gt; 0</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Emergencia sem resposta imediata. Autorizacao tacita conforme RN 566/2022. Registrar protocolo para cobranca."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Urgencia com prazo excedido - Autorizacao tacita</description>
        <inputEntry id="InputEntry_A1_1"><text>"URGENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>&gt; 1</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Urgencia sem resposta em 24h. Autorizacao tacita aplicavel. Registrar protocolo ANS para glosa indevida."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_2">
        <description>Internacao eletiva prazo excedido</description>
        <inputEntry id="InputEntry_A2_1"><text>"INTERNACAO_ELETIVA"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>&gt; 5</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Internacao eletiva sem resposta em 5 dias uteis. Considerar autorizacao tacita ou acionar ANS."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_2_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_3">
        <description>Alta complexidade prazo excedido</description>
        <inputEntry id="InputEntry_A3_1"><text>"ALTA_COMPLEXIDADE"</text></inputEntry>
        <inputEntry id="InputEntry_A3_2"><text>&gt; 10</text></inputEntry>
        <inputEntry id="InputEntry_A3_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A3_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A3_2"><text>"Alta complexidade sem resposta em 10 dias uteis. Prazo regulamentar excedido. Acionar SAC convenio."</text></outputEntry>
<outputEntry id="OutputEntry_A3_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_3_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_4">
        <description>Demais procedimentos prazo excedido</description>
        <inputEntry id="InputEntry_A4_1"><text>"DEMAIS"</text></inputEntry>
        <inputEntry id="InputEntry_A4_2"><text>&gt; 5</text></inputEntry>
        <inputEntry id="InputEntry_A4_3"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A4_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A4_2"><text>"Procedimento sem resposta em 5 dias uteis. Considerar autorizacao tacita conforme RN 566/2022."</text></outputEntry>
<outputEntry id="OutputEntry_A4_3"><text>"PRAZO_EXCEDIDO"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_4_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_4_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Resposta recebida dentro do prazo</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Resposta do convenio recebida. Verificar conteudo da autorizacao e prosseguir com faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_ANS_002">
        <description>Caso padrao - Aguardar prazo</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Aguardando resposta do convenio dentro do prazo regulamentar. Monitorar vencimento."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ANS_002_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ANS_002_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_ANS_002">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_ANS_002" dmnElementRef="Decision_COMP_ANS_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>