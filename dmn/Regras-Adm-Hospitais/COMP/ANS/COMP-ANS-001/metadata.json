{
  "ruleId": "COMP-ANS-001",
  "ruleName": "Cobertura ROL ANS (RN 465/2021)",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "ANS",
  "perspective": "HOSPITAL",
  "description": "Valida se procedimento esta dentro do ROL ANS, possui cobertura contratual e indicacao clinica adequada",
  "inputs": [
    {
      "name": "dentroDoRolANS",
      "type": "boolean",
      "description": "Indica se procedimento consta no ROL ANS vigente"
    },
    {
      "name": "cobertoContrato",
      "type": "boolean",
      "description": "Indica se procedimento esta coberto pelo contrato do beneficiario"
    },
    {
      "name": "indicacaoClinicaValida",
      "type": "boolean",
      "description": "Indica se indicacao clinica atende DUT (Diretrizes de Utilizacao)"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para faturamento"
    }
  ],
  "regulations": [
    {
      "code": "RN-465-2021",
      "name": "Rol de Procedimentos e Eventos em Saude",
      "source": "ANS"
    },
    {
      "code": "RN-469-2021",
      "name": "Atualizacao do Rol",
      "source": "ANS"
    },
    {
      "code": "LEI-9656-1998",
      "name": "Lei dos Planos de Saude",
      "source": "Federal"
    },
    {
      "code": "LEI-14454-2022",
      "name": "Rol Exemplificativo",
      "source": "Federal"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
