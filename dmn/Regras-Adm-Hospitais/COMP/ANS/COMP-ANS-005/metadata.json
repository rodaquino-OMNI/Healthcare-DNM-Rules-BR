{
  "ruleId": "COMP-ANS-005",
  "ruleName": "Validacao Cobertura Contratual",
  "version": "1.0.0",
  "category": "COMP",
  "subcategory": "ANS",
  "perspective": "HOSPITAL",
  "description": "Valida compatibilidade entre segmentacao do plano, tipo de procedimento e cobertura contratual",
  "inputs": [
    {
      "name": "segmentacaoPlano",
      "type": "string",
      "values": ["AMBULATORIAL", "HOSPITALAR", "HOSPITALAR_OBST", "ODONTOLOGICO", "REFERENCIA"],
      "description": "Segmentacao assistencial do plano de saude"
    },
    {
      "name": "tipoProcedimento",
      "type": "string",
      "values": ["CONSULTA", "EXAME", "INTERNACAO", "CIRURGIA", "ODONTO", "PARTO"],
      "description": "Tipo de procedimento solicitado"
    },
    {
      "name": "carenciaCumprida",
      "type": "boolean",
      "description": "Indica se periodo de carencia foi cumprido"
    },
    {
      "name": "cobertoContrato",
      "type": "boolean",
      "description": "Indica se procedimento esta coberto pelo contrato especifico"
    }
  ],
  "outputs": [
    {
      "name": "resultado",
      "type": "string",
      "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]
    },
    {
      "name": "observacao",
      "type": "string",
      "description": "Mensagem explicativa da decisao"
    },
    {
      "name": "prazoStatus",
      "type": "string",
      "values": ["DENTRO_PRAZO", "ALERTA_PROXIMIDADE", "PRAZO_EXCEDIDO"]
    },
    {
      "name": "diasRestantes",
      "type": "number",
      "description": "Dias restantes para faturamento"
    }
  ],
  "regulations": [
    {
      "code": "LEI-9656-1998",
      "name": "Segmentacoes Assistenciais",
      "source": "Federal"
    },
    {
      "code": "RN-465-2021",
      "name": "Rol de Procedimentos",
      "source": "ANS"
    },
    {
      "code": "RN-438-2018",
      "name": "Mecanismos de Regulacao",
      "source": "ANS"
    }
  ],
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "author": "Sistema DMN Hospital"
}
