<?xml version='1.0' encoding='UTF-8'?>
<!--
  ============================================================================
  COMP-ANS-005 - Validacao Cobertura Contratual
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-ANS
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_ANS_005" name="Validacao Cobertura Contratual" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_ANS_005" name="Validacao Cobertura Contratual">
    <decisionTable id="DecisionTable_COMP_ANS_005" hitPolicy="FIRST">

      <input id="Input_1" label="Segmentacao Plano">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>segmentacaoPlano</text>
        </inputExpression>
        <inputValues><text>"AMBULATORIAL", "HOSPITALAR", "HOSPITALAR_OBST", "ODONTOLOGICO", "REFERENCIA"</text></inputValues>
      </input>

      <input id="Input_2" label="Tipo Procedimento">
        <inputExpression id="InputExpression_2" typeRef="string">
          <text>tipoProcedimento</text>
        </inputExpression>
        <inputValues><text>"CONSULTA", "EXAME", "INTERNACAO", "CIRURGIA", "ODONTO", "PARTO"</text></inputValues>
      </input>

      <input id="Input_3" label="Carencia Cumprida">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>carenciaCumprida</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Coberto Contrato">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>cobertoContrato</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Bloquear_1">
        <description>Internacao nao coberta por plano ambulatorial</description>
        <inputEntry id="InputEntry_B1_1"><text>"AMBULATORIAL"</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>"INTERNACAO", "CIRURGIA"</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Plano ambulatorial nao cobre internacao/cirurgia. Segmentacao incompativel. Cobranca particular ou upgrade de plano."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_1_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_2">
        <description>Odontologia nao coberta por plano hospitalar</description>
        <inputEntry id="InputEntry_B2_1"><text>"HOSPITALAR", "HOSPITALAR_OBST"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>"ODONTO"</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Plano hospitalar nao inclui cobertura odontologica. Verificar plano odontologico separado."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_2_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_3">
        <description>Parto nao coberto - plano sem obstetricia</description>
        <inputEntry id="InputEntry_B3_1"><text>"HOSPITALAR"</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>"PARTO"</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Plano hospitalar sem cobertura obstetrica. Parto nao coberto. Necessario plano Hospitalar com Obstetricia."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_3_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alertar_1">
        <description>Carencia nao cumprida - Verificar urgencia</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Carencia nao cumprida. Verificar se caso de emergencia/urgencia (Art. 35-C) ou aguardar fim da carencia."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"ALERTA_PROXIMIDADE"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Alertar_1_5"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Bloquear_4">
        <description>Procedimento nao coberto pelo contrato especifico</description>
        <inputEntry id="InputEntry_B4_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B4_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B4_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B4_4"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B4_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B4_2"><text>"Procedimento nao coberto pelo contrato do beneficiario. Verificar clausulas especificas ou cobranca particular."</text></outputEntry>
<outputEntry id="OutputEntry_B4_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_4_4"><text>"DOC"</text></outputEntry>
<outputEntry id="OutputEntry_Bloquear_4_5"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_1">
        <description>Plano Referencia - Cobertura completa</description>
        <inputEntry id="InputEntry_P1_1"><text>"REFERENCIA"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Plano Referencia com cobertura completa. Carencia cumprida. Faturamento autorizado."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_1_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Prosseguir_2">
        <description>Cobertura contratual confirmada</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Cobertura contratual confirmada e carencia cumprida. Prosseguir com faturamento."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Prosseguir_2_5"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback_COMP_ANS_005">
        <description>Caso padrao - Revisar cobertura contratual</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Verificar segmentacao do plano, tipo de procedimento, carencia e cobertura contratual especifica."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"DENTRO_PRAZO"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ANS_005_4"><text>"NENHUM"</text></outputEntry>
<outputEntry id="OutputEntry_Fallback_COMP_ANS_005_5"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_ANS_005">
      <dmndi:DMNShape id="DMNShape_Decision_COMP_ANS_005" dmnElementRef="Decision_COMP_ANS_005">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>