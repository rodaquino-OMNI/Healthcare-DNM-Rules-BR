{
  "ruleId": "COMP-INTL-001",
  "version": "1.0.0",
  "name": "Atendimento Turista Medico",
  "category": "COMP",
  "subcategory": "INTL",
  "perspective": "HOSPITAL",
  "tier": "TIER4",
  "createdAt": "2026-02-06",
  "updatedAt": "2026-02-06",
  "status": "active",
  "description": "Valida atendimento a turistas medicos verificando seguro viagem, documentacao consular e garantias financeiras",
  "businessRationale": "Proteger receita em atendimentos internacionais garantindo cobertura adequada antes de procedimentos eletivos",
  "riskLevel": "HIGH",
  "glosaRisk": "Inadimplencia de turista sem seguro ou garantia financeira",
  "inputs": [
    {"name": "paisOrigem", "type": "string", "values": ["mercosul", "europa", "usa", "asia", "africa", "outros"]},
    {"name": "seguroViagemValido", "type": "boolean"},
    {"name": "coberturaSeguroSuficiente", "type": "boolean"},
    {"name": "documentacaoConsularCompleta", "type": "boolean"},
    {"name": "garantiaFinanceiraApresentada", "type": "boolean"},
    {"name": "procedimentoEletivo", "type": "boolean"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
    {"name": "observacao", "type": "string"},
    {"name": "acaoRecomendada", "type": "string"}
  ],
  "references": ["Acordos Bilaterais Saude Mercosul", "ANVISA Tourism Health Guidelines", "Consular Requirements"]
}
