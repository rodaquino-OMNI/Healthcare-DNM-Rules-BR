<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  COMP-INTL-001 - Atendimento Turista Medico
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-INTL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_INTL_001" name="Atendimento Turista Medico" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_INTL_001" name="Validar Atendimento Turista Medico">
    <decisionTable id="DecisionTable_COMP_INTL_001" hitPolicy="FIRST">

      <input id="Input_1" label="Pais Origem">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>paisOrigem</text>
        </inputExpression>
        <inputValues><text>"mercosul", "europa", "usa", "asia", "africa", "outros"</text></inputValues>
      </input>

      <input id="Input_2" label="Seguro Viagem Valido">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>seguroViagemValido</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Cobertura Seguro Suficiente">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>coberturaSeguroSuficiente</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Documentacao Consular Completa">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>documentacaoConsularCompleta</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Garantia Financeira Apresentada">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>garantiaFinanceiraApresentada</text>
        </inputExpression>
      </input>

      <input id="Input_6" label="Procedimento Eletivo">
        <inputExpression id="InputExpression_6" typeRef="boolean">
          <text>procedimentoEletivo</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Bloquear eletivo sem seguro e sem garantia</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Procedimento eletivo para turista sem seguro viagem e sem garantia financeira. Alto risco de inadimplencia."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Exigir deposito antecipado ou apresentacao de seguro viagem valido."</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear turista sem documentacao consular completa</description>
        <inputEntry id="InputEntry_B2_1"><text>"europa", "usa", "asia", "africa", "outros"</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Documentacao consular incompleta para turista de pais nao-Mercosul. Impossibilita cobranca internacional."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Solicitar passaporte, visto e comprovante de entrada no pais."</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar cobertura de seguro parcial</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Seguro viagem com cobertura insuficiente para procedimento estimado. Risco de saldo a pagar."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Informar paciente sobre possivel co-participacao e obter concordancia."</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar emergencia turista sem documentacao completa</description>
        <inputEntry id="InputEntry_A2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Atendimento de emergencia sem documentacao completa. Coletar dados assim que estabilizado."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Registrar atendimento emergencial e coletar documentos pos-estabilizacao."</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir turista Mercosul com acordo bilateral</description>
        <inputEntry id="InputEntry_P1_1"><text>"mercosul"</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Turista Mercosul com documentacao valida. Aplicar acordo de reciprocidade para emergencias."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Registrar atendimento via formulario de reciprocidade Mercosul."</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir com seguro valido e cobertura completa</description>
        <inputEntry id="InputEntry_P2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Turista medico com seguro valido e cobertura suficiente. Cobranca direta a seguradora."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Faturar via seguradora internacional com documentacao traduzida."</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_3">
        <description>Prosseguir com garantia financeira</description>
        <inputEntry id="InputEntry_P3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P3_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_P3_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P3_2"><text>"Garantia financeira apresentada pelo turista. Prosseguir com atendimento particular."</text></outputEntry>
<outputEntry id="OutputEntry_P3_3"><text>"Faturar como particular com garantia de pagamento."</text></outputEntry>
<outputEntry id="Rule_Proceed_3_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_3_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar atendimento a turista medico."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Verificar pais de origem, seguro, documentacao consular e garantias."</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_INTL_001">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_COMP_INTL_001">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>