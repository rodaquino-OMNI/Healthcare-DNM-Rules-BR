  "description": "Valida reembolso internacional verificando conversao de moeda, traducao de documentos e documentacao exterior",
  "businessRationale": "Garantir processamento correto de reembolsos internacionais com documentacao adequada",
  "riskLevel": "MEDIUM",
  "glosaRisk": "Documentacao nao traduzida ou taxa de conversao invalida",
  "inputs": [
    {"name": "moedaOrigem", "type": "string", "values": ["USD", "EUR", "GBP", "ARS", "PYG", "UYU", "outras"]},
    {"name": "taxaConversaoValida", "type": "boolean"},
    {"name": "variacaoTaxaAceitavel", "type": "boolean"},
    {"name": "documentacaoTraduzida", "type": "boolean"},
    {"name": "traducaoJuramentada", "type": "boolean"},
    {"name": "documentacaoExteriorCompleta", "type": "boolean"}
  ],
  "outputs": [
    {"name": "resultado", "type": "string", "values": ["Prosseguir", "Bloquear", "Alertar", "Revisar"]},
    {"name": "observacao", "type": "string"},
    {"name": "acaoRecomendada", "type": "string"}
  ],
  "references": ["Acordos Internacionais Reembolso", "Banco Central PTAX", "Lei Traducao Juramentada"]
