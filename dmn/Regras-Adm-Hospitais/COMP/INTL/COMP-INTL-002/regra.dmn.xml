<?xml version='1.0' encoding='utf-8'?>
<!--
  ============================================================================
  COMP-INTL-002 - Reembolso Internacional
  ============================================================================
  Versao: 1.0.0 (2026-02-08)
  Categoria: COMP-INTL
  Perspectiva: HOSPITAL (Administrativo)
  ============================================================================
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/"
             xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/"
             xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_COMP_INTL_002" name="Reembolso Internacional" targetNamespace="http://camunda.org/schema/1.0/dmn">

  <decision id="Decision_COMP_INTL_002" name="Validar Reembolso Internacional">
    <decisionTable id="DecisionTable_COMP_INTL_002" hitPolicy="FIRST">

      <input id="Input_1" label="Moeda Origem">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>moedaOrigem</text>
        </inputExpression>
        <inputValues><text>"USD", "EUR", "GBP", "ARS", "PYG", "UYU", "outras"</text></inputValues>
      </input>

      <input id="Input_2" label="Taxa Conversao Valida">
        <inputExpression id="InputExpression_2" typeRef="boolean">
          <text>taxaConversaoValida</text>
        </inputExpression>
      </input>

      <input id="Input_3" label="Variacao Taxa Aceitavel">
        <inputExpression id="InputExpression_3" typeRef="boolean">
          <text>variacaoTaxaAceitavel</text>
        </inputExpression>
      </input>

      <input id="Input_4" label="Documentacao Traduzida">
        <inputExpression id="InputExpression_4" typeRef="boolean">
          <text>documentacaoTraduzida</text>
        </inputExpression>
      </input>

      <input id="Input_5" label="Traducao Juramentada">
        <inputExpression id="InputExpression_5" typeRef="boolean">
          <text>traducaoJuramentada</text>
        </inputExpression>
      </input>

      <input id="Input_6" label="Documentacao Exterior Completa">
        <inputExpression id="InputExpression_6" typeRef="boolean">
          <text>documentacaoExteriorCompleta</text>
        </inputExpression>
      </input>

      <!-- ========== OUTPUTS (5 STANDARD) ========== -->
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string">
        <outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues>
      </output>

      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>

      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>

      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string">
        <outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues>
      </output>

      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string">
        <outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues>
      </output>

      <!-- ========== RULES ========== -->
<rule id="Rule_Block_1">
        <description>Bloquear documentacao nao traduzida</description>
        <inputEntry id="InputEntry_B1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_4"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B1_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B1_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B1_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B1_2"><text>"Documentacao medica sem traducao para portugues. Impossibilita reembolso internacional."</text></outputEntry>
<outputEntry id="OutputEntry_B1_3"><text>"Providenciar traducao dos documentos antes de submeter reembolso."</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Block_1_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_2">
        <description>Bloquear taxa de conversao invalida</description>
        <inputEntry id="InputEntry_B2_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_2"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_B2_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B2_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_B2_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B2_2"><text>"Taxa de conversao de moeda nao validada pelo Banco Central na data do atendimento."</text></outputEntry>
<outputEntry id="OutputEntry_B2_3"><text>"Utilizar taxa PTAX do Banco Central da data do atendimento."</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Block_2_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Block_3">
        <description>Bloquear documentacao exterior incompleta</description>
        <inputEntry id="InputEntry_B3_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_B3_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_B3_6"><text>false</text></inputEntry>
<outputEntry id="OutputEntry_B3_1"><text>"Bloquear"</text></outputEntry>
<outputEntry id="OutputEntry_B3_2"><text>"Documentacao do exterior incompleta. Faltam comprovantes ou notas fiscais originais."</text></outputEntry>
<outputEntry id="OutputEntry_B3_3"><text>"Solicitar documentos originais do prestador estrangeiro."</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Block_3_outputEntry_riscoDenial"><text>"ALTO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_1">
        <description>Alertar traducao parcial</description>
        <inputEntry id="InputEntry_A1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A1_5"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A1_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A1_2"><text>"Traducao disponivel mas nao juramentada. Alguns planos exigem traducao juramentada."</text></outputEntry>
<outputEntry id="OutputEntry_A1_3"><text>"Verificar exigencia do plano e providenciar traducao juramentada se necessario."</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Alert_1_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Alert_2">
        <description>Alertar variacao na taxa de conversao</description>
        <inputEntry id="InputEntry_A2_1"><text>"USD", "EUR", "GBP"</text></inputEntry>
        <inputEntry id="InputEntry_A2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_3"><text>false</text></inputEntry>
        <inputEntry id="InputEntry_A2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_A2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_A2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_A2_1"><text>"Alertar"</text></outputEntry>
<outputEntry id="OutputEntry_A2_2"><text>"Variacao de taxa de cambio acima de 5% desde data do atendimento. Pode haver ajuste no valor."</text></outputEntry>
<outputEntry id="OutputEntry_A2_3"><text>"Documentar taxa utilizada e possivel impacto de variacao cambial."</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_alertasConformidade"><text>"DOC"</text></outputEntry>
<outputEntry id="Rule_Alert_2_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_1">
        <description>Prosseguir com documentacao completa e traducao juramentada</description>
        <inputEntry id="InputEntry_P1_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P1_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_5"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P1_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P1_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P1_2"><text>"Documentacao internacional completa com traducao juramentada. Reembolso pode ser processado."</text></outputEntry>
<outputEntry id="OutputEntry_P1_3"><text>"Submeter solicitacao de reembolso internacional com toda documentacao."</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_1_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Proceed_2">
        <description>Prosseguir moedas Mercosul com documentacao simplificada</description>
        <inputEntry id="InputEntry_P2_1"><text>"ARS", "PYG", "UYU"</text></inputEntry>
        <inputEntry id="InputEntry_P2_2"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_3"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_4"><text>true</text></inputEntry>
        <inputEntry id="InputEntry_P2_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_P2_6"><text>true</text></inputEntry>
<outputEntry id="OutputEntry_P2_1"><text>"Prosseguir"</text></outputEntry>
<outputEntry id="OutputEntry_P2_2"><text>"Reembolso de pais Mercosul com documentacao traduzida. Processo simplificado aplicavel."</text></outputEntry>
<outputEntry id="OutputEntry_P2_3"><text>"Utilizar processo de reembolso simplificado Mercosul."</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Proceed_2_outputEntry_riscoDenial"><text>"BAIXO"</text></outputEntry>
      </rule>

      
      <rule id="Rule_Fallback">
        <description>Caso padrao - revisar manualmente</description>
        <inputEntry id="InputEntry_F_1"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_2"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_3"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_4"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_5"><text>-</text></inputEntry>
        <inputEntry id="InputEntry_F_6"><text>-</text></inputEntry>
<outputEntry id="OutputEntry_F_1"><text>"Revisar"</text></outputEntry>
<outputEntry id="OutputEntry_F_2"><text>"Dados insuficientes para validar reembolso internacional."</text></outputEntry>
<outputEntry id="OutputEntry_F_3"><text>"Verificar moeda, taxa de conversao, traducao e documentacao completa."</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_alertasConformidade"><text>"NENHUM"</text></outputEntry>
<outputEntry id="Rule_Fallback_outputEntry_riscoDenial"><text>"MEDIO"</text></outputEntry>
      </rule>

    </decisionTable>
  </decision>

  <dmndi:DMNDI>
    <dmndi:DMNDiagram id="DMNDiagram_COMP_INTL_002">
      <dmndi:DMNShape id="DMNShape_Decision" dmnElementRef="Decision_COMP_INTL_002">
        <dc:Bounds height="80" width="180" x="160" y="100" />
      </dmndi:DMNShape>
    </dmndi:DMNDiagram>
  </dmndi:DMNDI>

</definitions>