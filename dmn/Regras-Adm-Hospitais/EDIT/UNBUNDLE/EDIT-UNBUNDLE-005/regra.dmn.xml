<!--
  EDIT-UNBUNDLE-005 - Material Ja Incluso no Kit Cirurgico
  Versao: 1.0.0 (2026-02-08) | Categoria: EDIT-UNBUNDLE | Perspectiva: HOSPITAL (Auditoria Administrativa)
  Refs: RN ANS 465/2021, TISS 4.01.00, Lei 9.656/1998
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_EDIT_UNBUNDLE_005" name="Material Ja Incluso no Kit Cirurgico" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_EDIT_UNBUNDLE_005" name="Material Ja Incluso no Kit Cirurgico">
    <decisionTable id="DecisionTable_EDIT_UNBUNDLE_005" hitPolicy="FIRST">
      <input id="Input_1" label="Item Incluso"><inputExpression id="InputExpression_1" typeRef="boolean"><text>itemIncluso</text></inputExpression></input>
      <input id="Input_2" label="Tipo Pacote"><inputExpression id="InputExpression_2" typeRef="string"><text>tipoPacote</text></inputExpression><inputValues id="InputValues_2"><text>"DIARIA", "CIRURGIA", "PARTO", "EXAME"</text></inputValues></input>
      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>
      <rule id="Rule_1"><description>Material incluso - bloqueio</description><inputEntry id="InputEntry_1_1"><text>true</text></inputEntry><inputEntry id="InputEntry_1_2"><text>"CIRURGIA"</text></inputEntry><outputEntry id="OutputEntry_1_1"><text>"Bloquear"</text></outputEntry><outputEntry id="OutputEntry_1_2"><text>"GLOSA CERTA: Material JA INCLUSO no kit cirurgico. Unbundling indevido."</text></outputEntry><outputEntry id="OutputEntry_1_3"><text>"Excluir material separado. Valor ja contemplado no kit."</text></outputEntry><outputEntry id="OutputEntry_1_4"><text>"VALOR"</text></outputEntry><outputEntry id="OutputEntry_1_5"><text>"CRITICO"</text></outputEntry></rule>
      <rule id="Rule_2"><description>Material separado valido - prosseguir</description><inputEntry id="InputEntry_2_1"><text>false</text></inputEntry><inputEntry id="InputEntry_2_2"><text>-</text></inputEntry><outputEntry id="OutputEntry_2_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_2_2"><text>"Material nao incluso. Cobranca separada valida."</text></outputEntry><outputEntry id="OutputEntry_2_3"><text>"Prosseguir padrao."</text></outputEntry><outputEntry id="OutputEntry_2_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_2_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_3"><description>Fallback</description><inputEntry id="InputEntry_3_1"><text>-</text></inputEntry><inputEntry id="InputEntry_3_2"><text>-</text></inputEntry><outputEntry id="OutputEntry_3_1"><text>"Revisar"</text></outputEntry><outputEntry id="OutputEntry_3_2"><text>"Analise manual necessaria."</text></outputEntry><outputEntry id="OutputEntry_3_3"><text>"Encaminhar auditoria."</text></outputEntry><outputEntry id="OutputEntry_3_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_3_5"><text>"MEDIO"</text></outputEntry></rule>
      
    </decisionTable>
  </decision>
  <dmndi:DMNDI><dmndi:DMNDiagram id="DMNDiagram_EDIT_UNBUNDLE_005"><dmndi:DMNShape id="DMNShape_EDIT_UNBUNDLE_005" dmnElementRef="Decision_EDIT_UNBUNDLE_005"><dc:Bounds height="80" width="180" x="160" y="100"/></dmndi:DMNShape></dmndi:DMNDiagram></dmndi:DMNDI>
</definitions>
