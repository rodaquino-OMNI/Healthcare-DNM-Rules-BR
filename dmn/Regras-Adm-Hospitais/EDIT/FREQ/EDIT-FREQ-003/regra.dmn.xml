<?xml version="1.0" encoding="UTF-8"?>
<!--
  EDIT-FREQ-003 - Consulta Retorno vs Nova Consulta
  Versao: 1.0.0 (2026-02-08) | Categoria: EDIT-FREQ | Perspectiva: HOSPITAL (Auditoria Administrativa)
  Refs: RN ANS 465/2021, TISS 4.01.00, Lei 9.656/1998
-->
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" id="Definitions_EDIT_FREQ_003" name="Consulta Retorno vs Nova Consulta" targetNamespace="http://camunda.org/schema/1.0/dmn">
  <decision id="Decision_EDIT_FREQ_003" name="Consulta Retorno vs Nova Consulta">
    <decisionTable id="DecisionTable_EDIT_FREQ_003" hitPolicy="FIRST">
      <input id="Input_1" label="Tipo Consulta"><inputExpression id="InputExpression_1" typeRef="string"><text>tipoConsulta</text></inputExpression><inputValues id="InputValues_1"><text>"PRIMEIRA", "RETORNO"</text></inputValues></input>
      <input id="Input_2" label="Dias Intervalo"><inputExpression id="InputExpression_2" typeRef="number"><text>diasIntervalo</text></inputExpression></input>
      
      <output id="Output_1" label="Resultado" name="resultado" typeRef="string"><outputValues><text>"Prosseguir", "Bloquear", "Alertar", "Revisar"</text></outputValues></output>
      <output id="Output_2" label="Observacao" name="observacao" typeRef="string"/>
      <output id="Output_3" label="Acao Recomendada" name="acaoRecomendada" typeRef="string"/>
      <output id="Output_4" label="Alertas Conformidade" name="alertasConformidade" typeRef="string"><outputValues><text>"NENHUM", "DUP", "FREQ", "PRAZO", "DOC", "VALOR", "CRED", "CONTRATO"</text></outputValues></output>
      <output id="Output_5" label="Risco Denial" name="riscoDenial" typeRef="string"><outputValues><text>"BAIXO", "MEDIO", "ALTO", "CRITICO"</text></outputValues></output>
      <rule id="Rule_1"><description>Retorno fora do prazo - alerta</description><inputEntry id="InputEntry_1_1"><text>"RETORNO"</text></inputEntry><inputEntry id="InputEntry_1_2"><text>&gt; 30</text></inputEntry><outputEntry id="OutputEntry_1_1"><text>"Alertar"</text></outputEntry><outputEntry id="OutputEntry_1_2"><text>"RISCO MEDIO: Consulta de retorno apos 30 dias. Verificar se deveria ser cobrada como consulta nova."</text></outputEntry><outputEntry id="OutputEntry_1_3"><text>"Revisar classificacao da consulta. Verificar convencionamento e prazos contratuais."</text></outputEntry><outputEntry id="OutputEntry_1_4"><text>"FREQ"</text></outputEntry><outputEntry id="OutputEntry_1_5"><text>"MEDIO"</text></outputEntry></rule>
      <rule id="Rule_2"><description>Retorno no prazo - prosseguir</description><inputEntry id="InputEntry_2_1"><text>"RETORNO"</text></inputEntry><inputEntry id="InputEntry_2_2"><text>&lt;= 30</text></inputEntry><outputEntry id="OutputEntry_2_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_2_2"><text>"Consulta de retorno dentro do prazo. Cobranca conforme."</text></outputEntry><outputEntry id="OutputEntry_2_3"><text>"Prosseguir padrao."</text></outputEntry><outputEntry id="OutputEntry_2_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_2_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_3"><description>Primeira consulta - prosseguir</description><inputEntry id="InputEntry_3_1"><text>"PRIMEIRA"</text></inputEntry><inputEntry id="InputEntry_3_2"><text>-</text></inputEntry><outputEntry id="OutputEntry_3_1"><text>"Prosseguir"</text></outputEntry><outputEntry id="OutputEntry_3_2"><text>"Primeira consulta. Cobranca conforme."</text></outputEntry><outputEntry id="OutputEntry_3_3"><text>"Prosseguir padrao."</text></outputEntry><outputEntry id="OutputEntry_3_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_3_5"><text>"BAIXO"</text></outputEntry></rule>
      <rule id="Rule_4"><description>Fallback</description><inputEntry id="InputEntry_4_1"><text>-</text></inputEntry><inputEntry id="InputEntry_4_2"><text>-</text></inputEntry><outputEntry id="OutputEntry_4_1"><text>"Revisar"</text></outputEntry><outputEntry id="OutputEntry_4_2"><text>"Analise manual necessaria."</text></outputEntry><outputEntry id="OutputEntry_4_3"><text>"Encaminhar auditoria."</text></outputEntry><outputEntry id="OutputEntry_4_4"><text>"NENHUM"</text></outputEntry><outputEntry id="OutputEntry_4_5"><text>"MEDIO"</text></outputEntry></rule>
      
    </decisionTable>
  </decision>
  <dmndi:DMNDI><dmndi:DMNDiagram id="DMNDiagram_EDIT_FREQ_003"><dmndi:DMNShape id="DMNShape_EDIT_FREQ_003" dmnElementRef="Decision_EDIT_FREQ_003"><dc:Bounds height="80" width="180" x="160" y="100"/></dmndi:DMNShape></dmndi:DMNDiagram></dmndi:DMNDI>
</definitions>
